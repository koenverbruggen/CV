[32m+[m		[32mReveal.slide( 2, 1 );[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal section iframe[src]' ).length, 1, 'Iframe source is set' );[m
[32m+[m		[32mReveal.slide( 2, 2 );[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal section iframe[src]' ).length, 0, 'Iframe source is not set' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'background images', function() {[m
[32m+[m		[32mvar imageSource1 = Reveal.getSlide( 0 ).getAttribute( 'data-background-image' );[m
[32m+[m		[32mvar imageSource2 = Reveal.getSlide( 1, 0 ).getAttribute( 'data-background' );[m
[32m+[m
[32m+[m		[32m// check that the images are applied to the background elements[m
[32m+[m		[32mok( Reveal.getSlideBackground( 0 ).style.backgroundImage.indexOf( imageSource1 ) !== -1, 'data-background-image worked' );[m
[32m+[m		[32mok( Reveal.getSlideBackground( 1, 0 ).style.backgroundImage.indexOf( imageSource2 ) !== -1, 'data-background worked' );[m
[32m+[m	[32m});[m
