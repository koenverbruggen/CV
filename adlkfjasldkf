[32m+[m[32m.zoomed .reveal *,[m
[32m+[m[32m.zoomed .reveal *:before,[m
[32m+[m[32m.zoomed .reveal *:after {[m
[32m+[m	[32mbackface-visibility: visible !important;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.zoomed .reveal .progress,[m
[32m+[m[32m.zoomed .reveal .controls {[m
[32m+[m	[32mopacity: 0;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.zoomed .reveal .roll span {[m
[32m+[m	[32mbackground: none;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.zoomed .reveal .roll span:after {[m
[32m+[m	[32mvisibility: hidden;[m
[32m+[m[32m}[m
