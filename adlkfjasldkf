[32m+[m[32m@mixin vertical-gradient( $top, $bottom ) {[m
[32m+[m	[32mbackground: $top;[m
[32m+[m	[32mbackground: -moz-linear-gradient( top, $top 0%, $bottom 100% );[m
[32m+[m	[32mbackground: -webkit-gradient( linear, left top, left bottom, color-stop(0%,$top), color-stop(100%,$bottom) );[m
[32m+[m	[32mbackground: -webkit-linear-gradient( top, $top 0%, $bottom 100% );[m
[32m+[m	[32mbackground: -o-linear-gradient( top, $top 0%, $bottom 100% );[m
[32m+[m	[32mbackground: -ms-linear-gradient( top, $top 0%, $bottom 100% );[m
[32m+[m	[32mbackground: linear-gradient( top, $top 0%, $bottom 100% );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m@mixin horizontal-gradient( $top, $bottom ) {[m
[32m+[m	[32mbackground: $top;[m
[32m+[m	[32mbackground: -moz-linear-gradient( left, $top 0%, $bottom 100% );[m
[32m+[m	[32mbackground: -webkit-gradient( linear, left top, right top, color-stop(0%,$top), color-stop(100%,$bottom) );[m
[32m+[m	[32mbackground: -webkit-linear-gradient( left, $top 0%, $bottom 100% );[m
[32m+[m	[32mbackground: -o-linear-gradient( left, $top 0%, $bottom 100% );[m
[32m+[m	[32mbackground: -ms-linear-gradient( left, $top 0%, $bottom 100% );[m
[32m+[m	[32mbackground: linear-gradient( left, $top 0%, $bottom 100% );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m@mixin radial-gradient( $outer, $inner, $type: circle ) {[m
[32m+[m	[32mbackground: $outer;[m
[32m+[m	[32mbackground: -moz-radial-gradient( center, $type cover,  $inner 0%, $outer 100% );[m
[32m+[m	[32mbackground: -webkit-gradient( radial, center center, 0px, center center, 100%, color-stop(0%,$inner), color-stop(100%,$outer) );[m
[32m+[m	[32mbackground: -webkit-radial-gradient( center, $type cover,  $inner 0%, $outer 100% );[m
[32m+[m	[32mbackground: -o-radial-gradient( center, $type cover,  $inner 0%, $outer 100% );[m
[32m+[m	[32mbackground: -ms-radial-gradient( center, $type cover,  $inner 0%, $outer 100% );[m
[32m+[m	[32mbackground: radial-gradient( center, $type cover,  $inner 0%, $outer 100% );[m
[32m+[m[32m}[m
