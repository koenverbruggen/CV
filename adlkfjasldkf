[32m+[m		[32mReveal.configure({ controls: true });[m
[32m+[m		[32mequal( controlsElement.style.display, 'block', 'controls are visible' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Progress', function() {[m
