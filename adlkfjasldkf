[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.configure', function() {[m
[32m+[m		[32mstrictEqual( Reveal.getConfig().loop, false, '"loop" is false to start with' );[m
[32m+[m
[32m+[m		[32mReveal.configure({ loop: true });[m
[32m+[m		[32mstrictEqual( Reveal.getConfig().loop, true, '"loop" has changed to true' );[m
[32m+[m
[32m+[m		[32mReveal.configure({ loop: false, customTestValue: 1 });[m
[32m+[m		[32mstrictEqual( Reveal.getConfig().customTestValue, 1, 'supports custom values' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.availableRoutes', function() {[m
[32m+[m		[32mReveal.slide( 0, 0 );[m
[32m+[m		[32mdeepEqual( Reveal.availableRoutes(), { left: false, up: false, down: false, right: true }, 'correct for first slide' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 1, 0 );[m
[32m+[m		[32mdeepEqual( Reveal.availableRoutes(), { left: true, up: false, down: true, right: true }, 'correct for vertical slide' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.next', function() {[m
[32m+[m		[32mReveal.slide( 0, 0 );[m
[32m+[m
[32m+[m		[32m// Step through vertical child slides[m
[32m+[m		[32mReveal.next();[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 1, v: 0, f: undefined } );[m
[32m+[m
[32m+[m		[32mReveal.next();[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 1, v: 1, f: undefined } );[m
[32m+[m
[32m+[m		[32mReveal.next();[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 1, v: 2, f: undefined } );[m
[32m+[m
[32m+[m		[32m// Step through fragments[m
[32m+[m		[32mReveal.next();[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 2, v: 0, f: -1 } );[m
[32m+[m
[32m+[m		[32mReveal.next();[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 2, v: 0, f: 0 } );[m
[32m+[m
[32m+[m		[32mReveal.next();[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 2, v: 0, f: 1 } );[m
[32m+[m
[32m+[m		[32mReveal.next();[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 2, v: 0, f: 2 } );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.next at end', function() {[m
[32m+[m		[32mReveal.slide( 3 );[m
[32m+[m
[32m+[m		[32m// We're at the end, this should have no effect[m
[32m+[m		[32mReveal.next();[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 3, v: 0, f: undefined } );[m
[32m+[m
[32m+[m		[32mReveal.next();[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 3, v: 0, f: undefined } );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m
[32m+[m	[32m// ---------------------------------------------------------------[m
[32m+[m	[32m// FRAGMENT TESTS[m
[32m+[m
[32m+[m	[32mQUnit.module( 'Fragments' );[m
[32m+[m
[32m+[m	[32mtest( 'Sliding to fragments', function() {[m
[32m+[m		[32mReveal.slide( 2, 0, -1 );[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 2, v: 0, f: -1 }, 'Reveal.slide( 2, 0, -1 )' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 2, 0, 0 );[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 2, v: 0, f: 0 }, 'Reveal.slide( 2, 0, 0 )' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 2, 0, 2 );[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 2, v: 0, f: 2 }, 'Reveal.slide( 2, 0, 2 )' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 2, 0, 1 );[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 2, v: 0, f: 1 }, 'Reveal.slide( 2, 0, 1 )' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Hiding all fragments', function() {[m
[32m+[m		[32mvar fragmentSlide = document.querySelector( '#fragment-slides>section:nth-child(1)' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 2, 0, 0 );[m
[32m+[m		[32mstrictEqual( fragmentSlide.querySelectorAll( '.fragment.visible' ).length, 1, 'one fragment visible when index is 0' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 2, 0, -1 );[m
[32m+[m		[32mstrictEqual( fragmentSlide.querySelectorAll( '.fragment.visible' ).length, 0, 'no fragments visible when index is -1' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Current fragment', function() {[m
[32m+[m		[32mvar fragmentSlide = document.querySelector( '#fragment-slides>section:nth-child(1)' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 2, 0 );[m
[32m+[m		[32mstrictEqual( fragmentSlide.querySelectorAll( '.fragment.current-fragment' ).length, 0, 'no current fragment at index -1' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 2, 0, 0 );[m
[32m+[m		[32mstrictEqual( fragmentSlide.querySelectorAll( '.fragment.current-fragment' ).length, 1, 'one current fragment at index 0' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 1, 0, 0 );[m
[32m+[m		[32mstrictEqual( fragmentSlide.querySelectorAll( '.fragment.current-fragment' ).length, 0, 'no current fragment when navigating to previous slide' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 3, 0, 0 );[m
[32m+[m		[32mstrictEqual( fragmentSlide.querySelectorAll( '.fragment.current-fragment' ).length, 0, 'no current fragment when navigating to next slide' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Stepping through fragments', function() {[m
[32m+[m		[32mReveal.slide( 2, 0, -1 );[m
[32m+[m
[32m+[m		[32m// forwards:[m
[32m+[m
[32m+[m		[32mReveal.next();[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 2, v: 0, f: 0 }, 'next() goes to next fragment' );[m
[32m+[m
[32m+[m		[32mReveal.right();[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 2, v: 0, f: 1 }, 'right() goes to next fragment' );[m
[32m+[m
[32m+[m		[32mReveal.down();[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 2, v: 0, f: 2 }, 'down() goes to next fragment' );[m
[32m+[m
[32m+[m		[32mReveal.down(); // moves to f #3[m
[32m+[m
[32m+[m		[32m// backwards:[m
[32m+[m
[32m+[m		[32mReveal.prev();[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 2, v: 0, f: 2 }, 'prev() goes to prev fragment' );[m
[32m+[m
[32m+[m		[32mReveal.left();[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 2, v: 0, f: 1 }, 'left() goes to prev fragment' );[m
[32m+[m
[32m+[m		[32mReveal.up();[m
[32m+[m		[32mdeepEqual( Reveal.getIndices(), { h: 2, v: 0, f: 0 }, 'up() goes to prev fragment' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Stepping past fragments', function() {[m
[32m+[m		[32mvar fragmentSlide = document.querySelector( '#fragment-slides>section:nth-child(1)' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 0, 0, 0 );[m
[32m+[m		[32mequal( fragmentSlide.querySelectorAll( '.fragment.visible' ).length, 0, 'no fragments visible when on previous slide' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 3, 0, 0 );[m
[32m+[m		[32mequal( fragmentSlide.querySelectorAll( '.fragment.visible' ).length, 3, 'all fragments visible when on future slide' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Fragment indices', function() {[m
[32m+[m		[32mvar fragmentSlide = document.querySelector( '#fragment-slides>section:nth-child(2)' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 3, 0, 0 );[m
[32m+[m		[32mequal( fragmentSlide.querySelectorAll( '.fragment.visible' ).length, 2, 'both fragments of same index are shown' );[m
[32m+[m
[32m+[m		[32m// This slide has three fragments, first one is index 0, second and third have index 1[m
[32m+[m		[32mReveal.slide( 2, 2, 0 );[m
[32m+[m		[32mequal( Reveal.getIndices().f, 0, 'returns correct index for first fragment' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 2, 2, 1 );[m
[32m+[m		[32mequal( Reveal.getIndices().f, 1, 'returns correct index for two fragments with same index' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Index generation', function() {[m
[32m+[m		[32mvar fragmentSlide = document.querySelector( '#fragment-slides>section:nth-child(1)' );[m
[32m+[m
[32m+[m		[32m// These have no indices defined to start with[m
[32m+[m		[32mequal( fragmentSlide.querySelectorAll( '.fragment' )[0].getAttribute( 'data-fragment-index' ), '0' );[m
[32m+[m		[32mequal( fragmentSlide.querySelectorAll( '.fragment' )[1].getAttribute( 'data-fragment-index' ), '1' );[m
[32m+[m		[32mequal( fragmentSlide.querySelectorAll( '.fragment' )[2].getAttribute( 'data-fragment-index' ), '2' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Index normalization', function() {[m
[32m+[m		[32mvar fragmentSlide = document.querySelector( '#fragment-slides>section:nth-child(3)' );[m
[32m+[m
[32m+[m		[32m// These start out as 1-4-4 and should normalize to 0-1-1[m
[32m+[m		[32mequal( fragmentSlide.querySelectorAll( '.fragment' )[0].getAttribute( 'data-fragment-index' ), '0' );[m
[32m+[m		[32mequal( fragmentSlide.querySelectorAll( '.fragment' )[1].getAttribute( 'data-fragment-index' ), '1' );[m
[32m+[m		[32mequal( fragmentSlide.querySelectorAll( '.fragment' )[2].getAttribute( 'data-fragment-index' ), '1' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32masyncTest( 'fragmentshown event', function() {[m
[32m+[m		[32mexpect( 2 );[m
[32m+[m
[32m+[m		[32mvar _onEvent = function( event ) {[m
[32m+[m			[32mok( true, 'event fired' );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mReveal.addEventListener( 'fragmentshown', _onEvent );[m
[32m+[m
[32m+[m		[32mReveal.slide( 2, 0 );[m
[32m+[m		[32mReveal.slide( 2, 0 ); // should do nothing[m
[32m+[m		[32mReveal.slide( 2, 0, 0 ); // should do nothing[m
[32m+[m		[32mReveal.next();[m
[32m+[m		[32mReveal.next();[m
[32m+[m		[32mReveal.prev(); // shouldn't fire fragmentshown[m
[32m+[m
[32m+[m		[32mstart();[m
[32m+[m
[32m+[m		[32mReveal.removeEventListener( 'fragmentshown', _onEvent );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32masyncTest( 'fragmenthidden event', function() {[m
[32m+[m		[32mexpect( 2 );[m
[32m+[m
[32m+[m		[32mvar _onEvent = function( event ) {[m
[32m+[m			[32mok( true, 'event fired' );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mReveal.addEventListener( 'fragmenthidden', _onEvent );[m
[32m+[m
[32m+[m		[32mReveal.slide( 2, 0, 2 );[m
[32m+[m		[32mReveal.slide( 2, 0, 2 ); // should do nothing[m
[32m+[m		[32mReveal.prev();[m
[32m+[m		[32mReveal.prev();[m
[32m+[m		[32mReveal.next(); // shouldn't fire fragmenthidden[m
[32m+[m
[32m+[m		[32mstart();[m
[32m+[m
[32m+[m		[32mReveal.removeEventListener( 'fragmenthidden', _onEvent );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m
[32m+[m	[32m// ---------------------------------------------------------------[m
[32m+[m	[32m// AUTO-SLIDE TESTS[m
[32m+[m
[32m+[m	[32mQUnit.module( 'Auto Sliding' );[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.isAutoSliding', function() {[m
[32m+[m		[32mstrictEqual( Reveal.isAutoSliding(), false, 'false by default' );[m
[32m+[m
[32m+[m		[32mReveal.configure({ autoSlide: 10000 });[m
[32m+[m		[32mstrictEqual( Reveal.isAutoSliding(), true, 'true after starting' );[m
[32m+[m
[32m+[m		[32mReveal.configure({ autoSlide: 0 });[m
[32m+[m		[32mstrictEqual( Reveal.isAutoSliding(), false, 'false after setting to 0' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.toggleAutoSlide', function() {[m
[32m+[m		[32mReveal.configure({ autoSlide: 10000 });[m
[32m+[m
[32m+[m		[32mReveal.toggleAutoSlide();[m
[32m+[m		[32mstrictEqual( Reveal.isAutoSliding(), false, 'false after first toggle' );[m
[32m+[m		[32mReveal.toggleAutoSlide();[m
[32m+[m		[32mstrictEqual( Reveal.isAutoSliding(), true, 'true after second toggle' );[m
[32m+[m
[32m+[m		[32mReveal.configure({ autoSlide: 0 });[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32masyncTest( 'autoslidepaused', function() {[m
[32m+[m		[32mexpect( 1 );[m
[32m+[m
[32m+[m		[32mvar _onEvent = function( event ) {[m
