[32m+[m[32m  background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%, #555a5f), color-stop(100%, #1c1e20));[m
[32m+[m[32m  background: -webkit-radial-gradient(center, circle cover, #555a5f 0%, #1c1e20 100%);[m
[32m+[m[32m  background: -o-radial-gradient(center, circle cover, #555a5f 0%, #1c1e20 100%);[m
[32m+[m[32m  background: -ms-radial-gradient(center, circle cover, #555a5f 0%, #1c1e20 100%);[m
[32m+[m[32m  background: radial-gradient(center, circle cover, #555a5f 0%, #1c1e20 100%);[m
[32m+[m[32m  background-color: #2b2b2b; }[m
[32m+[m
[32m+[m[32m.reveal {[m
[32m+[m[32m  font-family: "Lato", sans-serif;[m
[32m+[m[32m  font-size: 36px;[m
[32m+[m[32m  font-weight: 200;[m
[32m+[m[32m  letter-spacing: -0.02em;[m
[32m+[m[32m  color: #eeeeee; }[m
[32m+[m
