[32m+[m		[32m<script src="../../lib/js/head.min.js"></script>[m
[32m+[m		[32m<script src="../../js/reveal.js"></script>[m
[32m+[m
[32m+[m		[32m<script>[m
[32m+[m
[32m+[m			[32mReveal.initialize({[m
[32m+[m				[32mhistory: true,[m
[32m+[m				[32mtransition: 'linear',[m
[32m+[m
[32m+[m				[32mmath: {[m
[32m+[m					[32m// mathjax: 'http://cdn.mathjax.org/mathjax/latest/MathJax.js',[m
