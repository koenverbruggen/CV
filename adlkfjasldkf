[32m+[m				[32mclear: both;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m@media screen and (max-width: 1080px) {[m
[32m+[m				[32m#speaker-controls {[m
[32m+[m					[32mfont-size: 16px;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m@media screen and (max-width: 900px) {[m
[32m+[m				[32m#speaker-controls {[m
[32m+[m					[32mfont-size: 14px;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m@media screen and (max-width: 800px) {[m
[32m+[m				[32m#speaker-controls {[m
[32m+[m					[32mfont-size: 12px;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m</style>[m
[32m+[m	[32m</head>[m
[32m+[m
[32m+[m	[32m<body>[m
[32m+[m
[32m+[m		[32m<div id="current-slide"></div>[m
[32m+[m		[32m<div id="upcoming-slide"><span class="label">UPCOMING:</span></div>[m
[32m+[m		[32m<div id="speaker-controls">[m
[32m+[m			[32m<div class="speaker-controls-time">[m
[32m+[m				[32m<h4 class="label">Time <span class="reset-button">Click to Reset</span></h4>[m
[32m+[m				[32m<div class="clock">[m
[32m+[m					[32m<span class="clock-value">0:00 AM</span>[m
[32m+[m				[32m</div>[m
[32m+[m				[32m<div class="timer">[m
[32m+[m					[32m<span class="hours-value">00</span><span class="minutes-value">:00</span><span class="seconds-value">:00</span>[m
[32m+[m				[32m</div>[m
[32m+[m				[32m<div class="clear"></div>[m
[32m+[m			[32m</div>[m
[32m+[m
[32m+[m			[32m<div class="speaker-controls-notes hidden">[m
[32m+[m				[32m<h4 class="label">Notes</h4>[m
[32m+[m				[32m<div class="value"></div>[m
[32m+[m			[32m</div>[m
[32m+[m		[32m</div>[m
[32m+[m
[32m+[m		[32m<script src="../../plugin/markdown/marked.js"></script>[m
[32m+[m		[32m<script>[m
[32m+[m
[32m+[m			[32m(function() {[m
[32m+[m
[32m+[m				[32mvar notes,[m
[32m+[m					[32mnotesValue,[m
[32m+[m					[32mcurrentState,[m
[32m+[m					[32mcurrentSlide,[m
[32m+[m					[32mupcomingSlide,[m
[32m+[m					[32mconnected = false;[m
[32m+[m
[32m+[m				[32mwindow.addEventListener( 'message', function( event ) {[m
[32m+[m
[32m+[m					[32mvar data = JSON.parse( event.data );[m
[32m+[m
[32m+[m					[32m// Messages sent by the notes plugin inside of the main window[m
[32m+[m					[32mif( data && data.namespace === 'reveal-notes' ) {[m
[32m+[m						[32mif( data.type === 'connect' ) {[m
[32m+[m							[32mhandleConnectMessage( data );[m
[32m+[m						[32m}[m
[32m+[m						[32melse if( data.type === 'state' ) {[m
[32m+[m							[32mhandleStateMessage( data );[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m					[32m// Messages sent by the reveal.js inside of the current slide preview[m
[32m+[m					[32melse if( data && data.namespace === 'reveal' ) {[m
[32m+[m						[32mif( /ready/.test( data.eventName ) ) {[m
[32m+[m							[32m// Send a message back to notify that the handshake is complete[m
[32m+[m							[32mwindow.opener.postMessage( JSON.stringify({ namespace: 'reveal-notes', type: 'connected'} ), '*' );[m
[32m+[m						[32m}[m
[32m+[m						[32melse if( /slidechanged|fragmentshown|fragmenthidden|overviewshown|overviewhidden|paused|resumed/.test( data.eventName ) && currentState !== JSON.stringify( data.state ) ) {[m
[32m+[m							[32mwindow.opener.postMessage( JSON.stringify({ method: 'setState', args: [ data.state ]} ), '*' );[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m				[32m} );[m
[32m+[m
[32m+[m				[32m/**[m
[32m+[m				[32m * Called when the main window is trying to establish a[m
[32m+[m				[32m * connection.[m
[32m+[m				[32m */[m
[32m+[m				[32mfunction handleConnectMessage( data ) {[m
[32m+[m
[32m+[m					[32mif( connected === false ) {[m
[32m+[m						[32mconnected = true;[m
[32m+[m
[32m+[m						[32msetupIframes( data );[m
[32m+[m						[32msetupKeyboard();[m
[32m+[m						[32msetupNotes();[m
[32m+[m						[32msetupTimer();[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m/**[m
[32m+[m				[32m * Called when the main window sends an updated state.[m
[32m+[m				[32m */[m
[32m+[m				[32mfunction handleStateMessage( data ) {[m
[32m+[m
[32m+[m					[32m// Store the most recently set state to avoid circular loops[m
[32m+[m					[32m// applying the same state[m
[32m+[m					[32mcurrentState = JSON.stringify( data.state );[m
[32m+[m
[32m+[m					[32m// No need for updating the notes in case of fragment changes[m
[32m+[m					[32mif ( data.notes ) {[m
[32m+[m						[32mnotes.classList.remove( 'hidden' );[m
[32m+[m						[32mnotesValue.style.whiteSpace = data.whitespace;[m
[32m+[m						[32mif( data.markdown ) {[m
[32m+[m							[32mnotesValue.innerHTML = marked( data.notes );[m
[32m+[m						[32m}[m
[32m+[m						[32melse {[m
[32m+[m							[32mnotesValue.innerHTML = data.notes;[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m					[32melse {[m
[32m+[m						[32mnotes.classList.add( 'hidden' );[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Update the note slides[m
[32m+[m					[32mcurrentSlide.contentWindow.postMessage( JSON.stringify({ method: 'setState', args: [ data.state ] }), '*' );[m
[32m+[m					[32mupcomingSlide.contentWindow.postMessage( JSON.stringify({ method: 'setState', args: [ data.state ] }), '*' );[m
[32m+[m					[32mupcomingSlide.contentWindow.postMessage( JSON.stringify({ method: 'next' }), '*' );[m
[32m+[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Limit to max one state update per X ms[m
[32m+[m				[32mhandleStateMessage = debounce( handleStateMessage, 200 );[m
[32m+[m
[32m+[m				[32m/**[m
[32m+[m				[32m * Forward keyboard events to the current slide window.[m
[32m+[m				[32m * This enables keyboard events to work even if focus[m
[32m+[m				[32m * isn't set on the current slide iframe.[m
[32m+[m				[32m */[m
[32m+[m				[32mfunction setupKeyboard() {[m
[32m+[m
[32m+[m					[32mdocument.addEventListener( 'keydown', function( event ) {[m
[32m+[m						[32mcurrentSlide.contentWindow.postMessage( JSON.stringify({ method: 'triggerKey', args: [ event.keyCode ] }), '*' );[m
[32m+[m					[32m} );[m
[32m+[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m/**[m
[32m+[m				[32m * Creates the preview iframes.[m
[32m+[m				[32m */[m
[32m+[m				[32mfunction setupIframes( data ) {[m
[32m+[m
[32m+[m					[32mvar params = [[m
[32m+[m						[32m'receiver',[m
[32m+[m						[32m'progress=false',[m
[32m+[m						[32m'history=false',[m
[32m+[m						[32m'transition=none',[m
[32m+[m						[32m'autoSlide=0',[m
[32m+[m						[32m'backgroundTransition=none'[m
[32m+[m					[32m].join( '&' );[m
[32m+[m
[32m+[m					[32mvar hash = '#/' + data.state.indexh + '/' + data.state.indexv;[m
[32m+[m					[32mvar currentURL = data.url + '?' + params + '&postMessageEvents=true' + hash;[m
[32m+[m					[32mvar upcomingURL = data.url + '?' + params + '&controls=false' + hash;[m
[32m+[m
[32m+[m					[32mcurrentSlide = document.createElement( 'iframe' );[m
[32m+[m					[32mcurrentSlide.setAttribute( 'width', 1280 );[m
[32m+[m					[32mcurrentSlide.setAttribute( 'height', 1024 );[m
[32m+[m					[32mcurrentSlide.setAttribute( 'src', currentURL );[m
[32m+[m					[32mdocument.querySelector( '#current-slide' ).appendChild( currentSlide );[m
[32m+[m
[32m+[m					[32mupcomingSlide = document.createElement( 'iframe' );[m
[32m+[m					[32mupcomingSlide.setAttribute( 'width', 640 );[m
[32m+[m					[32mupcomingSlide.setAttribute( 'height', 512 );[m
[32m+[m					[32mupcomingSlide.setAttribute( 'src', upcomingURL );[m
[32m+[m					[32mdocument.querySelector( '#upcoming-slide' ).appendChild( upcomingSlide );[m
[32m+[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m/**[m
[32m+[m				[32m * Setup the notes UI.[m
[32m+[m				[32m */[m
[32m+[m				[32mfunction setupNotes() {[m
[32m+[m
[32m+[m					[32mnotes = document.querySelector( '.speaker-controls-notes' );[m
[32m+[m					[32mnotesValue = document.querySelector( '.speaker-controls-notes .value' );[m
[32m+[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m/**[m
[32m+[m				[32m * Create the timer and clock and start updating them[m
[32m+[m				[32m * at an interval.[m
[32m+[m				[32m */[m
[32m+[m				[32mfunction setupTimer() {[m
[32m+[m
[32m+[m					[32mvar start = new Date(),[m
[32m+[m						[32mtimeEl = document.querySelector( '.speaker-controls-time' ),[m
[32m+[m						[32mclockEl = timeEl.querySelector( '.clock-value' ),[m
[32m+[m						[32mhoursEl = timeEl.querySelector( '.hours-value' ),[m
[32m+[m						[32mminutesEl = timeEl.querySelector( '.minutes-value' ),[m
[32m+[m						[32msecondsEl = timeEl.querySelector( '.seconds-value' );[m
[32m+[m
[32m+[m					[32mfunction _updateTimer() {[m
[32m+[m
[32m+[m						[32mvar diff, hours, minutes, seconds,[m
[32m+[m							[32mnow = new Date();[m
[32m+[m
[32m+[m						[32mdiff = now.getTime() - start.getTime();[m
[32m+[m						[32mhours = Math.floor( diff / ( 1000 * 60 * 60 ) );[m
[32m+[m						[32mminutes = Math.floor( ( diff / ( 1000 * 60 ) ) % 60 );[m
[32m+[m						[32mseconds = Math.floor( ( diff / 1000 ) % 60 );[m
[32m+[m
[32m+[m						[32mclockEl.innerHTML = now.toLocaleTimeString( 'en-US', { hour12: true, hour: '2-digit', minute:'2-digit' } );[m
[32m+[m						[32mhoursEl.innerHTML = zeroPadInteger( hours );[m
[32m+[m						[32mhoursEl.className = hours > 0 ? '' : 'mute';[m
[32m+[m						[32mminutesEl.innerHTML = ':' + zeroPadInteger( minutes );[m
[32m+[m						[32mminutesEl.className = minutes > 0 ? '' : 'mute';[m
[32m+[m						[32msecondsEl.innerHTML = ':' + zeroPadInteger( seconds );[m
[32m+[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Update once directly[m
[32m+[m					[32m_updateTimer();[m
[32m+[m
[32m+[m					[32m// Then update every second[m
[32m+[m					[32msetInterval( _updateTimer, 1000 );[m
[32m+[m
[32m+[m					[32mtimeEl.addEventListener( 'click', function() {[m
[32m+[m						[32mstart = new Date();[m
[32m+[m						[32m_updateTimer();[m
[32m+[m						[32mreturn false;[m
[32m+[m					[32m} );[m
[32m+[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mfunction zeroPadInteger( num ) {[m
[32m+[m
[32m+[m					[32mvar str = '00' + parseInt( num );[m
[32m+[m					[32mreturn str.substring( str.length - 2 );[m
[32m+[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m/**[m
[32m+[m				[32m * Limits the frequency at which a function can be called.[m
[32m+[m				[32m */[m
[32m+[m				[32mfunction debounce( fn, ms ) {[m
[32m+[m
[32m+[m					[32mvar lastTime = 0,[m
[32m+[m						[32mtimeout;[m
[32m+[m
[32m+[m					[32mreturn function() {[m
[32m+[m
[32m+[m						[32mvar args = arguments;[m
[32m+[m						[32mvar context = this;[m
[32m+[m
[32m+[m						[32mclearTimeout( timeout );[m
[32m+[m
[32m+[m						[32mvar timeSinceLastCall = Date.now() - lastTime;[m
[32m+[m						[32mif( timeSinceLastCall > ms ) {[m
[32m+[m							[32mfn.apply( context, args );[m
[32m+[m							[32mlastTime = Date.now();[m
[32m+[m						[32m}[m
[32m+[m						[32melse {[m
[32m+[m							[32mtimeout = setTimeout( function() {[m
[32m+[m								[32mfn.apply( context, args );[m
[32m+[m								[32mlastTime = Date.now();[m
[32m+[m							[32m}, ms - timeSinceLastCall );[m
[32m+[m						[32m}[m
[32m+[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m})();[m
[32m+[m
[32m+[m		[32m</script>[m
[32m+[m	[32m</body>[m
[32m+[m[32m</html>[m
[1mdiff --git a/Presentation/reveal.js/plugin/notes/notes.js b/Presentation/reveal.js/plugin/notes/notes.js[m
[1mnew file mode 100644[m
[1mindex 0000000..202e73b[m
[1m--- /dev/null[m
[1m+++ b/Presentation/reveal.js/plugin/notes/notes.js[m
[36m@@ -0,0 +1,127 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * Handles opening of and synchronization with the reveal.js[m
[32m+[m[32m * notes window.[m
[32m+[m[32m *[m
[32m+[m[32m * Handshake process:[m
[32m+[m[32m * 1. This window posts 'connect' to notes window[m
[32m+[m[32m *    - Includes URL of presentation to show[m
[32m+[m[32m * 2. Notes window responds with 'connected' when it is available[m
[32m+[m[32m * 3. This window proceeds to send the current presentation state[m
[32m+[m[32m *    to the notes window[m
[32m+[m[32m */[m
[32m+[m[32mvar RevealNotes = (function() {[m
[32m+[m
[32m+[m	[32mfunction openNotes() {[m
[32m+[m		[32mvar jsFileLocation = document.querySelector('script[src$="notes.js"]').src;  // this js file path[m
[32m+[m		[32mjsFileLocation = jsFileLocation.replace(/notes\.js(\?.*)?$/, '');   // the js folder path[m
[32m+[m		[32mvar notesPopup = window.open( jsFileLocation + 'notes.html', 'reveal.js - Notes', 'width=1100,height=700' );[m
[32m+[m
[32m+[m		[32m/**[m
[32m+[m		[32m * Connect to the notes window through a postmessage handshake.[m
[32m+[m		[32m * Using postmessage enables us to work in situations where the[m
[32m+[m		[32m * origins differ, such as a presentation being opened from the[m
[32m+[m		[32m * file system.[m
[32m+[m		[32m */[m
[32m+[m		[32mfunction connect() {[m
[32m+[m			[32m// Keep trying to connect until we get a 'connected' message back[m
[32m+[m			[32mvar connectInterval = setInterval( function() {[m
[32m+[m				[32mnotesPopup.postMessage( JSON.stringify( {[m
[32m+[m					[32mnamespace: 'reveal-notes',[m
[32m+[m					[32mtype: 'connect',[m
[32m+[m					[32murl: window.location.protocol + '//' + window.location.host + window.location.pathname + window.location.search,[m
[32m+[m					[32mstate: Reveal.getState()[m
[32m+[m				[32m} ), '*' );[m
[32m+[m			[32m}, 500 );[m
[32m+[m
[32m+[m			[32mwindow.addEventListener( 'message', function( event ) {[m
[32m+[m				[32mvar data = JSON.parse( event.data );[m
[32m+[m				[32mif( data && data.namespace === 'reveal-notes' && data.type === 'connected' ) {[m
[32m+[m					[32mclearInterval( connectInterval );[m
[32m+[m					[32monConnected();[m
[32m+[m				[32m}[m
[32m+[m			[32m} );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m/**[m
[32m+[m		[32m * Posts the current slide data to the notes window[m
[32m+[m		[32m */[m
[32m+[m		[32mfunction post() {[m
[32m+[m
[32m+[m			[32mvar slideElement = Reveal.getCurrentSlide(),[m
[32m+[m				[32mnotesElement = slideElement.querySelector( 'aside.notes' );[m
[32m+[m
[32m+[m			[32mvar messageData = {[m
[32m+[m				[32mnamespace: 'reveal-notes',[m
[32m+[m				[32mtype: 'state',[m
[32m+[m				[32mnotes: '',[m
[32m+[m				[32mmarkdown: false,[m
[32m+[m				[32mwhitespace: 'normal',[m
[32m+[m				[32mstate: Reveal.getState()[m
[32m+[m			[32m};[m
[32m+[m
[32m+[m			[32m// Look for notes defined in a slide attribute[m
[32m+[m			[32mif( slideElement.hasAttribute( 'data-notes' ) ) {[m
[32m+[m				[32mmessageData.notes = slideElement.getAttribute( 'data-notes' );[m
[32m+[m				[32mmessageData.whitespace = 'pre-wrap';[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Look for notes defined in an aside element[m
[32m+[m			[32mif( notesElement ) {[m
[32m+[m				[32mmessageData.notes = notesElement.innerHTML;[m
[32m+[m				[32mmessageData.markdown = typeof notesElement.getAttribute( 'data-markdown' ) === 'string';[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mnotesPopup.postMessage( JSON.stringify( messageData ), '*' );[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m/**[m
[32m+[m		[32m * Called once we have established a connection to the notes[m
[32m+[m		[32m * window.[m
[32m+[m		[32m */[m
[32m+[m		[32mfunction onConnected() {[m
[32m+[m
[32m+[m			[32m// Monitor events that trigger a change in state[m
[32m+[m			[32mReveal.addEventListener( 'slidechanged', post );[m
[32m+[m			[32mReveal.addEventListener( 'fragmentshown', post );[m
[32m+[m			[32mReveal.addEventListener( 'fragmenthidden', post );[m
[32m+[m			[32mReveal.addEventListener( 'overviewhidden', post );[m
[32m+[m			[32mReveal.addEventListener( 'overviewshown', post );[m
[32m+[m			[32mReveal.addEventListener( 'paused', post );[m
[32m+[m			[32mReveal.addEventListener( 'resumed', post );[m
[32m+[m
[32m+[m			[32m// Post the initial state[m
[32m+[m			[32mpost();[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mconnect();[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif( !/receiver/i.test( window.location.search ) ) {[m
[32m+[m
[32m+[m		[32m// If the there's a 'notes' query set, open directly[m
[32m+[m		[32mif( window.location.search.match( /(\?|\&)notes/gi ) !== null ) {[m
[32m+[m			[32mopenNotes();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Open the notes when the 's' key is hit[m
[32m+[m		[32mdocument.addEventListener( 'keydown', function( event ) {[m
[32m+[m			[32m// Disregard the event if the target is editable or a[m
[32m+[m			[32m// modifier is present[m
[32m+[m			[32mif ( document.querySelector( ':focus' ) !== null || event.shiftKey || event.altKey || event.ctrlKey || event.metaKey ) return;[m
[32m+[m
[32m+[m			[32m// Disregard the event if keyboard is disabled[m
[32m+[m			[32mif ( Reveal.getConfig().keyboard === false ) return;[m
[32m+[m
[32m+[m			[32mif( event.keyCode === 83 ) {[m
[32m+[m				[32mevent.preventDefault();[m
[32m+[m				[32mopenNotes();[m
[32m+[m			[32m}[m
[32m+[m		[32m}, false );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn { open: openNotes };[m
[32m+[m
[32m+[m[32m})();[m
