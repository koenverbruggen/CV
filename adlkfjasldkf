[32m+[m
[32m+[m		[32mReveal.left();[m
[32m+[m		[32mnotEqual( Reveal.getIndices().h, 0, 'looped from start to end' );[m
[32m+[m
[32m+[m		[32mReveal.right();[m
[32m+[m		[32mequal( Reveal.getIndices().h, 0, 'looped from end to start' );[m
[32m+[m
[32m+[m		[32mReveal.configure({ loop: false });[m
[32m+[m	[32m});[m
