[32m+[m	[32m<body>[m
[32m+[m
[32m+[m		[32m<div class="reveal">[m
[32m+[m
[32m+[m			[32m<div class="slides">[m
[32m+[m
[32m+[m				[32m<section>[m
[32m+[m					[32m<h2>Embedded Media Test</h2>[m
[32m+[m				[32m</section>[m
[32m+[m
[32m+[m				[32m<section>[m
[32m+[m					[32m<iframe data-autoplay width="420" height="345" src="http://www.youtube.com/embed/l3RQZ4mcr1c"></iframe>[m
[32m+[m				[32m</section>[m
[32m+[m
[32m+[m				[32m<section>[m
[32m+[m					[32m<h2>Empty Slide</h2>[m
[32m+[m				[32m</section>[m
[32m+[m
[32m+[m			[32m</div>[m
[32m+[m
[32m+[m		[32m</div>[m
[32m+[m
[32m+[m		[32m<script src="../../lib/js/head.min.js"></script>[m
[32m+[m		[32m<script src="../../js/reveal.js"></script>[m
