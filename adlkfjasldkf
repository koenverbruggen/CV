[32m+[m[32m\makeMPintoPDFobject=\count144[m
[32m+[m[32m\everyMPtoPDFconversion=\toks27[m
[32m+[m[32m)[m
[32m+[m[32m\c@lstlisting=\count145[m
[32m+[m[32m\c@maskedRefs=\count146[m
[32m+[m
