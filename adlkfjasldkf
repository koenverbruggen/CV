[32m+[m				[32m</section>[m
[32m+[m
[32m+[m				[32m<section>[m
[32m+[m					[32m<h2>Pretty Code</h2>[m
[32m+[m					[32m<pre><code class="hljs" data-trim contenteditable>[m
[32m+[m[32mfunction linkify( selector ) {[m
[32m+[m[32m  if( supports3DTransforms ) {[m
[32m+[m
[32m+[m[32m    var nodes = document.querySelectorAll( selector );[m
[32m+[m
[32m+[m[32m    for( var i = 0, len = nodes.length; i &lt; len; i++ ) {[m
[32m+[m[32m      var node = nodes[i];[m
[32m+[m
[32m+[m[32m      if( !node.className ) {[m
[32m+[m[32m        node.className += ' roll';[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m					[32m</code></pre>[m
[32m+[m					[32m<p>Code syntax highlighting courtesy of <a href="http://softwaremaniacs.org/soft/highlight/en/description/">highlight.js</a>.</p>[m
[32m+[m				[32m</section>[m
[32m+[m
