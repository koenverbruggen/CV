[32m+[m[32m/**[m
[32m+[m[32m * Blood theme for reveal.js[m
[32m+[m[32m * Author: Walther http://github.com/Walther[m
[32m+[m[32m *[m
[32m+[m[32m * Designed to be used with highlight.js theme[m
[32m+[m[32m * "monokai_sublime.css" available from[m
[32m+[m[32m * https://github.com/isagalaev/highlight.js/[m
[32m+[m[32m *[m
[32m+[m[32m * For other themes, change $codeBackground accordingly.[m
[32m+[m[32m *[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m // Default mixins and settings -----------------[m
[32m+[m[32m@import "../template/mixins";[m
[32m+[m[32m@import "../template/settings";[m
[32m+[m[32m// ---------------------------------------------[m
[32m+[m
[32m+[m[32m// Include theme-specific fonts[m
[32m+[m
[32m+[m[32m@import url(https://fonts.googleapis.com/css?family=Ubuntu:300,700,300italic,700italic);[m
[32m+[m
[32m+[m[32m// Colors used in the theme[m
[32m+[m[32m$blood: #a23;[m
[32m+[m[32m$coal: #222;[m
[32m+[m[32m$codeBackground: #23241f;[m
[32m+[m
[32m+[m[32m$backgroundColor: $coal;[m
[32m+[m
[32m+[m[32m// Main text[m
[32m+[m[32m$mainFont: Ubuntu, 'sans-serif';[m
[32m+[m[32m$mainFontSize: 36px;[m
[32m+[m[32m$mainColor: #eee;[m
[32m+[m
[32m+[m[32m// Headings[m
[32m+[m[32m$headingFont: Ubuntu, 'sans-serif';[m
[32m+[m[32m$headingTextShadow: 2px 2px 2px $coal;[m
[32m+[m
[32m+[m[32m// h1 shadow, borrowed humbly from[m[41m [m
