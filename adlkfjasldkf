[32m+[m				[32mdependencies: [[m
[32m+[m					[32m{ src: '../../lib/js/classList.js', condition: function() { return !document.body.classList; } },[m
[32m+[m					[32m{ src: 'marked.js', condition: function() { return !!document.querySelector( '[data-markdown]' ); } },[m
