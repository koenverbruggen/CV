[32m+[m[32m/**[m
[32m+[m[32m * A plugin which enables rendering of math equations inside[m
[32m+[m[32m * of reveal.js slides. Essentially a thin wrapper for MathJax.[m
[32m+[m[32m *[m
[32m+[m[32m * @author Hakim El Hattab[m
[32m+[m[32m */[m
[32m+[m[32mvar RevealMath = window.RevealMath || (function(){[m
[32m+[m
[32m+[m	[32mvar options = Reveal.getConfig().math || {};[m
[32m+[m	[32moptions.mathjax = options.mathjax || 'https://cdn.mathjax.org/mathjax/latest/MathJax.js';[m
[32m+[m	[32moptions.config = options.config || 'TeX-AMS_HTML-full';[m
[32m+[m
[32m+[m	[32mloadScript( options.mathjax + '?config=' + options.config, function() {[m
[32m+[m
[32m+[m		[32mMathJax.Hub.Config({[m
[32m+[m			[32mmessageStyle: 'none',[m
[32m+[m			[32mtex2jax: {[m
[32m+[m				[32minlineMath: [['$','$'],['\\(','\\)']] ,[m
[32m+[m				[32mskipTags: ['script','noscript','style','textarea','pre'][m
[32m+[m			[32m},[m
[32m+[m			[32mskipStartupTypeset: true[m
[32m+[m		[32m});[m
[32m+[m
[32m+[m		[32m// Typeset followed by an immediate reveal.js layout since[m
[32m+[m		[32m// the typesetting process could affect slide height[m
[32m+[m		[32mMathJax.Hub.Queue( [ 'Typeset', MathJax.Hub ] );[m
[32m+[m		[32mMathJax.Hub.Queue( Reveal.layout );[m
[32m+[m
[32m+[m		[32m// Reprocess equations in slides when they turn visible[m
[32m+[m		[32mReveal.addEventListener( 'slidechanged', function( event ) {[m
[32m+[m
[32m+[m			[32mMathJax.Hub.Queue( [ 'Typeset', MathJax.Hub, event.currentSlide ] );[m
[32m+[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32m} );[m
[32m+[m
[32m+[m	[32mfunction loadScript( url, callback ) {[m
[32m+[m
[32m+[m		[32mvar head = document.querySelector( 'head' );[m
[32m+[m		[32mvar script = document.createElement( 'script' );[m
[32m+[m		[32mscript.type = 'text/javascript';[m
[32m+[m		[32mscript.src = url;[m
[32m+[m
[32m+[m		[32m// Wrapper for callback to make sure it only fires once[m
[32m+[m		[32mvar finish = function() {[m
[32m+[m			[32mif( typeof callback === 'function' ) {[m
[32m+[m				[32mcallback.call();[m
[32m+[m				[32mcallback = null;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mscript.onload = finish;[m
[32m+[m
[32m+[m		[32m// IE[m
[32m+[m		[32mscript.onreadystatechange = function() {[m
[32m+[m			[32mif ( this.readyState === 'loaded' ) {[m
[32m+[m				[32mfinish();[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Normal browsers[m
[32m+[m		[32mhead.appendChild( script );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m})();[m
