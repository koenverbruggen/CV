[32m+[m[32m/**[m
[32m+[m[32m * phantomjs script for printing presentations to PDF.[m
[32m+[m[32m *[m
[32m+[m[32m * Example:[m
[32m+[m[32m * phantomjs print-pdf.js "http://lab.hakim.se/reveal-js?print-pdf" reveal-demo.pdf[m
[32m+[m[32m *[m
[32m+[m[32m * By Manuel Bieh (https://github.com/manuelbieh)[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m// html2pdf.js[m
[32m+[m[32mvar page = new WebPage();[m
[32m+[m[32mvar system = require( 'system' );[m
[32m+[m
[32m+[m[32mvar slideWidth = system.args[3] ? system.args[3].split( 'x' )[0] : 960;[m
[32m+[m[32mvar slideHeight = system.args[3] ? system.args[3].split( 'x' )[1] : 700;[m
[32m+[m
[32m+[m[32mpage.viewportSize = {[m
[32m+[m	[32mwidth: slideWidth,[m
[32m+[m	[32mheight: slideHeight[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// TODO[m
[32m+[m[32m// Something is wrong with these config values. An input[m
[32m+[m[32m// paper width of 1920px actually results in a 756px wide[m
[32m+[m[32m// PDF.[m
[32m+[m[32mpage.paperSize = {[m
[32m+[m	[32mwidth: Math.round( slideWidth * 2 ),[m
[32m+[m	[32mheight: Math.round( slideHeight * 2 ),[m
[32m+[m	[32mborder: 0[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar inputFile = system.args[1] || 'index.html?print-pdf';[m
[32m+[m[32mvar outputFile = system.args[2] || 'slides.pdf';[m
[32m+[m
[32m+[m[32mif( outputFile.match( /\.pdf$/gi ) === null ) {[m
[32m+[m	[32moutputFile += '.pdf';[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mconsole.log( 'Printing PDF (Paper size: '+ page.paperSize.width + 'x' + page.paperSize.height +')' );[m
[32m+[m
[32m+[m[32mpage.open( inputFile, function( status ) {[m
[32m+[m	[32mwindow.setTimeout( function() {[m
[32m+[m		[32mconsole.log( 'Printed succesfully' );[m
[32m+[m		[32mpage.render( outputFile );[m
[32m+[m		[32mphantom.exit();[m
[32m+[m	[32m}, 1000 );[m
[32m+[m[32m} );[m
[32m+[m
