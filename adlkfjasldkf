[32m+[m	[32m        transition-duration: 1200ms;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* Slide-specific transition speed overrides */[m
[32m+[m[32m.reveal .slides section[data-transition-speed="fast"] {[m
[32m+[m	[32m-webkit-transition-duration: 400ms;[m
[32m+[m	[32m   -moz-transition-duration: 400ms;[m
[32m+[m	[32m    -ms-transition-duration: 400ms;[m
[32m+[m	[32m        transition-duration: 400ms;[m
[32m+[m[32m}[m
[32m+[m[32m.reveal .slides section[data-transition-speed="slow"] {[m
