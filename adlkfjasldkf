[32m+[m[32m// Base theme template for reveal.js[m
[32m+[m
[32m+[m[32m/*********************************************[m
[32m+[m[32m * GLOBAL STYLES[m
[32m+[m[32m *********************************************/[m
[32m+[m
[32m+[m[32mbody {[m
[32m+[m	[32m@include bodyBackground();[m
[32m+[m	[32mbackground-color: $backgroundColor;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.reveal {[m
[32m+[m	[32mfont-family: $mainFont;[m
[32m+[m	[32mfont-size: $mainFontSize;[m
[32m+[m	[32mfont-weight: 200;[m
[32m+[m	[32mletter-spacing: -0.02em;[m
[32m+[m	[32mcolor: $mainColor;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m::selection {[m
[32m+[m	[32mcolor: $selectionColor;[m
[32m+[m	[32mbackground: $selectionBackgroundColor;[m
[32m+[m	[32mtext-shadow: none;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*********************************************[m
[32m+[m[32m * HEADERS[m
[32m+[m[32m *********************************************/[m
[32m+[m
[32m+[m[32m.reveal h1,[m
[32m+[m[32m.reveal h2,[m
[32m+[m[32m.reveal h3,[m
[32m+[m[32m.reveal h4,[m
[32m+[m[32m.reveal h5,[m
[32m+[m[32m.reveal h6 {[m
[32m+[m	[32mmargin: $headingMargin;[m
[32m+[m	[32mcolor: $headingColor;[m
[32m+[m
[32m+[m	[32mfont-family: $headingFont;[m
[32m+[m	[32mline-height: $headingLineHeight;[m
[32m+[m	[32mletter-spacing: $headingLetterSpacing;[m
[32m+[m
[32m+[m	[32mtext-transform: $headingTextTransform;[m
[32m+[m	[32mtext-shadow: $headingTextShadow;[m
