[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Starts up the presentation if the client is capable.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction initialize( options ) {[m
[32m+[m
[32m+[m		[32mcheckCapabilities();[m
[32m+[m
[32m+[m		[32mif( !features.transforms2d && !features.transforms3d ) {[m
[32m+[m			[32mdocument.body.setAttribute( 'class', 'no-transforms' );[m
[32m+[m
[32m+[m			[32m// Since JS won't be running any further, we load all lazy[m
[32m+[m			[32m// loading elements upfront[m
[32m+[m			[32mvar images = toArray( document.getElementsByTagName( 'img' ) ),[m
[32m+[m				[32miframes = toArray( document.getElementsByTagName( 'iframe' ) );[m
[32m+[m
[32m+[m			[32mvar lazyLoadable = images.concat( iframes );[m
[32m+[m
[32m+[m			[32mfor( var i = 0, len = lazyLoadable.length; i < len; i++ ) {[m
[32m+[m				[32mvar element = lazyLoadable[i];[m
[32m+[m				[32mif( element.getAttribute( 'data-src' ) ) {[m
[32m+[m					[32melement.setAttribute( 'src', element.getAttribute( 'data-src' ) );[m
[32m+[m					[32melement.removeAttribute( 'data-src' );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// If the browser doesn't support core features we won't be[m
[32m+[m			[32m// using JavaScript to control the presentation[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Cache references to key DOM elements[m
[32m+[m		[32mdom.wrapper = document.querySelector( '.reveal' );[m
[32m+[m		[32mdom.slides = document.querySelector( '.reveal .slides' );[m
[32m+[m
[32m+[m		[32m// Force a layout when the whole page, incl fonts, has loaded[m
[32m+[m		[32mwindow.addEventListener( 'load', layout, false );[m
[32m+[m
[32m+[m		[32mvar query = Reveal.getQueryHash();[m
[32m+[m
[32m+[m		[32m// Do not accept new dependencies via query config to avoid[m
[32m+[m		[32m// the potential of malicious script injection[m
[32m+[m		[32mif( typeof query['dependencies'] !== 'undefined' ) delete query['dependencies'];[m
[32m+[m
[32m+[m		[32m// Copy options over to our config object[m
[32m+[m		[32mextend( config, options );[m
[32m+[m		[32mextend( config, query );[m
[32m+[m
[32m+[m		[32m// Hide the address bar in mobile browsers[m
[32m+[m		[32mhideAddressBar();[m
[32m+[m
[32m+[m		[32m// Loads the dependencies and continues to #start() once done[m
[32m+[m		[32mload();[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Inspect the client to see what it's capable of, this[m
[32m+[m	[32m * should only happens once per runtime.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction checkCapabilities() {[m
[32m+[m
[32m+[m		[32mfeatures.transforms3d = 'WebkitPerspective' in document.body.style ||[m
[32m+[m								[32m'MozPerspective' in document.body.style ||[m
[32m+[m								[32m'msPerspective' in document.body.style ||[m
[32m+[m								[32m'OPerspective' in document.body.style ||[m
[32m+[m								[32m'perspective' in document.body.style;[m
[32m+[m
[32m+[m		[32mfeatures.transforms2d = 'WebkitTransform' in document.body.style ||[m
[32m+[m								[32m'MozTransform' in document.body.style ||[m
[32m+[m								[32m'msTransform' in document.body.style ||[m
[32m+[m								[32m'OTransform' in document.body.style ||[m
[32m+[m								[32m'transform' in document.body.style;[m
[32m+[m
[32m+[m		[32mfeatures.requestAnimationFrameMethod = window.requestAnimationFrame || window.webkitRequestAnimationFrame || window.mozRequestAnimationFrame;[m
[32m+[m		[32mfeatures.requestAnimationFrame = typeof features.requestAnimationFrameMethod === 'function';[m
[32m+[m
[32m+[m		[32mfeatures.canvas = !!document.createElement( 'canvas' ).getContext;[m
[32m+[m
[32m+[m		[32mfeatures.touch = !!( 'ontouchstart' in window );[m
[32m+[m
[32m+[m		[32m// Transitions in the overview are disabled in desktop and[m
[32m+[m		[32m// mobile Safari due to lag[m
[32m+[m		[32mfeatures.overviewTransitions = !/Version\/[\d\.]+.*Safari/.test( navigator.userAgent );[m
[32m+[m
[32m+[m		[32misMobileDevice = /(iphone|ipod|ipad|android)/gi.test( navigator.userAgent );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Loads the dependencies of reveal.js. Dependencies are[m
[32m+[m[32m     * defined via the configuration option 'dependencies'[m
[32m+[m[32m     * and will be loaded prior to starting/binding reveal.js.[m
[32m+[m[32m     * Some dependencies may have an 'async' flag, if so they[m
[32m+[m[32m     * will load after reveal.js has been started up.[m
[32m+[m[32m     */[m
[32m+[m	[32mfunction load() {[m
[32m+[m
[32m+[m		[32mvar scripts = [],[m
[32m+[m			[32mscriptsAsync = [],[m
[32m+[m			[32mscriptsToPreload = 0;[m
[32m+[m
[32m+[m		[32m// Called once synchronous scripts finish loading[m
[32m+[m		[32mfunction proceed() {[m
[32m+[m			[32mif( scriptsAsync.length ) {[m
[32m+[m				[32m// Load asynchronous scripts[m
[32m+[m				[32mhead.js.apply( null, scriptsAsync );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mstart();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mfunction loadScript( s ) {[m
[32m+[m			[32mhead.ready( s.src.match( /([\w\d_\-]*)\.?js$|[^\\\/]*$/i )[0], function() {[m
[32m+[m				[32m// Extension may contain callback functions[m
[32m+[m				[32mif( typeof s.callback === 'function' ) {[m
[32m+[m					[32ms.callback.apply( this );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif( --scriptsToPreload === 0 ) {[m
[32m+[m					[32mproceed();[m
[32m+[m				[32m}[m
[32m+[m			[32m});[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mfor( var i = 0, len = config.dependencies.length; i < len; i++ ) {[m
[32m+[m			[32mvar s = config.dependencies[i];[m
[32m+[m
[32m+[m			[32m// Load if there's no condition or the condition is truthy[m
[32m+[m			[32mif( !s.condition || s.condition() ) {[m
[32m+[m				[32mif( s.async ) {[m
[32m+[m					[32mscriptsAsync.push( s.src );[m
[32m+[m				[32m}[m
[32m+[m				[32melse {[m
[32m+[m					[32mscripts.push( s.src );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mloadScript( s );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif( scripts.length ) {[m
[32m+[m			[32mscriptsToPreload = scripts.length;[m
[32m+[m
[32m+[m			[32m// Load synchronous scripts[m
[32m+[m			[32mhead.js.apply( null, scripts );[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mproceed();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Starts up reveal.js by binding input events and navigating[m
[32m+[m	[32m * to the current URL deeplink if there is one.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction start() {[m
[32m+[m
[32m+[m		[32m// Make sure we've got all the DOM elements we need[m
[32m+[m		[32msetupDOM();[m
[32m+[m
[32m+[m		[32m// Listen to messages posted to this window[m
[32m+[m		[32msetupPostMessage();[m
[32m+[m
[32m+[m		[32m// Prevent iframes from scrolling the slides out of view[m
[32m+[m		[32msetupIframeScrollPrevention();[m
[32m+[m
[32m+[m		[32m// Resets all vertical slides so that only the first is visible[m
[32m+[m		[32mresetVerticalSlides();[m
[32m+[m
[32m+[m		[32m// Updates the presentation to match the current configuration values[m
[32m+[m		[32mconfigure();[m
[32m+[m
[32m+[m		[32m// Read the initial hash[m
[32m+[m		[32mreadURL();[m
[32m+[m
[32m+[m		[32m// Update all backgrounds[m
[32m+[m		[32mupdateBackground( true );[m
[32m+[m
[32m+[m		[32m// Notify listeners that the presentation is ready but use a 1ms[m
[32m+[m		[32m// timeout to ensure it's not fired synchronously after #initialize()[m
[32m+[m		[32msetTimeout( function() {[m
[32m+[m			[32m// Enable transitions now that we're loaded[m
[32m+[m			[32mdom.slides.classList.remove( 'no-transition' );[m
[32m+[m
[32m+[m			[32mloaded = true;[m
[32m+[m
[32m+[m			[32mdispatchEvent( 'ready', {[m
[32m+[m				[32m'indexh': indexh,[m
[32m+[m				[32m'indexv': indexv,[m
[32m+[m				[32m'currentSlide': currentSlide[m
[32m+[m			[32m} );[m
[32m+[m		[32m}, 1 );[m
[32m+[m
[32m+[m		[32m// Special setup and config is required when printing to PDF[m
[32m+[m		[32mif( isPrintingPDF() ) {[m
[32m+[m			[32mremoveEventListeners();[m
[32m+[m
[32m+[m			[32m// The document needs to have loaded for the PDF layout[m
[32m+[m			[32m// measurements to be accurate[m
[32m+[m			[32mif( document.readyState === 'complete' ) {[m
[32m+[m				[32msetupPDF();[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32mwindow.addEventListener( 'load', setupPDF );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Finds and stores references to DOM elements which are[m
[32m+[m	[32m * required by the presentation. If a required element is[m
[32m+[m	[32m * not found, it is created.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction setupDOM() {[m
[32m+[m
[32m+[m		[32m// Prevent transitions while we're loading[m
[32m+[m		[32mdom.slides.classList.add( 'no-transition' );[m
[32m+[m
[32m+[m		[32m// Background element[m
[32m+[m		[32mdom.background = createSingletonNode( dom.wrapper, 'div', 'backgrounds', null );[m
[32m+[m
[32m+[m		[32m// Progress bar[m
[32m+[m		[32mdom.progress = createSingletonNode( dom.wrapper, 'div', 'progress', '<span></span>' );[m
[32m+[m		[32mdom.progressbar = dom.progress.querySelector( 'span' );[m
[32m+[m
[32m+[m		[32m// Arrow controls[m
[32m+[m		[32mcreateSingletonNode( dom.wrapper, 'aside', 'controls',[m
[32m+[m			[32m'<button class="navigate-left" aria-label="previous slide"></button>' +[m
[32m+[m			[32m'<button class="navigate-right" aria-label="next slide"></button>' +[m
[32m+[m			[32m'<button class="navigate-up" aria-label="above slide"></button>' +[m
[32m+[m			[32m'<button class="navigate-down" aria-label="below slide"></button>' );[m
[32m+[m
[32m+[m		[32m// Slide number[m
[32m+[m		[32mdom.slideNumber = createSingletonNode( dom.wrapper, 'div', 'slide-number', '' );[m
[32m+[m
[32m+[m		[32m// Element containing notes that are visible to the audience[m
[32m+[m		[32mdom.speakerNotes = createSingletonNode( dom.wrapper, 'div', 'speaker-notes', null );[m
[32m+[m		[32mdom.speakerNotes.setAttribute( 'data-prevent-swipe', '' );[m
[32m+[m
[32m+[m		[32m// Overlay graphic which is displayed during the paused mode[m
[32m+[m		[32mcreateSingletonNode( dom.wrapper, 'div', 'pause-overlay', null );[m
[32m+[m
[32m+[m		[32m// Cache references to elements[m
[32m+[m		[32mdom.controls = document.querySelector( '.reveal .controls' );[m
[32m+[m		[32mdom.theme = document.querySelector( '#theme' );[m
[32m+[m
[32m+[m		[32mdom.wrapper.setAttribute( 'role', 'application' );[m
[32m+[m
[32m+[m		[32m// There can be multiple instances of controls throughout the page[m
[32m+[m		[32mdom.controlsLeft = toArray( document.querySelectorAll( '.navigate-left' ) );[m
[32m+[m		[32mdom.controlsRight = toArray( document.querySelectorAll( '.navigate-right' ) );[m
[32m+[m		[32mdom.controlsUp = toArray( document.querySelectorAll( '.navigate-up' ) );[m
[32m+[m		[32mdom.controlsDown = toArray( document.querySelectorAll( '.navigate-down' ) );[m
[32m+[m		[32mdom.controlsPrev = toArray( document.querySelectorAll( '.navigate-prev' ) );[m
[32m+[m		[32mdom.controlsNext = toArray( document.querySelectorAll( '.navigate-next' ) );[m
[32m+[m
[32m+[m		[32mdom.statusDiv = createStatusDiv();[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Creates a hidden div with role aria-live to announce the[m
[32m+[m	[32m * current slide content. Hide the div off-screen to make it[m
[32m+[m	[32m * available only to Assistive Technologies.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction createStatusDiv() {[m
[32m+[m
[32m+[m		[32mvar statusDiv = document.getElementById( 'aria-status-div' );[m
[32m+[m		[32mif( !statusDiv ) {[m
[32m+[m			[32mstatusDiv = document.createElement( 'div' );[m
[32m+[m			[32mstatusDiv.style.position = 'absolute';[m
[32m+[m			[32mstatusDiv.style.height = '1px';[m
[32m+[m			[32mstatusDiv.style.width = '1px';[m
[32m+[m			[32mstatusDiv.style.overflow ='hidden';[m
[32m+[m			[32mstatusDiv.style.clip = 'rect( 1px, 1px, 1px, 1px )';[m
[32m+[m			[32mstatusDiv.setAttribute( 'id', 'aria-status-div' );[m
[32m+[m			[32mstatusDiv.setAttribute( 'aria-live', 'polite' );[m
[32m+[m			[32mstatusDiv.setAttribute( 'aria-atomic','true' );[m
[32m+[m			[32mdom.wrapper.appendChild( statusDiv );[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn statusDiv;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Configures the presentation for printing to a static[m
[32m+[m	[32m * PDF.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction setupPDF() {[m
[32m+[m
[32m+[m		[32mvar slideSize = getComputedSlideSize( window.innerWidth, window.innerHeight );[m
[32m+[m
[32m+[m		[32m// Dimensions of the PDF pages[m
[32m+[m		[32mvar pageWidth = Math.floor( slideSize.width * ( 1 + config.margin ) ),[m
[32m+[m			[32mpageHeight = Math.floor( slideSize.height * ( 1 + config.margin  ) );[m
[32m+[m
[32m+[m		[32m// Dimensions of slides within the pages[m
[32m+[m		[32mvar slideWidth = slideSize.width,[m
[32m+[m			[32mslideHeight = slideSize.height;[m
[32m+[m
[32m+[m		[32m// Let the browser know what page size we want to print[m
[32m+[m		[32minjectStyleSheet( '@page{size:'+ pageWidth +'px '+ pageHeight +'px; margin: 0;}' );[m
[32m+[m
[32m+[m		[32m// Limit the size of certain elements to the dimensions of the slide[m
[32m+[m		[32minjectStyleSheet( '.reveal section>img, .reveal section>video, .reveal section>iframe{max-width: '+ slideWidth +'px; max-height:'+ slideHeight +'px}' );[m
[32m+[m
[32m+[m		[32mdocument.body.classList.add( 'print-pdf' );[m
[32m+[m		[32mdocument.body.style.width = pageWidth + 'px';[m
[32m+[m		[32mdocument.body.style.height = pageHeight + 'px';[m
[32m+[m
[32m+[m		[32m// Add each slide's index as attributes on itself, we need these[m
[32m+[m		[32m// indices to generate slide numbers below[m
[32m+[m		[32mtoArray( dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR ) ).forEach( function( hslide, h ) {[m
[32m+[m			[32mhslide.setAttribute( 'data-index-h', h );[m
[32m+[m
[32m+[m			[32mif( hslide.classList.contains( 'stack' ) ) {[m
[32m+[m				[32mtoArray( hslide.querySelectorAll( 'section' ) ).forEach( function( vslide, v ) {[m
[32m+[m					[32mvslide.setAttribute( 'data-index-h', h );[m
[32m+[m					[32mvslide.setAttribute( 'data-index-v', v );[m
[32m+[m				[32m} );[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m		[32m// Slide and slide background layout[m
[32m+[m		[32mtoArray( dom.wrapper.querySelectorAll( SLIDES_SELECTOR ) ).forEach( function( slide ) {[m
[32m+[m
[32m+[m			[32m// Vertical stacks are not centred since their section[m
[32m+[m			[32m// children will be[m
[32m+[m			[32mif( slide.classList.contains( 'stack' ) === false ) {[m
[32m+[m				[32m// Center the slide inside of the page, giving the slide some margin[m
[32m+[m				[32mvar left = ( pageWidth - slideWidth ) / 2,[m
[32m+[m					[32mtop = ( pageHeight - slideHeight ) / 2;[m
[32m+[m
[32m+[m				[32mvar contentHeight = getAbsoluteHeight( slide );[m
[32m+[m				[32mvar numberOfPages = Math.max( Math.ceil( contentHeight / pageHeight ), 1 );[m
[32m+[m
[32m+[m				[32m// Center slides vertically[m
[32m+[m				[32mif( numberOfPages === 1 && config.center || slide.classList.contains( 'center' ) ) {[m
[32m+[m					[32mtop = Math.max( ( pageHeight - contentHeight ) / 2, 0 );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Position the slide inside of the page[m
[32m+[m				[32mslide.style.left = left + 'px';[m
[32m+[m				[32mslide.style.top = top + 'px';[m
[32m+[m				[32mslide.style.width = slideWidth + 'px';[m
[32m+[m
[32m+[m				[32m// TODO Backgrounds need to be multiplied when the slide[m
[32m+[m				[32m// stretches over multiple pages[m
[32m+[m				[32mvar background = slide.querySelector( '.slide-background' );[m
[32m+[m				[32mif( background ) {[m
[32m+[m					[32mbackground.style.width = pageWidth + 'px';[m
[32m+[m					[32mbackground.style.height = ( pageHeight * numberOfPages ) + 'px';[m
[32m+[m					[32mbackground.style.top = -top + 'px';[m
[32m+[m					[32mbackground.style.left = -left + 'px';[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Inject notes if `showNotes` is enabled[m
[32m+[m				[32mif( config.showNotes ) {[m
[32m+[m					[32mvar notes = getSlideNotes( slide );[m
[32m+[m					[32mif( notes ) {[m
[32m+[m						[32mvar notesSpacing = 8;[m
[32m+[m						[32mvar notesElement = document.createElement( 'div' );[m
[32m+[m						[32mnotesElement.classList.add( 'speaker-notes' );[m
[32m+[m						[32mnotesElement.classList.add( 'speaker-notes-pdf' );[m
[32m+[m						[32mnotesElement.innerHTML = notes;[m
[32m+[m						[32mnotesElement.style.left = ( notesSpacing - left ) + 'px';[m
[32m+[m						[32mnotesElement.style.bottom = ( notesSpacing - top ) + 'px';[m
[32m+[m						[32mnotesElement.style.width = ( pageWidth - notesSpacing*2 ) + 'px';[m
[32m+[m						[32mslide.appendChild( notesElement );[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Inject slide numbers if `slideNumbers` are enabled[m
[32m+[m				[32mif( config.slideNumber ) {[m
[32m+[m					[32mvar slideNumberH = parseInt( slide.getAttribute( 'data-index-h' ), 10 ) + 1,[m
[32m+[m						[32mslideNumberV = parseInt( slide.getAttribute( 'data-index-v' ), 10 ) + 1;[m
[32m+[m
[32m+[m					[32mvar numberElement = document.createElement( 'div' );[m
[32m+[m					[32mnumberElement.classList.add( 'slide-number' );[m
[32m+[m					[32mnumberElement.classList.add( 'slide-number-pdf' );[m
[32m+[m					[32mnumberElement.innerHTML = formatSlideNumber( slideNumberH, '.', slideNumberV );[m
[32m+[m					[32mbackground.appendChild( numberElement );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m		[32m// Show all fragments[m
[32m+[m		[32mtoArray( dom.wrapper.querySelectorAll( SLIDES_SELECTOR + ' .fragment' ) ).forEach( function( fragment ) {[m
[32m+[m			[32mfragment.classList.add( 'visible' );[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * This is an unfortunate necessity. Iframes can trigger the[m
[32m+[m	[32m * parent window to scroll, for example by focusing an input.[m
[32m+[m	[32m * This scrolling can not be prevented by hiding overflow in[m
[32m+[m	[32m * CSS so we have to resort to repeatedly checking if the[m
[32m+[m	[32m * browser has decided to offset our slides :([m
[32m+[m	[32m */[m
[32m+[m	[32mfunction setupIframeScrollPrevention() {[m
[32m+[m
[32m+[m		[32mif( dom.slides.querySelector( 'iframe' ) ) {[m
[32m+[m			[32msetInterval( function() {[m
[32m+[m				[32mif( dom.wrapper.scrollTop !== 0 || dom.wrapper.scrollLeft !== 0 ) {[m
[32m+[m					[32mdom.wrapper.scrollTop = 0;[m
[32m+[m					[32mdom.wrapper.scrollLeft = 0;[m
[32m+[m				[32m}[m
[32m+[m			[32m}, 500 );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Creates an HTML element and returns a reference to it.[m
[32m+[m	[32m * If the element already exists the existing instance will[m
[32m+[m	[32m * be returned.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction createSingletonNode( container, tagname, classname, innerHTML ) {[m
[32m+[m
[32m+[m		[32m// Find all nodes matching the description[m
[32m+[m		[32mvar nodes = container.querySelectorAll( '.' + classname );[m
[32m+[m
[32m+[m		[32m// Check all matches to find one which is a direct child of[m
[32m+[m		[32m// the specified container[m
[32m+[m		[32mfor( var i = 0; i < nodes.length; i++ ) {[m
[32m+[m			[32mvar testNode = nodes[i];[m
[32m+[m			[32mif( testNode.parentNode === container ) {[m
[32m+[m				[32mreturn testNode;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// If no node was found, create it now[m
[32m+[m		[32mvar node = document.createElement( tagname );[m
[32m+[m		[32mnode.classList.add( classname );[m
[32m+[m		[32mif( typeof innerHTML === 'string' ) {[m
[32m+[m			[32mnode.innerHTML = innerHTML;[m
[32m+[m		[32m}[m
[32m+[m		[32mcontainer.appendChild( node );[m
[32m+[m
[32m+[m		[32mreturn node;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Creates the slide background elements and appends them[m
[32m+[m	[32m * to the background container. One element is created per[m
[32m+[m	[32m * slide no matter if the given slide has visible background.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction createBackgrounds() {[m
[32m+[m
[32m+[m		[32mvar printMode = isPrintingPDF();[m
[32m+[m
[32m+[m		[32m// Clear prior backgrounds[m
[32m+[m		[32mdom.background.innerHTML = '';[m
[32m+[m		[32mdom.background.classList.add( 'no-transition' );[m
[32m+[m
[32m+[m		[32m// Iterate over all horizontal slides[m
[32m+[m		[32mtoArray( dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR ) ).forEach( function( slideh ) {[m
[32m+[m
[32m+[m			[32mvar backgroundStack;[m
[32m+[m
[32m+[m			[32mif( printMode ) {[m
[32m+[m				[32mbackgroundStack = createBackground( slideh, slideh );[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32mbackgroundStack = createBackground( slideh, dom.background );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Iterate over all vertical slides[m
[32m+[m			[32mtoArray( slideh.querySelectorAll( 'section' ) ).forEach( function( slidev ) {[m
[32m+[m
[32m+[m				[32mif( printMode ) {[m
[32m+[m					[32mcreateBackground( slidev, slidev );[m
[32m+[m				[32m}[m
[32m+[m				[32melse {[m
[32m+[m					[32mcreateBackground( slidev, backgroundStack );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mbackgroundStack.classList.add( 'stack' );[m
[32m+[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m		[32m// Add parallax background if specified[m
[32m+[m		[32mif( config.parallaxBackgroundImage ) {[m
[32m+[m
[32m+[m			[32mdom.background.style.backgroundImage = 'url("' + config.parallaxBackgroundImage + '")';[m
[32m+[m			[32mdom.background.style.backgroundSize = config.parallaxBackgroundSize;[m
[32m+[m
[32m+[m			[32m// Make sure the below properties are set on the element - these properties are[m
[32m+[m			[32m// needed for proper transitions to be set on the element via CSS. To remove[m
[32m+[m			[32m// annoying background slide-in effect when the presentation starts, apply[m
[32m+[m			[32m// these properties after short time delay[m
[32m+[m			[32msetTimeout( function() {[m
[32m+[m				[32mdom.wrapper.classList.add( 'has-parallax-background' );[m
[32m+[m			[32m}, 1 );[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m
[32m+[m			[32mdom.background.style.backgroundImage = '';[m
[32m+[m			[32mdom.wrapper.classList.remove( 'has-parallax-background' );[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Creates a background for the given slide.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {HTMLElement} slide[m
[32m+[m	[32m * @param {HTMLElement} container The element that the background[m
[32m+[m	[32m * should be appended to[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction createBackground( slide, container ) {[m
[32m+[m
[32m+[m		[32mvar data = {[m
[32m+[m			[32mbackground: slide.getAttribute( 'data-background' ),[m
[32m+[m			[32mbackgroundSize: slide.getAttribute( 'data-background-size' ),[m
[32m+[m			[32mbackgroundImage: slide.getAttribute( 'data-background-image' ),[m
[32m+[m			[32mbackgroundVideo: slide.getAttribute( 'data-background-video' ),[m
[32m+[m			[32mbackgroundIframe: slide.getAttribute( 'data-background-iframe' ),[m
[32m+[m			[32mbackgroundColor: slide.getAttribute( 'data-background-color' ),[m
[32m+[m			[32mbackgroundRepeat: slide.getAttribute( 'data-background-repeat' ),[m
[32m+[m			[32mbackgroundPosition: slide.getAttribute( 'data-background-position' ),[m
[32m+[m			[32mbackgroundTransition: slide.getAttribute( 'data-background-transition' )[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32mvar element = document.createElement( 'div' );[m
[32m+[m
[32m+[m		[32m// Carry over custom classes from the slide to the background[m
[32m+[m		[32melement.className = 'slide-background ' + slide.className.replace( /present|past|future/, '' );[m
[32m+[m
[32m+[m		[32mif( data.background ) {[m
[32m+[m			[32m// Auto-wrap image urls in url(...)[m
[32m+[m			[32mif( /^(http|file|\/\/)/gi.test( data.background ) || /\.(svg|png|jpg|jpeg|gif|bmp)$/gi.test( data.background ) ) {[m
[32m+[m				[32mslide.setAttribute( 'data-background-image', data.background );[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32melement.style.background = data.background;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Create a hash for this combination of background settings.[m
[32m+[m		[32m// This is used to determine when two slide backgrounds are[m
[32m+[m		[32m// the same.[m
[32m+[m		[32mif( data.background || data.backgroundColor || data.backgroundImage || data.backgroundVideo || data.backgroundIframe ) {[m
[32m+[m			[32melement.setAttribute( 'data-background-hash', data.background +[m
[32m+[m															[32mdata.backgroundSize +[m
[32m+[m															[32mdata.backgroundImage +[m
[32m+[m															[32mdata.backgroundVideo +[m
[32m+[m															[32mdata.backgroundIframe +[m
[32m+[m															[32mdata.backgroundColor +[m
[32m+[m															[32mdata.backgroundRepeat +[m
[32m+[m															[32mdata.backgroundPosition +[m
[32m+[m															[32mdata.backgroundTransition );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Additional and optional background properties[m
[32m+[m		[32mif( data.backgroundSize ) element.style.backgroundSize = data.backgroundSize;[m
[32m+[m		[32mif( data.backgroundColor ) element.style.backgroundColor = data.backgroundColor;[m
[32m+[m		[32mif( data.backgroundRepeat ) element.style.backgroundRepeat = data.backgroundRepeat;[m
[32m+[m		[32mif( data.backgroundPosition ) element.style.backgroundPosition = data.backgroundPosition;[m
[32m+[m		[32mif( data.backgroundTransition ) element.setAttribute( 'data-background-transition', data.backgroundTransition );[m
[32m+[m
[32m+[m		[32mcontainer.appendChild( element );[m
[32m+[m
[32m+[m		[32m// If backgrounds are being recreated, clear old classes[m
[32m+[m		[32mslide.classList.remove( 'has-dark-background' );[m
[32m+[m		[32mslide.classList.remove( 'has-light-background' );[m
[32m+[m
[32m+[m		[32m// If this slide has a background color, add a class that[m
[32m+[m		[32m// signals if it is light or dark. If the slide has no background[m
[32m+[m		[32m// color, no class will be set[m
[32m+[m		[32mvar computedBackgroundColor = window.getComputedStyle( element ).backgroundColor;[m
[32m+[m		[32mif( computedBackgroundColor ) {[m
[32m+[m			[32mvar rgb = colorToRgb( computedBackgroundColor );[m
[32m+[m
[32m+[m			[32m// Ignore fully transparent backgrounds. Some browsers return[m
[32m+[m			[32m// rgba(0,0,0,0) when reading the computed background color of[m
[32m+[m			[32m// an element with no background[m
[32m+[m			[32mif( rgb && rgb.a !== 0 ) {[m
[32m+[m				[32mif( colorBrightness( computedBackgroundColor ) < 128 ) {[m
[32m+[m					[32mslide.classList.add( 'has-dark-background' );[m
[32m+[m				[32m}[m
[32m+[m				[32melse {[m
[32m+[m					[32mslide.classList.add( 'has-light-background' );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn element;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Registers a listener to postMessage events, this makes it[m
[32m+[m	[32m * possible to call all reveal.js API methods from another[m
[32m+[m	[32m * window. For example:[m
[32m+[m	[32m *[m
[32m+[m	[32m * revealWindow.postMessage( JSON.stringify({[m
[32m+[m	[32m *   method: 'slide',[m
[32m+[m	[32m *   args: [ 2 ][m
[32m+[m	[32m * }), '*' );[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction setupPostMessage() {[m
[32m+[m
[32m+[m		[32mif( config.postMessage ) {[m
[32m+[m			[32mwindow.addEventListener( 'message', function ( event ) {[m
[32m+[m				[32mvar data = event.data;[m
[32m+[m
[32m+[m				[32m// Make sure we're dealing with JSON[m
[32m+[m				[32mif( typeof data === 'string' && data.charAt( 0 ) === '{' && data.charAt( data.length - 1 ) === '}' ) {[m
[32m+[m					[32mdata = JSON.parse( data );[m
[32m+[m
[32m+[m					[32m// Check if the requested method can be found[m
[32m+[m					[32mif( data.method && typeof Reveal[data.method] === 'function' ) {[m
[32m+[m						[32mReveal[data.method].apply( Reveal, data.args );[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}, false );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Applies the configuration settings from the config[m
[32m+[m	[32m * object. May be called multiple times.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction configure( options ) {[m
[32m+[m
[32m+[m		[32mvar numberOfSlides = dom.wrapper.querySelectorAll( SLIDES_SELECTOR ).length;[m
[32m+[m
[32m+[m		[32mdom.wrapper.classList.remove( config.transition );[m
[32m+[m
[32m+[m		[32m// New config options may be passed when this method[m
[32m+[m		[32m// is invoked through the API after initialization[m
[32m+[m		[32mif( typeof options === 'object' ) extend( config, options );[m
[32m+[m
[32m+[m		[32m// Force linear transition based on browser capabilities[m
[32m+[m		[32mif( features.transforms3d === false ) config.transition = 'linear';[m
[32m+[m
[32m+[m		[32mdom.wrapper.classList.add( config.transition );[m
[32m+[m
[32m+[m		[32mdom.wrapper.setAttribute( 'data-transition-speed', config.transitionSpeed );[m
[32m+[m		[32mdom.wrapper.setAttribute( 'data-background-transition', config.backgroundTransition );[m
[32m+[m
[32m+[m		[32mdom.controls.style.display = config.controls ? 'block' : 'none';[m
[32m+[m		[32mdom.progress.style.display = config.progress ? 'block' : 'none';[m
[32m+[m		[32mdom.slideNumber.style.display = config.slideNumber && !isPrintingPDF() ? 'block' : 'none';[m
[32m+[m
[32m+[m		[32mif( config.rtl ) {[m
[32m+[m			[32mdom.wrapper.classList.add( 'rtl' );[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mdom.wrapper.classList.remove( 'rtl' );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif( config.center ) {[m
[32m+[m			[32mdom.wrapper.classList.add( 'center' );[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mdom.wrapper.classList.remove( 'center' );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Exit the paused mode if it was configured off[m
[32m+[m		[32mif( config.pause === false ) {[m
[32m+[m			[32mresume();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif( config.showNotes ) {[m
[32m+[m			[32mdom.speakerNotes.classList.add( 'visible' );[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mdom.speakerNotes.classList.remove( 'visible' );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif( config.mouseWheel ) {[m
[32m+[m			[32mdocument.addEventListener( 'DOMMouseScroll', onDocumentMouseScroll, false ); // FF[m
[32m+[m			[32mdocument.addEventListener( 'mousewheel', onDocumentMouseScroll, false );[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mdocument.removeEventListener( 'DOMMouseScroll', onDocumentMouseScroll, false ); // FF[m
[32m+[m			[32mdocument.removeEventListener( 'mousewheel', onDocumentMouseScroll, false );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Rolling 3D links[m
[32m+[m		[32mif( config.rollingLinks ) {[m
[32m+[m			[32menableRollingLinks();[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mdisableRollingLinks();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Iframe link previews[m
[32m+[m		[32mif( config.previewLinks ) {[m
[32m+[m			[32menablePreviewLinks();[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mdisablePreviewLinks();[m
[32m+[m			[32menablePreviewLinks( '[data-preview-link]' );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Remove existing auto-slide controls[m
[32m+[m		[32mif( autoSlidePlayer ) {[m
[32m+[m			[32mautoSlidePlayer.destroy();[m
[32m+[m			[32mautoSlidePlayer = null;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Generate auto-slide controls if needed[m
[32m+[m		[32mif( numberOfSlides > 1 && config.autoSlide && config.autoSlideStoppable && features.canvas && features.requestAnimationFrame ) {[m
[32m+[m			[32mautoSlidePlayer = new Playback( dom.wrapper, function() {[m
[32m+[m				[32mreturn Math.min( Math.max( ( Date.now() - autoSlideStartTime ) / autoSlide, 0 ), 1 );[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m			[32mautoSlidePlayer.on( 'click', onAutoSlidePlayerClick );[m
[32m+[m			[32mautoSlidePaused = false;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// When fragments are turned off they should be visible[m
[32m+[m		[32mif( config.fragments === false ) {[m
[32m+[m			[32mtoArray( dom.slides.querySelectorAll( '.fragment' ) ).forEach( function( element ) {[m
[32m+[m				[32melement.classList.add( 'visible' );[m
[32m+[m				[32melement.classList.remove( 'current-fragment' );[m
[32m+[m			[32m} );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32msync();[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Binds all event listeners.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction addEventListeners() {[m
[32m+[m
[32m+[m		[32meventsAreBound = true;[m
[32m+[m
[32m+[m		[32mwindow.addEventListener( 'hashchange', onWindowHashChange, false );[m
[32m+[m		[32mwindow.addEventListener( 'resize', onWindowResize, false );[m
[32m+[m
[32m+[m		[32mif( config.touch ) {[m
[32m+[m			[32mdom.wrapper.addEventListener( 'touchstart', onTouchStart, false );[m
[32m+[m			[32mdom.wrapper.addEventListener( 'touchmove', onTouchMove, false );[m
[32m+[m			[32mdom.wrapper.addEventListener( 'touchend', onTouchEnd, false );[m
[32m+[m
[32m+[m			[32m// Support pointer-style touch interaction as well[m
[32m+[m			[32mif( window.navigator.pointerEnabled ) {[m
[32m+[m				[32m// IE 11 uses un-prefixed version of pointer events[m
[32m+[m				[32mdom.wrapper.addEventListener( 'pointerdown', onPointerDown, false );[m
[32m+[m				[32mdom.wrapper.addEventListener( 'pointermove', onPointerMove, false );[m
[32m+[m				[32mdom.wrapper.addEventListener( 'pointerup', onPointerUp, false );[m
[32m+[m			[32m}[m
[32m+[m			[32melse if( window.navigator.msPointerEnabled ) {[m
[32m+[m				[32m// IE 10 uses prefixed version of pointer events[m
[32m+[m				[32mdom.wrapper.addEventListener( 'MSPointerDown', onPointerDown, false );[m
[32m+[m				[32mdom.wrapper.addEventListener( 'MSPointerMove', onPointerMove, false );[m
[32m+[m				[32mdom.wrapper.addEventListener( 'MSPointerUp', onPointerUp, false );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif( config.keyboard ) {[m
[32m+[m			[32mdocument.addEventListener( 'keydown', onDocumentKeyDown, false );[m
[32m+[m			[32mdocument.addEventListener( 'keypress', onDocumentKeyPress, false );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif( config.progress && dom.progress ) {[m
[32m+[m			[32mdom.progress.addEventListener( 'click', onProgressClicked, false );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif( config.focusBodyOnPageVisibilityChange ) {[m
[32m+[m			[32mvar visibilityChange;[m
[32m+[m
[32m+[m			[32mif( 'hidden' in document ) {[m
[32m+[m				[32mvisibilityChange = 'visibilitychange';[m
[32m+[m			[32m}[m
[32m+[m			[32melse if( 'msHidden' in document ) {[m
[32m+[m				[32mvisibilityChange = 'msvisibilitychange';[m
[32m+[m			[32m}[m
[32m+[m			[32melse if( 'webkitHidden' in document ) {[m
[32m+[m				[32mvisibilityChange = 'webkitvisibilitychange';[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif( visibilityChange ) {[m
[32m+[m				[32mdocument.addEventListener( visibilityChange, onPageVisibilityChange, false );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Listen to both touch and click events, in case the device[m
[32m+[m		[32m// supports both[m
[32m+[m		[32mvar pointerEvents = [ 'touchstart', 'click' ];[m
[32m+[m
[32m+[m		[32m// Only support touch for Android, fixes double navigations in[m
[32m+[m		[32m// stock browser[m
[32m+[m		[32mif( navigator.userAgent.match( /android/gi ) ) {[m
[32m+[m			[32mpointerEvents = [ 'touchstart' ];[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mpointerEvents.forEach( function( eventName ) {[m
[32m+[m			[32mdom.controlsLeft.forEach( function( el ) { el.addEventListener( eventName, onNavigateLeftClicked, false ); } );[m
[32m+[m			[32mdom.controlsRight.forEach( function( el ) { el.addEventListener( eventName, onNavigateRightClicked, false ); } );[m
[32m+[m			[32mdom.controlsUp.forEach( function( el ) { el.addEventListener( eventName, onNavigateUpClicked, false ); } );[m
[32m+[m			[32mdom.controlsDown.forEach( function( el ) { el.addEventListener( eventName, onNavigateDownClicked, false ); } );[m
[32m+[m			[32mdom.controlsPrev.forEach( function( el ) { el.addEventListener( eventName, onNavigatePrevClicked, false ); } );[m
[32m+[m			[32mdom.controlsNext.forEach( function( el ) { el.addEventListener( eventName, onNavigateNextClicked, false ); } );[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Unbinds all event listeners.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction removeEventListeners() {[m
[32m+[m
[32m+[m		[32meventsAreBound = false;[m
[32m+[m
[32m+[m		[32mdocument.removeEventListener( 'keydown', onDocumentKeyDown, false );[m
[32m+[m		[32mdocument.removeEventListener( 'keypress', onDocumentKeyPress, false );[m
[32m+[m		[32mwindow.removeEventListener( 'hashchange', onWindowHashChange, false );[m
[32m+[m		[32mwindow.removeEventListener( 'resize', onWindowResize, false );[m
[32m+[m
[32m+[m		[32mdom.wrapper.removeEventListener( 'touchstart', onTouchStart, false );[m
[32m+[m		[32mdom.wrapper.removeEventListener( 'touchmove', onTouchMove, false );[m
[32m+[m		[32mdom.wrapper.removeEventListener( 'touchend', onTouchEnd, false );[m
[32m+[m
[32m+[m		[32m// IE11[m
[32m+[m		[32mif( window.navigator.pointerEnabled ) {[m
[32m+[m			[32mdom.wrapper.removeEventListener( 'pointerdown', onPointerDown, false );[m
[32m+[m			[32mdom.wrapper.removeEventListener( 'pointermove', onPointerMove, false );[m
[32m+[m			[32mdom.wrapper.removeEventListener( 'pointerup', onPointerUp, false );[m
[32m+[m		[32m}[m
[32m+[m		[32m// IE10[m
[32m+[m		[32melse if( window.navigator.msPointerEnabled ) {[m
[32m+[m			[32mdom.wrapper.removeEventListener( 'MSPointerDown', onPointerDown, false );[m
[32m+[m			[32mdom.wrapper.removeEventListener( 'MSPointerMove', onPointerMove, false );[m
[32m+[m			[32mdom.wrapper.removeEventListener( 'MSPointerUp', onPointerUp, false );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( config.progress && dom.progress ) {[m
[32m+[m			[32mdom.progress.removeEventListener( 'click', onProgressClicked, false );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m[ 'touchstart', 'click' ].forEach( function( eventName ) {[m
[32m+[m			[32mdom.controlsLeft.forEach( function( el ) { el.removeEventListener( eventName, onNavigateLeftClicked, false ); } );[m
[32m+[m			[32mdom.controlsRight.forEach( function( el ) { el.removeEventListener( eventName, onNavigateRightClicked, false ); } );[m
[32m+[m			[32mdom.controlsUp.forEach( function( el ) { el.removeEventListener( eventName, onNavigateUpClicked, false ); } );[m
[32m+[m			[32mdom.controlsDown.forEach( function( el ) { el.removeEventListener( eventName, onNavigateDownClicked, false ); } );[m
[32m+[m			[32mdom.controlsPrev.forEach( function( el ) { el.removeEventListener( eventName, onNavigatePrevClicked, false ); } );[m
[32m+[m			[32mdom.controlsNext.forEach( function( el ) { el.removeEventListener( eventName, onNavigateNextClicked, false ); } );[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Extend object a with the properties of object b.[m
[32m+[m	[32m * If there's a conflict, object b takes precedence.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction extend( a, b ) {[m
[32m+[m
[32m+[m		[32mfor( var i in b ) {[m
[32m+[m			[32ma[ i ] = b[ i ];[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Converts the target object to an array.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction toArray( o ) {[m
[32m+[m
[32m+[m		[32mreturn Array.prototype.slice.call( o );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Utility for deserializing a value.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction deserialize( value ) {[m
[32m+[m
[32m+[m		[32mif( typeof value === 'string' ) {[m
[32m+[m			[32mif( value === 'null' ) return null;[m
[32m+[m			[32melse if( value === 'true' ) return true;[m
[32m+[m			[32melse if( value === 'false' ) return false;[m
[32m+[m			[32melse if( value.match( /^\d+$/ ) ) return parseFloat( value );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn value;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Measures the distance in pixels between point a[m
[32m+[m	[32m * and point b.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {Object} a point with x/y properties[m
[32m+[m	[32m * @param {Object} b point with x/y properties[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction distanceBetween( a, b ) {[m
[32m+[m
[32m+[m		[32mvar dx = a.x - b.x,[m
[32m+[m			[32mdy = a.y - b.y;[m
[32m+[m
[32m+[m		[32mreturn Math.sqrt( dx*dx + dy*dy );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Applies a CSS transform to the target element.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction transformElement( element, transform ) {[m
[32m+[m
[32m+[m		[32melement.style.WebkitTransform = transform;[m
[32m+[m		[32melement.style.MozTransform = transform;[m
[32m+[m		[32melement.style.msTransform = transform;[m
[32m+[m		[32melement.style.transform = transform;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Applies CSS transforms to the slides container. The container[m
[32m+[m	[32m * is transformed from two separate sources: layout and the overview[m
[32m+[m	[32m * mode.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction transformSlides( transforms ) {[m
[32m+[m
[32m+[m		[32m// Pick up new transforms from arguments[m
[32m+[m		[32mif( typeof transforms.layout === 'string' ) slidesTransform.layout = transforms.layout;[m
[32m+[m		[32mif( typeof transforms.overview === 'string' ) slidesTransform.overview = transforms.overview;[m
[32m+[m
[32m+[m		[32m// Apply the transforms to the slides container[m
[32m+[m		[32mif( slidesTransform.layout ) {[m
[32m+[m			[32mtransformElement( dom.slides, slidesTransform.layout + ' ' + slidesTransform.overview );[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mtransformElement( dom.slides, slidesTransform.overview );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Injects the given CSS styles into the DOM.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction injectStyleSheet( value ) {[m
[32m+[m
[32m+[m		[32mvar tag = document.createElement( 'style' );[m
[32m+[m		[32mtag.type = 'text/css';[m
[32m+[m		[32mif( tag.styleSheet ) {[m
[32m+[m			[32mtag.styleSheet.cssText = value;[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mtag.appendChild( document.createTextNode( value ) );[m
[32m+[m		[32m}[m
[32m+[m		[32mdocument.getElementsByTagName( 'head' )[0].appendChild( tag );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Converts various color input formats to an {r:0,g:0,b:0} object.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {String} color The string representation of a color,[m
[32m+[m	[32m * the following formats are supported:[m
[32m+[m	[32m * - #000[m
[32m+[m	[32m * - #000000[m
[32m+[m	[32m * - rgb(0,0,0)[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction colorToRgb( color ) {[m
[32m+[m
[32m+[m		[32mvar hex3 = color.match( /^#([0-9a-f]{3})$/i );[m
[32m+[m		[32mif( hex3 && hex3[1] ) {[m
[32m+[m			[32mhex3 = hex3[1];[m
[32m+[m			[32mreturn {[m
[32m+[m				[32mr: parseInt( hex3.charAt( 0 ), 16 ) * 0x11,[m
[32m+[m				[32mg: parseInt( hex3.charAt( 1 ), 16 ) * 0x11,[m
[32m+[m				[32mb: parseInt( hex3.charAt( 2 ), 16 ) * 0x11[m
[32m+[m			[32m};[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar hex6 = color.match( /^#([0-9a-f]{6})$/i );[m
[32m+[m		[32mif( hex6 && hex6[1] ) {[m
[32m+[m			[32mhex6 = hex6[1];[m
[32m+[m			[32mreturn {[m
[32m+[m				[32mr: parseInt( hex6.substr( 0, 2 ), 16 ),[m
[32m+[m				[32mg: parseInt( hex6.substr( 2, 2 ), 16 ),[m
[32m+[m				[32mb: parseInt( hex6.substr( 4, 2 ), 16 )[m
[32m+[m			[32m};[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar rgb = color.match( /^rgb\s*\(\s*(\d+)\s*,\s*(\d+)\s*,\s*(\d+)\s*\)$/i );[m
[32m+[m		[32mif( rgb ) {[m
[32m+[m			[32mreturn {[m
[32m+[m				[32mr: parseInt( rgb[1], 10 ),[m
[32m+[m				[32mg: parseInt( rgb[2], 10 ),[m
[32m+[m				[32mb: parseInt( rgb[3], 10 )[m
[32m+[m			[32m};[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar rgba = color.match( /^rgba\s*\(\s*(\d+)\s*,\s*(\d+)\s*,\s*(\d+)\s*\,\s*([\d]+|[\d]*.[\d]+)\s*\)$/i );[m
[32m+[m		[32mif( rgba ) {[m
[32m+[m			[32mreturn {[m
[32m+[m				[32mr: parseInt( rgba[1], 10 ),[m
[32m+[m				[32mg: parseInt( rgba[2], 10 ),[m
[32m+[m				[32mb: parseInt( rgba[3], 10 ),[m
[32m+[m				[32ma: parseFloat( rgba[4] )[m
[32m+[m			[32m};[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn null;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Calculates brightness on a scale of 0-255.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param color See colorStringToRgb for supported formats.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction colorBrightness( color ) {[m
[32m+[m
[32m+[m		[32mif( typeof color === 'string' ) color = colorToRgb( color );[m
[32m+[m
[32m+[m		[32mif( color ) {[m
[32m+[m			[32mreturn ( color.r * 299 + color.g * 587 + color.b * 114 ) / 1000;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn null;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Retrieves the height of the given element by looking[m
[32m+[m	[32m * at the position and height of its immediate children.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction getAbsoluteHeight( element ) {[m
[32m+[m
[32m+[m		[32mvar height = 0;[m
[32m+[m
[32m+[m		[32mif( element ) {[m
[32m+[m			[32mvar absoluteChildren = 0;[m
[32m+[m
[32m+[m			[32mtoArray( element.childNodes ).forEach( function( child ) {[m
[32m+[m
[32m+[m				[32mif( typeof child.offsetTop === 'number' && child.style ) {[m
[32m+[m					[32m// Count # of abs children[m
[32m+[m					[32mif( window.getComputedStyle( child ).position === 'absolute' ) {[m
[32m+[m						[32mabsoluteChildren += 1;[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32mheight = Math.max( height, child.offsetTop + child.offsetHeight );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m			[32m// If there are no absolute children, use offsetHeight[m
[32m+[m			[32mif( absoluteChildren === 0 ) {[m
[32m+[m				[32mheight = element.offsetHeight;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn height;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Returns the remaining height within the parent of the[m
[32m+[m	[32m * target element.[m
[32m+[m	[32m *[m
[32m+[m	[32m * remaining height = [ configured parent height ] - [ current parent height ][m
[32m+[m	[32m */[m
[32m+[m	[32mfunction getRemainingHeight( element, height ) {[m
[32m+[m
[32m+[m		[32mheight = height || 0;[m
[32m+[m
[32m+[m		[32mif( element ) {[m
[32m+[m			[32mvar newHeight, oldHeight = element.style.height;[m
[32m+[m
[32m+[m			[32m// Change the .stretch element height to 0 in order find the height of all[m
[32m+[m			[32m// the other elements[m
[32m+[m			[32melement.style.height = '0px';[m
[32m+[m			[32mnewHeight = height - element.parentNode.offsetHeight;[m
[32m+[m
[32m+[m			[32m// Restore the old height, just in case[m
[32m+[m			[32melement.style.height = oldHeight + 'px';[m
[32m+[m
[32m+[m			[32mreturn newHeight;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn height;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Checks if this instance is being used to print a PDF.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction isPrintingPDF() {[m
[32m+[m
[32m+[m		[32mreturn ( /print-pdf/gi ).test( window.location.search );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Hides the address bar if we're on a mobile device.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction hideAddressBar() {[m
[32m+[m
[32m+[m		[32mif( config.hideAddressBar && isMobileDevice ) {[m
[32m+[m			[32m// Events that should trigger the address bar to hide[m
[32m+[m			[32mwindow.addEventListener( 'load', removeAddressBar, false );[m
[32m+[m			[32mwindow.addEventListener( 'orientationchange', removeAddressBar, false );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Causes the address bar to hide on mobile devices,[m
[32m+[m	[32m * more vertical space ftw.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction removeAddressBar() {[m
[32m+[m
[32m+[m		[32msetTimeout( function() {[m
[32m+[m			[32mwindow.scrollTo( 0, 1 );[m
[32m+[m		[32m}, 10 );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Dispatches an event of the specified type from the[m
[32m+[m	[32m * reveal DOM element.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction dispatchEvent( type, args ) {[m
[32m+[m
[32m+[m		[32mvar event = document.createEvent( 'HTMLEvents', 1, 2 );[m
[32m+[m		[32mevent.initEvent( type, true, true );[m
[32m+[m		[32mextend( event, args );[m
[32m+[m		[32mdom.wrapper.dispatchEvent( event );[m
[32m+[m
[32m+[m		[32m// If we're in an iframe, post each reveal.js event to the[m
[32m+[m		[32m// parent window. Used by the notes plugin[m
[32m+[m		[32mif( config.postMessageEvents && window.parent !== window.self ) {[m
[32m+[m			[32mwindow.parent.postMessage( JSON.stringify({ namespace: 'reveal', eventName: type, state: getState() }), '*' );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Wrap all links in 3D goodness.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction enableRollingLinks() {[m
[32m+[m
[32m+[m		[32mif( features.transforms3d && !( 'msPerspective' in document.body.style ) ) {[m
[32m+[m			[32mvar anchors = dom.wrapper.querySelectorAll( SLIDES_SELECTOR + ' a' );[m
[32m+[m
[32m+[m			[32mfor( var i = 0, len = anchors.length; i < len; i++ ) {[m
[32m+[m				[32mvar anchor = anchors[i];[m
[32m+[m
[32m+[m				[32mif( anchor.textContent && !anchor.querySelector( '*' ) && ( !anchor.className || !anchor.classList.contains( anchor, 'roll' ) ) ) {[m
[32m+[m					[32mvar span = document.createElement('span');[m
[32m+[m					[32mspan.setAttribute('data-title', anchor.text);[m
[32m+[m					[32mspan.innerHTML = anchor.innerHTML;[m
[32m+[m
[32m+[m					[32manchor.classList.add( 'roll' );[m
[32m+[m					[32manchor.innerHTML = '';[m
[32m+[m					[32manchor.appendChild(span);[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Unwrap all 3D links.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction disableRollingLinks() {[m
[32m+[m
[32m+[m		[32mvar anchors = dom.wrapper.querySelectorAll( SLIDES_SELECTOR + ' a.roll' );[m
[32m+[m
[32m+[m		[32mfor( var i = 0, len = anchors.length; i < len; i++ ) {[m
[32m+[m			[32mvar anchor = anchors[i];[m
[32m+[m			[32mvar span = anchor.querySelector( 'span' );[m
[32m+[m
[32m+[m			[32mif( span ) {[m
[32m+[m				[32manchor.classList.remove( 'roll' );[m
[32m+[m				[32manchor.innerHTML = span.innerHTML;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Bind preview frame links.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction enablePreviewLinks( selector ) {[m
[32m+[m
[32m+[m		[32mvar anchors = toArray( document.querySelectorAll( selector ? selector : 'a' ) );[m
[32m+[m
[32m+[m		[32manchors.forEach( function( element ) {[m
[32m+[m			[32mif( /^(http|www)/gi.test( element.getAttribute( 'href' ) ) ) {[m
[32m+[m				[32melement.addEventListener( 'click', onPreviewLinkClicked, false );[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Unbind preview frame links.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction disablePreviewLinks() {[m
[32m+[m
[32m+[m		[32mvar anchors = toArray( document.querySelectorAll( 'a' ) );[m
[32m+[m
[32m+[m		[32manchors.forEach( function( element ) {[m
[32m+[m			[32mif( /^(http|www)/gi.test( element.getAttribute( 'href' ) ) ) {[m
[32m+[m				[32melement.removeEventListener( 'click', onPreviewLinkClicked, false );[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Opens a preview window for the target URL.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction showPreview( url ) {[m
[32m+[m
[32m+[m		[32mcloseOverlay();[m
[32m+[m
[32m+[m		[32mdom.overlay = document.createElement( 'div' );[m
[32m+[m		[32mdom.overlay.classList.add( 'overlay' );[m
[32m+[m		[32mdom.overlay.classList.add( 'overlay-preview' );[m
[32m+[m		[32mdom.wrapper.appendChild( dom.overlay );[m
[32m+[m
[32m+[m		[32mdom.overlay.innerHTML = [[m
[32m+[m			[32m'<header>',[m
[32m+[m				[32m'<a class="close" href="#"><span class="icon"></span></a>',[m
[32m+[m				[32m'<a class="external" href="'+ url +'" target="_blank"><span class="icon"></span></a>',[m
[32m+[m			[32m'</header>',[m
[32m+[m			[32m'<div class="spinner"></div>',[m
[32m+[m			[32m'<div class="viewport">',[m
[32m+[m				[32m'<iframe src="'+ url +'"></iframe>',[m
[32m+[m			[32m'</div>'[m
[32m+[m		[32m].join('');[m
[32m+[m
[32m+[m		[32mdom.overlay.querySelector( 'iframe' ).addEventListener( 'load', function( event ) {[m
[32m+[m			[32mdom.overlay.classList.add( 'loaded' );[m
[32m+[m		[32m}, false );[m
[32m+[m
[32m+[m		[32mdom.overlay.querySelector( '.close' ).addEventListener( 'click', function( event ) {[m
[32m+[m			[32mcloseOverlay();[m
[32m+[m			[32mevent.preventDefault();[m
[32m+[m		[32m}, false );[m
[32m+[m
[32m+[m		[32mdom.overlay.querySelector( '.external' ).addEventListener( 'click', function( event ) {[m
[32m+[m			[32mcloseOverlay();[m
[32m+[m		[32m}, false );[m
[32m+[m
[32m+[m		[32msetTimeout( function() {[m
[32m+[m			[32mdom.overlay.classList.add( 'visible' );[m
[32m+[m		[32m}, 1 );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Opens a overlay window with help material.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction showHelp() {[m
[32m+[m
[32m+[m		[32mif( config.help ) {[m
[32m+[m
[32m+[m			[32mcloseOverlay();[m
[32m+[m
[32m+[m			[32mdom.overlay = document.createElement( 'div' );[m
[32m+[m			[32mdom.overlay.classList.add( 'overlay' );[m
[32m+[m			[32mdom.overlay.classList.add( 'overlay-help' );[m
[32m+[m			[32mdom.wrapper.appendChild( dom.overlay );[m
[32m+[m
[32m+[m			[32mvar html = '<p class="title">Keyboard Shortcuts</p><br/>';[m
[32m+[m
[32m+[m			[32mhtml += '<table><th>KEY</th><th>ACTION</th>';[m
[32m+[m			[32mfor( var key in keyboardShortcuts ) {[m
[32m+[m				[32mhtml += '<tr><td>' + key + '</td><td>' + keyboardShortcuts[ key ] + '</td></tr>';[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mhtml += '</table>';[m
[32m+[m
[32m+[m			[32mdom.overlay.innerHTML = [[m
[32m+[m				[32m'<header>',[m
[32m+[m					[32m'<a class="close" href="#"><span class="icon"></span></a>',[m
[32m+[m				[32m'</header>',[m
[32m+[m				[32m'<div class="viewport">',[m
[32m+[m					[32m'<div class="viewport-inner">'+ html +'</div>',[m
[32m+[m				[32m'</div>'[m
[32m+[m			[32m].join('');[m
[32m+[m
[32m+[m			[32mdom.overlay.querySelector( '.close' ).addEventListener( 'click', function( event ) {[m
[32m+[m				[32mcloseOverlay();[m
[32m+[m				[32mevent.preventDefault();[m
[32m+[m			[32m}, false );[m
[32m+[m
[32m+[m			[32msetTimeout( function() {[m
[32m+[m				[32mdom.overlay.classList.add( 'visible' );[m
[32m+[m			[32m}, 1 );[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Closes any currently open overlay.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction closeOverlay() {[m
[32m+[m
[32m+[m		[32mif( dom.overlay ) {[m
[32m+[m			[32mdom.overlay.parentNode.removeChild( dom.overlay );[m
[32m+[m			[32mdom.overlay = null;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Applies JavaScript-controlled layout rules to the[m
[32m+[m	[32m * presentation.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction layout() {[m
[32m+[m
[32m+[m		[32mif( dom.wrapper && !isPrintingPDF() ) {[m
[32m+[m
[32m+[m			[32mvar size = getComputedSlideSize();[m
[32m+[m
[32m+[m			[32mvar slidePadding = 20; // TODO Dig this out of DOM[m
[32m+[m
[32m+[m			[32m// Layout the contents of the slides[m
[32m+[m			[32mlayoutSlideContents( config.width, config.height, slidePadding );[m
[32m+[m
[32m+[m			[32mdom.slides.style.width = size.width + 'px';[m
[32m+[m			[32mdom.slides.style.height = size.height + 'px';[m
[32m+[m
[32m+[m			[32m// Determine scale of content to fit within available space[m
[32m+[m			[32mscale = Math.min( size.presentationWidth / size.width, size.presentationHeight / size.height );[m
[32m+[m
[32m+[m			[32m// Respect max/min scale settings[m
[32m+[m			[32mscale = Math.max( scale, config.minScale );[m
[32m+[m			[32mscale = Math.min( scale, config.maxScale );[m
[32m+[m
[32m+[m			[32m// Don't apply any scaling styles if scale is 1[m
[32m+[m			[32mif( scale === 1 ) {[m
[32m+[m				[32mdom.slides.style.zoom = '';[m
[32m+[m				[32mdom.slides.style.left = '';[m
[32m+[m				[32mdom.slides.style.top = '';[m
[32m+[m				[32mdom.slides.style.bottom = '';[m
[32m+[m				[32mdom.slides.style.right = '';[m
[32m+[m				[32mtransformSlides( { layout: '' } );[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32m// Use zoom to scale up in desktop Chrome so that content[m
[32m+[m				[32m// remains crisp. We don't use zoom to scale down since that[m
[32m+[m				[32m// can lead to shifts in text layout/line breaks.[m
[32m+[m				[32mif( scale > 1 && !isMobileDevice && /chrome/i.test( navigator.userAgent ) && typeof dom.slides.style.zoom !== 'undefined' ) {[m
[32m+[m					[32mdom.slides.style.zoom = scale;[m
[32m+[m					[32mdom.slides.style.left = '';[m
[32m+[m					[32mdom.slides.style.top = '';[m
[32m+[m					[32mdom.slides.style.bottom = '';[m
[32m+[m					[32mdom.slides.style.right = '';[m
[32m+[m					[32mtransformSlides( { layout: '' } );[m
[32m+[m				[32m}[m
[32m+[m				[32m// Apply scale transform as a fallback[m
[32m+[m				[32melse {[m
[32m+[m					[32mdom.slides.style.zoom = '';[m
[32m+[m					[32mdom.slides.style.left = '50%';[m
[32m+[m					[32mdom.slides.style.top = '50%';[m
[32m+[m					[32mdom.slides.style.bottom = 'auto';[m
[32m+[m					[32mdom.slides.style.right = 'auto';[m
[32m+[m					[32mtransformSlides( { layout: 'translate(-50%, -50%) scale('+ scale +')' } );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Select all slides, vertical and horizontal[m
[32m+[m			[32mvar slides = toArray( dom.wrapper.querySelectorAll( SLIDES_SELECTOR ) );[m
[32m+[m
[32m+[m			[32mfor( var i = 0, len = slides.length; i < len; i++ ) {[m
[32m+[m				[32mvar slide = slides[ i ];[m
[32m+[m
[32m+[m				[32m// Don't bother updating invisible slides[m
[32m+[m				[32mif( slide.style.display === 'none' ) {[m
[32m+[m					[32mcontinue;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif( config.center || slide.classList.contains( 'center' ) ) {[m
[32m+[m					[32m// Vertical stacks are not centred since their section[m
[32m+[m					[32m// children will be[m
[32m+[m					[32mif( slide.classList.contains( 'stack' ) ) {[m
[32m+[m						[32mslide.style.top = 0;[m
[32m+[m					[32m}[m
[32m+[m					[32melse {[m
[32m+[m						[32mslide.style.top = Math.max( ( ( size.height - getAbsoluteHeight( slide ) ) / 2 ) - slidePadding, 0 ) + 'px';[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m				[32melse {[m
[32m+[m					[32mslide.style.top = '';[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mupdateProgress();[m
[32m+[m			[32mupdateParallax();[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Applies layout logic to the contents of all slides in[m
[32m+[m	[32m * the presentation.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction layoutSlideContents( width, height, padding ) {[m
[32m+[m
[32m+[m		[32m// Handle sizing of elements with the 'stretch' class[m
[32m+[m		[32mtoArray( dom.slides.querySelectorAll( 'section > .stretch' ) ).forEach( function( element ) {[m
[32m+[m
[32m+[m			[32m// Determine how much vertical space we can use[m
[32m+[m			[32mvar remainingHeight = getRemainingHeight( element, height );[m
[32m+[m
[32m+[m			[32m// Consider the aspect ratio of media elements[m
[32m+[m			[32mif( /(img|video)/gi.test( element.nodeName ) ) {[m
[32m+[m				[32mvar nw = element.naturalWidth || element.videoWidth,[m
[32m+[m					[32mnh = element.naturalHeight || element.videoHeight;[m
[32m+[m
[32m+[m				[32mvar es = Math.min( width / nw, remainingHeight / nh );[m
[32m+[m
[32m+[m				[32melement.style.width = ( nw * es ) + 'px';[m
[32m+[m				[32melement.style.height = ( nh * es ) + 'px';[m
[32m+[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32melement.style.width = width + 'px';[m
[32m+[m				[32melement.style.height = remainingHeight + 'px';[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Calculates the computed pixel size of our slides. These[m
[32m+[m	[32m * values are based on the width and height configuration[m
[32m+[m	[32m * options.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction getComputedSlideSize( presentationWidth, presentationHeight ) {[m
[32m+[m
[32m+[m		[32mvar size = {[m
[32m+[m			[32m// Slide size[m
[32m+[m			[32mwidth: config.width,[m
[32m+[m			[32mheight: config.height,[m
[32m+[m
[32m+[m			[32m// Presentation size[m
[32m+[m			[32mpresentationWidth: presentationWidth || dom.wrapper.offsetWidth,[m
[32m+[m			[32mpresentationHeight: presentationHeight || dom.wrapper.offsetHeight[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32m// Reduce available space by margin[m
[32m+[m		[32msize.presentationWidth -= ( size.presentationWidth * config.margin );[m
[32m+[m		[32msize.presentationHeight -= ( size.presentationHeight * config.margin );[m
[32m+[m
[32m+[m		[32m// Slide width may be a percentage of available width[m
[32m+[m		[32mif( typeof size.width === 'string' && /%$/.test( size.width ) ) {[m
[32m+[m			[32msize.width = parseInt( size.width, 10 ) / 100 * size.presentationWidth;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Slide height may be a percentage of available height[m
[32m+[m		[32mif( typeof size.height === 'string' && /%$/.test( size.height ) ) {[m
[32m+[m			[32msize.height = parseInt( size.height, 10 ) / 100 * size.presentationHeight;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn size;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Stores the vertical index of a stack so that the same[m
[32m+[m	[32m * vertical slide can be selected when navigating to and[m
[32m+[m	[32m * from the stack.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {HTMLElement} stack The vertical stack element[m
[32m+[m	[32m * @param {int} v Index to memorize[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction setPreviousVerticalIndex( stack, v ) {[m
[32m+[m
[32m+[m		[32mif( typeof stack === 'object' && typeof stack.setAttribute === 'function' ) {[m
[32m+[m			[32mstack.setAttribute( 'data-previous-indexv', v || 0 );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Retrieves the vertical index which was stored using[m
[32m+[m	[32m * #setPreviousVerticalIndex() or 0 if no previous index[m
[32m+[m	[32m * exists.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {HTMLElement} stack The vertical stack element[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction getPreviousVerticalIndex( stack ) {[m
[32m+[m
[32m+[m		[32mif( typeof stack === 'object' && typeof stack.setAttribute === 'function' && stack.classList.contains( 'stack' ) ) {[m
[32m+[m			[32m// Prefer manually defined start-indexv[m
[32m+[m			[32mvar attributeName = stack.hasAttribute( 'data-start-indexv' ) ? 'data-start-indexv' : 'data-previous-indexv';[m
[32m+[m
[32m+[m			[32mreturn parseInt( stack.getAttribute( attributeName ) || 0, 10 );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn 0;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Displays the overview of slides (quick nav) by scaling[m
[32m+[m	[32m * down and arranging all slide elements.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction activateOverview() {[m
[32m+[m
[32m+[m		[32m// Only proceed if enabled in config[m
[32m+[m		[32mif( config.overview && !isOverview() ) {[m
[32m+[m
[32m+[m			[32moverview = true;[m
[32m+[m
[32m+[m			[32mdom.wrapper.classList.add( 'overview' );[m
[32m+[m			[32mdom.wrapper.classList.remove( 'overview-deactivating' );[m
[32m+[m
[32m+[m			[32mif( features.overviewTransitions ) {[m
[32m+[m				[32msetTimeout( function() {[m
[32m+[m					[32mdom.wrapper.classList.add( 'overview-animated' );[m
[32m+[m				[32m}, 1 );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Don't auto-slide while in overview mode[m
[32m+[m			[32mcancelAutoSlide();[m
[32m+[m
[32m+[m			[32m// Move the backgrounds element into the slide container to[m
[32m+[m			[32m// that the same scaling is applied[m
[32m+[m			[32mdom.slides.appendChild( dom.background );[m
[32m+[m
[32m+[m			[32m// Clicking on an overview slide navigates to it[m
[32m+[m			[32mtoArray( dom.wrapper.querySelectorAll( SLIDES_SELECTOR ) ).forEach( function( slide ) {[m
[32m+[m				[32mif( !slide.classList.contains( 'stack' ) ) {[m
[32m+[m					[32mslide.addEventListener( 'click', onOverviewSlideClicked, true );[m
[32m+[m				[32m}[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m			[32mupdateSlidesVisibility();[m
[32m+[m			[32mlayoutOverview();[m
[32m+[m			[32mupdateOverview();[m
[32m+[m
[32m+[m			[32mlayout();[m
[32m+[m
[32m+[m			[32m// Notify observers of the overview showing[m
[32m+[m			[32mdispatchEvent( 'overviewshown', {[m
[32m+[m				[32m'indexh': indexh,[m
[32m+[m				[32m'indexv': indexv,[m
[32m+[m				[32m'currentSlide': currentSlide[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Uses CSS transforms to position all slides in a grid for[m
[32m+[m	[32m * display inside of the overview mode.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction layoutOverview() {[m
[32m+[m
[32m+[m		[32mvar margin = 70;[m
[32m+[m		[32mvar slideWidth = config.width + margin,[m
[32m+[m			[32mslideHeight = config.height + margin;[m
[32m+[m
[32m+[m		[32m// Reverse in RTL mode[m
[32m+[m		[32mif( config.rtl ) {[m
[32m+[m			[32mslideWidth = -slideWidth;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Layout slides[m
[32m+[m		[32mtoArray( dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR ) ).forEach( function( hslide, h ) {[m
[32m+[m			[32mhslide.setAttribute( 'data-index-h', h );[m
[32m+[m			[32mtransformElement( hslide, 'translate3d(' + ( h * slideWidth ) + 'px, 0, 0)' );[m
[32m+[m
[32m+[m			[32mif( hslide.classList.contains( 'stack' ) ) {[m
[32m+[m
[32m+[m				[32mtoArray( hslide.querySelectorAll( 'section' ) ).forEach( function( vslide, v ) {[m
[32m+[m					[32mvslide.setAttribute( 'data-index-h', h );[m
[32m+[m					[32mvslide.setAttribute( 'data-index-v', v );[m
[32m+[m
[32m+[m					[32mtransformElement( vslide, 'translate3d(0, ' + ( v * slideHeight ) + 'px, 0)' );[m
[32m+[m				[32m} );[m
[32m+[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m		[32m// Layout slide backgrounds[m
[32m+[m		[32mtoArray( dom.background.childNodes ).forEach( function( hbackground, h ) {[m
[32m+[m			[32mtransformElement( hbackground, 'translate3d(' + ( h * slideWidth ) + 'px, 0, 0)' );[m
[32m+[m
[32m+[m			[32mtoArray( hbackground.querySelectorAll( '.slide-background' ) ).forEach( function( vbackground, v ) {[m
[32m+[m				[32mtransformElement( vbackground, 'translate3d(0, ' + ( v * slideHeight ) + 'px, 0)' );[m
[32m+[m			[32m} );[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Moves the overview viewport to the current slides.[m
[32m+[m	[32m * Called each time the current slide changes.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction updateOverview() {[m
[32m+[m
[32m+[m		[32mvar margin = 70;[m
[32m+[m		[32mvar slideWidth = config.width + margin,[m
[32m+[m			[32mslideHeight = config.height + margin;[m
[32m+[m
[32m+[m		[32m// Reverse in RTL mode[m
[32m+[m		[32mif( config.rtl ) {[m
[32m+[m			[32mslideWidth = -slideWidth;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mtransformSlides( {[m
[32m+[m			[32moverview: [[m
[32m+[m				[32m'translateX('+ ( -indexh * slideWidth ) +'px)',[m
[32m+[m				[32m'translateY('+ ( -indexv * slideHeight ) +'px)',[m
[32m+[m				[32m'translateZ('+ ( window.innerWidth < 400 ? -1000 : -2500 ) +'px)'[m
[32m+[m			[32m].join( ' ' )[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Exits the slide overview and enters the currently[m
[32m+[m	[32m * active slide.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction deactivateOverview() {[m
[32m+[m
[32m+[m		[32m// Only proceed if enabled in config[m
[32m+[m		[32mif( config.overview ) {[m
[32m+[m
[32m+[m			[32moverview = false;[m
[32m+[m
[32m+[m			[32mdom.wrapper.classList.remove( 'overview' );[m
[32m+[m			[32mdom.wrapper.classList.remove( 'overview-animated' );[m
[32m+[m
[32m+[m			[32m// Temporarily add a class so that transitions can do different things[m
[32m+[m			[32m// depending on whether they are exiting/entering overview, or just[m
[32m+[m			[32m// moving from slide to slide[m
[32m+[m			[32mdom.wrapper.classList.add( 'overview-deactivating' );[m
[32m+[m
[32m+[m			[32msetTimeout( function () {[m
[32m+[m				[32mdom.wrapper.classList.remove( 'overview-deactivating' );[m
[32m+[m			[32m}, 1 );[m
[32m+[m
[32m+[m			[32m// Move the background element back out[m
[32m+[m			[32mdom.wrapper.appendChild( dom.background );[m
[32m+[m
[32m+[m			[32m// Clean up changes made to slides[m
[32m+[m			[32mtoArray( dom.wrapper.querySelectorAll( SLIDES_SELECTOR ) ).forEach( function( slide ) {[m
[32m+[m				[32mtransformElement( slide, '' );[m
[32m+[m
[32m+[m				[32mslide.removeEventListener( 'click', onOverviewSlideClicked, true );[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m			[32m// Clean up changes made to backgrounds[m
[32m+[m			[32mtoArray( dom.background.querySelectorAll( '.slide-background' ) ).forEach( function( background ) {[m
[32m+[m				[32mtransformElement( background, '' );[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m			[32mtransformSlides( { overview: '' } );[m
[32m+[m
[32m+[m			[32mslide( indexh, indexv );[m
[32m+[m
[32m+[m			[32mlayout();[m
[32m+[m
[32m+[m			[32mcueAutoSlide();[m
[32m+[m
[32m+[m			[32m// Notify observers of the overview hiding[m
[32m+[m			[32mdispatchEvent( 'overviewhidden', {[m
[32m+[m				[32m'indexh': indexh,[m
[32m+[m				[32m'indexv': indexv,[m
[32m+[m				[32m'currentSlide': currentSlide[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Toggles the slide overview mode on and off.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {Boolean} override Optional flag which overrides the[m
[32m+[m	[32m * toggle logic and forcibly sets the desired state. True means[m
[32m+[m	[32m * overview is open, false means it's closed.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction toggleOverview( override ) {[m
[32m+[m
[32m+[m		[32mif( typeof override === 'boolean' ) {[m
[32m+[m			[32moverride ? activateOverview() : deactivateOverview();[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32misOverview() ? deactivateOverview() : activateOverview();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Checks if the overview is currently active.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @return {Boolean} true if the overview is active,[m
[32m+[m	[32m * false otherwise[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction isOverview() {[m
[32m+[m
[32m+[m		[32mreturn overview;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Checks if the current or specified slide is vertical[m
[32m+[m	[32m * (nested within another slide).[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {HTMLElement} slide [optional] The slide to check[m
[32m+[m	[32m * orientation of[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction isVerticalSlide( slide ) {[m
[32m+[m
[32m+[m		[32m// Prefer slide argument, otherwise use current slide[m
[32m+[m		[32mslide = slide ? slide : currentSlide;[m
[32m+[m
[32m+[m		[32mreturn slide && slide.parentNode && !!slide.parentNode.nodeName.match( /section/i );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Handling the fullscreen functionality via the fullscreen API[m
[32m+[m	[32m *[m
[32m+[m	[32m * @see http://fullscreen.spec.whatwg.org/[m
[32m+[m	[32m * @see https://developer.mozilla.org/en-US/docs/DOM/Using_fullscreen_mode[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction enterFullscreen() {[m
[32m+[m
[32m+[m		[32mvar element = document.body;[m
[32m+[m
[32m+[m		[32m// Check which implementation is available[m
[32m+[m		[32mvar requestMethod = element.requestFullScreen ||[m
[32m+[m							[32melement.webkitRequestFullscreen ||[m
[32m+[m							[32melement.webkitRequestFullScreen ||[m
[32m+[m							[32melement.mozRequestFullScreen ||[m
[32m+[m							[32melement.msRequestFullscreen;[m
[32m+[m
[32m+[m		[32mif( requestMethod ) {[m
[32m+[m			[32mrequestMethod.apply( element );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Enters the paused mode which fades everything on screen to[m
[32m+[m	[32m * black.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction pause() {[m
[32m+[m
[32m+[m		[32mif( config.pause ) {[m
[32m+[m			[32mvar wasPaused = dom.wrapper.classList.contains( 'paused' );[m
[32m+[m
[32m+[m			[32mcancelAutoSlide();[m
[32m+[m			[32mdom.wrapper.classList.add( 'paused' );[m
[32m+[m
[32m+[m			[32mif( wasPaused === false ) {[m
[32m+[m				[32mdispatchEvent( 'paused' );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Exits from the paused mode.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction resume() {[m
[32m+[m
[32m+[m		[32mvar wasPaused = dom.wrapper.classList.contains( 'paused' );[m
[32m+[m		[32mdom.wrapper.classList.remove( 'paused' );[m
[32m+[m
[32m+[m		[32mcueAutoSlide();[m
[32m+[m
[32m+[m		[32mif( wasPaused ) {[m
[32m+[m			[32mdispatchEvent( 'resumed' );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Toggles the paused mode on and off.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction togglePause( override ) {[m
[32m+[m
[32m+[m		[32mif( typeof override === 'boolean' ) {[m
[32m+[m			[32moverride ? pause() : resume();[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32misPaused() ? resume() : pause();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Checks if we are currently in the paused mode.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction isPaused() {[m
[32m+[m
[32m+[m		[32mreturn dom.wrapper.classList.contains( 'paused' );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Toggles the auto slide mode on and off.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {Boolean} override Optional flag which sets the desired state.[m
[32m+[m	[32m * True means autoplay starts, false means it stops.[m
[32m+[m	[32m */[m
[32m+[m
[32m+[m	[32mfunction toggleAutoSlide( override ) {[m
[32m+[m
[32m+[m		[32mif( typeof override === 'boolean' ) {[m
[32m+[m			[32moverride ? resumeAutoSlide() : pauseAutoSlide();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32melse {[m
[32m+[m			[32mautoSlidePaused ? resumeAutoSlide() : pauseAutoSlide();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Checks if the auto slide mode is currently on.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction isAutoSliding() {[m
[32m+[m
[32m+[m		[32mreturn !!( autoSlide && !autoSlidePaused );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Steps from the current point in the presentation to the[m
[32m+[m	[32m * slide which matches the specified horizontal and vertical[m
[32m+[m	[32m * indices.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {int} h Horizontal index of the target slide[m
[32m+[m	[32m * @param {int} v Vertical index of the target slide[m
[32m+[m	[32m * @param {int} f Optional index of a fragment within the[m
[32m+[m	[32m * target slide to activate[m
[32m+[m	[32m * @param {int} o Optional origin for use in multimaster environments[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction slide( h, v, f, o ) {[m
[32m+[m
[32m+[m		[32m// Remember where we were at before[m
[32m+[m		[32mpreviousSlide = currentSlide;[m
[32m+[m
[32m+[m		[32m// Query all horizontal slides in the deck[m
[32m+[m		[32mvar horizontalSlides = dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR );[m
[32m+[m
[32m+[m		[32m// If no vertical index is specified and the upcoming slide is a[m
[32m+[m		[32m// stack, resume at its previous vertical index[m
[32m+[m		[32mif( v === undefined && !isOverview() ) {[m
[32m+[m			[32mv = getPreviousVerticalIndex( horizontalSlides[ h ] );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// If we were on a vertical stack, remember what vertical index[m
[32m+[m		[32m// it was on so we can resume at the same position when returning[m
[32m+[m		[32mif( previousSlide && previousSlide.parentNode && previousSlide.parentNode.classList.contains( 'stack' ) ) {[m
[32m+[m			[32msetPreviousVerticalIndex( previousSlide.parentNode, indexv );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Remember the state before this slide[m
[32m+[m		[32mvar stateBefore = state.concat();[m
[32m+[m
[32m+[m		[32m// Reset the state array[m
[32m+[m		[32mstate.length = 0;[m
[32m+[m
[32m+[m		[32mvar indexhBefore = indexh || 0,[m
[32m+[m			[32mindexvBefore = indexv || 0;[m
[32m+[m
[32m+[m		[32m// Activate and transition to the new slide[m
[32m+[m		[32mindexh = updateSlides( HORIZONTAL_SLIDES_SELECTOR, h === undefined ? indexh : h );[m
[32m+[m		[32mindexv = updateSlides( VERTICAL_SLIDES_SELECTOR, v === undefined ? indexv : v );[m
[32m+[m
[32m+[m		[32m// Update the visibility of slides now that the indices have changed[m
[32m+[m		[32mupdateSlidesVisibility();[m
[32m+[m
[32m+[m		[32mlayout();[m
[32m+[m
[32m+[m		[32m// Apply the new state[m
[32m+[m		[32mstateLoop: for( var i = 0, len = state.length; i < len; i++ ) {[m
[32m+[m			[32m// Check if this state existed on the previous slide. If it[m
[32m+[m			[32m// did, we will avoid adding it repeatedly[m
[32m+[m			[32mfor( var j = 0; j < stateBefore.length; j++ ) {[m
[32m+[m				[32mif( stateBefore[j] === state[i] ) {[m
[32m+[m					[32mstateBefore.splice( j, 1 );[m
[32m+[m					[32mcontinue stateLoop;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mdocument.documentElement.classList.add( state[i] );[m
[32m+[m
[32m+[m			[32m// Dispatch custom event matching the state's name[m
[32m+[m			[32mdispatchEvent( state[i] );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Clean up the remains of the previous state[m
[32m+[m		[32mwhile( stateBefore.length ) {[m
[32m+[m			[32mdocument.documentElement.classList.remove( stateBefore.pop() );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Update the overview if it's currently active[m
[32m+[m		[32mif( isOverview() ) {[m
[32m+[m			[32mupdateOverview();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Find the current horizontal slide and any possible vertical slides[m
[32m+[m		[32m// within it[m
[32m+[m		[32mvar currentHorizontalSlide = horizontalSlides[ indexh ],[m
[32m+[m			[32mcurrentVerticalSlides = currentHorizontalSlide.querySelectorAll( 'section' );[m
[32m+[m
[32m+[m		[32m// Store references to the previous and current slides[m
[32m+[m		[32mcurrentSlide = currentVerticalSlides[ indexv ] || currentHorizontalSlide;[m
[32m+[m
[32m+[m		[32m// Show fragment, if specified[m
[32m+[m		[32mif( typeof f !== 'undefined' ) {[m
[32m+[m			[32mnavigateFragment( f );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Dispatch an event if the slide changed[m
[32m+[m		[32mvar slideChanged = ( indexh !== indexhBefore || indexv !== indexvBefore );[m
[32m+[m		[32mif( slideChanged ) {[m
[32m+[m			[32mdispatchEvent( 'slidechanged', {[m
[32m+[m				[32m'indexh': indexh,[m
[32m+[m				[32m'indexv': indexv,[m
[32m+[m				[32m'previousSlide': previousSlide,[m
[32m+[m				[32m'currentSlide': currentSlide,[m
[32m+[m				[32m'origin': o[m
[32m+[m			[32m} );[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32m// Ensure that the previous slide is never the same as the current[m
[32m+[m			[32mpreviousSlide = null;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Solves an edge case where the previous slide maintains the[m
[32m+[m		[32m// 'present' class when navigating between adjacent vertical[m
[32m+[m		[32m// stacks[m
[32m+[m		[32mif( previousSlide ) {[m
[32m+[m			[32mpreviousSlide.classList.remove( 'present' );[m
[32m+[m			[32mpreviousSlide.setAttribute( 'aria-hidden', 'true' );[m
[32m+[m
[32m+[m			[32m// Reset all slides upon navigate to home[m
[32m+[m			[32m// Issue: #285[m
[32m+[m			[32mif ( dom.wrapper.querySelector( HOME_SLIDE_SELECTOR ).classList.contains( 'present' ) ) {[m
[32m+[m				[32m// Launch async task[m
[32m+[m				[32msetTimeout( function () {[m
[32m+[m					[32mvar slides = toArray( dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR + '.stack') ), i;[m
[32m+[m					[32mfor( i in slides ) {[m
[32m+[m						[32mif( slides[i] ) {[m
[32m+[m							[32m// Reset stack[m
[32m+[m							[32msetPreviousVerticalIndex( slides[i], 0 );[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m}, 0 );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Handle embedded content[m
[32m+[m		[32mif( slideChanged || !previousSlide ) {[m
[32m+[m			[32mstopEmbeddedContent( previousSlide );[m
[32m+[m			[32mstartEmbeddedContent( currentSlide );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Announce the current slide contents, for screen readers[m
[32m+[m		[32mdom.statusDiv.textContent = currentSlide.textContent;[m
[32m+[m
[32m+[m		[32mupdateControls();[m
[32m+[m		[32mupdateProgress();[m
[32m+[m		[32mupdateBackground();[m
[32m+[m		[32mupdateParallax();[m
[32m+[m		[32mupdateSlideNumber();[m
[32m+[m		[32mupdateNotes();[m
[32m+[m
[32m+[m		[32m// Update the URL hash[m
[32m+[m		[32mwriteURL();[m
[32m+[m
[32m+[m		[32mcueAutoSlide();[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Syncs the presentation with the current DOM. Useful[m
[32m+[m	[32m * when new slides or control elements are added or when[m
[32m+[m	[32m * the configuration has changed.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction sync() {[m
[32m+[m
[32m+[m		[32m// Subscribe to input[m
[32m+[m		[32mremoveEventListeners();[m
[32m+[m		[32maddEventListeners();[m
[32m+[m
[32m+[m		[32m// Force a layout to make sure the current config is accounted for[m
[32m+[m		[32mlayout();[m
[32m+[m
[32m+[m		[32m// Reflect the current autoSlide value[m
[32m+[m		[32mautoSlide = config.autoSlide;[m
[32m+[m
[32m+[m		[32m// Start auto-sliding if it's enabled[m
[32m+[m		[32mcueAutoSlide();[m
[32m+[m
[32m+[m		[32m// Re-create the slide backgrounds[m
[32m+[m		[32mcreateBackgrounds();[m
[32m+[m
[32m+[m		[32m// Write the current hash to the URL[m
[32m+[m		[32mwriteURL();[m
[32m+[m
[32m+[m		[32msortAllFragments();[m
[32m+[m
[32m+[m		[32mupdateControls();[m
[32m+[m		[32mupdateProgress();[m
[32m+[m		[32mupdateBackground( true );[m
[32m+[m		[32mupdateSlideNumber();[m
[32m+[m		[32mupdateSlidesVisibility();[m
[32m+[m		[32mupdateNotes();[m
[32m+[m
[32m+[m		[32mformatEmbeddedContent();[m
[32m+[m		[32mstartEmbeddedContent( currentSlide );[m
[32m+[m
[32m+[m		[32mif( isOverview() ) {[m
[32m+[m			[32mlayoutOverview();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Resets all vertical slides so that only the first[m
[32m+[m	[32m * is visible.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction resetVerticalSlides() {[m
[32m+[m
[32m+[m		[32mvar horizontalSlides = toArray( dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR ) );[m
[32m+[m		[32mhorizontalSlides.forEach( function( horizontalSlide ) {[m
[32m+[m
[32m+[m			[32mvar verticalSlides = toArray( horizontalSlide.querySelectorAll( 'section' ) );[m
[32m+[m			[32mverticalSlides.forEach( function( verticalSlide, y ) {[m
[32m+[m
[32m+[m				[32mif( y > 0 ) {[m
[32m+[m					[32mverticalSlide.classList.remove( 'present' );[m
[32m+[m					[32mverticalSlide.classList.remove( 'past' );[m
[32m+[m					[32mverticalSlide.classList.add( 'future' );[m
[32m+[m					[32mverticalSlide.setAttribute( 'aria-hidden', 'true' );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Sorts and formats all of fragments in the[m
[32m+[m	[32m * presentation.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction sortAllFragments() {[m
[32m+[m
[32m+[m		[32mvar horizontalSlides = toArray( dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR ) );[m
[32m+[m		[32mhorizontalSlides.forEach( function( horizontalSlide ) {[m
[32m+[m
[32m+[m			[32mvar verticalSlides = toArray( horizontalSlide.querySelectorAll( 'section' ) );[m
[32m+[m			[32mverticalSlides.forEach( function( verticalSlide, y ) {[m
[32m+[m
[32m+[m				[32msortFragments( verticalSlide.querySelectorAll( '.fragment' ) );[m
[32m+[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m			[32mif( verticalSlides.length === 0 ) sortFragments( horizontalSlide.querySelectorAll( '.fragment' ) );[m
[32m+[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Updates one dimension of slides by showing the slide[m
[32m+[m	[32m * with the specified index.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {String} selector A CSS selector that will fetch[m
[32m+[m	[32m * the group of slides we are working with[m
[32m+[m	[32m * @param {Number} index The index of the slide that should be[m
[32m+[m	[32m * shown[m
[32m+[m	[32m *[m
[32m+[m	[32m * @return {Number} The index of the slide that is now shown,[m
[32m+[m	[32m * might differ from the passed in index if it was out of[m
[32m+[m	[32m * bounds.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction updateSlides( selector, index ) {[m
[32m+[m
[32m+[m		[32m// Select all slides and convert the NodeList result to[m
[32m+[m		[32m// an array[m
[32m+[m		[32mvar slides = toArray( dom.wrapper.querySelectorAll( selector ) ),[m
[32m+[m			[32mslidesLength = slides.length;[m
[32m+[m
[32m+[m		[32mvar printMode = isPrintingPDF();[m
[32m+[m
[32m+[m		[32mif( slidesLength ) {[m
[32m+[m
[32m+[m			[32m// Should the index loop?[m
[32m+[m			[32mif( config.loop ) {[m
[32m+[m				[32mindex %= slidesLength;[m
[32m+[m
[32m+[m				[32mif( index < 0 ) {[m
[32m+[m					[32mindex = slidesLength + index;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Enforce max and minimum index bounds[m
[32m+[m			[32mindex = Math.max( Math.min( index, slidesLength - 1 ), 0 );[m
[32m+[m
[32m+[m			[32mfor( var i = 0; i < slidesLength; i++ ) {[m
[32m+[m				[32mvar element = slides[i];[m
[32m+[m
[32m+[m				[32mvar reverse = config.rtl && !isVerticalSlide( element );[m
[32m+[m
[32m+[m				[32melement.classList.remove( 'past' );[m
[32m+[m				[32melement.classList.remove( 'present' );[m
[32m+[m				[32melement.classList.remove( 'future' );[m
[32m+[m
[32m+[m				[32m// http://www.w3.org/html/wg/drafts/html/master/editing.html#the-hidden-attribute[m
[32m+[m				[32melement.setAttribute( 'hidden', '' );[m
[32m+[m				[32melement.setAttribute( 'aria-hidden', 'true' );[m
[32m+[m
[32m+[m				[32m// If this element contains vertical slides[m
[32m+[m				[32mif( element.querySelector( 'section' ) ) {[m
[32m+[m					[32melement.classList.add( 'stack' );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// If we're printing static slides, all slides are "present"[m
[32m+[m				[32mif( printMode ) {[m
[32m+[m					[32melement.classList.add( 'present' );[m
[32m+[m					[32mcontinue;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif( i < index ) {[m
[32m+[m					[32m// Any element previous to index is given the 'past' class[m
[32m+[m					[32melement.classList.add( reverse ? 'future' : 'past' );[m
[32m+[m
[32m+[m					[32mif( config.fragments ) {[m
[32m+[m						[32mvar pastFragments = toArray( element.querySelectorAll( '.fragment' ) );[m
[32m+[m
[32m+[m						[32m// Show all fragments on prior slides[m
[32m+[m						[32mwhile( pastFragments.length ) {[m
[32m+[m							[32mvar pastFragment = pastFragments.pop();[m
[32m+[m							[32mpastFragment.classList.add( 'visible' );[m
[32m+[m							[32mpastFragment.classList.remove( 'current-fragment' );[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m				[32melse if( i > index ) {[m
[32m+[m					[32m// Any element subsequent to index is given the 'future' class[m
[32m+[m					[32melement.classList.add( reverse ? 'past' : 'future' );[m
[32m+[m
[32m+[m					[32mif( config.fragments ) {[m
[32m+[m						[32mvar futureFragments = toArray( element.querySelectorAll( '.fragment.visible' ) );[m
[32m+[m
[32m+[m						[32m// No fragments in future slides should be visible ahead of time[m
[32m+[m						[32mwhile( futureFragments.length ) {[m
[32m+[m							[32mvar futureFragment = futureFragments.pop();[m
[32m+[m							[32mfutureFragment.classList.remove( 'visible' );[m
[32m+[m							[32mfutureFragment.classList.remove( 'current-fragment' );[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Mark the current slide as present[m
[32m+[m			[32mslides[index].classList.add( 'present' );[m
[32m+[m			[32mslides[index].removeAttribute( 'hidden' );[m
[32m+[m			[32mslides[index].removeAttribute( 'aria-hidden' );[m
[32m+[m
[32m+[m			[32m// If this slide has a state associated with it, add it[m
[32m+[m			[32m// onto the current state of the deck[m
[32m+[m			[32mvar slideState = slides[index].getAttribute( 'data-state' );[m
[32m+[m			[32mif( slideState ) {[m
[32m+[m				[32mstate = state.concat( slideState.split( ' ' ) );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32m// Since there are no slides we can't be anywhere beyond the[m
[32m+[m			[32m// zeroth index[m
[32m+[m			[32mindex = 0;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn index;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Optimization method; hide all slides that are far away[m
[32m+[m	[32m * from the present slide.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction updateSlidesVisibility() {[m
[32m+[m
[32m+[m		[32m// Select all slides and convert the NodeList result to[m
[32m+[m		[32m// an array[m
[32m+[m		[32mvar horizontalSlides = toArray( dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR ) ),[m
[32m+[m			[32mhorizontalSlidesLength = horizontalSlides.length,[m
[32m+[m			[32mdistanceX,[m
[32m+[m			[32mdistanceY;[m
[32m+[m
[32m+[m		[32mif( horizontalSlidesLength && typeof indexh !== 'undefined' ) {[m
[32m+[m
[32m+[m			[32m// The number of steps away from the present slide that will[m
[32m+[m			[32m// be visible[m
[32m+[m			[32mvar viewDistance = isOverview() ? 10 : config.viewDistance;[m
[32m+[m
[32m+[m			[32m// Limit view distance on weaker devices[m
[32m+[m			[32mif( isMobileDevice ) {[m
[32m+[m				[32mviewDistance = isOverview() ? 6 : 2;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// All slides need to be visible when exporting to PDF[m
[32m+[m			[32mif( isPrintingPDF() ) {[m
[32m+[m				[32mviewDistance = Number.MAX_VALUE;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mfor( var x = 0; x < horizontalSlidesLength; x++ ) {[m
[32m+[m				[32mvar horizontalSlide = horizontalSlides[x];[m
[32m+[m
[32m+[m				[32mvar verticalSlides = toArray( horizontalSlide.querySelectorAll( 'section' ) ),[m
[32m+[m					[32mverticalSlidesLength = verticalSlides.length;[m
[32m+[m
[32m+[m				[32m// Determine how far away this slide is from the present[m
[32m+[m				[32mdistanceX = Math.abs( ( indexh || 0 ) - x ) || 0;[m
[32m+[m
[32m+[m				[32m// If the presentation is looped, distance should measure[m
[32m+[m				[32m// 1 between the first and last slides[m
[32m+[m				[32mif( config.loop ) {[m
[32m+[m					[32mdistanceX = Math.abs( ( ( indexh || 0 ) - x ) % ( horizontalSlidesLength - viewDistance ) ) || 0;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Show the horizontal slide if it's within the view distance[m
[32m+[m				[32mif( distanceX < viewDistance ) {[m
[32m+[m					[32mshowSlide( horizontalSlide );[m
[32m+[m				[32m}[m
[32m+[m				[32melse {[m
[32m+[m					[32mhideSlide( horizontalSlide );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif( verticalSlidesLength ) {[m
[32m+[m
[32m+[m					[32mvar oy = getPreviousVerticalIndex( horizontalSlide );[m
[32m+[m
[32m+[m					[32mfor( var y = 0; y < verticalSlidesLength; y++ ) {[m
[32m+[m						[32mvar verticalSlide = verticalSlides[y];[m
[32m+[m
[32m+[m						[32mdistanceY = x === ( indexh || 0 ) ? Math.abs( ( indexv || 0 ) - y ) : Math.abs( y - oy );[m
[32m+[m
[32m+[m						[32mif( distanceX + distanceY < viewDistance ) {[m
[32m+[m							[32mshowSlide( verticalSlide );[m
[32m+[m						[32m}[m
[32m+[m						[32melse {[m
[32m+[m							[32mhideSlide( verticalSlide );[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Pick up notes from the current slide and display tham[m
[32m+[m	[32m * to the viewer.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @see `showNotes` config value[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction updateNotes() {[m
[32m+[m
[32m+[m		[32mif( config.showNotes && dom.speakerNotes && currentSlide && !isPrintingPDF() ) {[m
[32m+[m
[32m+[m			[32mdom.speakerNotes.innerHTML = getSlideNotes() || '';[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Updates the progress bar to reflect the current slide.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction updateProgress() {[m
[32m+[m
[32m+[m		[32m// Update progress if enabled[m
[32m+[m		[32mif( config.progress && dom.progressbar ) {[m
[32m+[m
[32m+[m			[32mdom.progressbar.style.width = getProgress() * dom.wrapper.offsetWidth + 'px';[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Updates the slide number div to reflect the current slide.[m
[32m+[m	[32m *[m
[32m+[m	[32m * The following slide number formats are available:[m
[32m+[m	[32m *  "h.v": 	horizontal . vertical slide number (default)[m
[32m+[m	[32m *  "h/v": 	horizontal / vertical slide number[m
[32m+[m	[32m *    "c": 	flattened slide number[m
[32m+[m	[32m *  "c/t": 	flattened slide number / total slides[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction updateSlideNumber() {[m
[32m+[m
[32m+[m		[32m// Update slide number if enabled[m
[32m+[m		[32mif( config.slideNumber && dom.slideNumber ) {[m
[32m+[m
[32m+[m			[32mvar value = [];[m
[32m+[m			[32mvar format = 'h.v';[m
[32m+[m
[32m+[m			[32m// Check if a custom number format is available[m
[32m+[m			[32mif( typeof config.slideNumber === 'string' ) {[m
[32m+[m				[32mformat = config.slideNumber;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mswitch( format ) {[m
[32m+[m				[32mcase 'c':[m
[32m+[m					[32mvalue.push( getSlidePastCount() + 1 );[m
[32m+[m					[32mbreak;[m
[32m+[m				[32mcase 'c/t':[m
[32m+[m					[32mvalue.push( getSlidePastCount() + 1, '/', getTotalSlides() );[m
[32m+[m					[32mbreak;[m
[32m+[m				[32mcase 'h/v':[m
[32m+[m					[32mvalue.push( indexh + 1 );[m
[32m+[m					[32mif( isVerticalSlide() ) value.push( '/', indexv + 1 );[m
[32m+[m					[32mbreak;[m
[32m+[m				[32mdefault:[m
[32m+[m					[32mvalue.push( indexh + 1 );[m
[32m+[m					[32mif( isVerticalSlide() ) value.push( '.', indexv + 1 );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mdom.slideNumber.innerHTML = formatSlideNumber( value[0], value[1], value[2] );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Applies HTML formatting to a slide number before it's[m
[32m+[m	[32m * written to the DOM.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction formatSlideNumber( a, delimiter, b ) {[m
[32m+[m
[32m+[m		[32mif( typeof b === 'number' && !isNaN( b ) ) {[m
[32m+[m			[32mreturn  '<span class="slide-number-a">'+ a +'</span>' +[m
[32m+[m					[32m'<span class="slide-number-delimiter">'+ delimiter +'</span>' +[m
[32m+[m					[32m'<span class="slide-number-b">'+ b +'</span>';[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mreturn '<span class="slide-number-a">'+ a +'</span>';[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Updates the state of all control/navigation arrows.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction updateControls() {[m
[32m+[m
[32m+[m		[32mvar routes = availableRoutes();[m
[32m+[m		[32mvar fragments = availableFragments();[m
[32m+[m
[32m+[m		[32m// Remove the 'enabled' class from all directions[m
[32m+[m		[32mdom.controlsLeft.concat( dom.controlsRight )[m
[32m+[m						[32m.concat( dom.controlsUp )[m
[32m+[m						[32m.concat( dom.controlsDown )[m
[32m+[m						[32m.concat( dom.controlsPrev )[m
[32m+[m						[32m.concat( dom.controlsNext ).forEach( function( node ) {[m
[32m+[m			[32mnode.classList.remove( 'enabled' );[m
[32m+[m			[32mnode.classList.remove( 'fragmented' );[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m		[32m// Add the 'enabled' class to the available routes[m
[32m+[m		[32mif( routes.left ) dom.controlsLeft.forEach( function( el ) { el.classList.add( 'enabled' );	} );[m
[32m+[m		[32mif( routes.right ) dom.controlsRight.forEach( function( el ) { el.classList.add( 'enabled' ); } );[m
[32m+[m		[32mif( routes.up ) dom.controlsUp.forEach( function( el ) { el.classList.add( 'enabled' );	} );[m
[32m+[m		[32mif( routes.down ) dom.controlsDown.forEach( function( el ) { el.classList.add( 'enabled' ); } );[m
[32m+[m
[32m+[m		[32m// Prev/next buttons[m
[32m+[m		[32mif( routes.left || routes.up ) dom.controlsPrev.forEach( function( el ) { el.classList.add( 'enabled' ); } );[m
[32m+[m		[32mif( routes.right || routes.down ) dom.controlsNext.forEach( function( el ) { el.classList.add( 'enabled' ); } );[m
[32m+[m
[32m+[m		[32m// Highlight fragment directions[m
[32m+[m		[32mif( currentSlide ) {[m
[32m+[m
[32m+[m			[32m// Always apply fragment decorator to prev/next buttons[m
[32m+[m			[32mif( fragments.prev ) dom.controlsPrev.forEach( function( el ) { el.classList.add( 'fragmented', 'enabled' ); } );[m
[32m+[m			[32mif( fragments.next ) dom.controlsNext.forEach( function( el ) { el.classList.add( 'fragmented', 'enabled' ); } );[m
[32m+[m
[32m+[m			[32m// Apply fragment decorators to directional buttons based on[m
[32m+[m			[32m// what slide axis they are in[m
[32m+[m			[32mif( isVerticalSlide( currentSlide ) ) {[m
[32m+[m				[32mif( fragments.prev ) dom.controlsUp.forEach( function( el ) { el.classList.add( 'fragmented', 'enabled' ); } );[m
[32m+[m				[32mif( fragments.next ) dom.controlsDown.forEach( function( el ) { el.classList.add( 'fragmented', 'enabled' ); } );[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32mif( fragments.prev ) dom.controlsLeft.forEach( function( el ) { el.classList.add( 'fragmented', 'enabled' ); } );[m
[32m+[m				[32mif( fragments.next ) dom.controlsRight.forEach( function( el ) { el.classList.add( 'fragmented', 'enabled' ); } );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Updates the background elements to reflect the current[m
[32m+[m	[32m * slide.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {Boolean} includeAll If true, the backgrounds of[m
[32m+[m	[32m * all vertical slides (not just the present) will be updated.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction updateBackground( includeAll ) {[m
[32m+[m
[32m+[m		[32mvar currentBackground = null;[m
[32m+[m
[32m+[m		[32m// Reverse past/future classes when in RTL mode[m
[32m+[m		[32mvar horizontalPast = config.rtl ? 'future' : 'past',[m
[32m+[m			[32mhorizontalFuture = config.rtl ? 'past' : 'future';[m
[32m+[m
[32m+[m		[32m// Update the classes of all backgrounds to match the[m
[32m+[m		[32m// states of their slides (past/present/future)[m
[32m+[m		[32mtoArray( dom.background.childNodes ).forEach( function( backgroundh, h ) {[m
[32m+[m
[32m+[m			[32mbackgroundh.classList.remove( 'past' );[m
[32m+[m			[32mbackgroundh.classList.remove( 'present' );[m
[32m+[m			[32mbackgroundh.classList.remove( 'future' );[m
[32m+[m
[32m+[m			[32mif( h < indexh ) {[m
[32m+[m				[32mbackgroundh.classList.add( horizontalPast );[m
[32m+[m			[32m}[m
[32m+[m			[32melse if ( h > indexh ) {[m
[32m+[m				[32mbackgroundh.classList.add( horizontalFuture );[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32mbackgroundh.classList.add( 'present' );[m
[32m+[m
[32m+[m				[32m// Store a reference to the current background element[m
[32m+[m				[32mcurrentBackground = backgroundh;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif( includeAll || h === indexh ) {[m
[32m+[m				[32mtoArray( backgroundh.querySelectorAll( '.slide-background' ) ).forEach( function( backgroundv, v ) {[m
[32m+[m
[32m+[m					[32mbackgroundv.classList.remove( 'past' );[m
[32m+[m					[32mbackgroundv.classList.remove( 'present' );[m
[32m+[m					[32mbackgroundv.classList.remove( 'future' );[m
[32m+[m
[32m+[m					[32mif( v < indexv ) {[m
[32m+[m						[32mbackgroundv.classList.add( 'past' );[m
[32m+[m					[32m}[m
[32m+[m					[32melse if ( v > indexv ) {[m
[32m+[m						[32mbackgroundv.classList.add( 'future' );[m
[32m+[m					[32m}[m
[32m+[m					[32melse {[m
[32m+[m						[32mbackgroundv.classList.add( 'present' );[m
[32m+[m
[32m+[m						[32m// Only if this is the present horizontal and vertical slide[m
[32m+[m						[32mif( h === indexh ) currentBackground = backgroundv;[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m				[32m} );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m		[32m// Stop any currently playing video background[m
[32m+[m		[32mif( previousBackground ) {[m
[32m+[m
[32m+[m			[32mvar previousVideo = previousBackground.querySelector( 'video' );[m
[32m+[m			[32mif( previousVideo ) previousVideo.pause();[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif( currentBackground ) {[m
[32m+[m
[32m+[m			[32m// Start video playback[m
[32m+[m			[32mvar currentVideo = currentBackground.querySelector( 'video' );[m
[32m+[m			[32mif( currentVideo ) {[m
[32m+[m				[32mif( currentVideo.currentTime > 0 ) currentVideo.currentTime = 0;[m
[32m+[m				[32mcurrentVideo.play();[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mvar backgroundImageURL = currentBackground.style.backgroundImage || '';[m
[32m+[m
[32m+[m			[32m// Restart GIFs (doesn't work in Firefox)[m
[32m+[m			[32mif( /\.gif/i.test( backgroundImageURL ) ) {[m
[32m+[m				[32mcurrentBackground.style.backgroundImage = '';[m
[32m+[m				[32mwindow.getComputedStyle( currentBackground ).opacity;[m
[32m+[m				[32mcurrentBackground.style.backgroundImage = backgroundImageURL;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Don't transition between identical backgrounds. This[m
[32m+[m			[32m// prevents unwanted flicker.[m
[32m+[m			[32mvar previousBackgroundHash = previousBackground ? previousBackground.getAttribute( 'data-background-hash' ) : null;[m
[32m+[m			[32mvar currentBackgroundHash = currentBackground.getAttribute( 'data-background-hash' );[m
[32m+[m			[32mif( currentBackgroundHash && currentBackgroundHash === previousBackgroundHash && currentBackground !== previousBackground ) {[m
[32m+[m				[32mdom.background.classList.add( 'no-transition' );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mpreviousBackground = currentBackground;[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// If there's a background brightness flag for this slide,[m
[32m+[m		[32m// bubble it to the .reveal container[m
[32m+[m		[32mif( currentSlide ) {[m
[32m+[m			[32m[ 'has-light-background', 'has-dark-background' ].forEach( function( classToBubble ) {[m
[32m+[m				[32mif( currentSlide.classList.contains( classToBubble ) ) {[m
[32m+[m					[32mdom.wrapper.classList.add( classToBubble );[m
[32m+[m				[32m}[m
[32m+[m				[32melse {[m
[32m+[m					[32mdom.wrapper.classList.remove( classToBubble );[m
[32m+[m				[32m}[m
[32m+[m			[32m} );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Allow the first background to apply without transition[m
[32m+[m		[32msetTimeout( function() {[m
[32m+[m			[32mdom.background.classList.remove( 'no-transition' );[m
[32m+[m		[32m}, 1 );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Updates the position of the parallax background based[m
[32m+[m	[32m * on the current slide index.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction updateParallax() {[m
[32m+[m
[32m+[m		[32mif( config.parallaxBackgroundImage ) {[m
[32m+[m
[32m+[m			[32mvar horizontalSlides = dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR ),[m
[32m+[m				[32mverticalSlides = dom.wrapper.querySelectorAll( VERTICAL_SLIDES_SELECTOR );[m
[32m+[m
[32m+[m			[32mvar backgroundSize = dom.background.style.backgroundSize.split( ' ' ),[m
[32m+[m				[32mbackgroundWidth, backgroundHeight;[m
[32m+[m
[32m+[m			[32mif( backgroundSize.length === 1 ) {[m
[32m+[m				[32mbackgroundWidth = backgroundHeight = parseInt( backgroundSize[0], 10 );[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32mbackgroundWidth = parseInt( backgroundSize[0], 10 );[m
[32m+[m				[32mbackgroundHeight = parseInt( backgroundSize[1], 10 );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mvar slideWidth = dom.background.offsetWidth,[m
[32m+[m				[32mhorizontalSlideCount = horizontalSlides.length,[m
[32m+[m				[32mhorizontalOffsetMultiplier,[m
[32m+[m				[32mhorizontalOffset;[m
[32m+[m
[32m+[m			[32mif( typeof config.parallaxBackgroundHorizontal === 'number' ) {[m
[32m+[m				[32mhorizontalOffsetMultiplier = config.parallaxBackgroundHorizontal;[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32mhorizontalOffsetMultiplier = ( backgroundWidth - slideWidth ) / ( horizontalSlideCount-1 );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mhorizontalOffset = horizontalOffsetMultiplier * indexh * -1;[m
[32m+[m
[32m+[m			[32mvar slideHeight = dom.background.offsetHeight,[m
[32m+[m				[32mverticalSlideCount = verticalSlides.length,[m
[32m+[m				[32mverticalOffsetMultiplier,[m
[32m+[m				[32mverticalOffset;[m
[32m+[m
[32m+[m			[32mif( typeof config.parallaxBackgroundVertical === 'number' ) {[m
[32m+[m				[32mverticalOffsetMultiplier = config.parallaxBackgroundVertical;[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32mverticalOffsetMultiplier = ( backgroundHeight - slideHeight ) / ( verticalSlideCount-1 );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mverticalOffset = verticalSlideCount > 0 ?  verticalOffsetMultiplier * indexv * 1 : 0;[m
[32m+[m
[32m+[m			[32mdom.background.style.backgroundPosition = horizontalOffset + 'px ' + -verticalOffset + 'px';[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Called when the given slide is within the configured view[m
[32m+[m	[32m * distance. Shows the slide element and loads any content[m
[32m+[m	[32m * that is set to load lazily (data-src).[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction showSlide( slide ) {[m
[32m+[m
[32m+[m		[32m// Show the slide element[m
[32m+[m		[32mslide.style.display = 'block';[m
[32m+[m
[32m+[m		[32m// Media elements with data-src attributes[m
[32m+[m		[32mtoArray( slide.querySelectorAll( 'img[data-src], video[data-src], audio[data-src]' ) ).forEach( function( element ) {[m
[32m+[m			[32melement.setAttribute( 'src', element.getAttribute( 'data-src' ) );[m
[32m+[m			[32melement.removeAttribute( 'data-src' );[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m		[32m// Media elements with <source> children[m
[32m+[m		[32mtoArray( slide.querySelectorAll( 'video, audio' ) ).forEach( function( media ) {[m
[32m+[m			[32mvar sources = 0;[m
[32m+[m
[32m+[m			[32mtoArray( media.querySelectorAll( 'source[data-src]' ) ).forEach( function( source ) {[m
[32m+[m				[32msource.setAttribute( 'src', source.getAttribute( 'data-src' ) );[m
[32m+[m				[32msource.removeAttribute( 'data-src' );[m
[32m+[m				[32msources += 1;[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m			[32m// If we rewrote sources for this video/audio element, we need[m
[32m+[m			[32m// to manually tell it to load from its new origin[m
[32m+[m			[32mif( sources > 0 ) {[m
[32m+[m				[32mmedia.load();[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m
[32m+[m		[32m// Show the corresponding background element[m
[32m+[m		[32mvar indices = getIndices( slide );[m
[32m+[m		[32mvar background = getSlideBackground( indices.h, indices.v );[m
[32m+[m		[32mif( background ) {[m
[32m+[m			[32mbackground.style.display = 'block';[m
[32m+[m
[32m+[m			[32m// If the background contains media, load it[m
[32m+[m			[32mif( background.hasAttribute( 'data-loaded' ) === false ) {[m
[32m+[m				[32mbackground.setAttribute( 'data-loaded', 'true' );[m
[32m+[m
[32m+[m				[32mvar backgroundImage = slide.getAttribute( 'data-background-image' ),[m
[32m+[m					[32mbackgroundVideo = slide.getAttribute( 'data-background-video' ),[m
[32m+[m					[32mbackgroundVideoLoop = slide.hasAttribute( 'data-background-video-loop' ),[m
[32m+[m					[32mbackgroundIframe = slide.getAttribute( 'data-background-iframe' );[m
[32m+[m
[32m+[m				[32m// Images[m
[32m+[m				[32mif( backgroundImage ) {[m
[32m+[m					[32mbackground.style.backgroundImage = 'url('+ backgroundImage +')';[m
[32m+[m				[32m}[m
[32m+[m				[32m// Videos[m
[32m+[m				[32melse if ( backgroundVideo && !isSpeakerNotes() ) {[m
[32m+[m					[32mvar video = document.createElement( 'video' );[m
[32m+[m
[32m+[m					[32mif( backgroundVideoLoop ) {[m
[32m+[m						[32mvideo.setAttribute( 'loop', '' );[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Support comma separated lists of video sources[m
[32m+[m					[32mbackgroundVideo.split( ',' ).forEach( function( source ) {[m
[32m+[m						[32mvideo.innerHTML += '<source src="'+ source +'">';[m
[32m+[m					[32m} );[m
[32m+[m
[32m+[m					[32mbackground.appendChild( video );[m
[32m+[m				[32m}[m
[32m+[m				[32m// Iframes[m
[32m+[m				[32melse if( backgroundIframe ) {[m
[32m+[m					[32mvar iframe = document.createElement( 'iframe' );[m
[32m+[m						[32miframe.setAttribute( 'src', backgroundIframe );[m
[32m+[m						[32miframe.style.width  = '100%';[m
[32m+[m						[32miframe.style.height = '100%';[m
[32m+[m						[32miframe.style.maxHeight = '100%';[m
[32m+[m						[32miframe.style.maxWidth = '100%';[m
[32m+[m
[32m+[m					[32mbackground.appendChild( iframe );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Called when the given slide is moved outside of the[m
[32m+[m	[32m * configured view distance.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction hideSlide( slide ) {[m
[32m+[m
[32m+[m		[32m// Hide the slide element[m
[32m+[m		[32mslide.style.display = 'none';[m
[32m+[m
[32m+[m		[32m// Hide the corresponding background element[m
[32m+[m		[32mvar indices = getIndices( slide );[m
[32m+[m		[32mvar background = getSlideBackground( indices.h, indices.v );[m
[32m+[m		[32mif( background ) {[m
[32m+[m			[32mbackground.style.display = 'none';[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Determine what available routes there are for navigation.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @return {Object} containing four booleans: left/right/up/down[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction availableRoutes() {[m
[32m+[m
[32m+[m		[32mvar horizontalSlides = dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR ),[m
[32m+[m			[32mverticalSlides = dom.wrapper.querySelectorAll( VERTICAL_SLIDES_SELECTOR );[m
[32m+[m
[32m+[m		[32mvar routes = {[m
[32m+[m			[32mleft: indexh > 0 || config.loop,[m
[32m+[m			[32mright: indexh < horizontalSlides.length - 1 || config.loop,[m
[32m+[m			[32mup: indexv > 0,[m
[32m+[m			[32mdown: indexv < verticalSlides.length - 1[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32m// reverse horizontal controls for rtl[m
[32m+[m		[32mif( config.rtl ) {[m
[32m+[m			[32mvar left = routes.left;[m
[32m+[m			[32mroutes.left = routes.right;[m
[32m+[m			[32mroutes.right = left;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn routes;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Returns an object describing the available fragment[m
[32m+[m	[32m * directions.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @return {Object} two boolean properties: prev/next[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction availableFragments() {[m
[32m+[m
[32m+[m		[32mif( currentSlide && config.fragments ) {[m
[32m+[m			[32mvar fragments = currentSlide.querySelectorAll( '.fragment' );[m
[32m+[m			[32mvar hiddenFragments = currentSlide.querySelectorAll( '.fragment:not(.visible)' );[m
[32m+[m
[32m+[m			[32mreturn {[m
[32m+[m				[32mprev: fragments.length - hiddenFragments.length > 0,[m
[32m+[m				[32mnext: !!hiddenFragments.length[m
[32m+[m			[32m};[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mreturn { prev: false, next: false };[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Enforces origin-specific format rules for embedded media.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction formatEmbeddedContent() {[m
[32m+[m
[32m+[m		[32mvar _appendParamToIframeSource = function( sourceAttribute, sourceURL, param ) {[m
[32m+[m			[32mtoArray( dom.slides.querySelectorAll( 'iframe['+ sourceAttribute +'*="'+ sourceURL +'"]' ) ).forEach( function( el ) {[m
[32m+[m				[32mvar src = el.getAttribute( sourceAttribute );[m
[32m+[m				[32mif( src && src.indexOf( param ) === -1 ) {[m
[32m+[m					[32mel.setAttribute( sourceAttribute, src + ( !/\?/.test( src ) ? '?' : '&' ) + param );[m
[32m+[m				[32m}[m
[32m+[m			[32m});[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32m// YouTube frames must include "?enablejsapi=1"[m
[32m+[m		[32m_appendParamToIframeSource( 'src', 'youtube.com/embed/', 'enablejsapi=1' );[m
[32m+[m		[32m_appendParamToIframeSource( 'data-src', 'youtube.com/embed/', 'enablejsapi=1' );[m
[32m+[m
[32m+[m		[32m// Vimeo frames must include "?api=1"[m
[32m+[m		[32m_appendParamToIframeSource( 'src', 'player.vimeo.com/', 'api=1' );[m
[32m+[m		[32m_appendParamToIframeSource( 'data-src', 'player.vimeo.com/', 'api=1' );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Start playback of any embedded content inside of[m
[32m+[m	[32m * the targeted slide.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction startEmbeddedContent( slide ) {[m
[32m+[m
[32m+[m		[32mif( slide && !isSpeakerNotes() ) {[m
[32m+[m			[32m// Restart GIFs[m
[32m+[m			[32mtoArray( slide.querySelectorAll( 'img[src$=".gif"]' ) ).forEach( function( el ) {[m
[32m+[m				[32m// Setting the same unchanged source like this was confirmed[m
[32m+[m				[32m// to work in Chrome, FF & Safari[m
[32m+[m				[32mel.setAttribute( 'src', el.getAttribute( 'src' ) );[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m			[32m// HTML5 media elements[m
[32m+[m			[32mtoArray( slide.querySelectorAll( 'video, audio' ) ).forEach( function( el ) {[m
[32m+[m				[32mif( el.hasAttribute( 'data-autoplay' ) && typeof el.play === 'function' ) {[m
[32m+[m					[32mel.play();[m
[32m+[m				[32m}[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m			[32m// Normal iframes[m
[32m+[m			[32mtoArray( slide.querySelectorAll( 'iframe[src]' ) ).forEach( function( el ) {[m
[32m+[m				[32mstartEmbeddedIframe( { target: el } );[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m			[32m// Lazy loading iframes[m
[32m+[m			[32mtoArray( slide.querySelectorAll( 'iframe[data-src]' ) ).forEach( function( el ) {[m
[32m+[m				[32mif( el.getAttribute( 'src' ) !== el.getAttribute( 'data-src' ) ) {[m
[32m+[m					[32mel.removeEventListener( 'load', startEmbeddedIframe ); // remove first to avoid dupes[m
[32m+[m					[32mel.addEventListener( 'load', startEmbeddedIframe );[m
[32m+[m					[32mel.setAttribute( 'src', el.getAttribute( 'data-src' ) );[m
[32m+[m				[32m}[m
[32m+[m			[32m} );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * "Starts" the content of an embedded iframe using the[m
[32m+[m	[32m * postmessage API.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction startEmbeddedIframe( event ) {[m
[32m+[m
[32m+[m		[32mvar iframe = event.target;[m
[32m+[m
[32m+[m		[32m// YouTube postMessage API[m
[32m+[m		[32mif( /youtube\.com\/embed\//.test( iframe.getAttribute( 'src' ) ) && iframe.hasAttribute( 'data-autoplay' ) ) {[m
[32m+[m			[32miframe.contentWindow.postMessage( '{"event":"command","func":"playVideo","args":""}', '*' );[m
[32m+[m		[32m}[m
[32m+[m		[32m// Vimeo postMessage API[m
[32m+[m		[32melse if( /player\.vimeo\.com\//.test( iframe.getAttribute( 'src' ) ) && iframe.hasAttribute( 'data-autoplay' ) ) {[m
[32m+[m			[32miframe.contentWindow.postMessage( '{"method":"play"}', '*' );[m
[32m+[m		[32m}[m
[32m+[m		[32m// Generic postMessage API[m
[32m+[m		[32melse {[m
[32m+[m			[32miframe.contentWindow.postMessage( 'slide:start', '*' );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Stop playback of any embedded content inside of[m
[32m+[m	[32m * the targeted slide.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction stopEmbeddedContent( slide ) {[m
[32m+[m
[32m+[m		[32mif( slide && slide.parentNode ) {[m
[32m+[m			[32m// HTML5 media elements[m
[32m+[m			[32mtoArray( slide.querySelectorAll( 'video, audio' ) ).forEach( function( el ) {[m
[32m+[m				[32mif( !el.hasAttribute( 'data-ignore' ) && typeof el.pause === 'function' ) {[m
[32m+[m					[32mel.pause();[m
[32m+[m				[32m}[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m			[32m// Generic postMessage API for non-lazy loaded iframes[m
[32m+[m			[32mtoArray( slide.querySelectorAll( 'iframe' ) ).forEach( function( el ) {[m
[32m+[m				[32mel.contentWindow.postMessage( 'slide:stop', '*' );[m
[32m+[m				[32mel.removeEventListener( 'load', startEmbeddedIframe );[m
[32m+[m			[32m});[m
[32m+[m
[32m+[m			[32m// YouTube postMessage API[m
[32m+[m			[32mtoArray( slide.querySelectorAll( 'iframe[src*="youtube.com/embed/"]' ) ).forEach( function( el ) {[m
[32m+[m				[32mif( !el.hasAttribute( 'data-ignore' ) && typeof el.contentWindow.postMessage === 'function' ) {[m
[32m+[m					[32mel.contentWindow.postMessage( '{"event":"command","func":"pauseVideo","args":""}', '*' );[m
[32m+[m				[32m}[m
[32m+[m			[32m});[m
[32m+[m
[32m+[m			[32m// Vimeo postMessage API[m
[32m+[m			[32mtoArray( slide.querySelectorAll( 'iframe[src*="player.vimeo.com/"]' ) ).forEach( function( el ) {[m
[32m+[m				[32mif( !el.hasAttribute( 'data-ignore' ) && typeof el.contentWindow.postMessage === 'function' ) {[m
[32m+[m					[32mel.contentWindow.postMessage( '{"method":"pause"}', '*' );[m
[32m+[m				[32m}[m
[32m+[m			[32m});[m
[32m+[m
[32m+[m			[32m// Lazy loading iframes[m
[32m+[m			[32mtoArray( slide.querySelectorAll( 'iframe[data-src]' ) ).forEach( function( el ) {[m
[32m+[m				[32m// Only removing the src doesn't actually unload the frame[m
[32m+[m				[32m// in all browsers (Firefox) so we set it to blank first[m
[32m+[m				[32mel.setAttribute( 'src', 'about:blank' );[m
[32m+[m				[32mel.removeAttribute( 'src' );[m
[32m+[m			[32m} );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Returns the number of past slides. This can be used as a global[m
[32m+[m	[32m * flattened index for slides.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction getSlidePastCount() {[m
[32m+[m
[32m+[m		[32mvar horizontalSlides = toArray( dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR ) );[m
[32m+[m
[32m+[m		[32m// The number of past slides[m
[32m+[m		[32mvar pastCount = 0;[m
[32m+[m
[32m+[m		[32m// Step through all slides and count the past ones[m
[32m+[m		[32mmainLoop: for( var i = 0; i < horizontalSlides.length; i++ ) {[m
[32m+[m
[32m+[m			[32mvar horizontalSlide = horizontalSlides[i];[m
[32m+[m			[32mvar verticalSlides = toArray( horizontalSlide.querySelectorAll( 'section' ) );[m
[32m+[m
[32m+[m			[32mfor( var j = 0; j < verticalSlides.length; j++ ) {[m
[32m+[m
[32m+[m				[32m// Stop as soon as we arrive at the present[m
[32m+[m				[32mif( verticalSlides[j].classList.contains( 'present' ) ) {[m
[32m+[m					[32mbreak mainLoop;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mpastCount++;[m
[32m+[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Stop as soon as we arrive at the present[m
[32m+[m			[32mif( horizontalSlide.classList.contains( 'present' ) ) {[m
[32m+[m				[32mbreak;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Don't count the wrapping section for vertical slides[m
[32m+[m			[32mif( horizontalSlide.classList.contains( 'stack' ) === false ) {[m
[32m+[m				[32mpastCount++;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn pastCount;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Returns a value ranging from 0-1 that represents[m
[32m+[m	[32m * how far into the presentation we have navigated.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction getProgress() {[m
[32m+[m
[32m+[m		[32m// The number of past and total slides[m
[32m+[m		[32mvar totalCount = getTotalSlides();[m
[32m+[m		[32mvar pastCount = getSlidePastCount();[m
[32m+[m
[32m+[m		[32mif( currentSlide ) {[m
[32m+[m
[32m+[m			[32mvar allFragments = currentSlide.querySelectorAll( '.fragment' );[m
[32m+[m
[32m+[m			[32m// If there are fragments in the current slide those should be[m
[32m+[m			[32m// accounted for in the progress.[m
[32m+[m			[32mif( allFragments.length > 0 ) {[m
[32m+[m				[32mvar visibleFragments = currentSlide.querySelectorAll( '.fragment.visible' );[m
[32m+[m
[32m+[m				[32m// This value represents how big a portion of the slide progress[m
[32m+[m				[32m// that is made up by its fragments (0-1)[m
[32m+[m				[32mvar fragmentWeight = 0.9;[m
[32m+[m
[32m+[m				[32m// Add fragment progress to the past slide count[m
[32m+[m				[32mpastCount += ( visibleFragments.length / allFragments.length ) * fragmentWeight;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn pastCount / ( totalCount - 1 );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Checks if this presentation is running inside of the[m
[32m+[m	[32m * speaker notes window.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction isSpeakerNotes() {[m
[32m+[m
[32m+[m		[32mreturn !!window.location.search.match( /receiver/gi );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Reads the current URL (hash) and navigates accordingly.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction readURL() {[m
[32m+[m
[32m+[m		[32mvar hash = window.location.hash;[m
[32m+[m
[32m+[m		[32m// Attempt to parse the hash as either an index or name[m
[32m+[m		[32mvar bits = hash.slice( 2 ).split( '/' ),[m
[32m+[m			[32mname = hash.replace( /#|\//gi, '' );[m
[32m+[m
[32m+[m		[32m// If the first bit is invalid and there is a name we can[m
[32m+[m		[32m// assume that this is a named link[m
[32m+[m		[32mif( isNaN( parseInt( bits[0], 10 ) ) && name.length ) {[m
[32m+[m			[32mvar element;[m
[32m+[m
[32m+[m			[32m// Ensure the named link is a valid HTML ID attribute[m
[32m+[m			[32mif( /^[a-zA-Z][\w:.-]*$/.test( name ) ) {[m
[32m+[m				[32m// Find the slide with the specified ID[m
[32m+[m				[32melement = document.getElementById( name );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif( element ) {[m
[32m+[m				[32m// Find the position of the named slide and navigate to it[m
[32m+[m				[32mvar indices = Reveal.getIndices( element );[m
[32m+[m				[32mslide( indices.h, indices.v );[m
[32m+[m			[32m}[m
[32m+[m			[32m// If the slide doesn't exist, navigate to the current slide[m
[32m+[m			[32melse {[m
[32m+[m				[32mslide( indexh || 0, indexv || 0 );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32m// Read the index components of the hash[m
[32m+[m			[32mvar h = parseInt( bits[0], 10 ) || 0,[m
[32m+[m				[32mv = parseInt( bits[1], 10 ) || 0;[m
[32m+[m
[32m+[m			[32mif( h !== indexh || v !== indexv ) {[m
[32m+[m				[32mslide( h, v );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Updates the page URL (hash) to reflect the current[m
[32m+[m	[32m * state.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {Number} delay The time in ms to wait before[m
[32m+[m	[32m * writing the hash[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction writeURL( delay ) {[m
[32m+[m
[32m+[m		[32mif( config.history ) {[m
[32m+[m
[32m+[m			[32m// Make sure there's never more than one timeout running[m
[32m+[m			[32mclearTimeout( writeURLTimeout );[m
[32m+[m
[32m+[m			[32m// If a delay is specified, timeout this call[m
[32m+[m			[32mif( typeof delay === 'number' ) {[m
[32m+[m				[32mwriteURLTimeout = setTimeout( writeURL, delay );[m
[32m+[m			[32m}[m
[32m+[m			[32melse if( currentSlide ) {[m
[32m+[m				[32mvar url = '/';[m
[32m+[m
[32m+[m				[32m// Attempt to create a named link based on the slide's ID[m
[32m+[m				[32mvar id = currentSlide.getAttribute( 'id' );[m
[32m+[m				[32mif( id ) {[m
[32m+[m					[32mid = id.replace( /[^a-zA-Z0-9\-\_\:\.]/g, '' );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// If the current slide has an ID, use that as a named link[m
[32m+[m				[32mif( typeof id === 'string' && id.length ) {[m
[32m+[m					[32murl = '/' + id;[m
[32m+[m				[32m}[m
[32m+[m				[32m// Otherwise use the /h/v index[m
[32m+[m				[32melse {[m
[32m+[m					[32mif( indexh > 0 || indexv > 0 ) url += indexh;[m
[32m+[m					[32mif( indexv > 0 ) url += '/' + indexv;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mwindow.location.hash = url;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Retrieves the h/v location of the current, or specified,[m
[32m+[m	[32m * slide.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {HTMLElement} slide If specified, the returned[m
[32m+[m	[32m * index will be for this slide rather than the currently[m
[32m+[m	[32m * active one[m
[32m+[m	[32m *[m
[32m+[m	[32m * @return {Object} { h: <int>, v: <int>, f: <int> }[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction getIndices( slide ) {[m
[32m+[m
[32m+[m		[32m// By default, return the current indices[m
[32m+[m		[32mvar h = indexh,[m
[32m+[m			[32mv = indexv,[m
[32m+[m			[32mf;[m
[32m+[m
[32m+[m		[32m// If a slide is specified, return the indices of that slide[m
[32m+[m		[32mif( slide ) {[m
[32m+[m			[32mvar isVertical = isVerticalSlide( slide );[m
[32m+[m			[32mvar slideh = isVertical ? slide.parentNode : slide;[m
[32m+[m
[32m+[m			[32m// Select all horizontal slides[m
[32m+[m			[32mvar horizontalSlides = toArray( dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR ) );[m
[32m+[m
[32m+[m			[32m// Now that we know which the horizontal slide is, get its index[m
[32m+[m			[32mh = Math.max( horizontalSlides.indexOf( slideh ), 0 );[m
[32m+[m
[32m+[m			[32m// Assume we're not vertical[m
[32m+[m			[32mv = undefined;[m
[32m+[m
[32m+[m			[32m// If this is a vertical slide, grab the vertical index[m
[32m+[m			[32mif( isVertical ) {[m
[32m+[m				[32mv = Math.max( toArray( slide.parentNode.querySelectorAll( 'section' ) ).indexOf( slide ), 0 );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif( !slide && currentSlide ) {[m
[32m+[m			[32mvar hasFragments = currentSlide.querySelectorAll( '.fragment' ).length > 0;[m
[32m+[m			[32mif( hasFragments ) {[m
[32m+[m				[32mvar currentFragment = currentSlide.querySelector( '.current-fragment' );[m
[32m+[m				[32mif( currentFragment && currentFragment.hasAttribute( 'data-fragment-index' ) ) {[m
[32m+[m					[32mf = parseInt( currentFragment.getAttribute( 'data-fragment-index' ), 10 );[m
[32m+[m				[32m}[m
[32m+[m				[32melse {[m
[32m+[m					[32mf = currentSlide.querySelectorAll( '.fragment.visible' ).length - 1;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn { h: h, v: v, f: f };[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Retrieves the total number of slides in this presentation.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction getTotalSlides() {[m
[32m+[m
[32m+[m		[32mreturn dom.wrapper.querySelectorAll( SLIDES_SELECTOR + ':not(.stack)' ).length;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Returns the slide element matching the specified index.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction getSlide( x, y ) {[m
[32m+[m
[32m+[m		[32mvar horizontalSlide = dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR )[ x ];[m
[32m+[m		[32mvar verticalSlides = horizontalSlide && horizontalSlide.querySelectorAll( 'section' );[m
[32m+[m
[32m+[m		[32mif( verticalSlides && verticalSlides.length && typeof y === 'number' ) {[m
[32m+[m			[32mreturn verticalSlides ? verticalSlides[ y ] : undefined;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn horizontalSlide;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Returns the background element for the given slide.[m
[32m+[m	[32m * All slides, even the ones with no background properties[m
[32m+[m	[32m * defined, have a background element so as long as the[m
[32m+[m	[32m * index is valid an element will be returned.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction getSlideBackground( x, y ) {[m
[32m+[m
[32m+[m		[32m// When printing to PDF the slide backgrounds are nested[m
[32m+[m		[32m// inside of the slides[m
[32m+[m		[32mif( isPrintingPDF() ) {[m
[32m+[m			[32mvar slide = getSlide( x, y );[m
[32m+[m			[32mif( slide ) {[m
[32m+[m				[32mvar background = slide.querySelector( '.slide-background' );[m
[32m+[m				[32mif( background && background.parentNode === slide ) {[m
[32m+[m					[32mreturn background;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn undefined;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar horizontalBackground = dom.wrapper.querySelectorAll( '.backgrounds>.slide-background' )[ x ];[m
[32m+[m		[32mvar verticalBackgrounds = horizontalBackground && horizontalBackground.querySelectorAll( '.slide-background' );[m
[32m+[m
[32m+[m		[32mif( verticalBackgrounds && verticalBackgrounds.length && typeof y === 'number' ) {[m
[32m+[m			[32mreturn verticalBackgrounds ? verticalBackgrounds[ y ] : undefined;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn horizontalBackground;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Retrieves the speaker notes from a slide. Notes can be[m
[32m+[m	[32m * defined in two ways:[m
[32m+[m	[32m * 1. As a data-notes attribute on the slide <section>[m
[32m+[m	[32m * 2. As an <aside class="notes"> inside of the slide[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction getSlideNotes( slide ) {[m
[32m+[m
[32m+[m		[32m// Default to the current slide[m
[32m+[m		[32mslide = slide || currentSlide;[m
[32m+[m
[32m+[m		[32m// Notes can be specified via the data-notes attribute...[m
[32m+[m		[32mif( slide.hasAttribute( 'data-notes' ) ) {[m
[32m+[m			[32mreturn slide.getAttribute( 'data-notes' );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// ... or using an <aside class="notes"> element[m
[32m+[m		[32mvar notesElement = slide.querySelector( 'aside.notes' );[m
[32m+[m		[32mif( notesElement ) {[m
[32m+[m			[32mreturn notesElement.innerHTML;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn null;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Retrieves the current state of the presentation as[m
[32m+[m	[32m * an object. This state can then be restored at any[m
[32m+[m	[32m * time.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction getState() {[m
[32m+[m
[32m+[m		[32mvar indices = getIndices();[m
[32m+[m
[32m+[m		[32mreturn {[m
[32m+[m			[32mindexh: indices.h,[m
[32m+[m			[32mindexv: indices.v,[m
[32m+[m			[32mindexf: indices.f,[m
[32m+[m			[32mpaused: isPaused(),[m
[32m+[m			[32moverview: isOverview()[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Restores the presentation to the given state.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {Object} state As generated by getState()[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction setState( state ) {[m
[32m+[m
[32m+[m		[32mif( typeof state === 'object' ) {[m
[32m+[m			[32mslide( deserialize( state.indexh ), deserialize( state.indexv ), deserialize( state.indexf ) );[m
[32m+[m
[32m+[m			[32mvar pausedFlag = deserialize( state.paused ),[m
[32m+[m				[32moverviewFlag = deserialize( state.overview );[m
[32m+[m
[32m+[m			[32mif( typeof pausedFlag === 'boolean' && pausedFlag !== isPaused() ) {[m
[32m+[m				[32mtogglePause( pausedFlag );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif( typeof overviewFlag === 'boolean' && overviewFlag !== isOverview() ) {[m
[32m+[m				[32mtoggleOverview( overviewFlag );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Return a sorted fragments list, ordered by an increasing[m
[32m+[m	[32m * "data-fragment-index" attribute.[m
[32m+[m	[32m *[m
[32m+[m	[32m * Fragments will be revealed in the order that they are returned by[m
[32m+[m	[32m * this function, so you can use the index attributes to control the[m
[32m+[m	[32m * order of fragment appearance.[m
[32m+[m	[32m *[m
[32m+[m	[32m * To maintain a sensible default fragment order, fragments are presumed[m
[32m+[m	[32m * to be passed in document order. This function adds a "fragment-index"[m
[32m+[m	[32m * attribute to each node if such an attribute is not already present,[m
[32m+[m	[32m * and sets that attribute to an integer value which is the position of[m
[32m+[m	[32m * the fragment within the fragments list.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction sortFragments( fragments ) {[m
[32m+[m
[32m+[m		[32mfragments = toArray( fragments );[m
[32m+[m
[32m+[m		[32mvar ordered = [],[m
[32m+[m			[32munordered = [],[m
[32m+[m			[32msorted = [];[m
[32m+[m
[32m+[m		[32m// Group ordered and unordered elements[m
[32m+[m		[32mfragments.forEach( function( fragment, i ) {[m
[32m+[m			[32mif( fragment.hasAttribute( 'data-fragment-index' ) ) {[m
[32m+[m				[32mvar index = parseInt( fragment.getAttribute( 'data-fragment-index' ), 10 );[m
[32m+[m
[32m+[m				[32mif( !ordered[index] ) {[m
[32m+[m					[32mordered[index] = [];[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mordered[index].push( fragment );[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32munordered.push( [ fragment ] );[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m		[32m// Append fragments without explicit indices in their[m
[32m+[m		[32m// DOM order[m
[32m+[m		[32mordered = ordered.concat( unordered );[m
[32m+[m
[32m+[m		[32m// Manually count the index up per group to ensure there[m
[32m+[m		[32m// are no gaps[m
[32m+[m		[32mvar index = 0;[m
[32m+[m
[32m+[m		[32m// Push all fragments in their sorted order to an array,[m
[32m+[m		[32m// this flattens the groups[m
[32m+[m		[32mordered.forEach( function( group ) {[m
[32m+[m			[32mgroup.forEach( function( fragment ) {[m
[32m+[m				[32msorted.push( fragment );[m
[32m+[m				[32mfragment.setAttribute( 'data-fragment-index', index );[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m			[32mindex ++;[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m		[32mreturn sorted;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Navigate to the specified slide fragment.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {Number} index The index of the fragment that[m
[32m+[m	[32m * should be shown, -1 means all are invisible[m
[32m+[m	[32m * @param {Number} offset Integer offset to apply to the[m
[32m+[m	[32m * fragment index[m
[32m+[m	[32m *[m
[32m+[m	[32m * @return {Boolean} true if a change was made in any[m
[32m+[m	[32m * fragments visibility as part of this call[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction navigateFragment( index, offset ) {[m
[32m+[m
[32m+[m		[32mif( currentSlide && config.fragments ) {[m
[32m+[m
[32m+[m			[32mvar fragments = sortFragments( currentSlide.querySelectorAll( '.fragment' ) );[m
[32m+[m			[32mif( fragments.length ) {[m
[32m+[m
[32m+[m				[32m// If no index is specified, find the current[m
[32m+[m				[32mif( typeof index !== 'number' ) {[m
[32m+[m					[32mvar lastVisibleFragment = sortFragments( currentSlide.querySelectorAll( '.fragment.visible' ) ).pop();[m
[32m+[m
[32m+[m					[32mif( lastVisibleFragment ) {[m
[32m+[m						[32mindex = parseInt( lastVisibleFragment.getAttribute( 'data-fragment-index' ) || 0, 10 );[m
[32m+[m					[32m}[m
[32m+[m					[32melse {[m
[32m+[m						[32mindex = -1;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// If an offset is specified, apply it to the index[m
[32m+[m				[32mif( typeof offset === 'number' ) {[m
[32m+[m					[32mindex += offset;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mvar fragmentsShown = [],[m
[32m+[m					[32mfragmentsHidden = [];[m
[32m+[m
[32m+[m				[32mtoArray( fragments ).forEach( function( element, i ) {[m
[32m+[m
[32m+[m					[32mif( element.hasAttribute( 'data-fragment-index' ) ) {[m
[32m+[m						[32mi = parseInt( element.getAttribute( 'data-fragment-index' ), 10 );[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Visible fragments[m
[32m+[m					[32mif( i <= index ) {[m
[32m+[m						[32mif( !element.classList.contains( 'visible' ) ) fragmentsShown.push( element );[m
[32m+[m						[32melement.classList.add( 'visible' );[m
[32m+[m						[32melement.classList.remove( 'current-fragment' );[m
[32m+[m
[32m+[m						[32m// Announce the fragments one by one to the Screen Reader[m
[32m+[m						[32mdom.statusDiv.textContent = element.textContent;[m
[32m+[m
[32m+[m						[32mif( i === index ) {[m
[32m+[m							[32melement.classList.add( 'current-fragment' );[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m					[32m// Hidden fragments[m
[32m+[m					[32melse {[m
[32m+[m						[32mif( element.classList.contains( 'visible' ) ) fragmentsHidden.push( element );[m
[32m+[m						[32melement.classList.remove( 'visible' );[m
[32m+[m						[32melement.classList.remove( 'current-fragment' );[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m
[32m+[m				[32m} );[m
[32m+[m
[32m+[m				[32mif( fragmentsHidden.length ) {[m
[32m+[m					[32mdispatchEvent( 'fragmenthidden', { fragment: fragmentsHidden[0], fragments: fragmentsHidden } );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif( fragmentsShown.length ) {[m
[32m+[m					[32mdispatchEvent( 'fragmentshown', { fragment: fragmentsShown[0], fragments: fragmentsShown } );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mupdateControls();[m
[32m+[m				[32mupdateProgress();[m
[32m+[m
[32m+[m				[32mreturn !!( fragmentsShown.length || fragmentsHidden.length );[m
[32m+[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn false;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Navigate to the next slide fragment.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @return {Boolean} true if there was a next fragment,[m
[32m+[m	[32m * false otherwise[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction nextFragment() {[m
[32m+[m
[32m+[m		[32mreturn navigateFragment( null, 1 );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Navigate to the previous slide fragment.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @return {Boolean} true if there was a previous fragment,[m
[32m+[m	[32m * false otherwise[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction previousFragment() {[m
[32m+[m
[32m+[m		[32mreturn navigateFragment( null, -1 );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Cues a new automated slide if enabled in the config.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction cueAutoSlide() {[m
[32m+[m
[32m+[m		[32mcancelAutoSlide();[m
[32m+[m
[32m+[m		[32mif( currentSlide ) {[m
[32m+[m
[32m+[m			[32mvar currentFragment = currentSlide.querySelector( '.current-fragment' );[m
[32m+[m
[32m+[m			[32mvar fragmentAutoSlide = currentFragment ? currentFragment.getAttribute( 'data-autoslide' ) : null;[m
[32m+[m			[32mvar parentAutoSlide = currentSlide.parentNode ? currentSlide.parentNode.getAttribute( 'data-autoslide' ) : null;[m
[32m+[m			[32mvar slideAutoSlide = currentSlide.getAttribute( 'data-autoslide' );[m
[32m+[m
[32m+[m			[32m// Pick value in the following priority order:[m
[32m+[m			[32m// 1. Current fragment's data-autoslide[m
[32m+[m			[32m// 2. Current slide's data-autoslide[m
[32m+[m			[32m// 3. Parent slide's data-autoslide[m
[32m+[m			[32m// 4. Global autoSlide setting[m
[32m+[m			[32mif( fragmentAutoSlide ) {[m
[32m+[m				[32mautoSlide = parseInt( fragmentAutoSlide, 10 );[m
[32m+[m			[32m}[m
[32m+[m			[32melse if( slideAutoSlide ) {[m
[32m+[m				[32mautoSlide = parseInt( slideAutoSlide, 10 );[m
[32m+[m			[32m}[m
[32m+[m			[32melse if( parentAutoSlide ) {[m
[32m+[m				[32mautoSlide = parseInt( parentAutoSlide, 10 );[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32mautoSlide = config.autoSlide;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// If there are media elements with data-autoplay,[m
[32m+[m			[32m// automatically set the autoSlide duration to the[m
[32m+[m			[32m// length of that media. Not applicable if the slide[m
[32m+[m			[32m// is divided up into fragments.[m
[32m+[m			[32mif( currentSlide.querySelectorAll( '.fragment' ).length === 0 ) {[m
[32m+[m				[32mtoArray( currentSlide.querySelectorAll( 'video, audio' ) ).forEach( function( el ) {[m
[32m+[m					[32mif( el.hasAttribute( 'data-autoplay' ) ) {[m
[32m+[m						[32mif( autoSlide && el.duration * 1000 > autoSlide ) {[m
[32m+[m							[32mautoSlide = ( el.duration * 1000 ) + 1000;[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m} );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Cue the next auto-slide if:[m
[32m+[m			[32m// - There is an autoSlide value[m
[32m+[m			[32m// - Auto-sliding isn't paused by the user[m
[32m+[m			[32m// - The presentation isn't paused[m
[32m+[m			[32m// - The overview isn't active[m
[32m+[m			[32m// - The presentation isn't over[m
[32m+[m			[32mif( autoSlide && !autoSlidePaused && !isPaused() && !isOverview() && ( !Reveal.isLastSlide() || availableFragments().next || config.loop === true ) ) {[m
[32m+[m				[32mautoSlideTimeout = setTimeout( navigateNext, autoSlide );[m
[32m+[m				[32mautoSlideStartTime = Date.now();[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif( autoSlidePlayer ) {[m
[32m+[m				[32mautoSlidePlayer.setPlaying( autoSlideTimeout !== -1 );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Cancels any ongoing request to auto-slide.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction cancelAutoSlide() {[m
[32m+[m
[32m+[m		[32mclearTimeout( autoSlideTimeout );[m
[32m+[m		[32mautoSlideTimeout = -1;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction pauseAutoSlide() {[m
[32m+[m
[32m+[m		[32mif( autoSlide && !autoSlidePaused ) {[m
[32m+[m			[32mautoSlidePaused = true;[m
[32m+[m			[32mdispatchEvent( 'autoslidepaused' );[m
[32m+[m			[32mclearTimeout( autoSlideTimeout );[m
[32m+[m
[32m+[m			[32mif( autoSlidePlayer ) {[m
[32m+[m				[32mautoSlidePlayer.setPlaying( false );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction resumeAutoSlide() {[m
[32m+[m
[32m+[m		[32mif( autoSlide && autoSlidePaused ) {[m
[32m+[m			[32mautoSlidePaused = false;[m
[32m+[m			[32mdispatchEvent( 'autoslideresumed' );[m
[32m+[m			[32mcueAutoSlide();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction navigateLeft() {[m
[32m+[m
[32m+[m		[32m// Reverse for RTL[m
[32m+[m		[32mif( config.rtl ) {[m
[32m+[m			[32mif( ( isOverview() || nextFragment() === false ) && availableRoutes().left ) {[m
[32m+[m				[32mslide( indexh + 1 );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32m// Normal navigation[m
[32m+[m		[32melse if( ( isOverview() || previousFragment() === false ) && availableRoutes().left ) {[m
[32m+[m			[32mslide( indexh - 1 );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction navigateRight() {[m
[32m+[m
[32m+[m		[32m// Reverse for RTL[m
[32m+[m		[32mif( config.rtl ) {[m
[32m+[m			[32mif( ( isOverview() || previousFragment() === false ) && availableRoutes().right ) {[m
[32m+[m				[32mslide( indexh - 1 );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32m// Normal navigation[m
[32m+[m		[32melse if( ( isOverview() || nextFragment() === false ) && availableRoutes().right ) {[m
[32m+[m			[32mslide( indexh + 1 );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction navigateUp() {[m
[32m+[m
[32m+[m		[32m// Prioritize hiding fragments[m
[32m+[m		[32mif( ( isOverview() || previousFragment() === false ) && availableRoutes().up ) {[m
[32m+[m			[32mslide( indexh, indexv - 1 );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction navigateDown() {[m
[32m+[m
[32m+[m		[32m// Prioritize revealing fragments[m
[32m+[m		[32mif( ( isOverview() || nextFragment() === false ) && availableRoutes().down ) {[m
[32m+[m			[32mslide( indexh, indexv + 1 );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Navigates backwards, prioritized in the following order:[m
[32m+[m	[32m * 1) Previous fragment[m
[32m+[m	[32m * 2) Previous vertical slide[m
[32m+[m	[32m * 3) Previous horizontal slide[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction navigatePrev() {[m
[32m+[m
[32m+[m		[32m// Prioritize revealing fragments[m
[32m+[m		[32mif( previousFragment() === false ) {[m
[32m+[m			[32mif( availableRoutes().up ) {[m
[32m+[m				[32mnavigateUp();[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32m// Fetch the previous horizontal slide, if there is one[m
[32m+[m				[32mvar previousSlide;[m
[32m+[m
[32m+[m				[32mif( config.rtl ) {[m
[32m+[m					[32mpreviousSlide = toArray( dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR + '.future' ) ).pop();[m
[32m+[m				[32m}[m
[32m+[m				[32melse {[m
[32m+[m					[32mpreviousSlide = toArray( dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR + '.past' ) ).pop();[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif( previousSlide ) {[m
[32m+[m					[32mvar v = ( previousSlide.querySelectorAll( 'section' ).length - 1 ) || undefined;[m
[32m+[m					[32mvar h = indexh - 1;[m
[32m+[m					[32mslide( h, v );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * The reverse of #navigatePrev().[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction navigateNext() {[m
[32m+[m
[32m+[m		[32m// Prioritize revealing fragments[m
[32m+[m		[32mif( nextFragment() === false ) {[m
[32m+[m			[32mif( availableRoutes().down ) {[m
[32m+[m				[32mnavigateDown();[m
[32m+[m			[32m}[m
[32m+[m			[32melse if( config.rtl ) {[m
[32m+[m				[32mnavigateLeft();[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32mnavigateRight();[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// If auto-sliding is enabled we need to cue up[m
[32m+[m		[32m// another timeout[m
[32m+[m		[32mcueAutoSlide();[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Checks if the target element prevents the triggering of[m
[32m+[m	[32m * swipe navigation.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction isSwipePrevented( target ) {[m
[32m+[m
[32m+[m		[32mwhile( target && typeof target.hasAttribute === 'function' ) {[m
[32m+[m			[32mif( target.hasAttribute( 'data-prevent-swipe' ) ) return true;[m
[32m+[m			[32mtarget = target.parentNode;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn false;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m
[32m+[m	[32m// --------------------------------------------------------------------//[m
[32m+[m	[32m// ----------------------------- EVENTS -------------------------------//[m
[32m+[m	[32m// --------------------------------------------------------------------//[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Called by all event handlers that are based on user[m
[32m+[m	[32m * input.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onUserInput( event ) {[m
[32m+[m
[32m+[m		[32mif( config.autoSlideStoppable ) {[m
[32m+[m			[32mpauseAutoSlide();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Handler for the document level 'keypress' event.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onDocumentKeyPress( event ) {[m
[32m+[m
[32m+[m		[32m// Check if the pressed key is question mark[m
[32m+[m		[32mif( event.shiftKey && event.charCode === 63 ) {[m
[32m+[m			[32mif( dom.overlay ) {[m
[32m+[m				[32mcloseOverlay();[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32mshowHelp( true );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Handler for the document level 'keydown' event.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onDocumentKeyDown( event ) {[m
[32m+[m
[32m+[m		[32m// If there's a condition specified and it returns false,[m
[32m+[m		[32m// ignore this event[m
[32m+[m		[32mif( typeof config.keyboardCondition === 'function' && config.keyboardCondition() === false ) {[m
[32m+[m			[32mreturn true;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Remember if auto-sliding was paused so we can toggle it[m
[32m+[m		[32mvar autoSlideWasPaused = autoSlidePaused;[m
[32m+[m
[32m+[m		[32monUserInput( event );[m
[32m+[m
[32m+[m		[32m// Check if there's a focused element that could be using[m
[32m+[m		[32m// the keyboard[m
[32m+[m		[32mvar activeElementIsCE = document.activeElement && document.activeElement.contentEditable !== 'inherit';[m
[32m+[m		[32mvar activeElementIsInput = document.activeElement && document.activeElement.tagName && /input|textarea/i.test( document.activeElement.tagName );[m
[32m+[m
[32m+[m		[32m// Disregard the event if there's a focused element or a[m
[32m+[m		[32m// keyboard modifier key is present[m
[32m+[m		[32mif( activeElementIsCE || activeElementIsInput || (event.shiftKey && event.keyCode !== 32) || event.altKey || event.ctrlKey || event.metaKey ) return;[m
[32m+[m
[32m+[m		[32m// While paused only allow resume keyboard events; 'b', '.''[m
[32m+[m		[32mvar resumeKeyCodes = [66,190,191];[m
[32m+[m		[32mvar key;[m
[32m+[m
[32m+[m		[32m// Custom key bindings for togglePause should be able to resume[m
[32m+[m		[32mif( typeof config.keyboard === 'object' ) {[m
[32m+[m			[32mfor( key in config.keyboard ) {[m
[32m+[m				[32mif( config.keyboard[key] === 'togglePause' ) {[m
[32m+[m					[32mresumeKeyCodes.push( parseInt( key, 10 ) );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif( isPaused() && resumeKeyCodes.indexOf( event.keyCode ) === -1 ) {[m
[32m+[m			[32mreturn false;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar triggered = false;[m
[32m+[m
[32m+[m		[32m// 1. User defined key bindings[m
[32m+[m		[32mif( typeof config.keyboard === 'object' ) {[m
[32m+[m
[32m+[m			[32mfor( key in config.keyboard ) {[m
[32m+[m
[32m+[m				[32m// Check if this binding matches the pressed key[m
[32m+[m				[32mif( parseInt( key, 10 ) === event.keyCode ) {[m
[32m+[m
[32m+[m					[32mvar value = config.keyboard[ key ];[m
[32m+[m
[32m+[m					[32m// Callback function[m
[32m+[m					[32mif( typeof value === 'function' ) {[m
[32m+[m						[32mvalue.apply( null, [ event ] );[m
[32m+[m					[32m}[m
[32m+[m					[32m// String shortcuts to reveal.js API[m
[32m+[m					[32melse if( typeof value === 'string' && typeof Reveal[ value ] === 'function' ) {[m
[32m+[m						[32mReveal[ value ].call();[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32mtriggered = true;[m
[32m+[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// 2. System defined key bindings[m
[32m+[m		[32mif( triggered === false ) {[m
[32m+[m
[32m+[m			[32m// Assume true and try to prove false[m
[32m+[m			[32mtriggered = true;[m
[32m+[m
[32m+[m			[32mswitch( event.keyCode ) {[m
[32m+[m				[32m// p, page up[m
[32m+[m				[32mcase 80: case 33: navigatePrev(); break;[m
[32m+[m				[32m// n, page down[m
[32m+[m				[32mcase 78: case 34: navigateNext(); break;[m
[32m+[m				[32m// h, left[m
[32m+[m				[32mcase 72: case 37: navigateLeft(); break;[m
[32m+[m				[32m// l, right[m
[32m+[m				[32mcase 76: case 39: navigateRight(); break;[m
[32m+[m				[32m// k, up[m
[32m+[m				[32mcase 75: case 38: navigateUp(); break;[m
[32m+[m				[32m// j, down[m
[32m+[m				[32mcase 74: case 40: navigateDown(); break;[m
[32m+[m				[32m// home[m
[32m+[m				[32mcase 36: slide( 0 ); break;[m
[32m+[m				[32m// end[m
[32m+[m				[32mcase 35: slide( Number.MAX_VALUE ); break;[m
[32m+[m				[32m// space[m
[32m+[m				[32mcase 32: isOverview() ? deactivateOverview() : event.shiftKey ? navigatePrev() : navigateNext(); break;[m
[32m+[m				[32m// return[m
[32m+[m				[32mcase 13: isOverview() ? deactivateOverview() : triggered = false; break;[m
[32m+[m				[32m// two-spot, semicolon, b, period, Logitech presenter tools "black screen" button[m
[32m+[m				[32mcase 58: case 59: case 66: case 190: case 191: togglePause(); break;[m
[32m+[m				[32m// f[m
[32m+[m				[32mcase 70: enterFullscreen(); break;[m
[32m+[m				[32m// a[m
[32m+[m				[32mcase 65: if ( config.autoSlideStoppable ) toggleAutoSlide( autoSlideWasPaused ); break;[m
[32m+[m				[32mdefault:[m
[32m+[m					[32mtriggered = false;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// If the input resulted in a triggered action we should prevent[m
[32m+[m		[32m// the browsers default behavior[m
[32m+[m		[32mif( triggered ) {[m
[32m+[m			[32mevent.preventDefault && event.preventDefault();[m
[32m+[m		[32m}[m
[32m+[m		[32m// ESC or O key[m
[32m+[m		[32melse if ( ( event.keyCode === 27 || event.keyCode === 79 ) && features.transforms3d ) {[m
[32m+[m			[32mif( dom.overlay ) {[m
[32m+[m				[32mcloseOverlay();[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32mtoggleOverview();[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mevent.preventDefault && event.preventDefault();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// If auto-sliding is enabled we need to cue up[m
[32m+[m		[32m// another timeout[m
[32m+[m		[32mcueAutoSlide();[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Handler for the 'touchstart' event, enables support for[m
[32m+[m	[32m * swipe and pinch gestures.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onTouchStart( event ) {[m
[32m+[m
[32m+[m		[32mif( isSwipePrevented( event.target ) ) return true;[m
[32m+[m
[32m+[m		[32mtouch.startX = event.touches[0].clientX;[m
[32m+[m		[32mtouch.startY = event.touches[0].clientY;[m
[32m+[m		[32mtouch.startCount = event.touches.length;[m
[32m+[m
[32m+[m		[32m// If there's two touches we need to memorize the distance[m
[32m+[m		[32m// between those two points to detect pinching[m
[32m+[m		[32mif( event.touches.length === 2 && config.overview ) {[m
[32m+[m			[32mtouch.startSpan = distanceBetween( {[m
[32m+[m				[32mx: event.touches[1].clientX,[m
[32m+[m				[32my: event.touches[1].clientY[m
[32m+[m			[32m}, {[m
[32m+[m				[32mx: touch.startX,[m
[32m+[m				[32my: touch.startY[m
[32m+[m			[32m} );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Handler for the 'touchmove' event.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onTouchMove( event ) {[m
[32m+[m
[32m+[m		[32mif( isSwipePrevented( event.target ) ) return true;[m
[32m+[m
[32m+[m		[32m// Each touch should only trigger one action[m
[32m+[m		[32mif( !touch.captured ) {[m
[32m+[m			[32monUserInput( event );[m
[32m+[m
[32m+[m			[32mvar currentX = event.touches[0].clientX;[m
[32m+[m			[32mvar currentY = event.touches[0].clientY;[m
[32m+[m
[32m+[m			[32m// If the touch started with two points and still has[m
[32m+[m			[32m// two active touches; test for the pinch gesture[m
[32m+[m			[32mif( event.touches.length === 2 && touch.startCount === 2 && config.overview ) {[m
[32m+[m
[32m+[m				[32m// The current distance in pixels between the two touch points[m
[32m+[m				[32mvar currentSpan = distanceBetween( {[m
[32m+[m					[32mx: event.touches[1].clientX,[m
[32m+[m					[32my: event.touches[1].clientY[m
[32m+[m				[32m}, {[m
[32m+[m					[32mx: touch.startX,[m
[32m+[m					[32my: touch.startY[m
[32m+[m				[32m} );[m
[32m+[m
[32m+[m				[32m// If the span is larger than the desire amount we've got[m
[32m+[m				[32m// ourselves a pinch[m
[32m+[m				[32mif( Math.abs( touch.startSpan - currentSpan ) > touch.threshold ) {[m
[32m+[m					[32mtouch.captured = true;[m
[32m+[m
[32m+[m					[32mif( currentSpan < touch.startSpan ) {[m
[32m+[m						[32mactivateOverview();[m
[32m+[m					[32m}[m
[32m+[m					[32melse {[m
[32m+[m						[32mdeactivateOverview();[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mevent.preventDefault();[m
[32m+[m
[32m+[m			[32m}[m
[32m+[m			[32m// There was only one touch point, look for a swipe[m
[32m+[m			[32melse if( event.touches.length === 1 && touch.startCount !== 2 ) {[m
[32m+[m
[32m+[m				[32mvar deltaX = currentX - touch.startX,[m
[32m+[m					[32mdeltaY = currentY - touch.startY;[m
[32m+[m
[32m+[m				[32mif( deltaX > touch.threshold && Math.abs( deltaX ) > Math.abs( deltaY ) ) {[m
[32m+[m					[32mtouch.captured = true;[m
[32m+[m					[32mnavigateLeft();[m
[32m+[m				[32m}[m
[32m+[m				[32melse if( deltaX < -touch.threshold && Math.abs( deltaX ) > Math.abs( deltaY ) ) {[m
[32m+[m					[32mtouch.captured = true;[m
[32m+[m					[32mnavigateRight();[m
[32m+[m				[32m}[m
[32m+[m				[32melse if( deltaY > touch.threshold ) {[m
[32m+[m					[32mtouch.captured = true;[m
[32m+[m					[32mnavigateUp();[m
[32m+[m				[32m}[m
[32m+[m				[32melse if( deltaY < -touch.threshold ) {[m
[32m+[m					[32mtouch.captured = true;[m
[32m+[m					[32mnavigateDown();[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// If we're embedded, only block touch events if they have[m
[32m+[m				[32m// triggered an action[m
[32m+[m				[32mif( config.embedded ) {[m
[32m+[m					[32mif( touch.captured || isVerticalSlide( currentSlide ) ) {[m
[32m+[m						[32mevent.preventDefault();[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m				[32m// Not embedded? Block them all to avoid needless tossing[m
[32m+[m				[32m// around of the viewport in iOS[m
[32m+[m				[32melse {[m
[32m+[m					[32mevent.preventDefault();[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32m// There's a bug with swiping on some Android devices unless[m
[32m+[m		[32m// the default action is always prevented[m
[32m+[m		[32melse if( navigator.userAgent.match( /android/gi ) ) {[m
[32m+[m			[32mevent.preventDefault();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Handler for the 'touchend' event.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onTouchEnd( event ) {[m
[32m+[m
[32m+[m		[32mtouch.captured = false;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Convert pointer down to touch start.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onPointerDown( event ) {[m
[32m+[m
[32m+[m		[32mif( event.pointerType === event.MSPOINTER_TYPE_TOUCH || event.pointerType === "touch" ) {[m
[32m+[m			[32mevent.touches = [{ clientX: event.clientX, clientY: event.clientY }];[m
[32m+[m			[32monTouchStart( event );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Convert pointer move to touch move.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onPointerMove( event ) {[m
[32m+[m
[32m+[m		[32mif( event.pointerType === event.MSPOINTER_TYPE_TOUCH || event.pointerType === "touch" )  {[m
[32m+[m			[32mevent.touches = [{ clientX: event.clientX, clientY: event.clientY }];[m
[32m+[m			[32monTouchMove( event );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Convert pointer up to touch end.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onPointerUp( event ) {[m
[32m+[m
[32m+[m		[32mif( event.pointerType === event.MSPOINTER_TYPE_TOUCH || event.pointerType === "touch" )  {[m
[32m+[m			[32mevent.touches = [{ clientX: event.clientX, clientY: event.clientY }];[m
[32m+[m			[32monTouchEnd( event );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Handles mouse wheel scrolling, throttled to avoid skipping[m
[32m+[m	[32m * multiple slides.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onDocumentMouseScroll( event ) {[m
[32m+[m
[32m+[m		[32mif( Date.now() - lastMouseWheelStep > 600 ) {[m
[32m+[m
[32m+[m			[32mlastMouseWheelStep = Date.now();[m
[32m+[m
[32m+[m			[32mvar delta = event.detail || -event.wheelDelta;[m
[32m+[m			[32mif( delta > 0 ) {[m
[32m+[m				[32mnavigateNext();[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32mnavigatePrev();[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Clicking on the progress bar results in a navigation to the[m
[32m+[m	[32m * closest approximate horizontal slide using this equation:[m
[32m+[m	[32m *[m
[32m+[m	[32m * ( clickX / presentationWidth ) * numberOfSlides[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onProgressClicked( event ) {[m
[32m+[m
[32m+[m		[32monUserInput( event );[m
[32m+[m
[32m+[m		[32mevent.preventDefault();[m
[32m+[m
[32m+[m		[32mvar slidesTotal = toArray( dom.wrapper.querySelectorAll( HORIZONTAL_SLIDES_SELECTOR ) ).length;[m
[32m+[m		[32mvar slideIndex = Math.floor( ( event.clientX / dom.wrapper.offsetWidth ) * slidesTotal );[m
[32m+[m
[32m+[m		[32mif( config.rtl ) {[m
[32m+[m			[32mslideIndex = slidesTotal - slideIndex;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mslide( slideIndex );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Event handler for navigation control buttons.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onNavigateLeftClicked( event ) { event.preventDefault(); onUserInput(); navigateLeft(); }[m
[32m+[m	[32mfunction onNavigateRightClicked( event ) { event.preventDefault(); onUserInput(); navigateRight(); }[m
[32m+[m	[32mfunction onNavigateUpClicked( event ) { event.preventDefault(); onUserInput(); navigateUp(); }[m
[32m+[m	[32mfunction onNavigateDownClicked( event ) { event.preventDefault(); onUserInput(); navigateDown(); }[m
[32m+[m	[32mfunction onNavigatePrevClicked( event ) { event.preventDefault(); onUserInput(); navigatePrev(); }[m
[32m+[m	[32mfunction onNavigateNextClicked( event ) { event.preventDefault(); onUserInput(); navigateNext(); }[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Handler for the window level 'hashchange' event.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onWindowHashChange( event ) {[m
[32m+[m
[32m+[m		[32mreadURL();[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Handler for the window level 'resize' event.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onWindowResize( event ) {[m
[32m+[m
[32m+[m		[32mlayout();[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Handle for the window level 'visibilitychange' event.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onPageVisibilityChange( event ) {[m
[32m+[m
[32m+[m		[32mvar isHidden =  document.webkitHidden ||[m
[32m+[m						[32mdocument.msHidden ||[m
[32m+[m						[32mdocument.hidden;[m
[32m+[m
[32m+[m		[32m// If, after clicking a link or similar and we're coming back,[m
[32m+[m		[32m// focus the document.body to ensure we can use keyboard shortcuts[m
[32m+[m		[32mif( isHidden === false && document.activeElement !== document.body ) {[m
[32m+[m			[32m// Not all elements support .blur() - SVGs among them.[m
[32m+[m			[32mif( typeof document.activeElement.blur === 'function' ) {[m
[32m+[m				[32mdocument.activeElement.blur();[m
[32m+[m			[32m}[m
[32m+[m			[32mdocument.body.focus();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Invoked when a slide is and we're in the overview.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onOverviewSlideClicked( event ) {[m
[32m+[m
[32m+[m		[32m// TODO There's a bug here where the event listeners are not[m
[32m+[m		[32m// removed after deactivating the overview.[m
[32m+[m		[32mif( eventsAreBound && isOverview() ) {[m
[32m+[m			[32mevent.preventDefault();[m
[32m+[m
[32m+[m			[32mvar element = event.target;[m
[32m+[m
[32m+[m			[32mwhile( element && !element.nodeName.match( /section/gi ) ) {[m
[32m+[m				[32melement = element.parentNode;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif( element && !element.classList.contains( 'disabled' ) ) {[m
[32m+[m
[32m+[m				[32mdeactivateOverview();[m
[32m+[m
[32m+[m				[32mif( element.nodeName.match( /section/gi ) ) {[m
[32m+[m					[32mvar h = parseInt( element.getAttribute( 'data-index-h' ), 10 ),[m
[32m+[m						[32mv = parseInt( element.getAttribute( 'data-index-v' ), 10 );[m
[32m+[m
[32m+[m					[32mslide( h, v );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Handles clicks on links that are set to preview in the[m
[32m+[m	[32m * iframe overlay.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onPreviewLinkClicked( event ) {[m
[32m+[m
[32m+[m		[32mif( event.currentTarget && event.currentTarget.hasAttribute( 'href' ) ) {[m
[32m+[m			[32mvar url = event.currentTarget.getAttribute( 'href' );[m
[32m+[m			[32mif( url ) {[m
[32m+[m				[32mshowPreview( url );[m
[32m+[m				[32mevent.preventDefault();[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Handles click on the auto-sliding controls element.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction onAutoSlidePlayerClick( event ) {[m
[32m+[m
[32m+[m		[32m// Replay[m
[32m+[m		[32mif( Reveal.isLastSlide() && config.loop === false ) {[m
[32m+[m			[32mslide( 0, 0 );[m
[32m+[m			[32mresumeAutoSlide();[m
[32m+[m		[32m}[m
[32m+[m		[32m// Resume[m
[32m+[m		[32melse if( autoSlidePaused ) {[m
[32m+[m			[32mresumeAutoSlide();[m
[32m+[m		[32m}[m
[32m+[m		[32m// Pause[m
[32m+[m		[32melse {[m
[32m+[m			[32mpauseAutoSlide();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m
[32m+[m	[32m// --------------------------------------------------------------------//[m
[32m+[m	[32m// ------------------------ PLAYBACK COMPONENT ------------------------//[m
[32m+[m	[32m// --------------------------------------------------------------------//[m
[32m+[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Constructor for the playback component, which displays[m
[32m+[m	[32m * play/pause/progress controls.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {HTMLElement} container The component will append[m
[32m+[m	[32m * itself to this[m
[32m+[m	[32m * @param {Function} progressCheck A method which will be[m
[32m+[m	[32m * called frequently to get the current progress on a range[m
[32m+[m	[32m * of 0-1[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction Playback( container, progressCheck ) {[m
[32m+[m
[32m+[m		[32m// Cosmetics[m
[32m+[m		[32mthis.diameter = 50;[m
[32m+[m		[32mthis.thickness = 3;[m
[32m+[m
[32m+[m		[32m// Flags if we are currently playing[m
[32m+[m		[32mthis.playing = false;[m
[32m+[m
[32m+[m		[32m// Current progress on a 0-1 range[m
[32m+[m		[32mthis.progress = 0;[m
[32m+[m
[32m+[m		[32m// Used to loop the animation smoothly[m
[32m+[m		[32mthis.progressOffset = 1;[m
[32m+[m
[32m+[m		[32mthis.container = container;[m
[32m+[m		[32mthis.progressCheck = progressCheck;[m
[32m+[m
[32m+[m		[32mthis.canvas = document.createElement( 'canvas' );[m
[32m+[m		[32mthis.canvas.className = 'playback';[m
[32m+[m		[32mthis.canvas.width = this.diameter;[m
[32m+[m		[32mthis.canvas.height = this.diameter;[m
[32m+[m		[32mthis.context = this.canvas.getContext( '2d' );[m
[32m+[m
[32m+[m		[32mthis.container.appendChild( this.canvas );[m
[32m+[m
[32m+[m		[32mthis.render();[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mPlayback.prototype.setPlaying = function( value ) {[m
[32m+[m
[32m+[m		[32mvar wasPlaying = this.playing;[m
[32m+[m
[32m+[m		[32mthis.playing = value;[m
[32m+[m
[32m+[m		[32m// Start repainting if we weren't already[m
[32m+[m		[32mif( !wasPlaying && this.playing ) {[m
[32m+[m			[32mthis.animate();[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mthis.render();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32mPlayback.prototype.animate = function() {[m
[32m+[m
[32m+[m		[32mvar progressBefore = this.progress;[m
[32m+[m
[32m+[m		[32mthis.progress = this.progressCheck();[m
[32m+[m
[32m+[m		[32m// When we loop, offset the progress so that it eases[m
[32m+[m		[32m// smoothly rather than immediately resetting[m
[32m+[m		[32mif( progressBefore > 0.8 && this.progress < 0.2 ) {[m
[32m+[m			[32mthis.progressOffset = this.progress;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mthis.render();[m
[32m+[m
[32m+[m		[32mif( this.playing ) {[m
[32m+[m			[32mfeatures.requestAnimationFrameMethod.call( window, this.animate.bind( this ) );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Renders the current progress and playback state.[m
[32m+[m	[32m */[m
[32m+[m	[32mPlayback.prototype.render = function() {[m
[32m+[m
[32m+[m		[32mvar progress = this.playing ? this.progress : 0,[m
[32m+[m			[32mradius = ( this.diameter / 2 ) - this.thickness,[m
[32m+[m			[32mx = this.diameter / 2,[m
[32m+[m			[32my = this.diameter / 2,[m
[32m+[m			[32miconSize = 14;[m
[32m+[m
[32m+[m		[32m// Ease towards 1[m
[32m+[m		[32mthis.progressOffset += ( 1 - this.progressOffset ) * 0.1;[m
[32m+[m
[32m+[m		[32mvar endAngle = ( - Math.PI / 2 ) + ( progress * ( Math.PI * 2 ) );[m
[32m+[m		[32mvar startAngle = ( - Math.PI / 2 ) + ( this.progressOffset * ( Math.PI * 2 ) );[m
[32m+[m
[32m+[m		[32mthis.context.save();[m
[32m+[m		[32mthis.context.clearRect( 0, 0, this.diameter, this.diameter );[m
[32m+[m
[32m+[m		[32m// Solid background color[m
[32m+[m		[32mthis.context.beginPath();[m
[32m+[m		[32mthis.context.arc( x, y, radius + 2, 0, Math.PI * 2, false );[m
[32m+[m		[32mthis.context.fillStyle = 'rgba( 0, 0, 0, 0.4 )';[m
[32m+[m		[32mthis.context.fill();[m
[32m+[m
[32m+[m		[32m// Draw progress track[m
[32m+[m		[32mthis.context.beginPath();[m
[32m+[m		[32mthis.context.arc( x, y, radius, 0, Math.PI * 2, false );[m
[32m+[m		[32mthis.context.lineWidth = this.thickness;[m
[32m+[m		[32mthis.context.strokeStyle = '#666';[m
[32m+[m		[32mthis.context.stroke();[m
[32m+[m
[32m+[m		[32mif( this.playing ) {[m
[32m+[m			[32m// Draw progress on top of track[m
[32m+[m			[32mthis.context.beginPath();[m
[32m+[m			[32mthis.context.arc( x, y, radius, startAngle, endAngle, false );[m
[32m+[m			[32mthis.context.lineWidth = this.thickness;[m
[32m+[m			[32mthis.context.strokeStyle = '#fff';[m
[32m+[m			[32mthis.context.stroke();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mthis.context.translate( x - ( iconSize / 2 ), y - ( iconSize / 2 ) );[m
[32m+[m
[32m+[m		[32m// Draw play/pause icons[m
[32m+[m		[32mif( this.playing ) {[m
[32m+[m			[32mthis.context.fillStyle = '#fff';[m
[32m+[m			[32mthis.context.fillRect( 0, 0, iconSize / 2 - 2, iconSize );[m
[32m+[m			[32mthis.context.fillRect( iconSize / 2 + 2, 0, iconSize / 2 - 2, iconSize );[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mthis.context.beginPath();[m
[32m+[m			[32mthis.context.translate( 2, 0 );[m
[32m+[m			[32mthis.context.moveTo( 0, 0 );[m
[32m+[m			[32mthis.context.lineTo( iconSize - 2, iconSize / 2 );[m
[32m+[m			[32mthis.context.lineTo( 0, iconSize );[m
[32m+[m			[32mthis.context.fillStyle = '#fff';[m
[32m+[m			[32mthis.context.fill();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mthis.context.restore();[m
[32m+[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32mPlayback.prototype.on = function( type, listener ) {[m
[32m+[m		[32mthis.canvas.addEventListener( type, listener, false );[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32mPlayback.prototype.off = function( type, listener ) {[m
[32m+[m		[32mthis.canvas.removeEventListener( type, listener, false );[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32mPlayback.prototype.destroy = function() {[m
[32m+[m
[32m+[m		[32mthis.playing = false;[m
[32m+[m
[32m+[m		[32mif( this.canvas.parentNode ) {[m
[32m+[m			[32mthis.container.removeChild( this.canvas );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m
[32m+[m	[32m// --------------------------------------------------------------------//[m
[32m+[m	[32m// ------------------------------- API --------------------------------//[m
[32m+[m	[32m// --------------------------------------------------------------------//[m
[32m+[m
[32m+[m
[32m+[m	[32mReveal = {[m
[32m+[m		[32minitialize: initialize,[m
[32m+[m		[32mconfigure: configure,[m
[32m+[m		[32msync: sync,[m
[32m+[m
[32m+[m		[32m// Navigation methods[m
[32m+[m		[32mslide: slide,[m
[32m+[m		[32mleft: navigateLeft,[m
[32m+[m		[32mright: navigateRight,[m
[32m+[m		[32mup: navigateUp,[m
[32m+[m		[32mdown: navigateDown,[m
[32m+[m		[32mprev: navigatePrev,[m
[32m+[m		[32mnext: navigateNext,[m
[32m+[m
[32m+[m		[32m// Fragment methods[m
[32m+[m		[32mnavigateFragment: navigateFragment,[m
[32m+[m		[32mprevFragment: previousFragment,[m
[32m+[m		[32mnextFragment: nextFragment,[m
[32m+[m
[32m+[m		[32m// Deprecated aliases[m
[32m+[m		[32mnavigateTo: slide,[m
[32m+[m		[32mnavigateLeft: navigateLeft,[m
[32m+[m		[32mnavigateRight: navigateRight,[m
[32m+[m		[32mnavigateUp: navigateUp,[m
[32m+[m		[32mnavigateDown: navigateDown,[m
[32m+[m		[32mnavigatePrev: navigatePrev,[m
[32m+[m		[32mnavigateNext: navigateNext,[m
[32m+[m
[32m+[m		[32m// Forces an update in slide layout[m
[32m+[m		[32mlayout: layout,[m
[32m+[m
[32m+[m		[32m// Returns an object with the available routes as booleans (left/right/top/bottom)[m
[32m+[m		[32mavailableRoutes: availableRoutes,[m
[32m+[m
[32m+[m		[32m// Returns an object with the available fragments as booleans (prev/next)[m
[32m+[m		[32mavailableFragments: availableFragments,[m
[32m+[m
[32m+[m		[32m// Toggles the overview mode on/off[m
[32m+[m		[32mtoggleOverview: toggleOverview,[m
[32m+[m
[32m+[m		[32m// Toggles the "black screen" mode on/off[m
[32m+[m		[32mtogglePause: togglePause,[m
[32m+[m
[32m+[m		[32m// Toggles the auto slide mode on/off[m
[32m+[m		[32mtoggleAutoSlide: toggleAutoSlide,[m
[32m+[m
[32m+[m		[32m// State checks[m
[32m+[m		[32misOverview: isOverview,[m
[32m+[m		[32misPaused: isPaused,[m
[32m+[m		[32misAutoSliding: isAutoSliding,[m
[32m+[m
[32m+[m		[32m// Adds or removes all internal event listeners (such as keyboard)[m
[32m+[m		[32maddEventListeners: addEventListeners,[m
[32m+[m		[32mremoveEventListeners: removeEventListeners,[m
[32m+[m
[32m+[m		[32m// Facility for persisting and restoring the presentation state[m
[32m+[m		[32mgetState: getState,[m
[32m+[m		[32msetState: setState,[m
[32m+[m
[32m+[m		[32m// Presentation progress on range of 0-1[m
[32m+[m		[32mgetProgress: getProgress,[m
[32m+[m
[32m+[m		[32m// Returns the indices of the current, or specified, slide[m
[32m+[m		[32mgetIndices: getIndices,[m
[32m+[m
[32m+[m		[32mgetTotalSlides: getTotalSlides,[m
[32m+[m
[32m+[m		[32m// Returns the slide element at the specified index[m
[32m+[m		[32mgetSlide: getSlide,[m
[32m+[m
[32m+[m		[32m// Returns the slide background element at the specified index[m
[32m+[m		[32mgetSlideBackground: getSlideBackground,[m
[32m+[m
[32m+[m		[32m// Returns the speaker notes string for a slide, or null[m
[32m+[m		[32mgetSlideNotes: getSlideNotes,[m
[32m+[m
[32m+[m		[32m// Returns the previous slide element, may be null[m
[32m+[m		[32mgetPreviousSlide: function() {[m
[32m+[m			[32mreturn previousSlide;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Returns the current slide element[m
[32m+[m		[32mgetCurrentSlide: function() {[m
[32m+[m			[32mreturn currentSlide;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Returns the current scale of the presentation content[m
[32m+[m		[32mgetScale: function() {[m
[32m+[m			[32mreturn scale;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Returns the current configuration object[m
[32m+[m		[32mgetConfig: function() {[m
[32m+[m			[32mreturn config;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Helper method, retrieves query string as a key/value hash[m
[32m+[m		[32mgetQueryHash: function() {[m
[32m+[m			[32mvar query = {};[m
[32m+[m
[32m+[m			[32mlocation.search.replace( /[A-Z0-9]+?=([\w\.%-]*)/gi, function(a) {[m
[32m+[m				[32mquery[ a.split( '=' ).shift() ] = a.split( '=' ).pop();[m
[32m+[m			[32m} );[m
[32m+[m
[32m+[m			[32m// Basic deserialization[m
[32m+[m			[32mfor( var i in query ) {[m
[32m+[m				[32mvar value = query[ i ];[m
[32m+[m
[32m+[m				[32mquery[ i ] = deserialize( unescape( value ) );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn query;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Returns true if we're currently on the first slide[m
[32m+[m		[32misFirstSlide: function() {[m
[32m+[m			[32mreturn ( indexh === 0 && indexv === 0 );[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Returns true if we're currently on the last slide[m
[32m+[m		[32misLastSlide: function() {[m
[32m+[m			[32mif( currentSlide ) {[m
[32m+[m				[32m// Does this slide has next a sibling?[m
[32m+[m				[32mif( currentSlide.nextElementSibling ) return false;[m
[32m+[m
[32m+[m				[32m// If it's vertical, does its parent have a next sibling?[m
[32m+[m				[32mif( isVerticalSlide( currentSlide ) && currentSlide.parentNode.nextElementSibling ) return false;[m
[32m+[m
[32m+[m				[32mreturn true;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn false;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Checks if reveal.js has been loaded and is ready for use[m
[32m+[m		[32misReady: function() {[m
[32m+[m			[32mreturn loaded;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Forward event binding to the reveal DOM element[m
[32m+[m		[32maddEventListener: function( type, listener, useCapture ) {[m
[32m+[m			[32mif( 'addEventListener' in window ) {[m
[32m+[m				[32m( dom.wrapper || document.querySelector( '.reveal' ) ).addEventListener( type, listener, useCapture );[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m		[32mremoveEventListener: function( type, listener, useCapture ) {[m
[32m+[m			[32mif( 'addEventListener' in window ) {[m
[32m+[m				[32m( dom.wrapper || document.querySelector( '.reveal' ) ).removeEventListener( type, listener, useCapture );[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Programatically triggers a keyboard event[m
[32m+[m		[32mtriggerKey: function( keyCode ) {[m
[32m+[m			[32monDocumentKeyDown( { keyCode: keyCode } );[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32mreturn Reveal;[m
[32m+[m
[32m+[m[32m}));[m
