[32m+[m[32m/**[m
[32m+[m[32m * Touch-based remote controller for your presentation courtesy[m[41m [m
[32m+[m[32m * of the folks at http://remotes.io[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m(function(window){[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Detects if we are dealing with a touch enabled device (with some false positives)[m
[32m+[m[32m     * Borrowed from modernizr: https://github.com/Modernizr/Modernizr/blob/master/feature-detects/touch.js[m[41m   [m
[32m+[m[32m     */[m
[32m+[m[32m    var hasTouch  = (function(){[m
[32m+[m[32m        return ('ontouchstart' in window) || window.DocumentTouch && document instanceof DocumentTouch;[m
[32m+[m[32m    })();[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Detects if notes are enable and the current page is opened inside an /iframe[m
[32m+[m[32m     * this prevents loading Remotes.io several times[m
[32m+[m[32m     */[m
[32m+[m[32m    var isNotesAndIframe = (function(){[m
[32m+[m[32m        return window.RevealNotes && !(self == top);[m
[32m+[m[32m    })();[m
[32m+[m
[32m+[m[32m    if(!hasTouch && !isNotesAndIframe){[m
[32m+[m[32m        head.ready( 'remotes.ne.min.js', function() {[m
[32m+[m[32m            new Remotes("preview")[m
[32m+[m[32m                .on("swipe-left", function(e){ Reveal.right(); })[m
[32m+[m[32m                .on("swipe-right", function(e){ Reveal.left(); })[m
[32m+[m[32m                .on("swipe-up", function(e){ Reveal.down(); })[m
[32m+[m[32m                .on("swipe-down", function(e){ Reveal.up(); })[m
[32m+[m[32m                .on("tap", function(e){ Reveal.next(); })[m
[32m+[m[32m                .on("zoom-out", function(e){ Reveal.toggleOverview(true); })[m
[32m+[m[32m                .on("zoom-in", function(e){ Reveal.toggleOverview(false); })[m
[32m+[m[32m            ;[m
[32m+[m[32m        } );[m
[32m+[m
[32m+[m[32m        head.js('https://raw.github.com/Remotes/Remotes/master/dist/remotes.ne.min.js');[m
[32m+[m[32m    }[m
[32m+[m[32m})(window);[m
