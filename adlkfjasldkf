[32m+[m[32m.state-background {[m
[32m+[m	[32mdisplay: none !important;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* SECTION 3: Set body font face, size, and color.[m
[32m+[m[32m   Consider using a serif font for readability. */[m
[32m+[m[32mbody, p, td, li, div {[m
[32m+[m	[32mfont-size: 18pt;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* SECTION 4: Set heading font face, sizes, and color.[m
[32m+[m[32m   Differentiate your headings from your body text.[m
[32m+[m[32m   Perhaps use a large sans-serif for distinction. */[m
[32m+[m[32mh1,h2,h3,h4,h5,h6 {[m
[32m+[m	[32mtext-shadow: 0 0 0 #000 !important;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* SECTION 5: Make hyperlinks more usable.[m
[32m+[m[32m   Ensure links are underlined, and consider appending[m
[32m+[m[32m   the URL to the end of the link for usability. */[m
[32m+[m[32ma:link,[m
[32m+[m[32ma:visited {[m
[32m+[m	[32mfont-weight: bold;[m
[32m+[m	[32mtext-decoration: underline;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m.reveal pre code {[m
[32m+[m	[32moverflow: hidden !important;[m
[32m+[m	[32mfont-family: monospace !important;[m
[32m+[m[32m}[m
[32m+[m
