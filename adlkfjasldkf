[32m+[m[32m# Markdown Demo[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m## External 1.1[m
[32m+[m
[32m+[m[32mContent 1.1[m
[32m+[m
[32m+[m[32mNote: This will only appear in the speaker notes window.[m
[32m+[m
[32m+[m
[32m+[m[32m## External 1.2[m
[32m+[m
[32m+[m[32mContent 1.2[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m## External 2[m
[32m+[m
[32m+[m[32mContent 2.1[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m## External 3.1[m
[32m+[m
[32m+[m[32mContent 3.1[m
[32m+[m
[32m+[m
[32m+[m[32m## External 3.2[m
[32m+[m
[32m+[m[32mContent 3.2[m
[1mdiff --git a/Presentation/reveal.js/plugin/markdown/markdown.js b/Presentation/reveal.js/plugin/markdown/markdown.js[m
[1mnew file mode 100644[m
[1mindex 0000000..f4035e2[m
[1m--- /dev/null[m
[1m+++ b/Presentation/reveal.js/plugin/markdown/markdown.js[m
[36m@@ -0,0 +1,402 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * The reveal.js markdown plugin. Handles parsing of[m
[32m+[m[32m * markdown inside of presentations as well as loading[m
[32m+[m[32m * of external markdown documents.[m
[32m+[m[32m */[m
[32m+[m[32m(function( root, factory ) {[m
[32m+[m	[32mif( typeof exports === 'object' ) {[m
[32m+[m		[32mmodule.exports = factory( require( './marked' ) );[m
[32m+[m	[32m}[m
[32m+[m	[32melse {[m
[32m+[m		[32m// Browser globals (root is window)[m
[32m+[m		[32mroot.RevealMarkdown = factory( root.marked );[m
[32m+[m		[32mroot.RevealMarkdown.initialize();[m
[32m+[m	[32m}[m
[32m+[m[32m}( this, function( marked ) {[m
[32m+[m
[32m+[m	[32mif( typeof marked === 'undefined' ) {[m
[32m+[m		[32mthrow 'The reveal.js Markdown plugin requires marked to be loaded';[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif( typeof hljs !== 'undefined' ) {[m
[32m+[m		[32mmarked.setOptions({[m
[32m+[m			[32mhighlight: function( lang, code ) {[m
[32m+[m				[32mreturn hljs.highlightAuto( lang, code ).value;[m
[32m+[m			[32m}[m
[32m+[m		[32m});[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mvar DEFAULT_SLIDE_SEPARATOR = '^\r?\n---\r?\n$',[m
[32m+[m		[32mDEFAULT_NOTES_SEPARATOR = 'note:',[m
[32m+[m		[32mDEFAULT_ELEMENT_ATTRIBUTES_SEPARATOR = '\\\.element\\\s*?(.+?)$',[m
[32m+[m		[32mDEFAULT_SLIDE_ATTRIBUTES_SEPARATOR = '\\\.slide:\\\s*?(\\\S.+?)$';[m
[32m+[m
[32m+[m	[32mvar SCRIPT_END_PLACEHOLDER = '__SCRIPT_END__';[m
[32m+[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Retrieves the markdown contents of a slide section[m
[32m+[m	[32m * element. Normalizes leading tabs/whitespace.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction getMarkdownFromSlide( section ) {[m
[32m+[m
[32m+[m		[32mvar template = section.querySelector( 'script' );[m
[32m+[m
[32m+[m		[32m// strip leading whitespace so it isn't evaluated as code[m
[32m+[m		[32mvar text = ( template || section ).textContent;[m
[32m+[m
[32m+[m		[32m// restore script end tags[m
[32m+[m		[32mtext = text.replace( new RegExp( SCRIPT_END_PLACEHOLDER, 'g' ), '</script>' );[m
[32m+[m
[32m+[m		[32mvar leadingWs = text.match( /^\n?(\s*)/ )[1].length,[m
[32m+[m			[32mleadingTabs = text.match( /^\n?(\t*)/ )[1].length;[m
[32m+[m
[32m+[m		[32mif( leadingTabs > 0 ) {[m
[32m+[m			[32mtext = text.replace( new RegExp('\\n?\\t{' + leadingTabs + '}','g'), '\n' );[m
[32m+[m		[32m}[m
[32m+[m		[32melse if( leadingWs > 1 ) {[m
[32m+[m			[32mtext = text.replace( new RegExp('\\n? {' + leadingWs + '}', 'g'), '\n' );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn text;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Given a markdown slide section element, this will[m
[32m+[m	[32m * return all arguments that aren't related to markdown[m
[32m+[m	[32m * parsing. Used to forward any other user-defined arguments[m
[32m+[m	[32m * to the output markdown slide.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction getForwardedAttributes( section ) {[m
[32m+[m
[32m+[m		[32mvar attributes = section.attributes;[m
[32m+[m		[32mvar result = [];[m
[32m+[m
[32m+[m		[32mfor( var i = 0, len = attributes.length; i < len; i++ ) {[m
[32m+[m			[32mvar name = attributes[i].name,[m
[32m+[m				[32mvalue = attributes[i].value;[m
[32m+[m
[32m+[m			[32m// disregard attributes that are used for markdown loading/parsing[m
[32m+[m			[32mif( /data\-(markdown|separator|vertical|notes)/gi.test( name ) ) continue;[m
[32m+[m
[32m+[m			[32mif( value ) {[m
[32m+[m				[32mresult.push( name + '="' + value + '"' );[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32mresult.push( name );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn result.join( ' ' );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Inspects the given options and fills out default[m
[32m+[m	[32m * values for what's not defined.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction getSlidifyOptions( options ) {[m
[32m+[m
[32m+[m		[32moptions = options || {};[m
[32m+[m		[32moptions.separator = options.separator || DEFAULT_SLIDE_SEPARATOR;[m
[32m+[m		[32moptions.notesSeparator = options.notesSeparator || DEFAULT_NOTES_SEPARATOR;[m
[32m+[m		[32moptions.attributes = options.attributes || '';[m
[32m+[m
[32m+[m		[32mreturn options;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Helper function for constructing a markdown slide.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction createMarkdownSlide( content, options ) {[m
[32m+[m
[32m+[m		[32moptions = getSlidifyOptions( options );[m
[32m+[m
[32m+[m		[32mvar notesMatch = content.split( new RegExp( options.notesSeparator, 'mgi' ) );[m
[32m+[m
[32m+[m		[32mif( notesMatch.length === 2 ) {[m
[32m+[m			[32mcontent = notesMatch[0] + '<aside class="notes" data-markdown>' + notesMatch[1].trim() + '</aside>';[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// prevent script end tags in the content from interfering[m
[32m+[m		[32m// with parsing[m
[32m+[m		[32mcontent = content.replace( /<\/script>/g, SCRIPT_END_PLACEHOLDER );[m
[32m+[m
[32m+[m		[32mreturn '<script type="text/template">' + content + '</script>';[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Parses a data string into multiple slides based[m
[32m+[m	[32m * on the passed in separator arguments.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction slidify( markdown, options ) {[m
[32m+[m
[32m+[m		[32moptions = getSlidifyOptions( options );[m
[32m+[m
[32m+[m		[32mvar separatorRegex = new RegExp( options.separator + ( options.verticalSeparator ? '|' + options.verticalSeparator : '' ), 'mg' ),[m
[32m+[m			[32mhorizontalSeparatorRegex = new RegExp( options.separator );[m
[32m+[m
[32m+[m		[32mvar matches,[m
[32m+[m			[32mlastIndex = 0,[m
[32m+[m			[32misHorizontal,[m
[32m+[m			[32mwasHorizontal = true,[m
[32m+[m			[32mcontent,[m
[32m+[m			[32msectionStack = [];[m
[32m+[m
[32m+[m		[32m// iterate until all blocks between separators are stacked up[m
[32m+[m		[32mwhile( matches = separatorRegex.exec( markdown ) ) {[m
[32m+[m			[32mnotes = null;[m
[32m+[m
[32m+[m			[32m// determine direction (horizontal by default)[m
[32m+[m			[32misHorizontal = horizontalSeparatorRegex.test( matches[0] );[m
[32m+[m
[32m+[m			[32mif( !isHorizontal && wasHorizontal ) {[m
[32m+[m				[32m// create vertical stack[m
[32m+[m				[32msectionStack.push( [] );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// pluck slide content from markdown input[m
[32m+[m			[32mcontent = markdown.substring( lastIndex, matches.index );[m
[32m+[m
[32m+[m			[32mif( isHorizontal && wasHorizontal ) {[m
[32m+[m				[32m// add to horizontal stack[m
[32m+[m				[32msectionStack.push( content );[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32m// add to vertical stack[m
[32m+[m				[32msectionStack[sectionStack.length-1].push( content );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mlastIndex = separatorRegex.lastIndex;[m
[32m+[m			[32mwasHorizontal = isHorizontal;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// add the remaining slide[m
[32m+[m		[32m( wasHorizontal ? sectionStack : sectionStack[sectionStack.length-1] ).push( markdown.substring( lastIndex ) );[m
[32m+[m
[32m+[m		[32mvar markdownSections = '';[m
[32m+[m
[32m+[m		[32m// flatten the hierarchical stack, and insert <section data-markdown> tags[m
[32m+[m		[32mfor( var i = 0, len = sectionStack.length; i < len; i++ ) {[m
[32m+[m			[32m// vertical[m
[32m+[m			[32mif( sectionStack[i] instanceof Array ) {[m
[32m+[m				[32mmarkdownSections += '<section '+ options.attributes +'>';[m
[32m+[m
[32m+[m				[32msectionStack[i].forEach( function( child ) {[m
[32m+[m					[32mmarkdownSections += '<section data-markdown>' +  createMarkdownSlide( child, options ) + '</section>';[m
[32m+[m				[32m} );[m
[32m+[m
[32m+[m				[32mmarkdownSections += '</section>';[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32mmarkdownSections += '<section '+ options.attributes +' data-markdown>' + createMarkdownSlide( sectionStack[i], options ) + '</section>';[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn markdownSections;[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Parses any current data-markdown slides, splits[m
[32m+[m	[32m * multi-slide markdown into separate sections and[m
[32m+[m	[32m * handles loading of external markdown.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction processSlides() {[m
[32m+[m
[32m+[m		[32mvar sections = document.querySelectorAll( '[data-markdown]'),[m
[32m+[m			[32msection;[m
[32m+[m
[32m+[m		[32mfor( var i = 0, len = sections.length; i < len; i++ ) {[m
[32m+[m
[32m+[m			[32msection = sections[i];[m
[32m+[m
[32m+[m			[32mif( section.getAttribute( 'data-markdown' ).length ) {[m
[32m+[m
[32m+[m				[32mvar xhr = new XMLHttpRequest(),[m
[32m+[m					[32murl = section.getAttribute( 'data-markdown' );[m
[32m+[m
[32m+[m				[32mdatacharset = section.getAttribute( 'data-charset' );[m
[32m+[m
[32m+[m				[32m// see https://developer.mozilla.org/en-US/docs/Web/API/element.getAttribute#Notes[m
[32m+[m				[32mif( datacharset != null && datacharset != '' ) {[m
[32m+[m					[32mxhr.overrideMimeType( 'text/html; charset=' + datacharset );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mxhr.onreadystatechange = function() {[m
[32m+[m					[32mif( xhr.readyState === 4 ) {[m
[32m+[m						[32m// file protocol yields status code 0 (useful for local debug, mobile applications etc.)[m
[32m+[m						[32mif ( ( xhr.status >= 200 && xhr.status < 300 ) || xhr.status === 0 ) {[m
[32m+[m
[32m+[m							[32msection.outerHTML = slidify( xhr.responseText, {[m
[32m+[m								[32mseparator: section.getAttribute( 'data-separator' ),[m
[32m+[m								[32mverticalSeparator: section.getAttribute( 'data-separator-vertical' ),[m
[32m+[m								[32mnotesSeparator: section.getAttribute( 'data-separator-notes' ),[m
[32m+[m								[32mattributes: getForwardedAttributes( section )[m
[32m+[m							[32m});[m
[32m+[m
[32m+[m						[32m}[m
[32m+[m						[32melse {[m
[32m+[m
[32m+[m							[32msection.outerHTML = '<section data-state="alert">' +[m
[32m+[m								[32m'ERROR: The attempt to fetch ' + url + ' failed with HTTP status ' + xhr.status + '.' +[m
[32m+[m								[32m'Check your browser\'s JavaScript console for more details.' +[m
[32m+[m								[32m'<p>Remember that you need to serve the presentation HTML from a HTTP server.</p>' +[m
[32m+[m								[32m'</section>';[m
[32m+[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m};[m
[32m+[m
[32m+[m				[32mxhr.open( 'GET', url, false );[m
[32m+[m
[32m+[m				[32mtry {[m
[32m+[m					[32mxhr.send();[m
[32m+[m				[32m}[m
[32m+[m				[32mcatch ( e ) {[m
[32m+[m					[32malert( 'Failed to get the Markdown file ' + url + '. Make sure that the presentation and the file are served by a HTTP server and the file can be found there. ' + e );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m}[m
[32m+[m			[32melse if( section.getAttribute( 'data-separator' ) || section.getAttribute( 'data-separator-vertical' ) || section.getAttribute( 'data-separator-notes' ) ) {[m
[32m+[m
[32m+[m				[32msection.outerHTML = slidify( getMarkdownFromSlide( section ), {[m
[32m+[m					[32mseparator: section.getAttribute( 'data-separator' ),[m
[32m+[m					[32mverticalSeparator: section.getAttribute( 'data-separator-vertical' ),[m
[32m+[m					[32mnotesSeparator: section.getAttribute( 'data-separator-notes' ),[m
[32m+[m					[32mattributes: getForwardedAttributes( section )[m
[32m+[m				[32m});[m
[32m+[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32msection.innerHTML = createMarkdownSlide( getMarkdownFromSlide( section ) );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Check if a node value has the attributes pattern.[m
[32m+[m	[32m * If yes, extract it and add that value as one or several attributes[m
[32m+[m	[32m * the the terget element.[m
[32m+[m	[32m *[m
[32m+[m	[32m * You need Cache Killer on Chrome to see the effect on any FOM transformation[m
[32m+[m	[32m * directly on refresh (F5)[m
[32m+[m	[32m * http://stackoverflow.com/questions/5690269/disabling-chrome-cache-for-website-development/7000899#answer-11786277[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction addAttributeInElement( node, elementTarget, separator ) {[m
[32m+[m
[32m+[m		[32mvar mardownClassesInElementsRegex = new RegExp( separator, 'mg' );[m
[32m+[m		[32mvar mardownClassRegex = new RegExp( "([^\"= ]+?)=\"([^\"=]+?)\"", 'mg' );[m
[32m+[m		[32mvar nodeValue = node.nodeValue;[m
[32m+[m		[32mif( matches = mardownClassesInElementsRegex.exec( nodeValue ) ) {[m
[32m+[m
[32m+[m			[32mvar classes = matches[1];[m
[32m+[m			[32mnodeValue = nodeValue.substring( 0, matches.index ) + nodeValue.substring( mardownClassesInElementsRegex.lastIndex );[m
[32m+[m			[32mnode.nodeValue = nodeValue;[m
[32m+[m			[32mwhile( matchesClass = mardownClassRegex.exec( classes ) ) {[m
[32m+[m				[32melementTarget.setAttribute( matchesClass[1], matchesClass[2] );[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn true;[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn false;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Add attributes to the parent element of a text node,[m
[32m+[m	[32m * or the element of an attribute node.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction addAttributes( section, element, previousElement, separatorElementAttributes, separatorSectionAttributes ) {[m
[32m+[m
[32m+[m		[32mif ( element != null && element.childNodes != undefined && element.childNodes.length > 0 ) {[m
[32m+[m			[32mpreviousParentElement = element;[m
[32m+[m			[32mfor( var i = 0; i < element.childNodes.length; i++ ) {[m
[32m+[m				[32mchildElement = element.childNodes[i];[m
[32m+[m				[32mif ( i > 0 ) {[m
[32m+[m					[32mj = i - 1;[m
[32m+[m					[32mwhile ( j >= 0 ) {[m
[32m+[m						[32maPreviousChildElement = element.childNodes[j];[m
[32m+[m						[32mif ( typeof aPreviousChildElement.setAttribute == 'function' && aPreviousChildElement.tagName != "BR" ) {[m
[32m+[m							[32mpreviousParentElement = aPreviousChildElement;[m
[32m+[m							[32mbreak;[m
[32m+[m						[32m}[m
[32m+[m						[32mj = j - 1;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m				[32mparentSection = section;[m
[32m+[m				[32mif( childElement.nodeName ==  "section" ) {[m
[32m+[m					[32mparentSection = childElement ;[m
[32m+[m					[32mpreviousParentElement = childElement ;[m
[32m+[m				[32m}[m
[32m+[m				[32mif ( typeof childElement.setAttribute == 'function' || childElement.nodeType == Node.COMMENT_NODE ) {[m
[32m+[m					[32maddAttributes( parentSection, childElement, previousParentElement, separatorElementAttributes, separatorSectionAttributes );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( element.nodeType == Node.COMMENT_NODE ) {[m
[32m+[m			[32mif ( addAttributeInElement( element, previousElement, separatorElementAttributes ) == false ) {[m
[32m+[m				[32maddAttributeInElement( element, section, separatorSectionAttributes );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Converts any current data-markdown slides in the[m
[32m+[m	[32m * DOM to HTML.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction convertSlides() {[m
[32m+[m
[32m+[m		[32mvar sections = document.querySelectorAll( '[data-markdown]');[m
[32m+[m
[32m+[m		[32mfor( var i = 0, len = sections.length; i < len; i++ ) {[m
[32m+[m
[32m+[m			[32mvar section = sections[i];[m
[32m+[m
[32m+[m			[32m// Only parse the same slide once[m
[32m+[m			[32mif( !section.getAttribute( 'data-markdown-parsed' ) ) {[m
[32m+[m
[32m+[m				[32msection.setAttribute( 'data-markdown-parsed', true )[m
[32m+[m
[32m+[m				[32mvar notes = section.querySelector( 'aside.notes' );[m
[32m+[m				[32mvar markdown = getMarkdownFromSlide( section );[m
[32m+[m
[32m+[m				[32msection.innerHTML = marked( markdown );[m
[32m+[m				[32maddAttributes( 	section, section, null, section.getAttribute( 'data-element-attributes' ) ||[m
[32m+[m								[32msection.parentNode.getAttribute( 'data-element-attributes' ) ||[m
[32m+[m								[32mDEFAULT_ELEMENT_ATTRIBUTES_SEPARATOR,[m
[32m+[m								[32msection.getAttribute( 'data-attributes' ) ||[m
[32m+[m								[32msection.parentNode.getAttribute( 'data-attributes' ) ||[m
[32m+[m								[32mDEFAULT_SLIDE_ATTRIBUTES_SEPARATOR);[m
[32m+[m
[32m+[m				[32m// If there were notes, we need to re-add them after[m
[32m+[m				[32m// having overwritten the section's HTML[m
[32m+[m				[32mif( notes ) {[m
[32m+[m					[32msection.appendChild( notes );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// API[m
[32m+[m	[32mreturn {[m
[32m+[m
[32m+[m		[32minitialize: function() {[m
[32m+[m			[32mprocessSlides();[m
[32m+[m			[32mconvertSlides();[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// TODO: Do these belong in the API?[m
[32m+[m		[32mprocessSlides: processSlides,[m
[32m+[m		[32mconvertSlides: convertSlides,[m
[32m+[m		[32mslidify: slidify[m
[32m+[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m[32m}));[m
