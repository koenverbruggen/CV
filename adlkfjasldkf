[32m+[m[32mvar http        = require('http');[m
[32m+[m[32mvar express		= require('express');[m
[32m+[m[32mvar fs			= require('fs');[m
[32m+[m[32mvar io			= require('socket.io');[m
[32m+[m[32mvar crypto		= require('crypto');[m
[32m+[m
[32m+[m[32mvar app       	= express();[m
[32m+[m[32mvar staticDir 	= express.static;[m
[32m+[m[32mvar server    	= http.createServer(app);[m
[32m+[m
[32m+[m[32mio = io(server);[m
[32m+[m
[32m+[m[32mvar opts = {[m
[32m+[m	[32mport: process.env.PORT || 1948,[m
[32m+[m	[32mbaseDir : __dirname + '/../../'[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mio.on( 'connection', function( socket ) {[m
[32m+[m	[32msocket.on('multiplex-statechanged', function(data) {[m
[32m+[m		[32mif (typeof data.secret == 'undefined' || data.secret == null || data.secret === '') return;[m
[32m+[m		[32mif (createHash(data.secret) === data.socketId) {[m
[32m+[m			[32mdata.secret = null;[m
[32m+[m			[32msocket.broadcast.emit(data.socketId, data);[m
[32m+[m		[32m};[m
[32m+[m	[32m});[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m[ 'css', 'js', 'plugin', 'lib' ].forEach(function(dir) {[m
[32m+[m	[32mapp.use('/' + dir, staticDir(opts.baseDir + dir));[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mapp.get("/", function(req, res) {[m
[32m+[m	[32mres.writeHead(200, {'Content-Type': 'text/html'});[m
[32m+[m	[32mfs.createReadStream(opts.baseDir + '/index.html').pipe(res);[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mapp.get("/token", function(req,res) {[m
[32m+[m	[32mvar ts = new Date().getTime();[m
[32m+[m	[32mvar rand = Math.floor(Math.random()*9999999);[m
[32m+[m	[32mvar secret = ts.toString() + rand.toString();[m
[32m+[m	[32mres.send({secret: secret, socketId: createHash(secret)});[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mvar createHash = function(secret) {[m
[32m+[m	[32mvar cipher = crypto.createCipher('blowfish', secret);[m
[32m+[m	[32mreturn(cipher.final('hex'));[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Actually listen[m
[32m+[m[32mserver.listen( opts.port || null );[m
[32m+[m
[32m+[m[32mvar brown = '\033[33m',[m
[32m+[m	[32mgreen = '\033[32m',[m
[32m+[m	[32mreset = '\033[0m';[m
[32m+[m
[32m+[m[32mconsole.log( brown + "reveal.js:" + reset + " Multiplex running on port " + green + opts.port + reset );[m
\ No newline at end of file[m
