[32m+[m[32m/*[m
[32m+[m
[32m+[m	[32msimple postmessage plugin[m
[32m+[m
[32m+[m	[32mUseful when a reveal slideshow is inside an iframe.[m
[32m+[m	[32mIt allows to call reveal methods from outside.[m
[32m+[m
[32m+[m	[32mExample:[m
[32m+[m		[32m var reveal =  window.frames[0];[m
[32m+[m
[32m+[m		[32m // Reveal.prev();[m[41m [m
[32m+[m		[32m reveal.postMessage(JSON.stringify({method: 'prev', args: []}), '*');[m
[32m+[m		[32m // Reveal.next();[m[41m [m
[32m+[m		[32m reveal.postMessage(JSON.stringify({method: 'next', args: []}), '*');[m
[32m+[m		[32m // Reveal.slide(2, 2);[m[41m [m
[32m+[m		[32m reveal.postMessage(JSON.stringify({method: 'slide', args: [2,2]}), '*');[m
[32m+[m
[32m+[m	[32mAdd to the slideshow:[m
[32m+[m
[32m+[m		[32mdependencies: [[m
[32m+[m			[32m...[m
[32m+[m			[32m{ src: 'plugin/postmessage/postmessage.js', async: true, condition: function() { return !!document.body.classList; } }[m
[32m+[m		[32m][m
[32m+[m
[32m+[m[32m*/[m
[32m+[m
[32m+[m[32m(function (){[m
[32m+[m
[32m+[m	[32mwindow.addEventListener( "message", function ( event ) {[m
[32m+[m		[32mvar data = JSON.parse( event.data ),[m
[32m+[m				[32mmethod = data.method,[m
[32m+[m				[32margs = data.args;[m
[32m+[m
[32m+[m		[32mif( typeof Reveal[method] === 'function' ) {[m
[32m+[m			[32mReveal[method].apply( Reveal, data.args );[m
[32m+[m		[32m}[m
[32m+[m	[32m}, false);[m
[32m+[m
[32m+[m[32m}());[m
[32m+[m
