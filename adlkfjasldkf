[1mdiff --git a/Presentation/reveal.js/plugin/search/search.js b/Presentation/reveal.js/plugin/search/search.js[m
[1mnew file mode 100644[m
[1mindex 0000000..ae6582e[m
[1m--- /dev/null[m
[1m+++ b/Presentation/reveal.js/plugin/search/search.js[m
[36m@@ -0,0 +1,196 @@[m
[32m+[m[32m/*[m
[32m+[m[32m * Handles finding a text string anywhere in the slides and showing the next occurrence to the user[m
[32m+[m[32m * by navigatating to that slide and highlighting it.[m
[32m+[m[32m *[m
[32m+[m[32m * By Jon Snyder <snyder.jon@gmail.com>, February 2013[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mvar RevealSearch = (function() {[m
[32m+[m
[32m+[m	[32mvar matchedSlides;[m
[32m+[m	[32mvar currentMatchedIndex;[m
[32m+[m	[32mvar searchboxDirty;[m
[32m+[m	[32mvar myHilitor;[m
[32m+[m
[32m+[m[32m// Original JavaScript code by Chirp Internet: www.chirp.com.au[m
[32m+[m[32m// Please acknowledge use of this code by including this header.[m
[32m+[m[32m// 2/2013 jon: modified regex to display any match, not restricted to word boundaries.[m
[32m+[m
[32m+[m[32mfunction Hilitor(id, tag)[m
[32m+[m[32m{[m
[32m+[m
[32m+[m[32m  var targetNode = document.getElementById(id) || document.body;[m
[32m+[m[32m  var hiliteTag = tag || "EM";[m
[32m+[m[32m  var skipTags = new RegExp("^(?:" + hiliteTag + "|SCRIPT|FORM|SPAN)$");[m
[32m+[m[32m  var colors = ["#ff6", "#a0ffff", "#9f9", "#f99", "#f6f"];[m
[32m+[m[32m  var wordColor = [];[m
[32m+[m[32m  var colorIdx = 0;[m
[32m+[m[32m  var matchRegex = "";[m
[32m+[m[32m  var matchingSlides = [];[m
[32m+[m
[32m+[m[32m  this.setRegex = function(input)[m
[32m+[m[32m  {[m
[32m+[m[32m    input = input.replace(/^[^\w]+|[^\w]+$/g, "").replace(/[^\w'-]+/g, "|");[m
[32m+[m[32m    matchRegex = new RegExp("(" + input + ")","i");[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  this.getRegex = function()[m
[32m+[m[32m  {[m
[32m+[m[32m    return matchRegex.toString().replace(/^\/\\b\(|\)\\b\/i$/g, "").replace(/\|/g, " ");[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // recursively apply word highlighting[m
[32m+[m[32m  this.hiliteWords = function(node)[m
[32m+[m[32m  {[m
[32m+[m[32m    if(node == undefined || !node) return;[m
[32m+[m[32m    if(!matchRegex) return;[m
[32m+[m[32m    if(skipTags.test(node.nodeName)) return;[m
[32m+[m
[32m+[m[32m    if(node.hasChildNodes()) {[m
[32m+[m[32m      for(var i=0; i < node.childNodes.length; i++)[m
[32m+[m[32m        this.hiliteWords(node.childNodes[i]);[m
[32m+[m[32m    }[m
[32m+[m[32m    if(node.nodeType == 3) { // NODE_TEXT[m
[32m+[m[32m      if((nv = node.nodeValue) && (regs = matchRegex.exec(nv))) {[m
[32m+[m[41m      [m	[32m//find the slide's section element and save it in our list of matching slides[m
[32m+[m[41m      [m	[32mvar secnode = node.parentNode;[m
[32m+[m[41m      [m	[32mwhile (secnode.nodeName != 'SECTION') {[m
[32m+[m[41m      [m		[32msecnode = secnode.parentNode;[m
[32m+[m[41m      [m	[32m}[m
[32m+[m[41m      	[m
[32m+[m[41m      [m	[32mvar slideIndex = Reveal.getIndices(secnode);[m
[32m+[m[41m      [m	[32mvar slidelen = matchingSlides.length;[m
[32m+[m[41m      [m	[32mvar alreadyAdded = false;[m
[32m+[m[41m      [m	[32mfor (var i=0; i < slidelen; i++) {[m
[32m+[m[41m      [m		[32mif ( (matchingSlides[i].h === slideIndex.h) && (matchingSlides[i].v === slideIndex.v) ) {[m
[32m+[m[41m      [m			[32malreadyAdded = true;[m
[32m+[m[41m      [m		[32m}[m
[32m+[m[41m      [m	[32m}[m
[32m+[m[41m      [m	[32mif (! alreadyAdded) {[m
[32m+[m[41m      [m		[32mmatchingSlides.push(slideIndex);[m
[32m+[m[41m      [m	[32m}[m
[32m+[m[41m      	[m
[32m+[m[32m        if(!wordColor[regs[0].toLowerCase()]) {[m
[32m+[m[32m          wordColor[regs[0].toLowerCase()] = colors[colorIdx++ % colors.length];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var match = document.createElement(hiliteTag);[m
[32m+[m[32m        match.appendChild(document.createTextNode(regs[0]));[m
[32m+[m[32m        match.style.backgroundColor = wordColor[regs[0].toLowerCase()];[m
[32m+[m[32m        match.style.fontStyle = "inherit";[m
[32m+[m[32m        match.style.color = "#000";[m
[32m+[m
[32m+[m[32m        var after = node.splitText(regs.index);[m
[32m+[m[32m        after.nodeValue = after.nodeValue.substring(regs[0].length);[m
[32m+[m[32m        node.parentNode.insertBefore(match, after);[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // remove highlighting[m
[32m+[m[32m  this.remove = function()[m
[32m+[m[32m  {[m
[32m+[m[32m    var arr = document.getElementsByTagName(hiliteTag);[m
[32m+[m[32m    while(arr.length && (el = arr[0])) {[m
[32m+[m[32m      el.parentNode.replaceChild(el.firstChild, el);[m
[32m+[m[32m    }[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m  // start highlighting at target node[m
[32m+[m[32m  this.apply = function(input)[m
[32m+[m[32m  {[m
[32m+[m[32m    if(input == undefined || !input) return;[m
[32m+[m[32m    this.remove();[m
[32m+[m[32m    this.setRegex(input);[m
[32m+[m[32m    this.hiliteWords(targetNode);[m
[32m+[m[32m    return matchingSlides;[m
[32m+[m[32m  };[m
[32m+[m
[32m+[m[32m}[m
[32m+[m
[32m+[m	[32mfunction openSearch() {[m
[32m+[m		[32m//ensure the search term input dialog is visible and has focus:[m
[32m+[m		[32mvar inputbox = document.getElementById("searchinput");[m
[32m+[m		[32minputbox.style.display = "inline";[m
[32m+[m		[32minputbox.focus();[m
[32m+[m		[32minputbox.select();[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction toggleSearch() {[m
[32m+[m		[32mvar inputbox = document.getElementById("searchinput");[m
[32m+[m		[32mif (inputbox.style.display !== "inline") {[m
[32m+[m			[32mopenSearch();[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32minputbox.style.display = "none";[m
[32m+[m			[32mmyHilitor.remove();[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction doSearch() {[m
[32m+[m		[32m//if there's been a change in the search term, perform a new search:[m
[32m+[m		[32mif (searchboxDirty) {[m
[32m+[m			[32mvar searchstring = document.getElementById("searchinput").value;[m
[32m+[m
[32m+[m			[32m//find the keyword amongst the slides[m
[32m+[m			[32mmyHilitor = new Hilitor("slidecontent");[m
[32m+[m			[32mmatchedSlides = myHilitor.apply(searchstring);[m
[32m+[m			[32mcurrentMatchedIndex = 0;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m//navigate to the next slide that has the keyword, wrapping to the first if necessary[m
[32m+[m		[32mif (matchedSlides.length && (matchedSlides.length <= currentMatchedIndex)) {[m
[32m+[m			[32mcurrentMatchedIndex = 0;[m
[32m+[m		[32m}[m
[32m+[m		[32mif (matchedSlides.length > currentMatchedIndex) {[m
[32m+[m			[32mReveal.slide(matchedSlides[currentMatchedIndex].h, matchedSlides[currentMatchedIndex].v);[m
[32m+[m			[32mcurrentMatchedIndex++;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mvar dom = {};[m
[32m+[m	[32mdom.wrapper = document.querySelector( '.reveal' );[m
[32m+[m
[32m+[m	[32mif( !dom.wrapper.querySelector( '.searchbox' ) ) {[m
[32m+[m			[32mvar searchElement = document.createElement( 'div' );[m
[32m+[m			[32msearchElement.id = "searchinputdiv";[m
[32m+[m			[32msearchElement.classList.add( 'searchdiv' );[m
[32m+[m[32m      searchElement.style.position = 'absolute';[m
[32m+[m[32m      searchElement.style.top = '10px';[m
[32m+[m[32m      searchElement.style.left = '10px';[m
[32m+[m[32m      //embedded base64 search icon Designed by Sketchdock - http://www.sketchdock.com/:[m
[32m+[m			[32msearchElement.innerHTML = '<span><input type="search" id="searchinput" class="searchinput" style="vertical-align: top;"/><img src="data:image/png;base64," id="searchbutton" class="searchicon" style="vertical-align: top; margin-top: -1px;"/></span>';[m
[32m+[m			[32mdom.wrapper.appendChild( searchElement );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mdocument.getElementById("searchbutton").addEventListener( 'click', function(event) {[m
[32m+[m		[32mdoSearch();[m
[32m+[m	[32m}, false );[m
[32m+[m
[32m+[m	[32mdocument.getElementById("searchinput").addEventListener( 'keyup', function( event ) {[m
[32m+[m		[32mswitch (event.keyCode) {[m
[32m+[m			[32mcase 13:[m
[32m+[m				[32mevent.preventDefault();[m
[32m+[m				[32mdoSearch();[m
[32m+[m				[32msearchboxDirty = false;[m
[32m+[m				[32mbreak;[m
[32m+[m			[32mdefault:[m
[32m+[m				[32msearchboxDirty = true;[m
[32m+[m		[32m}[m
[32m+[m	[32m}, false );[m
[32m+[m
[32m+[m	[32m// Open the search when the 's' key is hit (yes, this conflicts with the notes plugin, disabling for now)[m
[32m+[m	[32m/*[m
[32m+[m	[32mdocument.addEventListener( 'keydown', function( event ) {[m
[32m+[m		[32m// Disregard the event if the target is editable or a[m
[32m+[m		[32m// modifier is present[m
[32m+[m		[32mif ( document.querySelector( ':focus' ) !== null || event.shiftKey || event.altKey || event.ctrlKey || event.metaKey ) return;[m
[32m+[m
[32m+[m		[32mif( event.keyCode === 83 ) {[m
[32m+[m			[32mevent.preventDefault();[m
[32m+[m			[32mopenSearch();[m
[32m+[m		[32m}[m
[32m+[m	[32m}, false );[m
[32m+[m[32m*/[m
[32m+[m	[32mreturn { open: openSearch };[m
[32m+[m[32m})();[m
[1mdiff --git a/Presentation/reveal.js/plugin/zoom-js/zoom.js b/Presentation/reveal.js/plugin/zoom-js/zoom.js[m
[1mnew file mode 100644[m
[1mindex 0000000..95093e0[m
[1m--- /dev/null[m
[1m+++ b/Presentation/reveal.js/plugin/zoom-js/zoom.js[m
[36m@@ -0,0 +1,278 @@[m
[32m+[m[32m// Custom reveal.js integration[m
[32m+[m[32m(function(){[m
[32m+[m	[32mvar isEnabled = true;[m
[32m+[m
[32m+[m	[32mdocument.querySelector( '.reveal .slides' ).addEventListener( 'mousedown', function( event ) {[m
[32m+[m		[32mvar modifier = ( Reveal.getConfig().zoomKey ? Reveal.getConfig().zoomKey : 'alt' ) + 'Key';[m
[32m+[m
[32m+[m		[32mvar zoomPadding = 20;[m
[32m+[m		[32mvar revealScale = Reveal.getScale();[m
[32m+[m
[32m+[m		[32mif( event[ modifier ] && isEnabled ) {[m
[32m+[m			[32mevent.preventDefault();[m
[32m+[m
[32m+[m			[32mvar bounds = event.target.getBoundingClientRect();[m
[32m+[m
[32m+[m			[32mzoom.to({[m
[32m+[m				[32mx: ( bounds.left * revealScale ) - zoomPadding,[m
[32m+[m				[32my: ( bounds.top * revealScale ) - zoomPadding,[m
[32m+[m				[32mwidth: ( bounds.width * revealScale ) + ( zoomPadding * 2 ),[m
[32m+[m				[32mheight: ( bounds.height * revealScale ) + ( zoomPadding * 2 ),[m
[32m+[m				[32mpan: false[m
[32m+[m			[32m});[m
[32m+[m		[32m}[m
[32m+[m	[32m} );[m
[32m+[m
[32m+[m	[32mReveal.addEventListener( 'overviewshown', function() { isEnabled = false; } );[m
[32m+[m	[32mReveal.addEventListener( 'overviewhidden', function() { isEnabled = true; } );[m
[32m+[m[32m})();[m
[32m+[m
[32m+[m[32m/*![m
[32m+[m[32m * zoom.js 0.3 (modified for use with reveal.js)[m
[32m+[m[32m * http://lab.hakim.se/zoom-js[m
[32m+[m[32m * MIT licensed[m
[32m+[m[32m *[m
[32m+[m[32m * Copyright (C) 2011-2014 Hakim El Hattab, http://hakim.se[m
[32m+[m[32m */[m
[32m+[m[32mvar zoom = (function(){[m
[32m+[m
[32m+[m	[32m// The current zoom level (scale)[m
[32m+[m	[32mvar level = 1;[m
[32m+[m
[32m+[m	[32m// The current mouse position, used for panning[m
[32m+[m	[32mvar mouseX = 0,[m
[32m+[m		[32mmouseY = 0;[m
[32m+[m
[32m+[m	[32m// Timeout before pan is activated[m
[32m+[m	[32mvar panEngageTimeout = -1,[m
[32m+[m		[32mpanUpdateInterval = -1;[m
[32m+[m
[32m+[m	[32m// Check for transform support so that we can fallback otherwise[m
[32m+[m	[32mvar supportsTransforms = 	'WebkitTransform' in document.body.style ||[m
[32m+[m								[32m'MozTransform' in document.body.style ||[m
[32m+[m								[32m'msTransform' in document.body.style ||[m
[32m+[m								[32m'OTransform' in document.body.style ||[m
[32m+[m								[32m'transform' in document.body.style;[m
[32m+[m
[32m+[m	[32mif( supportsTransforms ) {[m
[32m+[m		[32m// The easing that will be applied when we zoom in/out[m
[32m+[m		[32mdocument.body.style.transition = 'transform 0.8s ease';[m
[32m+[m		[32mdocument.body.style.OTransition = '-o-transform 0.8s ease';[m
[32m+[m		[32mdocument.body.style.msTransition = '-ms-transform 0.8s ease';[m
[32m+[m		[32mdocument.body.style.MozTransition = '-moz-transform 0.8s ease';[m
[32m+[m		[32mdocument.body.style.WebkitTransition = '-webkit-transform 0.8s ease';[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Zoom out if the user hits escape[m
[32m+[m	[32mdocument.addEventListener( 'keyup', function( event ) {[m
[32m+[m		[32mif( level !== 1 && event.keyCode === 27 ) {[m
[32m+[m			[32mzoom.out();[m
[32m+[m		[32m}[m
[32m+[m	[32m} );[m
[32m+[m
[32m+[m	[32m// Monitor mouse movement for panning[m
[32m+[m	[32mdocument.addEventListener( 'mousemove', function( event ) {[m
[32m+[m		[32mif( level !== 1 ) {[m
[32m+[m			[32mmouseX = event.clientX;[m
[32m+[m			[32mmouseY = event.clientY;[m
[32m+[m		[32m}[m
[32m+[m	[32m} );[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Applies the CSS required to zoom in, prefers the use of CSS3[m
[32m+[m	[32m * transforms but falls back on zoom for IE.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {Object} rect[m
[32m+[m	[32m * @param {Number} scale[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction magnify( rect, scale ) {[m
[32m+[m
[32m+[m		[32mvar scrollOffset = getScrollOffset();[m
[32m+[m
[32m+[m		[32m// Ensure a width/height is set[m
[32m+[m		[32mrect.width = rect.width || 1;[m
[32m+[m		[32mrect.height = rect.height || 1;[m
[32m+[m
[32m+[m		[32m// Center the rect within the zoomed viewport[m
[32m+[m		[32mrect.x -= ( window.innerWidth - ( rect.width * scale ) ) / 2;[m
[32m+[m		[32mrect.y -= ( window.innerHeight - ( rect.height * scale ) ) / 2;[m
[32m+[m
[32m+[m		[32mif( supportsTransforms ) {[m
[32m+[m			[32m// Reset[m
[32m+[m			[32mif( scale === 1 ) {[m
[32m+[m				[32mdocument.body.style.transform = '';[m
[32m+[m				[32mdocument.body.style.OTransform = '';[m
[32m+[m				[32mdocument.body.style.msTransform = '';[m
[32m+[m				[32mdocument.body.style.MozTransform = '';[m
[32m+[m				[32mdocument.body.style.WebkitTransform = '';[m
[32m+[m			[32m}[m
[32m+[m			[32m// Scale[m
[32m+[m			[32melse {[m
[32m+[m				[32mvar origin = scrollOffset.x +'px '+ scrollOffset.y +'px',[m
[32m+[m					[32mtransform = 'translate('+ -rect.x +'px,'+ -rect.y +'px) scale('+ scale +')';[m
[32m+[m
[32m+[m				[32mdocument.body.style.transformOrigin = origin;[m
[32m+[m				[32mdocument.body.style.OTransformOrigin = origin;[m
[32m+[m				[32mdocument.body.style.msTransformOrigin = origin;[m
[32m+[m				[32mdocument.body.style.MozTransformOrigin = origin;[m
[32m+[m				[32mdocument.body.style.WebkitTransformOrigin = origin;[m
[32m+[m
[32m+[m				[32mdocument.body.style.transform = transform;[m
[32m+[m				[32mdocument.body.style.OTransform = transform;[m
[32m+[m				[32mdocument.body.style.msTransform = transform;[m
[32m+[m				[32mdocument.body.style.MozTransform = transform;[m
[32m+[m				[32mdocument.body.style.WebkitTransform = transform;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32m// Reset[m
[32m+[m			[32mif( scale === 1 ) {[m
[32m+[m				[32mdocument.body.style.position = '';[m
[32m+[m				[32mdocument.body.style.left = '';[m
[32m+[m				[32mdocument.body.style.top = '';[m
[32m+[m				[32mdocument.body.style.width = '';[m
[32m+[m				[32mdocument.body.style.height = '';[m
[32m+[m				[32mdocument.body.style.zoom = '';[m
[32m+[m			[32m}[m
[32m+[m			[32m// Scale[m
[32m+[m			[32melse {[m
[32m+[m				[32mdocument.body.style.position = 'relative';[m
[32m+[m				[32mdocument.body.style.left = ( - ( scrollOffset.x + rect.x ) / scale ) + 'px';[m
[32m+[m				[32mdocument.body.style.top = ( - ( scrollOffset.y + rect.y ) / scale ) + 'px';[m
[32m+[m				[32mdocument.body.style.width = ( scale * 100 ) + '%';[m
[32m+[m				[32mdocument.body.style.height = ( scale * 100 ) + '%';[m
[32m+[m				[32mdocument.body.style.zoom = scale;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mlevel = scale;[m
[32m+[m
[32m+[m		[32mif( document.documentElement.classList ) {[m
[32m+[m			[32mif( level !== 1 ) {[m
[32m+[m				[32mdocument.documentElement.classList.add( 'zoomed' );[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32mdocument.documentElement.classList.remove( 'zoomed' );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Pan the document when the mosue cursor approaches the edges[m
[32m+[m	[32m * of the window.[m
[32m+[m	[32m */[m
[32m+[m	[32mfunction pan() {[m
[32m+[m		[32mvar range = 0.12,[m
[32m+[m			[32mrangeX = window.innerWidth * range,[m
[32m+[m			[32mrangeY = window.innerHeight * range,[m
[32m+[m			[32mscrollOffset = getScrollOffset();[m
[32m+[m
[32m+[m		[32m// Up[m
[32m+[m		[32mif( mouseY < rangeY ) {[m
[32m+[m			[32mwindow.scroll( scrollOffset.x, scrollOffset.y - ( 1 - ( mouseY / rangeY ) ) * ( 14 / level ) );[m
[32m+[m		[32m}[m
[32m+[m		[32m// Down[m
[32m+[m		[32melse if( mouseY > window.innerHeight - rangeY ) {[m
[32m+[m			[32mwindow.scroll( scrollOffset.x, scrollOffset.y + ( 1 - ( window.innerHeight - mouseY ) / rangeY ) * ( 14 / level ) );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Left[m
[32m+[m		[32mif( mouseX < rangeX ) {[m
[32m+[m			[32mwindow.scroll( scrollOffset.x - ( 1 - ( mouseX / rangeX ) ) * ( 14 / level ), scrollOffset.y );[m
[32m+[m		[32m}[m
[32m+[m		[32m// Right[m
[32m+[m		[32melse if( mouseX > window.innerWidth - rangeX ) {[m
[32m+[m			[32mwindow.scroll( scrollOffset.x + ( 1 - ( window.innerWidth - mouseX ) / rangeX ) * ( 14 / level ), scrollOffset.y );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfunction getScrollOffset() {[m
[32m+[m		[32mreturn {[m
[32m+[m			[32mx: window.scrollX !== undefined ? window.scrollX : window.pageXOffset,[m
[32m+[m			[32my: window.scrollY !== undefined ? window.scrollY : window.pageYOffset[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn {[m
[32m+[m		[32m/**[m
[32m+[m		[32m * Zooms in on either a rectangle or HTML element.[m
[32m+[m		[32m *[m
[32m+[m		[32m * @param {Object} options[m
[32m+[m		[32m *   - element: HTML element to zoom in on[m
[32m+[m		[32m *   OR[m
[32m+[m		[32m *   - x/y: coordinates in non-transformed space to zoom in on[m
[32m+[m		[32m *   - width/height: the portion of the screen to zoom in on[m
[32m+[m		[32m *   - scale: can be used instead of width/height to explicitly set scale[m
[32m+[m		[32m */[m
[32m+[m		[32mto: function( options ) {[m
[32m+[m
[32m+[m			[32m// Due to an implementation limitation we can't zoom in[m
[32m+[m			[32m// to another element without zooming out first[m
[32m+[m			[32mif( level !== 1 ) {[m
[32m+[m				[32mzoom.out();[m
[32m+[m			[32m}[m
[32m+[m			[32melse {[m
[32m+[m				[32moptions.x = options.x || 0;[m
[32m+[m				[32moptions.y = options.y || 0;[m
[32m+[m
[32m+[m				[32m// If an element is set, that takes precedence[m
[32m+[m				[32mif( !!options.element ) {[m
[32m+[m					[32m// Space around the zoomed in element to leave on screen[m
[32m+[m					[32mvar padding = 20;[m
[32m+[m					[32mvar bounds = options.element.getBoundingClientRect();[m
[32m+[m
[32m+[m					[32moptions.x = bounds.left - padding;[m
[32m+[m					[32moptions.y = bounds.top - padding;[m
[32m+[m					[32moptions.width = bounds.width + ( padding * 2 );[m
[32m+[m					[32moptions.height = bounds.height + ( padding * 2 );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// If width/height values are set, calculate scale from those values[m
[32m+[m				[32mif( options.width !== undefined && options.height !== undefined ) {[m
[32m+[m					[32moptions.scale = Math.max( Math.min( window.innerWidth / options.width, window.innerHeight / options.height ), 1 );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif( options.scale > 1 ) {[m
[32m+[m					[32moptions.x *= options.scale;[m
[32m+[m					[32moptions.y *= options.scale;[m
[32m+[m
[32m+[m					[32mmagnify( options, options.scale );[m
[32m+[m
[32m+[m					[32mif( options.pan !== false ) {[m
[32m+[m
[32m+[m						[32m// Wait with engaging panning as it may conflict with the[m
[32m+[m						[32m// zoom transition[m
[32m+[m						[32mpanEngageTimeout = setTimeout( function() {[m
[32m+[m							[32mpanUpdateInterval = setInterval( pan, 1000 / 60 );[m
[32m+[m						[32m}, 800 );[m
[32m+[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m/**[m
[32m+[m		[32m * Resets the document zoom state to its default.[m
[32m+[m		[32m */[m
[32m+[m		[32mout: function() {[m
[32m+[m			[32mclearTimeout( panEngageTimeout );[m
[32m+[m			[32mclearInterval( panUpdateInterval );[m
[32m+[m
[32m+[m			[32mmagnify( { x: 0, y: 0 }, 1 );[m
[32m+[m
[32m+[m			[32mlevel = 1;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Alias[m
[32m+[m		[32mmagnify: function( options ) { this.to( options ) },[m
[32m+[m		[32mreset: function() { this.out() },[m
[32m+[m
[32m+[m		[32mzoomLevel: function() {[m
[32m+[m			[32mreturn level;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m})();[m
[32m+[m
[32m+[m
[32m+[m
