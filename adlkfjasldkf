[32m+[m[32m/**[m
[32m+[m[32m * Solarized Light theme for reveal.js.[m
[32m+[m[32m * Author: Achim Staebler[m
[32m+[m[32m */[m
[32m+[m[32m@import url(../../lib/font/league-gothic/league-gothic.css);[m
[32m+[m[32m@import url(https://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic);[m
[32m+[m[32m/**[m
[32m+[m[32m * Solarized colors by Ethan Schoonover[m
[32m+[m[32m */[m
[32m+[m[32mhtml * {[m
[32m+[m[32m  color-profile: sRGB;[m
[32m+[m[32m  rendering-intent: auto; }[m
