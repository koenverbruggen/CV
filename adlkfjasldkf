[32m+[m
[32m+[m[32m(function () {[m
[32m+[m[32m  var body        = document.body,[m
[32m+[m[32m      controller  = new Leap.Controller({ enableGestures: true }),[m
[32m+[m[32m      lastGesture = 0,[m
[32m+[m[32m      leapConfig  = Reveal.getConfig().leap,[m
[32m+[m[32m      pointer     = document.createElement( 'div' ),[m
[32m+[m[32m      config      = {[m
[32m+[m[32m        autoCenter       : true,      // Center pointer around detected position.[m
[32m+[m[32m        gestureDelay     : 500,       // How long to delay between gestures.[m
[32m+[m[32m        naturalSwipe     : true,      // Swipe as if it were a touch screen.[m
[32m+[m[32m        pointerColor     : '#00aaff', // Default color of the pointer.[m
[32m+[m[32m        pointerOpacity   : 0.7,       // Default opacity of the pointer.[m
[32m+[m[32m        pointerSize      : 15,        // Default minimum height/width of the pointer.[m
[32m+[m[32m        pointerTolerance : 120        // Bigger = slower pointer.[m
[32m+[m[32m      },[m
[32m+[m[32m      entered, enteredPosition, now, size, tipPosition; // Other vars we need later, but don't need to redeclare.[m
[32m+[m
[32m+[m[32m      // Merge user defined settings with defaults[m
[32m+[m[32m      if( leapConfig ) {[m
[32m+[m[32m        for( key in leapConfig ) {[m
[32m+[m[32m          config[key] = leapConfig[key];[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      pointer.id = 'leap';[m
[32m+[m
[32m+[m[32m      pointer.style.position        = 'absolute';[m
[32m+[m[32m      pointer.style.visibility      = 'hidden';[m
[32m+[m[32m      pointer.style.zIndex          = 50;[m
[32m+[m[32m      pointer.style.opacity         = config.pointerOpacity;[m
[32m+[m[32m      pointer.style.backgroundColor = config.pointerColor;[m
[32m+[m
[32m+[m[32m      body.appendChild( pointer );[m
[32m+[m
[32m+[m[32m  // Leap's loop[m
[32m+[m[32m  controller.on( 'frame', function ( frame ) {[m
[32m+[m[32m    // Timing code to rate limit gesture execution[m
[32m+[m[32m    now = new Date().getTime();[m
[32m+[m
[32m+[m[32m    // Pointer: 1 to 2 fingers. Strictly one finger works but may cause innaccuracies.[m
[32m+[m[32m    // The innaccuracies were observed on a development model and may not be an issue with consumer models.[m
[32m+[m[32m    if( frame.fingers.length > 0 && frame.fingers.length < 3 ) {[m
[32m+[m[32m      // Invert direction and multiply by 3 for greater effect.[m
[32m+[m[32m      size = -3 * frame.fingers[0].tipPosition[2];[m
[32m+[m
[32m+[m[32m      if( size < config.pointerSize ) {[m
[32m+[m[32m        size = config.pointerSize;[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      pointer.style.width        = size     + 'px';[m
[32m+[m[32m      pointer.style.height       = size     + 'px';[m
[32m+[m[32m      pointer.style.borderRadius = size - 5 + 'px';[m
[32m+[m[32m      pointer.style.visibility   = 'visible';[m
[32m+[m
[32m+[m[32m      if( config.autoCenter ) {[m
[32m+[m[32m        tipPosition = frame.fingers[0].tipPosition;[m
[32m+[m
[32m+[m[32m        // Check whether the finger has entered the z range of the Leap Motion. Used for the autoCenter option.[m
[32m+[m[32m        if( !entered ) {[m
[32m+[m[32m          entered         = true;[m
[32m+[m[32m          enteredPosition = frame.fingers[0].tipPosition;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        pointer.style.top =[m
[32m+[m[32m          (-1 * (( tipPosition[1] - enteredPosition[1] ) * body.offsetHeight / config.pointerTolerance )) +[m
[32m+[m[32m            ( body.offsetHeight / 2 ) + 'px';[m
[32m+[m
[32m+[m[32m        pointer.style.left =[m
[32m+[m[32m          (( tipPosition[0] - enteredPosition[0] ) * body.offsetWidth / config.pointerTolerance ) +[m
[32m+[m[32m            ( body.offsetWidth / 2 ) + 'px';[m
[32m+[m[32m      }[m
[32m+[m[32m      else {[m
[32m+[m[32m        pointer.style.top  = ( 1 - (( tipPosition[1] - 50) / config.pointerTolerance )) *[m
[32m+[m[32m          body.offsetHeight + 'px';[m
[32m+[m
[32m+[m[32m        pointer.style.left = ( tipPosition[0] * body.offsetWidth / config.pointerTolerance ) +[m
[32m+[m[32m          ( body.offsetWidth / 2 ) + 'px';[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m    else {[m
[32m+[m[32m      // Hide pointer on exit[m
[32m+[m[32m      entered                  = false;[m
[32m+[m[32m      pointer.style.visibility = 'hidden';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Gestures[m
[32m+[m[32m    if( frame.gestures.length > 0 && (now - lastGesture) > config.gestureDelay ) {[m
[32m+[m[32m      var gesture = frame.gestures[0];[m
[32m+[m
[32m+[m[32m      // One hand gestures[m
[32m+[m[32m      if( frame.hands.length === 1 ) {[m
[32m+[m[32m        // Swipe gestures. 3+ fingers.[m
[32m+[m[32m        if( frame.fingers.length > 2 && gesture.type === 'swipe' ) {[m
[32m+[m[32m          // Define here since some gestures will throw undefined for these.[m
[32m+[m[32m          var x = gesture.direction[0],[m
[32m+[m[32m              y = gesture.direction[1];[m
[32m+[m
[32m+[m[32m          // Left/right swipe gestures[m
[32m+[m[32m          if( Math.abs( x ) > Math.abs( y )) {[m
[32m+[m[32m            if( x > 0 ) {[m
[32m+[m[32m              config.naturalSwipe ? Reveal.left() : Reveal.right();[m
[32m+[m[32m            }[m
[32m+[m[32m            else {[m
[32m+[m[32m              config.naturalSwipe ? Reveal.right() : Reveal.left();[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m[32m          // Up/down swipe gestures[m
[32m+[m[32m          else {[m
[32m+[m[32m            if( y > 0 ) {[m
[32m+[m[32m              config.naturalSwipe ? Reveal.down() : Reveal.up();[m
[32m+[m[32m            }[m
[32m+[m[32m            else {[m
[32m+[m[32m              config.naturalSwipe ? Reveal.up() : Reveal.down();[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          lastGesture = now;[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m      // Two hand gestures[m
[32m+[m[32m      else if( frame.hands.length === 2 ) {[m
[32m+[m[32m        // Upward two hand swipe gesture[m
[32m+[m[32m        if( gesture.direction[1] > 0 && gesture.type === 'swipe' ) {[m
[32m+[m[32m          Reveal.toggleOverview();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        lastGesture = now;[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  });[m
[32m+[m
[32m+[m[32m  controller.connect();[m
[32m+[m[32m})();[m
[1mdiff --git a/Presentation/reveal.js/plugin/markdown/example.html b/Presentation/reveal.js/plugin/markdown/example.html[m
