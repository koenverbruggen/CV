[32m+[m[32mReveal.addEventListener( 'ready', function() {[m
[32m+[m
[32m+[m	[32mQUnit.module( 'Markdown' );[m
[32m+[m
[32m+[m	[32mtest( 'Vertical separator', function() {[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides>section>section' ).length, 6, 'found six vertical slides' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Id on slide', function() {[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides>section>section#slide2' ).length, 1, 'found one slide with id slide2' );[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides>section>section a[href="#/slide2"]' ).length, 1, 'found one slide with a link to slide2' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'data-background attributes', function() {[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides>section>section[data-background="#A0C66B"]' ).length, 1, 'found one vertical slide with data-background="#A0C66B"' );[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides>section>section[data-background="#ff0000"]' ).length, 1, 'found one vertical slide with data-background="#ff0000"' );[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides>section[data-background="#C6916B"]' ).length, 1, 'found one slide with data-background="#C6916B"' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'data-transition attributes', function() {[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides>section>section[data-transition="zoom"]' ).length, 1, 'found one vertical slide with data-transition="zoom"' );[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides>section>section[data-transition="fade"]' ).length, 1, 'found one vertical slide with data-transition="fade"' );[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides section [data-transition="zoom"]' ).length, 1, 'found one slide with data-transition="zoom"' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'data-background attributes with default separator', function() {[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides>section>section[data-background="#A7C66B"]' ).length, 1, 'found one vertical slide with data-background="#A0C66B"' );[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides>section>section[data-background="#f70000"]' ).length, 1, 'found one vertical slide with data-background="#ff0000"' );[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides>section[data-background="#C7916B"]' ).length, 1, 'found one slide with data-background="#C6916B"' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'data-transition attributes with default separator', function() {[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides>section>section[data-transition="concave"]' ).length, 1, 'found one vertical slide with data-transition="zoom"' );[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides>section>section[data-transition="page"]' ).length, 1, 'found one vertical slide with data-transition="fade"' );[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides section [data-transition="concave"]' ).length, 1, 'found one slide with data-transition="zoom"' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'data-transition attributes with inline content', function() {[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides>section[data-background="#ff0000"]' ).length, 3, 'found three horizontal slides with data-background="#ff0000"' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m[32m} );[m
