[32m+[m[32m// Base settings for all themes that can optionally be[m
[32m+[m[32m// overridden by the super-theme[m
[32m+[m
[32m+[m[32m// Background of the presentation[m
[32m+[m[32m$backgroundColor: #2b2b2b;[m
[32m+[m
[32m+[m[32m// Primary/body text[m
[32m+[m[32m$mainFont: 'Lato', sans-serif;[m
[32m+[m[32m$mainFontSize: 36px;[m
[32m+[m[32m$mainColor: #eee;[m
[32m+[m
[32m+[m[32m// Headings[m
[32m+[m[32m$headingMargin: 0 0 20px 0;[m
[32m+[m[32m$headingFont: 'League Gothic', Impact, sans-serif;[m
[32m+[m[32m$headingColor: #eee;[m
[32m+[m[32m$headingLineHeight: 0.9em;[m
[32m+[m[32m$headingLetterSpacing: 0.02em;[m
[32m+[m[32m$headingTextTransform: uppercase;[m
[32m+[m[32m$headingTextShadow: 0px 0px 6px rgba(0,0,0,0.2);[m
[32m+[m[32m$heading1TextShadow: $headingTextShadow;[m
[32m+[m
[32m+[m[32m// Links and actions[m
[32m+[m[32m$linkColor: #13DAEC;[m
[32m+[m[32m$linkColorHover: lighten( $linkColor, 20% );[m
[32m+[m
[32m+[m[32m// Text selection[m
[32m+[m[32m$selectionBackgroundColor: #FF5E99;[m
[32m+[m[32m$selectionColor: #fff;[m
[32m+[m
[32m+[m[32m// Generates the presentation background, can be overridden[m
[32m+[m[32m// to return a background image or gradient[m
[32m+[m[32m@mixin bodyBackground() {[m
[32m+[m	[32mbackground: $backgroundColor;[m
[32m+[m[32m}[m
