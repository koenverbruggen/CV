[32m+[m[32m/**[m
[32m+[m[32m * QUnit v1.12.0 - A JavaScript Unit Testing Framework[m
[32m+[m[32m *[m
[32m+[m[32m * http://qunitjs.com[m
[32m+[m[32m *[m
[32m+[m[32m * Copyright 2013 jQuery Foundation and other contributors[m
[32m+[m[32m * Released under the MIT license.[m
[32m+[m[32m * https://jquery.org/license/[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m(function( window ) {[m
[32m+[m
[32m+[m[32mvar QUnit,[m
[32m+[m	[32massert,[m
[32m+[m	[32mconfig,[m
[32m+[m	[32monErrorFnPrev,[m
[32m+[m	[32mtestId = 0,[m
[32m+[m	[32mfileName = (sourceFromStacktrace( 0 ) || "" ).replace(/(:\d+)+\)?/, "").replace(/.+\//, ""),[m
[32m+[m	[32mtoString = Object.prototype.toString,[m
[32m+[m	[32mhasOwn = Object.prototype.hasOwnProperty,[m
[32m+[m	[32m// Keep a local reference to Date (GH-283)[m
[32m+[m	[32mDate = window.Date,[m
[32m+[m	[32msetTimeout = window.setTimeout,[m
[32m+[m	[32mdefined = {[m
[32m+[m		[32msetTimeout: typeof window.setTimeout !== "undefined",[m
[32m+[m		[32msessionStorage: (function() {[m
[32m+[m			[32mvar x = "qunit-test-string";[m
[32m+[m			[32mtry {[m
[32m+[m				[32msessionStorage.setItem( x, x );[m
[32m+[m				[32msessionStorage.removeItem( x );[m
[32m+[m				[32mreturn true;[m
[32m+[m			[32m} catch( e ) {[m
[32m+[m				[32mreturn false;[m
[32m+[m			[32m}[m
[32m+[m		[32m}())[m
[32m+[m	[32m},[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Provides a normalized error string, correcting an issue[m
[32m+[m	[32m * with IE 7 (and prior) where Error.prototype.toString is[m
[32m+[m	[32m * not properly implemented[m
[32m+[m	[32m *[m
[32m+[m	[32m * Based on http://es5.github.com/#x15.11.4.4[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {String|Error} error[m
[32m+[m	[32m * @return {String} error message[m
[32m+[m	[32m */[m
[32m+[m	[32merrorString = function( error ) {[m
[32m+[m		[32mvar name, message,[m
[32m+[m			[32merrorString = error.toString();[m
[32m+[m		[32mif ( errorString.substring( 0, 7 ) === "[object" ) {[m
[32m+[m			[32mname = error.name ? error.name.toString() : "Error";[m
[32m+[m			[32mmessage = error.message ? error.message.toString() : "";[m
[32m+[m			[32mif ( name && message ) {[m
[32m+[m				[32mreturn name + ": " + message;[m
[32m+[m			[32m} else if ( name ) {[m
[32m+[m				[32mreturn name;[m
[32m+[m			[32m} else if ( message ) {[m
[32m+[m				[32mreturn message;[m
[32m+[m			[32m} else {[m
[32m+[m				[32mreturn "Error";[m
[32m+[m			[32m}[m
[32m+[m		[32m} else {[m
[32m+[m			[32mreturn errorString;[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Makes a clone of an object using only Array or Object as base,[m
[32m+[m	[32m * and copies over the own enumerable properties.[m
[32m+[m	[32m *[m
[32m+[m	[32m * @param {Object} obj[m
[32m+[m	[32m * @return {Object} New object with only the own properties (recursively).[m
[32m+[m	[32m */[m
[32m+[m	[32mobjectValues = function( obj ) {[m
[32m+[m		[32m// Grunt 0.3.x uses an older version of jshint that still has jshint/jshint#392.[m
[32m+[m		[32m/*jshint newcap: false */[m
[32m+[m		[32mvar key, val,[m
[32m+[m			[32mvals = QUnit.is( "array", obj ) ? [] : {};[m
[32m+[m		[32mfor ( key in obj ) {[m
[32m+[m			[32mif ( hasOwn.call( obj, key ) ) {[m
[32m+[m				[32mval = obj[key];[m
[32m+[m				[32mvals[key] = val === Object(val) ? objectValues(val) : val;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn vals;[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m[32mfunction Test( settings ) {[m
[32m+[m	[32mextend( this, settings );[m
[32m+[m	[32mthis.assertions = [];[m
[32m+[m	[32mthis.testNumber = ++Test.count;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mTest.count = 0;[m
[32m+[m
[32m+[m[32mTest.prototype = {[m
[32m+[m	[32minit: function() {[m
[32m+[m		[32mvar a, b, li,[m
[32m+[m			[32mtests = id( "qunit-tests" );[m
[32m+[m
[32m+[m		[32mif ( tests ) {[m
[32m+[m			[32mb = document.createElement( "strong" );[m
[32m+[m			[32mb.innerHTML = this.nameHtml;[m
[32m+[m
[32m+[m			[32m// `a` initialized at top of scope[m
[32m+[m			[32ma = document.createElement( "a" );[m
[32m+[m			[32ma.innerHTML = "Rerun";[m
[32m+[m			[32ma.href = QUnit.url({ testNumber: this.testNumber });[m
[32m+[m
[32m+[m			[32mli = document.createElement( "li" );[m
[32m+[m			[32mli.appendChild( b );[m
[32m+[m			[32mli.appendChild( a );[m
[32m+[m			[32mli.className = "running";[m
[32m+[m			[32mli.id = this.id = "qunit-test-output" + testId++;[m
[32m+[m
[32m+[m			[32mtests.appendChild( li );[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m	[32msetup: function() {[m
[32m+[m		[32mif ([m
[32m+[m			[32m// Emit moduleStart when we're switching from one module to another[m
[32m+[m			[32mthis.module !== config.previousModule ||[m
[32m+[m				[32m// They could be equal (both undefined) but if the previousModule property doesn't[m
[32m+[m				[32m// yet exist it means this is the first test in a suite that isn't wrapped in a[m
[32m+[m				[32m// module, in which case we'll just emit a moduleStart event for 'undefined'.[m
[32m+[m				[32m// Without this, reporters can get testStart before moduleStart  which is a problem.[m
[32m+[m				[32m!hasOwn.call( config, "previousModule" )[m
[32m+[m		[32m) {[m
[32m+[m			[32mif ( hasOwn.call( config, "previousModule" ) ) {[m
[32m+[m				[32mrunLoggingCallbacks( "moduleDone", QUnit, {[m
[32m+[m					[32mname: config.previousModule,[m
[32m+[m					[32mfailed: config.moduleStats.bad,[m
[32m+[m					[32mpassed: config.moduleStats.all - config.moduleStats.bad,[m
[32m+[m					[32mtotal: config.moduleStats.all[m
[32m+[m				[32m});[m
[32m+[m			[32m}[m
[32m+[m			[32mconfig.previousModule = this.module;[m
[32m+[m			[32mconfig.moduleStats = { all: 0, bad: 0 };[m
[32m+[m			[32mrunLoggingCallbacks( "moduleStart", QUnit, {[m
[32m+[m				[32mname: this.module[m
[32m+[m			[32m});[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mconfig.current = this;[m
[32m+[m
[32m+[m		[32mthis.testEnvironment = extend({[m
[32m+[m			[32msetup: function() {},[m
[32m+[m			[32mteardown: function() {}[m
[32m+[m		[32m}, this.moduleTestEnvironment );[m
[32m+[m
[32m+[m		[32mthis.started = +new Date();[m
[32m+[m		[32mrunLoggingCallbacks( "testStart", QUnit, {[m
[32m+[m			[32mname: this.testName,[m
[32m+[m			[32mmodule: this.module[m
[32m+[m		[32m});[m
[32m+[m
[32m+[m		[32m/*jshint camelcase:false */[m
[32m+[m
[32m+[m
[32m+[m		[32m/**[m
[32m+[m		[32m * Expose the current test environment.[m
[32m+[m		[32m *[m
[32m+[m		[32m * @deprecated since 1.12.0: Use QUnit.config.current.testEnvironment instead.[m
[32m+[m		[32m */[m
[32m+[m		[32mQUnit.current_testEnvironment = this.testEnvironment;[m
[32m+[m
[32m+[m		[32m/*jshint camelcase:true */[m
[32m+[m
[32m+[m		[32mif ( !config.pollution ) {[m
[32m+[m			[32msaveGlobal();[m
[32m+[m		[32m}[m
[32m+[m		[32mif ( config.notrycatch ) {[m
[32m+[m			[32mthis.testEnvironment.setup.call( this.testEnvironment, QUnit.assert );[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m		[32mtry {[m
[32m+[m			[32mthis.testEnvironment.setup.call( this.testEnvironment, QUnit.assert );[m
[32m+[m		[32m} catch( e ) {[m
[32m+[m			[32mQUnit.pushFailure( "Setup failed on " + this.testName + ": " + ( e.message || e ), extractStacktrace( e, 1 ) );[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m	[32mrun: function() {[m
[32m+[m		[32mconfig.current = this;[m
[32m+[m
[32m+[m		[32mvar running = id( "qunit-testresult" );[m
[32m+[m
[32m+[m		[32mif ( running ) {[m
[32m+[m			[32mrunning.innerHTML = "Running: <br/>" + this.nameHtml;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( this.async ) {[m
[32m+[m			[32mQUnit.stop();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mthis.callbackStarted = +new Date();[m
[32m+[m
[32m+[m		[32mif ( config.notrycatch ) {[m
[32m+[m			[32mthis.callback.call( this.testEnvironment, QUnit.assert );[m
[32m+[m			[32mthis.callbackRuntime = +new Date() - this.callbackStarted;[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mtry {[m
[32m+[m			[32mthis.callback.call( this.testEnvironment, QUnit.assert );[m
[32m+[m			[32mthis.callbackRuntime = +new Date() - this.callbackStarted;[m
[32m+[m		[32m} catch( e ) {[m
[32m+[m			[32mthis.callbackRuntime = +new Date() - this.callbackStarted;[m
[32m+[m
[32m+[m			[32mQUnit.pushFailure( "Died on test #" + (this.assertions.length + 1) + " " + this.stack + ": " + ( e.message || e ), extractStacktrace( e, 0 ) );[m
[32m+[m			[32m// else next test will carry the responsibility[m
[32m+[m			[32msaveGlobal();[m
[32m+[m
[32m+[m			[32m// Restart the tests if they're blocking[m
[32m+[m			[32mif ( config.blocking ) {[m
[32m+[m				[32mQUnit.start();[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m	[32mteardown: function() {[m
[32m+[m		[32mconfig.current = this;[m
[32m+[m		[32mif ( config.notrycatch ) {[m
[32m+[m			[32mif ( typeof this.callbackRuntime === "undefined" ) {[m
[32m+[m				[32mthis.callbackRuntime = +new Date() - this.callbackStarted;[m
[32m+[m			[32m}[m
[32m+[m			[32mthis.testEnvironment.teardown.call( this.testEnvironment, QUnit.assert );[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m} else {[m
[32m+[m			[32mtry {[m
[32m+[m				[32mthis.testEnvironment.teardown.call( this.testEnvironment, QUnit.assert );[m
[32m+[m			[32m} catch( e ) {[m
[32m+[m				[32mQUnit.pushFailure( "Teardown failed on " + this.testName + ": " + ( e.message || e ), extractStacktrace( e, 1 ) );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32mcheckPollution();[m
[32m+[m	[32m},[m
[32m+[m	[32mfinish: function() {[m
[32m+[m		[32mconfig.current = this;[m
[32m+[m		[32mif ( config.requireExpects && this.expected === null ) {[m
[32m+[m			[32mQUnit.pushFailure( "Expected number of assertions to be defined, but expect() was not called.", this.stack );[m
[32m+[m		[32m} else if ( this.expected !== null && this.expected !== this.assertions.length ) {[m
[32m+[m			[32mQUnit.pushFailure( "Expected " + this.expected + " assertions, but " + this.assertions.length + " were run", this.stack );[m
[32m+[m		[32m} else if ( this.expected === null && !this.assertions.length ) {[m
[32m+[m			[32mQUnit.pushFailure( "Expected at least one assertion, but none were run - call expect(0) to accept zero assertions.", this.stack );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar i, assertion, a, b, time, li, ol,[m
[32m+[m			[32mtest = this,[m
[32m+[m			[32mgood = 0,[m
[32m+[m			[32mbad = 0,[m
[32m+[m			[32mtests = id( "qunit-tests" );[m
[32m+[m
[32m+[m		[32mthis.runtime = +new Date() - this.started;[m
[32m+[m		[32mconfig.stats.all += this.assertions.length;[m
[32m+[m		[32mconfig.moduleStats.all += this.assertions.length;[m
[32m+[m
[32m+[m		[32mif ( tests ) {[m
[32m+[m			[32mol = document.createElement( "ol" );[m
[32m+[m			[32mol.className = "qunit-assert-list";[m
[32m+[m
[32m+[m			[32mfor ( i = 0; i < this.assertions.length; i++ ) {[m
[32m+[m				[32massertion = this.assertions[i];[m
[32m+[m
[32m+[m				[32mli = document.createElement( "li" );[m
[32m+[m				[32mli.className = assertion.result ? "pass" : "fail";[m
[32m+[m				[32mli.innerHTML = assertion.message || ( assertion.result ? "okay" : "failed" );[m
[32m+[m				[32mol.appendChild( li );[m
[32m+[m
[32m+[m				[32mif ( assertion.result ) {[m
[32m+[m					[32mgood++;[m
[32m+[m				[32m} else {[m
[32m+[m					[32mbad++;[m
[32m+[m					[32mconfig.stats.bad++;[m
[32m+[m					[32mconfig.moduleStats.bad++;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// store result when possible[m
[32m+[m			[32mif ( QUnit.config.reorder && defined.sessionStorage ) {[m
[32m+[m				[32mif ( bad ) {[m
[32m+[m					[32msessionStorage.setItem( "qunit-test-" + this.module + "-" + this.testName, bad );[m
[32m+[m				[32m} else {[m
[32m+[m					[32msessionStorage.removeItem( "qunit-test-" + this.module + "-" + this.testName );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif ( bad === 0 ) {[m
[32m+[m				[32maddClass( ol, "qunit-collapsed" );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// `b` initialized at top of scope[m
[32m+[m			[32mb = document.createElement( "strong" );[m
[32m+[m			[32mb.innerHTML = this.nameHtml + " <b class='counts'>(<b class='failed'>" + bad + "</b>, <b class='passed'>" + good + "</b>, " + this.assertions.length + ")</b>";[m
[32m+[m
[32m+[m			[32maddEvent(b, "click", function() {[m
[32m+[m				[32mvar next = b.parentNode.lastChild,[m
[32m+[m					[32mcollapsed = hasClass( next, "qunit-collapsed" );[m
[32m+[m				[32m( collapsed ? removeClass : addClass )( next, "qunit-collapsed" );[m
[32m+[m			[32m});[m
[32m+[m
[32m+[m			[32maddEvent(b, "dblclick", function( e ) {[m
[32m+[m				[32mvar target = e && e.target ? e.target : window.event.srcElement;[m
[32m+[m				[32mif ( target.nodeName.toLowerCase() === "span" || target.nodeName.toLowerCase() === "b" ) {[m
[32m+[m					[32mtarget = target.parentNode;[m
[32m+[m				[32m}[m
[32m+[m				[32mif ( window.location && target.nodeName.toLowerCase() === "strong" ) {[m
[32m+[m					[32mwindow.location = QUnit.url({ testNumber: test.testNumber });[m
[32m+[m				[32m}[m
[32m+[m			[32m});[m
[32m+[m
[32m+[m			[32m// `time` initialized at top of scope[m
[32m+[m			[32mtime = document.createElement( "span" );[m
[32m+[m			[32mtime.className = "runtime";[m
[32m+[m			[32mtime.innerHTML = this.runtime + " ms";[m
[32m+[m
[32m+[m			[32m// `li` initialized at top of scope[m
[32m+[m			[32mli = id( this.id );[m
[32m+[m			[32mli.className = bad ? "fail" : "pass";[m
[32m+[m			[32mli.removeChild( li.firstChild );[m
[32m+[m			[32ma = li.firstChild;[m
[32m+[m			[32mli.appendChild( b );[m
[32m+[m			[32mli.appendChild( a );[m
[32m+[m			[32mli.appendChild( time );[m
[32m+[m			[32mli.appendChild( ol );[m
[32m+[m
[32m+[m		[32m} else {[m
[32m+[m			[32mfor ( i = 0; i < this.assertions.length; i++ ) {[m
[32m+[m				[32mif ( !this.assertions[i].result ) {[m
[32m+[m					[32mbad++;[m
[32m+[m					[32mconfig.stats.bad++;[m
[32m+[m					[32mconfig.moduleStats.bad++;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mrunLoggingCallbacks( "testDone", QUnit, {[m
[32m+[m			[32mname: this.testName,[m
[32m+[m			[32mmodule: this.module,[m
[32m+[m			[32mfailed: bad,[m
[32m+[m			[32mpassed: this.assertions.length - bad,[m
[32m+[m			[32mtotal: this.assertions.length,[m
[32m+[m			[32mduration: this.runtime[m
[32m+[m		[32m});[m
[32m+[m
[32m+[m		[32mQUnit.reset();[m
[32m+[m
[32m+[m		[32mconfig.current = undefined;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mqueue: function() {[m
[32m+[m		[32mvar bad,[m
[32m+[m			[32mtest = this;[m
[32m+[m
[32m+[m		[32msynchronize(function() {[m
[32m+[m			[32mtest.init();[m
[32m+[m		[32m});[m
[32m+[m		[32mfunction run() {[m
[32m+[m			[32m// each of these can by async[m
[32m+[m			[32msynchronize(function() {[m
[32m+[m				[32mtest.setup();[m
[32m+[m			[32m});[m
[32m+[m			[32msynchronize(function() {[m
[32m+[m				[32mtest.run();[m
[32m+[m			[32m});[m
[32m+[m			[32msynchronize(function() {[m
[32m+[m				[32mtest.teardown();[m
[32m+[m			[32m});[m
[32m+[m			[32msynchronize(function() {[m
[32m+[m				[32mtest.finish();[m
[32m+[m			[32m});[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// `bad` initialized at top of scope[m
[32m+[m		[32m// defer when previous test run passed, if storage is available[m
[32m+[m		[32mbad = QUnit.config.reorder && defined.sessionStorage &&[m
[32m+[m						[32m+sessionStorage.getItem( "qunit-test-" + this.module + "-" + this.testName );[m
[32m+[m
[32m+[m		[32mif ( bad ) {[m
[32m+[m			[32mrun();[m
[32m+[m		[32m} else {[m
[32m+[m			[32msynchronize( run, true );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Root QUnit object.[m
[32m+[m[32m// `QUnit` initialized at top of scope[m
[32m+[m[32mQUnit = {[m
[32m+[m
[32m+[m	[32m// call on start of module test to prepend name to all tests[m
[32m+[m	[32mmodule: function( name, testEnvironment ) {[m
[32m+[m		[32mconfig.currentModule = name;[m
[32m+[m		[32mconfig.currentModuleTestEnvironment = testEnvironment;[m
[32m+[m		[32mconfig.modules[name] = true;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32masyncTest: function( testName, expected, callback ) {[m
[32m+[m		[32mif ( arguments.length === 2 ) {[m
[32m+[m			[32mcallback = expected;[m
[32m+[m			[32mexpected = null;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mQUnit.test( testName, expected, callback, true );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mtest: function( testName, expected, callback, async ) {[m
[32m+[m		[32mvar test,[m
[32m+[m			[32mnameHtml = "<span class='test-name'>" + escapeText( testName ) + "</span>";[m
[32m+[m
[32m+[m		[32mif ( arguments.length === 2 ) {[m
[32m+[m			[32mcallback = expected;[m
[32m+[m			[32mexpected = null;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( config.currentModule ) {[m
[32m+[m			[32mnameHtml = "<span class='module-name'>" + escapeText( config.currentModule ) + "</span>: " + nameHtml;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mtest = new Test({[m
[32m+[m			[32mnameHtml: nameHtml,[m
[32m+[m			[32mtestName: testName,[m
[32m+[m			[32mexpected: expected,[m
[32m+[m			[32masync: async,[m
[32m+[m			[32mcallback: callback,[m
[32m+[m			[32mmodule: config.currentModule,[m
[32m+[m			[32mmoduleTestEnvironment: config.currentModuleTestEnvironment,[m
[32m+[m			[32mstack: sourceFromStacktrace( 2 )[m
[32m+[m		[32m});[m
[32m+[m
[32m+[m		[32mif ( !validTest( test ) ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mtest.queue();[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Specify the number of expected assertions to guarantee that failed test (no assertions are run at all) don't slip through.[m
[32m+[m	[32mexpect: function( asserts ) {[m
[32m+[m		[32mif (arguments.length === 1) {[m
[32m+[m			[32mconfig.current.expected = asserts;[m
[32m+[m		[32m} else {[m
[32m+[m			[32mreturn config.current.expected;[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mstart: function( count ) {[m
[32m+[m		[32m// QUnit hasn't been initialized yet.[m
[32m+[m		[32m// Note: RequireJS (et al) may delay onLoad[m
[32m+[m		[32mif ( config.semaphore === undefined ) {[m
[32m+[m			[32mQUnit.begin(function() {[m
[32m+[m				[32m// This is triggered at the top of QUnit.load, push start() to the event loop, to allow QUnit.load to finish first[m
[32m+[m				[32msetTimeout(function() {[m
[32m+[m					[32mQUnit.start( count );[m
[32m+[m				[32m});[m
[32m+[m			[32m});[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mconfig.semaphore -= count || 1;[m
[32m+[m		[32m// don't start until equal number of stop-calls[m
[32m+[m		[32mif ( config.semaphore > 0 ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m		[32m// ignore if start is called more often then stop[m
[32m+[m		[32mif ( config.semaphore < 0 ) {[m
[32m+[m			[32mconfig.semaphore = 0;[m
[32m+[m			[32mQUnit.pushFailure( "Called start() while already started (QUnit.config.semaphore was 0 already)", null, sourceFromStacktrace(2) );[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m		[32m// A slight delay, to avoid any current callbacks[m
[32m+[m		[32mif ( defined.setTimeout ) {[m
[32m+[m			[32msetTimeout(function() {[m
[32m+[m				[32mif ( config.semaphore > 0 ) {[m
[32m+[m					[32mreturn;[m
[32m+[m				[32m}[m
[32m+[m				[32mif ( config.timeout ) {[m
[32m+[m					[32mclearTimeout( config.timeout );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mconfig.blocking = false;[m
[32m+[m				[32mprocess( true );[m
[32m+[m			[32m}, 13);[m
[32m+[m		[32m} else {[m
[32m+[m			[32mconfig.blocking = false;[m
[32m+[m			[32mprocess( true );[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mstop: function( count ) {[m
[32m+[m		[32mconfig.semaphore += count || 1;[m
[32m+[m		[32mconfig.blocking = true;[m
[32m+[m
[32m+[m		[32mif ( config.testTimeout && defined.setTimeout ) {[m
[32m+[m			[32mclearTimeout( config.timeout );[m
[32m+[m			[32mconfig.timeout = setTimeout(function() {[m
[32m+[m				[32mQUnit.ok( false, "Test timed out" );[m
[32m+[m				[32mconfig.semaphore = 1;[m
[32m+[m				[32mQUnit.start();[m
[32m+[m			[32m}, config.testTimeout );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// `assert` initialized at top of scope[m
[32m+[m[32m// Assert helpers[m
[32m+[m[32m// All of these must either call QUnit.push() or manually do:[m
[32m+[m[32m// - runLoggingCallbacks( "log", .. );[m
[32m+[m[32m// - config.current.assertions.push({ .. });[m
[32m+[m[32m// We attach it to the QUnit object *after* we expose the public API,[m
[32m+[m[32m// otherwise `assert` will become a global variable in browsers (#341).[m
[32m+[m[32massert = {[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Asserts rough true-ish result.[m
[32m+[m	[32m * @name ok[m
[32m+[m	[32m * @function[m
[32m+[m	[32m * @example ok( "asdfasdf".length > 5, "There must be at least 5 chars" );[m
[32m+[m	[32m */[m
[32m+[m	[32mok: function( result, msg ) {[m
[32m+[m		[32mif ( !config.current ) {[m
[32m+[m			[32mthrow new Error( "ok() assertion outside test context, was " + sourceFromStacktrace(2) );[m
[32m+[m		[32m}[m
[32m+[m		[32mresult = !!result;[m
[32m+[m		[32mmsg = msg || (result ? "okay" : "failed" );[m
[32m+[m
[32m+[m		[32mvar source,[m
[32m+[m			[32mdetails = {[m
[32m+[m				[32mmodule: config.current.module,[m
[32m+[m				[32mname: config.current.testName,[m
[32m+[m				[32mresult: result,[m
[32m+[m				[32mmessage: msg[m
[32m+[m			[32m};[m
[32m+[m
[32m+[m		[32mmsg = "<span class='test-message'>" + escapeText( msg ) + "</span>";[m
[32m+[m
[32m+[m		[32mif ( !result ) {[m
[32m+[m			[32msource = sourceFromStacktrace( 2 );[m
[32m+[m			[32mif ( source ) {[m
[32m+[m				[32mdetails.source = source;[m
[32m+[m				[32mmsg += "<table><tr class='test-source'><th>Source: </th><td><pre>" + escapeText( source ) + "</pre></td></tr></table>";[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32mrunLoggingCallbacks( "log", QUnit, details );[m
[32m+[m		[32mconfig.current.assertions.push({[m
[32m+[m			[32mresult: result,[m
[32m+[m			[32mmessage: msg[m
[32m+[m		[32m});[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * Assert that the first two arguments are equal, with an optional message.[m
[32m+[m	[32m * Prints out both actual and expected values.[m
[32m+[m	[32m * @name equal[m
[32m+[m	[32m * @function[m
[32m+[m	[32m * @example equal( format( "Received {0} bytes.", 2), "Received 2 bytes.", "format() replaces {0} with next argument" );[m
[32m+[m	[32m */[m
[32m+[m	[32mequal: function( actual, expected, message ) {[m
[32m+[m		[32m/*jshint eqeqeq:false */[m
[32m+[m		[32mQUnit.push( expected == actual, actual, expected, message );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * @name notEqual[m
[32m+[m	[32m * @function[m
[32m+[m	[32m */[m
[32m+[m	[32mnotEqual: function( actual, expected, message ) {[m
[32m+[m		[32m/*jshint eqeqeq:false */[m
[32m+[m		[32mQUnit.push( expected != actual, actual, expected, message );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * @name propEqual[m
[32m+[m	[32m * @function[m
[32m+[m	[32m */[m
[32m+[m	[32mpropEqual: function( actual, expected, message ) {[m
[32m+[m		[32mactual = objectValues(actual);[m
[32m+[m		[32mexpected = objectValues(expected);[m
[32m+[m		[32mQUnit.push( QUnit.equiv(actual, expected), actual, expected, message );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * @name notPropEqual[m
[32m+[m	[32m * @function[m
[32m+[m	[32m */[m
[32m+[m	[32mnotPropEqual: function( actual, expected, message ) {[m
[32m+[m		[32mactual = objectValues(actual);[m
[32m+[m		[32mexpected = objectValues(expected);[m
[32m+[m		[32mQUnit.push( !QUnit.equiv(actual, expected), actual, expected, message );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * @name deepEqual[m
[32m+[m	[32m * @function[m
[32m+[m	[32m */[m
[32m+[m	[32mdeepEqual: function( actual, expected, message ) {[m
[32m+[m		[32mQUnit.push( QUnit.equiv(actual, expected), actual, expected, message );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * @name notDeepEqual[m
[32m+[m	[32m * @function[m
[32m+[m	[32m */[m
[32m+[m	[32mnotDeepEqual: function( actual, expected, message ) {[m
[32m+[m		[32mQUnit.push( !QUnit.equiv(actual, expected), actual, expected, message );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * @name strictEqual[m
[32m+[m	[32m * @function[m
[32m+[m	[32m */[m
[32m+[m	[32mstrictEqual: function( actual, expected, message ) {[m
[32m+[m		[32mQUnit.push( expected === actual, actual, expected, message );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m/**[m
[32m+[m	[32m * @name notStrictEqual[m
[32m+[m	[32m * @function[m
[32m+[m	[32m */[m
[32m+[m	[32mnotStrictEqual: function( actual, expected, message ) {[m
[32m+[m		[32mQUnit.push( expected !== actual, actual, expected, message );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m"throws": function( block, expected, message ) {[m
[32m+[m		[32mvar actual,[m
[32m+[m			[32mexpectedOutput = expected,[m
[32m+[m			[32mok = false;[m
[32m+[m
[32m+[m		[32m// 'expected' is optional[m
[32m+[m		[32mif ( typeof expected === "string" ) {[m
[32m+[m			[32mmessage = expected;[m
[32m+[m			[32mexpected = null;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mconfig.current.ignoreGlobalErrors = true;[m
[32m+[m		[32mtry {[m
[32m+[m			[32mblock.call( config.current.testEnvironment );[m
[32m+[m		[32m} catch (e) {[m
[32m+[m			[32mactual = e;[m
[32m+[m		[32m}[m
[32m+[m		[32mconfig.current.ignoreGlobalErrors = false;[m
[32m+[m
[32m+[m		[32mif ( actual ) {[m
[32m+[m			[32m// we don't want to validate thrown error[m
[32m+[m			[32mif ( !expected ) {[m
[32m+[m				[32mok = true;[m
[32m+[m				[32mexpectedOutput = null;[m
[32m+[m			[32m// expected is a regexp[m
[32m+[m			[32m} else if ( QUnit.objectType( expected ) === "regexp" ) {[m
[32m+[m				[32mok = expected.test( errorString( actual ) );[m
[32m+[m			[32m// expected is a constructor[m
[32m+[m			[32m} else if ( actual instanceof expected ) {[m
[32m+[m				[32mok = true;[m
[32m+[m			[32m// expected is a validation function which returns true is validation passed[m
[32m+[m			[32m} else if ( expected.call( {}, actual ) === true ) {[m
[32m+[m				[32mexpectedOutput = null;[m
[32m+[m				[32mok = true;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mQUnit.push( ok, actual, expectedOutput, message );[m
[32m+[m		[32m} else {[m
[32m+[m			[32mQUnit.pushFailure( message, null, "No exception was thrown." );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * @deprecated since 1.8.0[m
[32m+[m[32m * Kept assertion helpers in root for backwards compatibility.[m
[32m+[m[32m */[m
[32m+[m[32mextend( QUnit, assert );[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * @deprecated since 1.9.0[m
[32m+[m[32m * Kept root "raises()" for backwards compatibility.[m
[32m+[m[32m * (Note that we don't introduce assert.raises).[m
[32m+[m[32m */[m
[32m+[m[32mQUnit.raises = assert[ "throws" ];[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * @deprecated since 1.0.0, replaced with error pushes since 1.3.0[m
[32m+[m[32m * Kept to avoid TypeErrors for undefined methods.[m
[32m+[m[32m */[m
[32m+[m[32mQUnit.equals = function() {[m
[32m+[m	[32mQUnit.push( false, false, false, "QUnit.equals has been deprecated since 2009 (e88049a0), use QUnit.equal instead" );[m
[32m+[m[32m};[m
[32m+[m[32mQUnit.same = function() {[m
[32m+[m	[32mQUnit.push( false, false, false, "QUnit.same has been deprecated since 2009 (e88049a0), use QUnit.deepEqual instead" );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// We want access to the constructor's prototype[m
[32m+[m[32m(function() {[m
[32m+[m	[32mfunction F() {}[m
[32m+[m	[32mF.prototype = QUnit;[m
[32m+[m	[32mQUnit = new F();[m
[32m+[m	[32m// Make F QUnit's constructor so that we can add to the prototype later[m
[32m+[m	[32mQUnit.constructor = F;[m
[32m+[m[32m}());[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Config object: Maintain internal state[m
[32m+[m[32m * Later exposed as QUnit.config[m
[32m+[m[32m * `config` initialized at top of scope[m
[32m+[m[32m */[m
[32m+[m[32mconfig = {[m
[32m+[m	[32m// The queue of tests to run[m
[32m+[m	[32mqueue: [],[m
[32m+[m
[32m+[m	[32m// block until document ready[m
[32m+[m	[32mblocking: true,[m
[32m+[m
[32m+[m	[32m// when enabled, show only failing tests[m
[32m+[m	[32m// gets persisted through sessionStorage and can be changed in UI via checkbox[m
[32m+[m	[32mhidepassed: false,[m
[32m+[m
[32m+[m	[32m// by default, run previously failed tests first[m
[32m+[m	[32m// very useful in combination with "Hide passed tests" checked[m
[32m+[m	[32mreorder: true,[m
[32m+[m
[32m+[m	[32m// by default, modify document.title when suite is done[m
[32m+[m	[32maltertitle: true,[m
[32m+[m
[32m+[m	[32m// when enabled, all tests must call expect()[m
[32m+[m	[32mrequireExpects: false,[m
[32m+[m
[32m+[m	[32m// add checkboxes that are persisted in the query-string[m
[32m+[m	[32m// when enabled, the id is set to `true` as a `QUnit.config` property[m
[32m+[m	[32murlConfig: [[m
[32m+[m		[32m{[m
[32m+[m			[32mid: "noglobals",[m
[32m+[m			[32mlabel: "Check for Globals",[m
[32m+[m			[32mtooltip: "Enabling this will test if any test introduces new properties on the `window` object. Stored as query-strings."[m
[32m+[m		[32m},[m
[32m+[m		[32m{[m
[32m+[m			[32mid: "notrycatch",[m
[32m+[m			[32mlabel: "No try-catch",[m
[32m+[m			[32mtooltip: "Enabling this will run tests outside of a try-catch block. Makes debugging exceptions in IE reasonable. Stored as query-strings."[m
[32m+[m		[32m}[m
[32m+[m	[32m],[m
[32m+[m
[32m+[m	[32m// Set of all modules.[m
[32m+[m	[32mmodules: {},[m
[32m+[m
[32m+[m	[32m// logging callback queues[m
[32m+[m	[32mbegin: [],[m
[32m+[m	[32mdone: [],[m
[32m+[m	[32mlog: [],[m
[32m+[m	[32mtestStart: [],[m
[32m+[m	[32mtestDone: [],[m
[32m+[m	[32mmoduleStart: [],[m
[32m+[m	[32mmoduleDone: [][m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Export global variables, unless an 'exports' object exists,[m
[32m+[m[32m// in that case we assume we're in CommonJS (dealt with on the bottom of the script)[m
[32m+[m[32mif ( typeof exports === "undefined" ) {[m
[32m+[m	[32mextend( window, QUnit.constructor.prototype );[m
[32m+[m
[32m+[m	[32m// Expose QUnit object[m
[32m+[m	[32mwindow.QUnit = QUnit;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Initialize more QUnit.config and QUnit.urlParams[m
[32m+[m[32m(function() {[m
[32m+[m	[32mvar i,[m
[32m+[m		[32mlocation = window.location || { search: "", protocol: "file:" },[m
[32m+[m		[32mparams = location.search.slice( 1 ).split( "&" ),[m
[32m+[m		[32mlength = params.length,[m
[32m+[m		[32murlParams = {},[m
[32m+[m		[32mcurrent;[m
[32m+[m
[32m+[m	[32mif ( params[ 0 ] ) {[m
[32m+[m		[32mfor ( i = 0; i < length; i++ ) {[m
[32m+[m			[32mcurrent = params[ i ].split( "=" );[m
[32m+[m			[32mcurrent[ 0 ] = decodeURIComponent( current[ 0 ] );[m
[32m+[m			[32m// allow just a key to turn on a flag, e.g., test.html?noglobals[m
[32m+[m			[32mcurrent[ 1 ] = current[ 1 ] ? decodeURIComponent( current[ 1 ] ) : true;[m
[32m+[m			[32murlParams[ current[ 0 ] ] = current[ 1 ];[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mQUnit.urlParams = urlParams;[m
[32m+[m
[32m+[m	[32m// String search anywhere in moduleName+testName[m
[32m+[m	[32mconfig.filter = urlParams.filter;[m
[32m+[m
[32m+[m	[32m// Exact match of the module name[m
[32m+[m	[32mconfig.module = urlParams.module;[m
[32m+[m
[32m+[m	[32mconfig.testNumber = parseInt( urlParams.testNumber, 10 ) || null;[m
[32m+[m
[32m+[m	[32m// Figure out if we're running the tests from a server or not[m
[32m+[m	[32mQUnit.isLocal = location.protocol === "file:";[m
[32m+[m[32m}());[m
[32m+[m
[32m+[m[32m// Extend QUnit object,[m
[32m+[m[32m// these after set here because they should not be exposed as global functions[m
[32m+[m[32mextend( QUnit, {[m
[32m+[m	[32massert: assert,[m
[32m+[m
[32m+[m	[32mconfig: config,[m
[32m+[m
[32m+[m	[32m// Initialize the configuration options[m
[32m+[m	[32minit: function() {[m
[32m+[m		[32mextend( config, {[m
[32m+[m			[32mstats: { all: 0, bad: 0 },[m
[32m+[m			[32mmoduleStats: { all: 0, bad: 0 },[m
[32m+[m			[32mstarted: +new Date(),[m
[32m+[m			[32mupdateRate: 1000,[m
[32m+[m			[32mblocking: false,[m
[32m+[m			[32mautostart: true,[m
[32m+[m			[32mautorun: false,[m
[32m+[m			[32mfilter: "",[m
[32m+[m			[32mqueue: [],[m
[32m+[m			[32msemaphore: 1[m
[32m+[m		[32m});[m
[32m+[m
[32m+[m		[32mvar tests, banner, result,[m
[32m+[m			[32mqunit = id( "qunit" );[m
[32m+[m
[32m+[m		[32mif ( qunit ) {[m
[32m+[m			[32mqunit.innerHTML =[m
[32m+[m				[32m"<h1 id='qunit-header'>" + escapeText( document.title ) + "</h1>" +[m
[32m+[m				[32m"<h2 id='qunit-banner'></h2>" +[m
[32m+[m				[32m"<div id='qunit-testrunner-toolbar'></div>" +[m
[32m+[m				[32m"<h2 id='qunit-userAgent'></h2>" +[m
[32m+[m				[32m"<ol id='qunit-tests'></ol>";[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mtests = id( "qunit-tests" );[m
[32m+[m		[32mbanner = id( "qunit-banner" );[m
[32m+[m		[32mresult = id( "qunit-testresult" );[m
[32m+[m
[32m+[m		[32mif ( tests ) {[m
[32m+[m			[32mtests.innerHTML = "";[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( banner ) {[m
[32m+[m			[32mbanner.className = "";[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( result ) {[m
[32m+[m			[32mresult.parentNode.removeChild( result );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( tests ) {[m
[32m+[m			[32mresult = document.createElement( "p" );[m
[32m+[m			[32mresult.id = "qunit-testresult";[m
[32m+[m			[32mresult.className = "result";[m
[32m+[m			[32mtests.parentNode.insertBefore( result, tests );[m
[32m+[m			[32mresult.innerHTML = "Running...<br/>&nbsp;";[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Resets the test setup. Useful for tests that modify the DOM.[m
[32m+[m	[32m/*[m
[32m+[m	[32mDEPRECATED: Use multiple tests instead of resetting inside a test.[m
[32m+[m	[32mUse testStart or testDone for custom cleanup.[m
[32m+[m	[32mThis method will throw an error in 2.0, and will be removed in 2.1[m
[32m+[m	[32m*/[m
[32m+[m	[32mreset: function() {[m
[32m+[m		[32mvar fixture = id( "qunit-fixture" );[m
[32m+[m		[32mif ( fixture ) {[m
[32m+[m			[32mfixture.innerHTML = config.fixture;[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Trigger an event on an element.[m
[32m+[m	[32m// @example triggerEvent( document.body, "click" );[m
[32m+[m	[32mtriggerEvent: function( elem, type, event ) {[m
[32m+[m		[32mif ( document.createEvent ) {[m
[32m+[m			[32mevent = document.createEvent( "MouseEvents" );[m
[32m+[m			[32mevent.initMouseEvent(type, true, true, elem.ownerDocument.defaultView,[m
[32m+[m				[32m0, 0, 0, 0, 0, false, false, false, false, 0, null);[m
[32m+[m
[32m+[m			[32melem.dispatchEvent( event );[m
[32m+[m		[32m} else if ( elem.fireEvent ) {[m
[32m+[m			[32melem.fireEvent( "on" + type );[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Safe object type checking[m
[32m+[m	[32mis: function( type, obj ) {[m
[32m+[m		[32mreturn QUnit.objectType( obj ) === type;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mobjectType: function( obj ) {[m
[32m+[m		[32mif ( typeof obj === "undefined" ) {[m
[32m+[m				[32mreturn "undefined";[m
[32m+[m		[32m// consider: typeof null === object[m
[32m+[m		[32m}[m
[32m+[m		[32mif ( obj === null ) {[m
[32m+[m				[32mreturn "null";[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar match = toString.call( obj ).match(/^\[object\s(.*)\]$/),[m
[32m+[m			[32mtype = match && match[1] || "";[m
[32m+[m
[32m+[m		[32mswitch ( type ) {[m
[32m+[m			[32mcase "Number":[m
[32m+[m				[32mif ( isNaN(obj) ) {[m
[32m+[m					[32mreturn "nan";[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn "number";[m
[32m+[m			[32mcase "String":[m
[32m+[m			[32mcase "Boolean":[m
[32m+[m			[32mcase "Array":[m
[32m+[m			[32mcase "Date":[m
[32m+[m			[32mcase "RegExp":[m
[32m+[m			[32mcase "Function":[m
[32m+[m				[32mreturn type.toLowerCase();[m
[32m+[m		[32m}[m
[32m+[m		[32mif ( typeof obj === "object" ) {[m
[32m+[m			[32mreturn "object";[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn undefined;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mpush: function( result, actual, expected, message ) {[m
[32m+[m		[32mif ( !config.current ) {[m
[32m+[m			[32mthrow new Error( "assertion outside test context, was " + sourceFromStacktrace() );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar output, source,[m
[32m+[m			[32mdetails = {[m
[32m+[m				[32mmodule: config.current.module,[m
[32m+[m				[32mname: config.current.testName,[m
[32m+[m				[32mresult: result,[m
[32m+[m				[32mmessage: message,[m
[32m+[m				[32mactual: actual,[m
[32m+[m				[32mexpected: expected[m
[32m+[m			[32m};[m
[32m+[m
[32m+[m		[32mmessage = escapeText( message ) || ( result ? "okay" : "failed" );[m
[32m+[m		[32mmessage = "<span class='test-message'>" + message + "</span>";[m
[32m+[m		[32moutput = message;[m
[32m+[m
[32m+[m		[32mif ( !result ) {[m
[32m+[m			[32mexpected = escapeText( QUnit.jsDump.parse(expected) );[m
[32m+[m			[32mactual = escapeText( QUnit.jsDump.parse(actual) );[m
[32m+[m			[32moutput += "<table><tr class='test-expected'><th>Expected: </th><td><pre>" + expected + "</pre></td></tr>";[m
[32m+[m
[32m+[m			[32mif ( actual !== expected ) {[m
[32m+[m				[32moutput += "<tr class='test-actual'><th>Result: </th><td><pre>" + actual + "</pre></td></tr>";[m
[32m+[m				[32moutput += "<tr class='test-diff'><th>Diff: </th><td><pre>" + QUnit.diff( expected, actual ) + "</pre></td></tr>";[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32msource = sourceFromStacktrace();[m
[32m+[m
[32m+[m			[32mif ( source ) {[m
[32m+[m				[32mdetails.source = source;[m
[32m+[m				[32moutput += "<tr class='test-source'><th>Source: </th><td><pre>" + escapeText( source ) + "</pre></td></tr>";[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32moutput += "</table>";[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mrunLoggingCallbacks( "log", QUnit, details );[m
[32m+[m
[32m+[m		[32mconfig.current.assertions.push({[m
[32m+[m			[32mresult: !!result,[m
[32m+[m			[32mmessage: output[m
[32m+[m		[32m});[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mpushFailure: function( message, source, actual ) {[m
[32m+[m		[32mif ( !config.current ) {[m
[32m+[m			[32mthrow new Error( "pushFailure() assertion outside test context, was " + sourceFromStacktrace(2) );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar output,[m
[32m+[m			[32mdetails = {[m
[32m+[m				[32mmodule: config.current.module,[m
[32m+[m				[32mname: config.current.testName,[m
[32m+[m				[32mresult: false,[m
[32m+[m				[32mmessage: message[m
[32m+[m			[32m};[m
[32m+[m
[32m+[m		[32mmessage = escapeText( message ) || "error";[m
[32m+[m		[32mmessage = "<span class='test-message'>" + message + "</span>";[m
[32m+[m		[32moutput = message;[m
[32m+[m
[32m+[m		[32moutput += "<table>";[m
[32m+[m
[32m+[m		[32mif ( actual ) {[m
[32m+[m			[32moutput += "<tr class='test-actual'><th>Result: </th><td><pre>" + escapeText( actual ) + "</pre></td></tr>";[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( source ) {[m
[32m+[m			[32mdetails.source = source;[m
[32m+[m			[32moutput += "<tr class='test-source'><th>Source: </th><td><pre>" + escapeText( source ) + "</pre></td></tr>";[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32moutput += "</table>";[m
[32m+[m
[32m+[m		[32mrunLoggingCallbacks( "log", QUnit, details );[m
[32m+[m
[32m+[m		[32mconfig.current.assertions.push({[m
[32m+[m			[32mresult: false,[m
[32m+[m			[32mmessage: output[m
[32m+[m		[32m});[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32murl: function( params ) {[m
[32m+[m		[32mparams = extend( extend( {}, QUnit.urlParams ), params );[m
[32m+[m		[32mvar key,[m
[32m+[m			[32mquerystring = "?";[m
[32m+[m
[32m+[m		[32mfor ( key in params ) {[m
[32m+[m			[32mif ( hasOwn.call( params, key ) ) {[m
[32m+[m				[32mquerystring += encodeURIComponent( key ) + "=" +[m
[32m+[m					[32mencodeURIComponent( params[ key ] ) + "&";[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn window.location.protocol + "//" + window.location.host +[m
[32m+[m			[32mwindow.location.pathname + querystring.slice( 0, -1 );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mextend: extend,[m
[32m+[m	[32mid: id,[m
[32m+[m	[32maddEvent: addEvent,[m
[32m+[m	[32maddClass: addClass,[m
[32m+[m	[32mhasClass: hasClass,[m
[32m+[m	[32mremoveClass: removeClass[m
[32m+[m	[32m// load, equiv, jsDump, diff: Attached later[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * @deprecated: Created for backwards compatibility with test runner that set the hook function[m
[32m+[m[32m * into QUnit.{hook}, instead of invoking it and passing the hook function.[m
[32m+[m[32m * QUnit.constructor is set to the empty F() above so that we can add to it's prototype here.[m
[32m+[m[32m * Doing this allows us to tell if the following methods have been overwritten on the actual[m
[32m+[m[32m * QUnit object.[m
[32m+[m[32m */[m
[32m+[m[32mextend( QUnit.constructor.prototype, {[m
[32m+[m
[32m+[m	[32m// Logging callbacks; all receive a single argument with the listed properties[m
[32m+[m	[32m// run test/logs.html for any related changes[m
[32m+[m	[32mbegin: registerLoggingCallback( "begin" ),[m
[32m+[m
[32m+[m	[32m// done: { failed, passed, total, runtime }[m
[32m+[m	[32mdone: registerLoggingCallback( "done" ),[m
[32m+[m
[32m+[m	[32m// log: { result, actual, expected, message }[m
[32m+[m	[32mlog: registerLoggingCallback( "log" ),[m
[32m+[m
[32m+[m	[32m// testStart: { name }[m
[32m+[m	[32mtestStart: registerLoggingCallback( "testStart" ),[m
[32m+[m
[32m+[m	[32m// testDone: { name, failed, passed, total, duration }[m
[32m+[m	[32mtestDone: registerLoggingCallback( "testDone" ),[m
[32m+[m
[32m+[m	[32m// moduleStart: { name }[m
[32m+[m	[32mmoduleStart: registerLoggingCallback( "moduleStart" ),[m
[32m+[m
[32m+[m	[32m// moduleDone: { name, failed, passed, total }[m
[32m+[m	[32mmoduleDone: registerLoggingCallback( "moduleDone" )[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32mif ( typeof document === "undefined" || document.readyState === "complete" ) {[m
[32m+[m	[32mconfig.autorun = true;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mQUnit.load = function() {[m
[32m+[m	[32mrunLoggingCallbacks( "begin", QUnit, {} );[m
[32m+[m
[32m+[m	[32m// Initialize the config, saving the execution queue[m
[32m+[m	[32mvar banner, filter, i, label, len, main, ol, toolbar, userAgent, val,[m
[32m+[m		[32murlConfigCheckboxesContainer, urlConfigCheckboxes, moduleFilter,[m
[32m+[m		[32mnumModules = 0,[m
[32m+[m		[32mmoduleNames = [],[m
[32m+[m		[32mmoduleFilterHtml = "",[m
[32m+[m		[32murlConfigHtml = "",[m
[32m+[m		[32moldconfig = extend( {}, config );[m
[32m+[m
[32m+[m	[32mQUnit.init();[m
[32m+[m	[32mextend(config, oldconfig);[m
[32m+[m
[32m+[m	[32mconfig.blocking = false;[m
[32m+[m
[32m+[m	[32mlen = config.urlConfig.length;[m
[32m+[m
[32m+[m	[32mfor ( i = 0; i < len; i++ ) {[m
[32m+[m		[32mval = config.urlConfig[i];[m
[32m+[m		[32mif ( typeof val === "string" ) {[m
[32m+[m			[32mval = {[m
[32m+[m				[32mid: val,[m
[32m+[m				[32mlabel: val,[m
[32m+[m				[32mtooltip: "[no tooltip available]"[m
[32m+[m			[32m};[m
[32m+[m		[32m}[m
[32m+[m		[32mconfig[ val.id ] = QUnit.urlParams[ val.id ];[m
[32m+[m		[32murlConfigHtml += "<input id='qunit-urlconfig-" + escapeText( val.id ) +[m
[32m+[m			[32m"' name='" + escapeText( val.id ) +[m
[32m+[m			[32m"' type='checkbox'" + ( config[ val.id ] ? " checked='checked'" : "" ) +[m
[32m+[m			[32m" title='" + escapeText( val.tooltip ) +[m
[32m+[m			[32m"'><label for='qunit-urlconfig-" + escapeText( val.id ) +[m
[32m+[m			[32m"' title='" + escapeText( val.tooltip ) + "'>" + val.label + "</label>";[m
[32m+[m	[32m}[m
[32m+[m	[32mfor ( i in config.modules ) {[m
[32m+[m		[32mif ( config.modules.hasOwnProperty( i ) ) {[m
[32m+[m			[32mmoduleNames.push(i);[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mnumModules = moduleNames.length;[m
[32m+[m	[32mmoduleNames.sort( function( a, b ) {[m
[32m+[m		[32mreturn a.localeCompare( b );[m
[32m+[m	[32m});[m
[32m+[m	[32mmoduleFilterHtml += "<label for='qunit-modulefilter'>Module: </label><select id='qunit-modulefilter' name='modulefilter'><option value='' " +[m
[32m+[m		[32m( config.module === undefined  ? "selected='selected'" : "" ) +[m
[32m+[m		[32m">< All Modules ></option>";[m
[32m+[m
[32m+[m
[32m+[m	[32mfor ( i = 0; i < numModules; i++) {[m
[32m+[m			[32mmoduleFilterHtml += "<option value='" + escapeText( encodeURIComponent(moduleNames[i]) ) + "' " +[m
[32m+[m				[32m( config.module === moduleNames[i] ? "selected='selected'" : "" ) +[m
[32m+[m				[32m">" + escapeText(moduleNames[i]) + "</option>";[m
[32m+[m	[32m}[m
[32m+[m	[32mmoduleFilterHtml += "</select>";[m
[32m+[m
[32m+[m	[32m// `userAgent` initialized at top of scope[m
[32m+[m	[32muserAgent = id( "qunit-userAgent" );[m
[32m+[m	[32mif ( userAgent ) {[m
[32m+[m		[32muserAgent.innerHTML = navigator.userAgent;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// `banner` initialized at top of scope[m
[32m+[m	[32mbanner = id( "qunit-header" );[m
[32m+[m	[32mif ( banner ) {[m
[32m+[m		[32mbanner.innerHTML = "<a href='" + QUnit.url({ filter: undefined, module: undefined, testNumber: undefined }) + "'>" + banner.innerHTML + "</a> ";[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// `toolbar` initialized at top of scope[m
[32m+[m	[32mtoolbar = id( "qunit-testrunner-toolbar" );[m
[32m+[m	[32mif ( toolbar ) {[m
[32m+[m		[32m// `filter` initialized at top of scope[m
[32m+[m		[32mfilter = document.createElement( "input" );[m
[32m+[m		[32mfilter.type = "checkbox";[m
[32m+[m		[32mfilter.id = "qunit-filter-pass";[m
[32m+[m
[32m+[m		[32maddEvent( filter, "click", function() {[m
[32m+[m			[32mvar tmp,[m
[32m+[m				[32mol = document.getElementById( "qunit-tests" );[m
[32m+[m
[32m+[m			[32mif ( filter.checked ) {[m
[32m+[m				[32mol.className = ol.className + " hidepass";[m
[32m+[m			[32m} else {[m
[32m+[m				[32mtmp = " " + ol.className.replace( /[\n\t\r]/g, " " ) + " ";[m
[32m+[m				[32mol.className = tmp.replace( / hidepass /, " " );[m
[32m+[m			[32m}[m
[32m+[m			[32mif ( defined.sessionStorage ) {[m
[32m+[m				[32mif (filter.checked) {[m
[32m+[m					[32msessionStorage.setItem( "qunit-filter-passed-tests", "true" );[m
[32m+[m				[32m} else {[m
[32m+[m					[32msessionStorage.removeItem( "qunit-filter-passed-tests" );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m});[m
[32m+[m
[32m+[m		[32mif ( config.hidepassed || defined.sessionStorage && sessionStorage.getItem( "qunit-filter-passed-tests" ) ) {[m
[32m+[m			[32mfilter.checked = true;[m
[32m+[m			[32m// `ol` initialized at top of scope[m
[32m+[m			[32mol = document.getElementById( "qunit-tests" );[m
[32m+[m			[32mol.className = ol.className + " hidepass";[m
[32m+[m		[32m}[m
[32m+[m		[32mtoolbar.appendChild( filter );[m
[32m+[m
[32m+[m		[32m// `label` initialized at top of scope[m
[32m+[m		[32mlabel = document.createElement( "label" );[m
[32m+[m		[32mlabel.setAttribute( "for", "qunit-filter-pass" );[m
[32m+[m		[32mlabel.setAttribute( "title", "Only show tests and assertions that fail. Stored in sessionStorage." );[m
[32m+[m		[32mlabel.innerHTML = "Hide passed tests";[m
[32m+[m		[32mtoolbar.appendChild( label );[m
[32m+[m
[32m+[m		[32murlConfigCheckboxesContainer = document.createElement("span");[m
[32m+[m		[32murlConfigCheckboxesContainer.innerHTML = urlConfigHtml;[m
[32m+[m		[32murlConfigCheckboxes = urlConfigCheckboxesContainer.getElementsByTagName("input");[m
[32m+[m		[32m// For oldIE support:[m
[32m+[m		[32m// * Add handlers to the individual elements instead of the container[m
[32m+[m		[32m// * Use "click" instead of "change"[m
[32m+[m		[32m// * Fallback from event.target to event.srcElement[m
[32m+[m		[32maddEvents( urlConfigCheckboxes, "click", function( event ) {[m
[32m+[m			[32mvar params = {},[m
[32m+[m				[32mtarget = event.target || event.srcElement;[m
[32m+[m			[32mparams[ target.name ] = target.checked ? true : undefined;[m
[32m+[m			[32mwindow.location = QUnit.url( params );[m
[32m+[m		[32m});[m
[32m+[m		[32mtoolbar.appendChild( urlConfigCheckboxesContainer );[m
[32m+[m
[32m+[m		[32mif (numModules > 1) {[m
[32m+[m			[32mmoduleFilter = document.createElement( "span" );[m
[32m+[m			[32mmoduleFilter.setAttribute( "id", "qunit-modulefilter-container" );[m
[32m+[m			[32mmoduleFilter.innerHTML = moduleFilterHtml;[m
[32m+[m			[32maddEvent( moduleFilter.lastChild, "change", function() {[m
[32m+[m				[32mvar selectBox = moduleFilter.getElementsByTagName("select")[0],[m
[32m+[m					[32mselectedModule = decodeURIComponent(selectBox.options[selectBox.selectedIndex].value);[m
[32m+[m
[32m+[m				[32mwindow.location = QUnit.url({[m
[32m+[m					[32mmodule: ( selectedModule === "" ) ? undefined : selectedModule,[m
[32m+[m					[32m// Remove any existing filters[m
[32m+[m					[32mfilter: undefined,[m
[32m+[m					[32mtestNumber: undefined[m
[32m+[m				[32m});[m
[32m+[m			[32m});[m
[32m+[m			[32mtoolbar.appendChild(moduleFilter);[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// `main` initialized at top of scope[m
[32m+[m	[32mmain = id( "qunit-fixture" );[m
[32m+[m	[32mif ( main ) {[m
[32m+[m		[32mconfig.fixture = main.innerHTML;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( config.autostart ) {[m
[32m+[m		[32mQUnit.start();[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32maddEvent( window, "load", QUnit.load );[m
[32m+[m
[32m+[m[32m// `onErrorFnPrev` initialized at top of scope[m
[32m+[m[32m// Preserve other handlers[m
[32m+[m[32monErrorFnPrev = window.onerror;[m
[32m+[m
[32m+[m[32m// Cover uncaught exceptions[m
[32m+[m[32m// Returning true will suppress the default browser handler,[m
[32m+[m[32m// returning false will let it run.[m
[32m+[m[32mwindow.onerror = function ( error, filePath, linerNr ) {[m
[32m+[m	[32mvar ret = false;[m
[32m+[m	[32mif ( onErrorFnPrev ) {[m
[32m+[m		[32mret = onErrorFnPrev( error, filePath, linerNr );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Treat return value as window.onerror itself does,[m
[32m+[m	[32m// Only do our handling if not suppressed.[m
[32m+[m	[32mif ( ret !== true ) {[m
[32m+[m		[32mif ( QUnit.config.current ) {[m
[32m+[m			[32mif ( QUnit.config.current.ignoreGlobalErrors ) {[m
[32m+[m				[32mreturn true;[m
[32m+[m			[32m}[m
[32m+[m			[32mQUnit.pushFailure( error, filePath + ":" + linerNr );[m
[32m+[m		[32m} else {[m
[32m+[m			[32mQUnit.test( "global failure", extend( function() {[m
[32m+[m				[32mQUnit.pushFailure( error, filePath + ":" + linerNr );[m
[32m+[m			[32m}, { validTest: validTest } ) );[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn false;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn ret;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mfunction done() {[m
[32m+[m	[32mconfig.autorun = true;[m
[32m+[m
[32m+[m	[32m// Log the last module results[m
[32m+[m	[32mif ( config.currentModule ) {[m
[32m+[m		[32mrunLoggingCallbacks( "moduleDone", QUnit, {[m
[32m+[m			[32mname: config.currentModule,[m
[32m+[m			[32mfailed: config.moduleStats.bad,[m
[32m+[m			[32mpassed: config.moduleStats.all - config.moduleStats.bad,[m
[32m+[m			[32mtotal: config.moduleStats.all[m
[32m+[m		[32m});[m
[32m+[m	[32m}[m
[32m+[m	[32mdelete config.previousModule;[m
[32m+[m
[32m+[m	[32mvar i, key,[m
[32m+[m		[32mbanner = id( "qunit-banner" ),[m
[32m+[m		[32mtests = id( "qunit-tests" ),[m
[32m+[m		[32mruntime = +new Date() - config.started,[m
[32m+[m		[32mpassed = config.stats.all - config.stats.bad,[m
[32m+[m		[32mhtml = [[m
[32m+[m			[32m"Tests completed in ",[m
[32m+[m			[32mruntime,[m
[32m+[m			[32m" milliseconds.<br/>",[m
[32m+[m			[32m"<span class='passed'>",[m
[32m+[m			[32mpassed,[m
[32m+[m			[32m"</span> assertions of <span class='total'>",[m
[32m+[m			[32mconfig.stats.all,[m
[32m+[m			[32m"</span> passed, <span class='failed'>",[m
[32m+[m			[32mconfig.stats.bad,[m
[32m+[m			[32m"</span> failed."[m
[32m+[m		[32m].join( "" );[m
[32m+[m
[32m+[m	[32mif ( banner ) {[m
[32m+[m		[32mbanner.className = ( config.stats.bad ? "qunit-fail" : "qunit-pass" );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( tests ) {[m
[32m+[m		[32mid( "qunit-testresult" ).innerHTML = html;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( config.altertitle && typeof document !== "undefined" && document.title ) {[m
[32m+[m		[32m// show ✖ for good, ✔ for bad suite result in title[m
[32m+[m		[32m// use escape sequences in case file gets loaded with non-utf-8-charset[m
[32m+[m		[32mdocument.title = [[m
[32m+[m			[32m( config.stats.bad ? "\u2716" : "\u2714" ),[m
[32m+[m			[32mdocument.title.replace( /^[\u2714\u2716] /i, "" )[m
[32m+[m		[32m].join( " " );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// clear own sessionStorage items if all tests passed[m
[32m+[m	[32mif ( config.reorder && defined.sessionStorage && config.stats.bad === 0 ) {[m
[32m+[m		[32m// `key` & `i` initialized at top of scope[m
[32m+[m		[32mfor ( i = 0; i < sessionStorage.length; i++ ) {[m
[32m+[m			[32mkey = sessionStorage.key( i++ );[m
[32m+[m			[32mif ( key.indexOf( "qunit-test-" ) === 0 ) {[m
[32m+[m				[32msessionStorage.removeItem( key );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// scroll back to top to show results[m
[32m+[m	[32mif ( window.scrollTo ) {[m
[32m+[m		[32mwindow.scrollTo(0, 0);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mrunLoggingCallbacks( "done", QUnit, {[m
[32m+[m		[32mfailed: config.stats.bad,[m
[32m+[m		[32mpassed: passed,[m
[32m+[m		[32mtotal: config.stats.all,[m
[32m+[m		[32mruntime: runtime[m
[32m+[m	[32m});[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/** @return Boolean: true if this test should be ran */[m
[32m+[m[32mfunction validTest( test ) {[m
[32m+[m	[32mvar include,[m
[32m+[m		[32mfilter = config.filter && config.filter.toLowerCase(),[m
[32m+[m		[32mmodule = config.module && config.module.toLowerCase(),[m
[32m+[m		[32mfullName = (test.module + ": " + test.testName).toLowerCase();[m
[32m+[m
[32m+[m	[32m// Internally-generated tests are always valid[m
[32m+[m	[32mif ( test.callback && test.callback.validTest === validTest ) {[m
[32m+[m		[32mdelete test.callback.validTest;[m
[32m+[m		[32mreturn true;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( config.testNumber ) {[m
[32m+[m		[32mreturn test.testNumber === config.testNumber;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( module && ( !test.module || test.module.toLowerCase() !== module ) ) {[m
[32m+[m		[32mreturn false;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( !filter ) {[m
[32m+[m		[32mreturn true;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32minclude = filter.charAt( 0 ) !== "!";[m
[32m+[m	[32mif ( !include ) {[m
[32m+[m		[32mfilter = filter.slice( 1 );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// If the filter matches, we need to honour include[m
[32m+[m	[32mif ( fullName.indexOf( filter ) !== -1 ) {[m
[32m+[m		[32mreturn include;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Otherwise, do the opposite[m
[32m+[m	[32mreturn !include;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// so far supports only Firefox, Chrome and Opera (buggy), Safari (for real exceptions)[m
[32m+[m[32m// Later Safari and IE10 are supposed to support error.stack as well[m
[32m+[m[32m// See also https://developer.mozilla.org/en/JavaScript/Reference/Global_Objects/Error/Stack[m
[32m+[m[32mfunction extractStacktrace( e, offset ) {[m
[32m+[m	[32moffset = offset === undefined ? 3 : offset;[m
[32m+[m
[32m+[m	[32mvar stack, include, i;[m
[32m+[m
[32m+[m	[32mif ( e.stacktrace ) {[m
[32m+[m		[32m// Opera[m
[32m+[m		[32mreturn e.stacktrace.split( "\n" )[ offset + 3 ];[m
[32m+[m	[32m} else if ( e.stack ) {[m
[32m+[m		[32m// Firefox, Chrome[m
[32m+[m		[32mstack = e.stack.split( "\n" );[m
[32m+[m		[32mif (/^error$/i.test( stack[0] ) ) {[m
[32m+[m			[32mstack.shift();[m
[32m+[m		[32m}[m
[32m+[m		[32mif ( fileName ) {[m
[32m+[m			[32minclude = [];[m
[32m+[m			[32mfor ( i = offset; i < stack.length; i++ ) {[m
[32m+[m				[32mif ( stack[ i ].indexOf( fileName ) !== -1 ) {[m
[32m+[m					[32mbreak;[m
[32m+[m				[32m}[m
[32m+[m				[32minclude.push( stack[ i ] );[m
[32m+[m			[32m}[m
[32m+[m			[32mif ( include.length ) {[m
[32m+[m				[32mreturn include.join( "\n" );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn stack[ offset ];[m
[32m+[m	[32m} else if ( e.sourceURL ) {[m
[32m+[m		[32m// Safari, PhantomJS[m
[32m+[m		[32m// hopefully one day Safari provides actual stacktraces[m
[32m+[m		[32m// exclude useless self-reference for generated Error objects[m
[32m+[m		[32mif ( /qunit.js$/.test( e.sourceURL ) ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m		[32m// for actual exceptions, this is useful[m
[32m+[m		[32mreturn e.sourceURL + ":" + e.line;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m[32mfunction sourceFromStacktrace( offset ) {[m
[32m+[m	[32mtry {[m
[32m+[m		[32mthrow new Error();[m
[32m+[m	[32m} catch ( e ) {[m
[32m+[m		[32mreturn extractStacktrace( e, offset );[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Escape text for attribute or text content.[m
[32m+[m[32m */[m
[32m+[m[32mfunction escapeText( s ) {[m
[32m+[m	[32mif ( !s ) {[m
[32m+[m		[32mreturn "";[m
[32m+[m	[32m}[m
[32m+[m	[32ms = s + "";[m
[32m+[m	[32m// Both single quotes and double quotes (for attributes)[m
[32m+[m	[32mreturn s.replace( /['"<>&]/g, function( s ) {[m
[32m+[m		[32mswitch( s ) {[m
[32m+[m			[32mcase "'":[m
[32m+[m				[32mreturn "&#039;";[m
[32m+[m			[32mcase "\"":[m
[32m+[m				[32mreturn "&quot;";[m
[32m+[m			[32mcase "<":[m
[32m+[m				[32mreturn "&lt;";[m
[32m+[m			[32mcase ">":[m
[32m+[m				[32mreturn "&gt;";[m
[32m+[m			[32mcase "&":[m
[32m+[m				[32mreturn "&amp;";[m
[32m+[m		[32m}[m
[32m+[m	[32m});[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction synchronize( callback, last ) {[m
[32m+[m	[32mconfig.queue.push( callback );[m
[32m+[m
[32m+[m	[32mif ( config.autorun && !config.blocking ) {[m
[32m+[m		[32mprocess( last );[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction process( last ) {[m
[32m+[m	[32mfunction next() {[m
[32m+[m		[32mprocess( last );[m
[32m+[m	[32m}[m
[32m+[m	[32mvar start = new Date().getTime();[m
[32m+[m	[32mconfig.depth = config.depth ? config.depth + 1 : 1;[m
[32m+[m
[32m+[m	[32mwhile ( config.queue.length && !config.blocking ) {[m
[32m+[m		[32mif ( !defined.setTimeout || config.updateRate <= 0 || ( ( new Date().getTime() - start ) < config.updateRate ) ) {[m
[32m+[m			[32mconfig.queue.shift()();[m
[32m+[m		[32m} else {[m
[32m+[m			[32msetTimeout( next, 13 );[m
[32m+[m			[32mbreak;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mconfig.depth--;[m
[32m+[m	[32mif ( last && !config.blocking && !config.queue.length && config.depth === 0 ) {[m
[32m+[m		[32mdone();[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction saveGlobal() {[m
[32m+[m	[32mconfig.pollution = [];[m
[32m+[m
[32m+[m	[32mif ( config.noglobals ) {[m
[32m+[m		[32mfor ( var key in window ) {[m
[32m+[m			[32mif ( hasOwn.call( window, key ) ) {[m
[32m+[m				[32m// in Opera sometimes DOM element ids show up here, ignore them[m
[32m+[m				[32mif ( /^qunit-test-output/.test( key ) ) {[m
[32m+[m					[32mcontinue;[m
[32m+[m				[32m}[m
[32m+[m				[32mconfig.pollution.push( key );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction checkPollution() {[m
[32m+[m	[32mvar newGlobals,[m
[32m+[m		[32mdeletedGlobals,[m
[32m+[m		[32mold = config.pollution;[m
[32m+[m
[32m+[m	[32msaveGlobal();[m
[32m+[m
[32m+[m	[32mnewGlobals = diff( config.pollution, old );[m
[32m+[m	[32mif ( newGlobals.length > 0 ) {[m
[32m+[m		[32mQUnit.pushFailure( "Introduced global variable(s): " + newGlobals.join(", ") );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mdeletedGlobals = diff( old, config.pollution );[m
[32m+[m	[32mif ( deletedGlobals.length > 0 ) {[m
[32m+[m		[32mQUnit.pushFailure( "Deleted global variable(s): " + deletedGlobals.join(", ") );[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// returns a new Array with the elements that are in a but not in b[m
[32m+[m[32mfunction diff( a, b ) {[m
[32m+[m	[32mvar i, j,[m
[32m+[m		[32mresult = a.slice();[m
[32m+[m
[32m+[m	[32mfor ( i = 0; i < result.length; i++ ) {[m
[32m+[m		[32mfor ( j = 0; j < b.length; j++ ) {[m
[32m+[m			[32mif ( result[i] === b[j] ) {[m
[32m+[m				[32mresult.splice( i, 1 );[m
[32m+[m				[32mi--;[m
[32m+[m				[32mbreak;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn result;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction extend( a, b ) {[m
[32m+[m	[32mfor ( var prop in b ) {[m
[32m+[m		[32mif ( hasOwn.call( b, prop ) ) {[m
[32m+[m			[32m// Avoid "Member not found" error in IE8 caused by messing with window.constructor[m
[32m+[m			[32mif ( !( prop === "constructor" && a === window ) ) {[m
[32m+[m				[32mif ( b[ prop ] === undefined ) {[m
[32m+[m					[32mdelete a[ prop ];[m
[32m+[m				[32m} else {[m
[32m+[m					[32ma[ prop ] = b[ prop ];[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn a;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * @param {HTMLElement} elem[m
[32m+[m[32m * @param {string} type[m
[32m+[m[32m * @param {Function} fn[m
[32m+[m[32m */[m
[32m+[m[32mfunction addEvent( elem, type, fn ) {[m
[32m+[m	[32m// Standards-based browsers[m
[32m+[m	[32mif ( elem.addEventListener ) {[m
[32m+[m		[32melem.addEventListener( type, fn, false );[m
[32m+[m	[32m// IE[m
[32m+[m	[32m} else {[m
[32m+[m		[32melem.attachEvent( "on" + type, fn );[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * @param {Array|NodeList} elems[m
[32m+[m[32m * @param {string} type[m
[32m+[m[32m * @param {Function} fn[m
[32m+[m[32m */[m
[32m+[m[32mfunction addEvents( elems, type, fn ) {[m
[32m+[m	[32mvar i = elems.length;[m
[32m+[m	[32mwhile ( i-- ) {[m
[32m+[m		[32maddEvent( elems[i], type, fn );[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction hasClass( elem, name ) {[m
[32m+[m	[32mreturn (" " + elem.className + " ").indexOf(" " + name + " ") > -1;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction addClass( elem, name ) {[m
[32m+[m	[32mif ( !hasClass( elem, name ) ) {[m
[32m+[m		[32melem.className += (elem.className ? " " : "") + name;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction removeClass( elem, name ) {[m
[32m+[m	[32mvar set = " " + elem.className + " ";[m
[32m+[m	[32m// Class name may appear multiple times[m
[32m+[m	[32mwhile ( set.indexOf(" " + name + " ") > -1 ) {[m
[32m+[m		[32mset = set.replace(" " + name + " " , " ");[m
[32m+[m	[32m}[m
[32m+[m	[32m// If possible, trim it for prettiness, but not necessarily[m
[32m+[m	[32melem.className = typeof set.trim === "function" ? set.trim() : set.replace(/^\s+|\s+$/g, "");[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction id( name ) {[m
[32m+[m	[32mreturn !!( typeof document !== "undefined" && document && document.getElementById ) &&[m
[32m+[m		[32mdocument.getElementById( name );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction registerLoggingCallback( key ) {[m
[32m+[m	[32mreturn function( callback ) {[m
[32m+[m		[32mconfig[key].push( callback );[m
[32m+[m	[32m};[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Supports deprecated method of completely overwriting logging callbacks[m
[32m+[m[32mfunction runLoggingCallbacks( key, scope, args ) {[m
[32m+[m	[32mvar i, callbacks;[m
[32m+[m	[32mif ( QUnit.hasOwnProperty( key ) ) {[m
[32m+[m		[32mQUnit[ key ].call(scope, args );[m
[32m+[m	[32m} else {[m
[32m+[m		[32mcallbacks = config[ key ];[m
[32m+[m		[32mfor ( i = 0; i < callbacks.length; i++ ) {[m
[32m+[m			[32mcallbacks[ i ].call( scope, args );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Test for equality any JavaScript type.[m
[32m+[m[32m// Author: Philippe Rathé <prathe@gmail.com>[m
[32m+[m[32mQUnit.equiv = (function() {[m
[32m+[m
[32m+[m	[32m// Call the o related callback with the given arguments.[m
[32m+[m	[32mfunction bindCallbacks( o, callbacks, args ) {[m
[32m+[m		[32mvar prop = QUnit.objectType( o );[m
[32m+[m		[32mif ( prop ) {[m
[32m+[m			[32mif ( QUnit.objectType( callbacks[ prop ] ) === "function" ) {[m
[32m+[m				[32mreturn callbacks[ prop ].apply( callbacks, args );[m
[32m+[m			[32m} else {[m
[32m+[m				[32mreturn callbacks[ prop ]; // or undefined[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// the real equiv function[m
[32m+[m	[32mvar innerEquiv,[m
[32m+[m		[32m// stack to decide between skip/abort functions[m
[32m+[m		[32mcallers = [],[m
[32m+[m		[32m// stack to avoiding loops from circular referencing[m
[32m+[m		[32mparents = [],[m
[32m+[m		[32mparentsB = [],[m
[32m+[m
[32m+[m		[32mgetProto = Object.getPrototypeOf || function ( obj ) {[m
[32m+[m			[32m/*jshint camelcase:false */[m
[32m+[m			[32mreturn obj.__proto__;[m
[32m+[m		[32m},[m
[32m+[m		[32mcallbacks = (function () {[m
[32m+[m
[32m+[m			[32m// for string, boolean, number and null[m
[32m+[m			[32mfunction useStrictEquality( b, a ) {[m
[32m+[m				[32m/*jshint eqeqeq:false */[m
[32m+[m				[32mif ( b instanceof a.constructor || a instanceof b.constructor ) {[m
[32m+[m					[32m// to catch short annotation VS 'new' annotation of a[m
[32m+[m					[32m// declaration[m
[32m+[m					[32m// e.g. var i = 1;[m
[32m+[m					[32m// var j = new Number(1);[m
[32m+[m					[32mreturn a == b;[m
[32m+[m				[32m} else {[m
[32m+[m					[32mreturn a === b;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn {[m
[32m+[m				[32m"string": useStrictEquality,[m
[32m+[m				[32m"boolean": useStrictEquality,[m
[32m+[m				[32m"number": useStrictEquality,[m
[32m+[m				[32m"null": useStrictEquality,[m
[32m+[m				[32m"undefined": useStrictEquality,[m
[32m+[m
[32m+[m				[32m"nan": function( b ) {[m
[32m+[m					[32mreturn isNaN( b );[m
[32m+[m				[32m},[m
[32m+[m
[32m+[m				[32m"date": function( b, a ) {[m
[32m+[m					[32mreturn QUnit.objectType( b ) === "date" && a.valueOf() === b.valueOf();[m
[32m+[m				[32m},[m
[32m+[m
[32m+[m				[32m"regexp": function( b, a ) {[m
[32m+[m					[32mreturn QUnit.objectType( b ) === "regexp" &&[m
[32m+[m						[32m// the regex itself[m
[32m+[m						[32ma.source === b.source &&[m
[32m+[m						[32m// and its modifiers[m
[32m+[m						[32ma.global === b.global &&[m
[32m+[m						[32m// (gmi) ...[m
[32m+[m						[32ma.ignoreCase === b.ignoreCase &&[m
[32m+[m						[32ma.multiline === b.multiline &&[m
[32m+[m						[32ma.sticky === b.sticky;[m
[32m+[m				[32m},[m
[32m+[m
[32m+[m				[32m// - skip when the property is a method of an instance (OOP)[m
[32m+[m				[32m// - abort otherwise,[m
[32m+[m				[32m// initial === would have catch identical references anyway[m
[32m+[m				[32m"function": function() {[m
[32m+[m					[32mvar caller = callers[callers.length - 1];[m
[32m+[m					[32mreturn caller !== Object && typeof caller !== "undefined";[m
[32m+[m				[32m},[m
[32m+[m
[32m+[m				[32m"array": function( b, a ) {[m
[32m+[m					[32mvar i, j, len, loop, aCircular, bCircular;[m
[32m+[m
[32m+[m					[32m// b could be an object literal here[m
[32m+[m					[32mif ( QUnit.objectType( b ) !== "array" ) {[m
[32m+[m						[32mreturn false;[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32mlen = a.length;[m
[32m+[m					[32mif ( len !== b.length ) {[m
[32m+[m						[32m// safe and faster[m
[32m+[m						[32mreturn false;[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// track reference to avoid circular references[m
[32m+[m					[32mparents.push( a );[m
[32m+[m					[32mparentsB.push( b );[m
[32m+[m					[32mfor ( i = 0; i < len; i++ ) {[m
[32m+[m						[32mloop = false;[m
[32m+[m						[32mfor ( j = 0; j < parents.length; j++ ) {[m
[32m+[m							[32maCircular = parents[j] === a[i];[m
[32m+[m							[32mbCircular = parentsB[j] === b[i];[m
[32m+[m							[32mif ( aCircular || bCircular ) {[m
[32m+[m								[32mif ( a[i] === b[i] || aCircular && bCircular ) {[m
[32m+[m									[32mloop = true;[m
[32m+[m								[32m} else {[m
[32m+[m									[32mparents.pop();[m
[32m+[m									[32mparentsB.pop();[m
[32m+[m									[32mreturn false;[m
[32m+[m								[32m}[m
[32m+[m							[32m}[m
[32m+[m						[32m}[m
[32m+[m						[32mif ( !loop && !innerEquiv(a[i], b[i]) ) {[m
[32m+[m							[32mparents.pop();[m
[32m+[m							[32mparentsB.pop();[m
[32m+[m							[32mreturn false;[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m					[32mparents.pop();[m
[32m+[m					[32mparentsB.pop();[m
[32m+[m					[32mreturn true;[m
[32m+[m				[32m},[m
[32m+[m
[32m+[m				[32m"object": function( b, a ) {[m
[32m+[m					[32m/*jshint forin:false */[m
[32m+[m					[32mvar i, j, loop, aCircular, bCircular,[m
[32m+[m						[32m// Default to true[m
[32m+[m						[32meq = true,[m
[32m+[m						[32maProperties = [],[m
[32m+[m						[32mbProperties = [];[m
[32m+[m
[32m+[m					[32m// comparing constructors is more strict than using[m
[32m+[m					[32m// instanceof[m
[32m+[m					[32mif ( a.constructor !== b.constructor ) {[m
[32m+[m						[32m// Allow objects with no prototype to be equivalent to[m
[32m+[m						[32m// objects with Object as their constructor.[m
[32m+[m						[32mif ( !(( getProto(a) === null && getProto(b) === Object.prototype ) ||[m
[32m+[m							[32m( getProto(b) === null && getProto(a) === Object.prototype ) ) ) {[m
[32m+[m								[32mreturn false;[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// stack constructor before traversing properties[m
[32m+[m					[32mcallers.push( a.constructor );[m
[32m+[m
[32m+[m					[32m// track reference to avoid circular references[m
[32m+[m					[32mparents.push( a );[m
[32m+[m					[32mparentsB.push( b );[m
[32m+[m
[32m+[m					[32m// be strict: don't ensure hasOwnProperty and go deep[m
[32m+[m					[32mfor ( i in a ) {[m
[32m+[m						[32mloop = false;[m
[32m+[m						[32mfor ( j = 0; j < parents.length; j++ ) {[m
[32m+[m							[32maCircular = parents[j] === a[i];[m
[32m+[m							[32mbCircular = parentsB[j] === b[i];[m
[32m+[m							[32mif ( aCircular || bCircular ) {[m
[32m+[m								[32mif ( a[i] === b[i] || aCircular && bCircular ) {[m
[32m+[m									[32mloop = true;[m
[32m+[m								[32m} else {[m
[32m+[m									[32meq = false;[m
[32m+[m									[32mbreak;[m
[32m+[m								[32m}[m
[32m+[m							[32m}[m
[32m+[m						[32m}[m
[32m+[m						[32maProperties.push(i);[m
[32m+[m						[32mif ( !loop && !innerEquiv(a[i], b[i]) ) {[m
[32m+[m							[32meq = false;[m
[32m+[m							[32mbreak;[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32mparents.pop();[m
[32m+[m					[32mparentsB.pop();[m
[32m+[m					[32mcallers.pop(); // unstack, we are done[m
[32m+[m
[32m+[m					[32mfor ( i in b ) {[m
[32m+[m						[32mbProperties.push( i ); // collect b's properties[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Ensures identical properties name[m
[32m+[m					[32mreturn eq && innerEquiv( aProperties.sort(), bProperties.sort() );[m
[32m+[m				[32m}[m
[32m+[m			[32m};[m
[32m+[m		[32m}());[m
[32m+[m
[32m+[m	[32minnerEquiv = function() { // can take multiple arguments[m
[32m+[m		[32mvar args = [].slice.apply( arguments );[m
[32m+[m		[32mif ( args.length < 2 ) {[m
[32m+[m			[32mreturn true; // end transition[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn (function( a, b ) {[m
[32m+[m			[32mif ( a === b ) {[m
[32m+[m				[32mreturn true; // catch the most you can[m
[32m+[m			[32m} else if ( a === null || b === null || typeof a === "undefined" ||[m
[32m+[m					[32mtypeof b === "undefined" ||[m
[32m+[m					[32mQUnit.objectType(a) !== QUnit.objectType(b) ) {[m
[32m+[m				[32mreturn false; // don't lose time with error prone cases[m
[32m+[m			[32m} else {[m
[32m+[m				[32mreturn bindCallbacks(a, callbacks, [ b, a ]);[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// apply transition with (1..n) arguments[m
[32m+[m		[32m}( args[0], args[1] ) && innerEquiv.apply( this, args.splice(1, args.length - 1 )) );[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32mreturn innerEquiv;[m
[32m+[m[32m}());[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * jsDump Copyright (c) 2008 Ariel Flesler - aflesler(at)gmail(dot)com |[m
[32m+[m[32m * http://flesler.blogspot.com Licensed under BSD[m
[32m+[m[32m * (http://www.opensource.org/licenses/bsd-license.php) Date: 5/15/2008[m
[32m+[m[32m *[m
[32m+[m[32m * @projectDescription Advanced and extensible data dumping for Javascript.[m
[32m+[m[32m * @version 1.0.0[m
[32m+[m[32m * @author Ariel Flesler[m
[32m+[m[32m * @link {http://flesler.blogspot.com/2008/05/jsdump-pretty-dump-of-any-javascript.html}[m
[32m+[m[32m */[m
[32m+[m[32mQUnit.jsDump = (function() {[m
[32m+[m	[32mfunction quote( str ) {[m
[32m+[m		[32mreturn "\"" + str.toString().replace( /"/g, "\\\"" ) + "\"";[m
[32m+[m	[32m}[m
[32m+[m	[32mfunction literal( o ) {[m
[32m+[m		[32mreturn o + "";[m
[32m+[m	[32m}[m
[32m+[m	[32mfunction join( pre, arr, post ) {[m
[32m+[m		[32mvar s = jsDump.separator(),[m
[32m+[m			[32mbase = jsDump.indent(),[m
[32m+[m			[32minner = jsDump.indent(1);[m
[32m+[m		[32mif ( arr.join ) {[m
[32m+[m			[32marr = arr.join( "," + s + inner );[m
[32m+[m		[32m}[m
[32m+[m		[32mif ( !arr ) {[m
[32m+[m			[32mreturn pre + post;[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn [ pre, inner + arr, base + post ].join(s);[m
[32m+[m	[32m}[m
[32m+[m	[32mfunction array( arr, stack ) {[m
[32m+[m		[32mvar i = arr.length, ret = new Array(i);[m
[32m+[m		[32mthis.up();[m
[32m+[m		[32mwhile ( i-- ) {[m
[32m+[m			[32mret[i] = this.parse( arr[i] , undefined , stack);[m
[32m+[m		[32m}[m
[32m+[m		[32mthis.down();[m
[32m+[m		[32mreturn join( "[", ret, "]" );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mvar reName = /^function (\w+)/,[m
[32m+[m		[32mjsDump = {[m
[32m+[m			[32m// type is used mostly internally, you can fix a (custom)type in advance[m
[32m+[m			[32mparse: function( obj, type, stack ) {[m
[32m+[m				[32mstack = stack || [ ];[m
[32m+[m				[32mvar inStack, res,[m
[32m+[m					[32mparser = this.parsers[ type || this.typeOf(obj) ];[m
[32m+[m
[32m+[m				[32mtype = typeof parser;[m
[32m+[m				[32minStack = inArray( obj, stack );[m
[32m+[m
[32m+[m				[32mif ( inStack !== -1 ) {[m
[32m+[m					[32mreturn "recursion(" + (inStack - stack.length) + ")";[m
[32m+[m				[32m}[m
[32m+[m				[32mif ( type === "function" )  {[m
[32m+[m					[32mstack.push( obj );[m
[32m+[m					[32mres = parser.call( this, obj, stack );[m
[32m+[m					[32mstack.pop();[m
[32m+[m					[32mreturn res;[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn ( type === "string" ) ? parser : this.parsers.error;[m
[32m+[m			[32m},[m
[32m+[m			[32mtypeOf: function( obj ) {[m
[32m+[m				[32mvar type;[m
[32m+[m				[32mif ( obj === null ) {[m
[32m+[m					[32mtype = "null";[m
[32m+[m				[32m} else if ( typeof obj === "undefined" ) {[m
[32m+[m					[32mtype = "undefined";[m
[32m+[m				[32m} else if ( QUnit.is( "regexp", obj) ) {[m
[32m+[m					[32mtype = "regexp";[m
[32m+[m				[32m} else if ( QUnit.is( "date", obj) ) {[m
[32m+[m					[32mtype = "date";[m
[32m+[m				[32m} else if ( QUnit.is( "function", obj) ) {[m
[32m+[m					[32mtype = "function";[m
[32m+[m				[32m} else if ( typeof obj.setInterval !== undefined && typeof obj.document !== "undefined" && typeof obj.nodeType === "undefined" ) {[m
[32m+[m					[32mtype = "window";[m
[32m+[m				[32m} else if ( obj.nodeType === 9 ) {[m
[32m+[m					[32mtype = "document";[m
[32m+[m				[32m} else if ( obj.nodeType ) {[m
[32m+[m					[32mtype = "node";[m
[32m+[m				[32m} else if ([m
[32m+[m					[32m// native arrays[m
[32m+[m					[32mtoString.call( obj ) === "[object Array]" ||[m
[32m+[m					[32m// NodeList objects[m
[32m+[m					[32m( typeof obj.length === "number" && typeof obj.item !== "undefined" && ( obj.length ? obj.item(0) === obj[0] : ( obj.item( 0 ) === null && typeof obj[0] === "undefined" ) ) )[m
[32m+[m				[32m) {[m
[32m+[m					[32mtype = "array";[m
[32m+[m				[32m} else if ( obj.constructor === Error.prototype.constructor ) {[m
[32m+[m					[32mtype = "error";[m
[32m+[m				[32m} else {[m
[32m+[m					[32mtype = typeof obj;[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn type;[m
[32m+[m			[32m},[m
[32m+[m			[32mseparator: function() {[m
[32m+[m				[32mreturn this.multiline ?	this.HTML ? "<br />" : "\n" : this.HTML ? "&nbsp;" : " ";[m
[32m+[m			[32m},[m
[32m+[m			[32m// extra can be a number, shortcut for increasing-calling-decreasing[m
[32m+[m			[32mindent: function( extra ) {[m
[32m+[m				[32mif ( !this.multiline ) {[m
[32m+[m					[32mreturn "";[m
[32m+[m				[32m}[m
[32m+[m				[32mvar chr = this.indentChar;[m
[32m+[m				[32mif ( this.HTML ) {[m
[32m+[m					[32mchr = chr.replace( /\t/g, "   " ).replace( / /g, "&nbsp;" );[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn new Array( this.depth + ( extra || 0 ) ).join(chr);[m
[32m+[m			[32m},[m
[32m+[m			[32mup: function( a ) {[m
[32m+[m				[32mthis.depth += a || 1;[m
[32m+[m			[32m},[m
[32m+[m			[32mdown: function( a ) {[m
[32m+[m				[32mthis.depth -= a || 1;[m
[32m+[m			[32m},[m
[32m+[m			[32msetParser: function( name, parser ) {[m
[32m+[m				[32mthis.parsers[name] = parser;[m
[32m+[m			[32m},[m
[32m+[m			[32m// The next 3 are exposed so you can use them[m
[32m+[m			[32mquote: quote,[m
[32m+[m			[32mliteral: literal,[m
[32m+[m			[32mjoin: join,[m
[32m+[m			[32m//[m
[32m+[m			[32mdepth: 1,[m
[32m+[m			[32m// This is the list of parsers, to modify them, use jsDump.setParser[m
[32m+[m			[32mparsers: {[m
[32m+[m				[32mwindow: "[Window]",[m
[32m+[m				[32mdocument: "[Document]",[m
[32m+[m				[32merror: function(error) {[m
[32m+[m					[32mreturn "Error(\"" + error.message + "\")";[m
[32m+[m				[32m},[m
[32m+[m				[32munknown: "[Unknown]",[m
[32m+[m				[32m"null": "null",[m
[32m+[m				[32m"undefined": "undefined",[m
[32m+[m				[32m"function": function( fn ) {[m
[32m+[m					[32mvar ret = "function",[m
[32m+[m						[32m// functions never have name in IE[m
[32m+[m						[32mname = "name" in fn ? fn.name : (reName.exec(fn) || [])[1];[m
[32m+[m
[32m+[m					[32mif ( name ) {[m
[32m+[m						[32mret += " " + name;[m
[32m+[m					[32m}[m
[32m+[m					[32mret += "( ";[m
[32m+[m
[32m+[m					[32mret = [ ret, QUnit.jsDump.parse( fn, "functionArgs" ), "){" ].join( "" );[m
[32m+[m					[32mreturn join( ret, QUnit.jsDump.parse(fn,"functionCode" ), "}" );[m
[32m+[m				[32m},[m
[32m+[m				[32marray: array,[m
[32m+[m				[32mnodelist: array,[m
[32m+[m				[32m"arguments": array,[m
[32m+[m				[32mobject: function( map, stack ) {[m
[32m+[m					[32m/*jshint forin:false */[m
[32m+[m					[32mvar ret = [ ], keys, key, val, i;[m
[32m+[m					[32mQUnit.jsDump.up();[m
[32m+[m					[32mkeys = [];[m
[32m+[m					[32mfor ( key in map ) {[m
[32m+[m						[32mkeys.push( key );[m
[32m+[m					[32m}[m
[32m+[m					[32mkeys.sort();[m
[32m+[m					[32mfor ( i = 0; i < keys.length; i++ ) {[m
[32m+[m						[32mkey = keys[ i ];[m
[32m+[m						[32mval = map[ key ];[m
[32m+[m						[32mret.push( QUnit.jsDump.parse( key, "key" ) + ": " + QUnit.jsDump.parse( val, undefined, stack ) );[m
[32m+[m					[32m}[m
[32m+[m					[32mQUnit.jsDump.down();[m
[32m+[m					[32mreturn join( "{", ret, "}" );[m
[32m+[m				[32m},[m
[32m+[m				[32mnode: function( node ) {[m
[32m+[m					[32mvar len, i, val,[m
[32m+[m						[32mopen = QUnit.jsDump.HTML ? "&lt;" : "<",[m
[32m+[m						[32mclose = QUnit.jsDump.HTML ? "&gt;" : ">",[m
[32m+[m						[32mtag = node.nodeName.toLowerCase(),[m
[32m+[m						[32mret = open + tag,[m
[32m+[m						[32mattrs = node.attributes;[m
[32m+[m
[32m+[m					[32mif ( attrs ) {[m
[32m+[m						[32mfor ( i = 0, len = attrs.length; i < len; i++ ) {[m
[32m+[m							[32mval = attrs[i].nodeValue;[m
[32m+[m							[32m// IE6 includes all attributes in .attributes, even ones not explicitly set.[m
[32m+[m							[32m// Those have values like undefined, null, 0, false, "" or "inherit".[m
[32m+[m							[32mif ( val && val !== "inherit" ) {[m
[32m+[m								[32mret += " " + attrs[i].nodeName + "=" + QUnit.jsDump.parse( val, "attribute" );[m
[32m+[m							[32m}[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m					[32mret += close;[m
[32m+[m
[32m+[m					[32m// Show content of TextNode or CDATASection[m
[32m+[m					[32mif ( node.nodeType === 3 || node.nodeType === 4 ) {[m
[32m+[m						[32mret += node.nodeValue;[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32mreturn ret + open + "/" + tag + close;[m
[32m+[m				[32m},[m
[32m+[m				[32m// function calls it internally, it's the arguments part of the function[m
[32m+[m				[32mfunctionArgs: function( fn ) {[m
[32m+[m					[32mvar args,[m
[32m+[m						[32ml = fn.length;[m
[32m+[m
[32m+[m					[32mif ( !l ) {[m
[32m+[m						[32mreturn "";[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32margs = new Array(l);[m
[32m+[m					[32mwhile ( l-- ) {[m
[32m+[m						[32m// 97 is 'a'[m
[32m+[m						[32margs[l] = String.fromCharCode(97+l);[m
[32m+[m					[32m}[m
[32m+[m					[32mreturn " " + args.join( ", " ) + " ";[m
[32m+[m				[32m},[m
[32m+[m				[32m// object calls it internally, the key part of an item in a map[m
[32m+[m				[32mkey: quote,[m
[32m+[m				[32m// function calls it internally, it's the content of the function[m
[32m+[m				[32mfunctionCode: "[code]",[m
[32m+[m				[32m// node calls it internally, it's an html attribute value[m
[32m+[m				[32mattribute: quote,[m
[32m+[m				[32mstring: quote,[m
[32m+[m				[32mdate: quote,[m
[32m+[m				[32mregexp: literal,[m
[32m+[m				[32mnumber: literal,[m
[32m+[m				[32m"boolean": literal[m
[32m+[m			[32m},[m
[32m+[m			[32m// if true, entities are escaped ( <, >, \t, space and \n )[m
[32m+[m			[32mHTML: false,[m
[32m+[m			[32m// indentation unit[m
[32m+[m			[32mindentChar: "  ",[m
[32m+[m			[32m// if true, items in a collection, are separated by a \n, else just a space.[m
[32m+[m			[32mmultiline: true[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m	[32mreturn jsDump;[m
[32m+[m[32m}());[m
[32m+[m
[32m+[m[32m// from jquery.js[m
[32m+[m[32mfunction inArray( elem, array ) {[m
[32m+[m	[32mif ( array.indexOf ) {[m
[32m+[m		[32mreturn array.indexOf( elem );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfor ( var i = 0, length = array.length; i < length; i++ ) {[m
[32m+[m		[32mif ( array[ i ] === elem ) {[m
[32m+[m			[32mreturn i;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn -1;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m * Javascript Diff Algorithm[m
[32m+[m[32m *  By John Resig (http://ejohn.org/)[m
[32m+[m[32m *  Modified by Chu Alan "sprite"[m
[32m+[m[32m *[m
[32m+[m[32m * Released under the MIT license.[m
[32m+[m[32m *[m
[32m+[m[32m * More Info:[m
[32m+[m[32m *  http://ejohn.org/projects/javascript-diff-algorithm/[m
[32m+[m[32m *[m
[32m+[m[32m * Usage: QUnit.diff(expected, actual)[m
[32m+[m[32m *[m
[32m+[m[32m * QUnit.diff( "the quick brown fox jumped over", "the quick fox jumps over" ) == "the  quick <del>brown </del> fox <del>jumped </del><ins>jumps </ins> over"[m
[32m+[m[32m */[m
[32m+[m[32mQUnit.diff = (function() {[m
[32m+[m	[32m/*jshint eqeqeq:false, eqnull:true */[m
[32m+[m	[32mfunction diff( o, n ) {[m
[32m+[m		[32mvar i,[m
[32m+[m			[32mns = {},[m
[32m+[m			[32mos = {};[m
[32m+[m
[32m+[m		[32mfor ( i = 0; i < n.length; i++ ) {[m
[32m+[m			[32mif ( !hasOwn.call( ns, n[i] ) ) {[m
[32m+[m				[32mns[ n[i] ] = {[m
[32m+[m					[32mrows: [],[m
[32m+[m					[32mo: null[m
[32m+[m				[32m};[m
[32m+[m			[32m}[m
[32m+[m			[32mns[ n[i] ].rows.push( i );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mfor ( i = 0; i < o.length; i++ ) {[m
[32m+[m			[32mif ( !hasOwn.call( os, o[i] ) ) {[m
[32m+[m				[32mos[ o[i] ] = {[m
[32m+[m					[32mrows: [],[m
[32m+[m					[32mn: null[m
[32m+[m				[32m};[m
[32m+[m			[32m}[m
[32m+[m			[32mos[ o[i] ].rows.push( i );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mfor ( i in ns ) {[m
[32m+[m			[32mif ( hasOwn.call( ns, i ) ) {[m
[32m+[m				[32mif ( ns[i].rows.length === 1 && hasOwn.call( os, i ) && os[i].rows.length === 1 ) {[m
[32m+[m					[32mn[ ns[i].rows[0] ] = {[m
[32m+[m						[32mtext: n[ ns[i].rows[0] ],[m
[32m+[m						[32mrow: os[i].rows[0][m
[32m+[m					[32m};[m
[32m+[m					[32mo[ os[i].rows[0] ] = {[m
[32m+[m						[32mtext: o[ os[i].rows[0] ],[m
[32m+[m						[32mrow: ns[i].rows[0][m
[32m+[m					[32m};[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mfor ( i = 0; i < n.length - 1; i++ ) {[m
[32m+[m			[32mif ( n[i].text != null && n[ i + 1 ].text == null && n[i].row + 1 < o.length && o[ n[i].row + 1 ].text == null &&[m
[32m+[m						[32mn[ i + 1 ] == o[ n[i].row + 1 ] ) {[m
[32m+[m
[32m+[m				[32mn[ i + 1 ] = {[m
[32m+[m					[32mtext: n[ i + 1 ],[m
[32m+[m					[32mrow: n[i].row + 1[m
[32m+[m				[32m};[m
[32m+[m				[32mo[ n[i].row + 1 ] = {[m
[32m+[m					[32mtext: o[ n[i].row + 1 ],[m
[32m+[m					[32mrow: i + 1[m
[32m+[m				[32m};[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mfor ( i = n.length - 1; i > 0; i-- ) {[m
[32m+[m			[32mif ( n[i].text != null && n[ i - 1 ].text == null && n[i].row > 0 && o[ n[i].row - 1 ].text == null &&[m
[32m+[m						[32mn[ i - 1 ] == o[ n[i].row - 1 ]) {[m
[32m+[m
[32m+[m				[32mn[ i - 1 ] = {[m
[32m+[m					[32mtext: n[ i - 1 ],[m
[32m+[m					[32mrow: n[i].row - 1[m
[32m+[m				[32m};[m
[32m+[m				[32mo[ n[i].row - 1 ] = {[m
[32m+[m					[32mtext: o[ n[i].row - 1 ],[m
[32m+[m					[32mrow: i - 1[m
[32m+[m				[32m};[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn {[m
[32m+[m			[32mo: o,[m
[32m+[m			[32mn: n[m
[32m+[m		[32m};[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn function( o, n ) {[m
[32m+[m		[32mo = o.replace( /\s+$/, "" );[m
[32m+[m		[32mn = n.replace( /\s+$/, "" );[m
[32m+[m
[32m+[m		[32mvar i, pre,[m
[32m+[m			[32mstr = "",[m
[32m+[m			[32mout = diff( o === "" ? [] : o.split(/\s+/), n === "" ? [] : n.split(/\s+/) ),[m
[32m+[m			[32moSpace = o.match(/\s+/g),[m
[32m+[m			[32mnSpace = n.match(/\s+/g);[m
[32m+[m
[32m+[m		[32mif ( oSpace == null ) {[m
[32m+[m			[32moSpace = [ " " ];[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32moSpace.push( " " );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( nSpace == null ) {[m
[32m+[m			[32mnSpace = [ " " ];[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mnSpace.push( " " );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( out.n.length === 0 ) {[m
[32m+[m			[32mfor ( i = 0; i < out.o.length; i++ ) {[m
[32m+[m				[32mstr += "<del>" + out.o[i] + oSpace[i] + "</del>";[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32melse {[m
[32m+[m			[32mif ( out.n[0].text == null ) {[m
[32m+[m				[32mfor ( n = 0; n < out.o.length && out.o[n].text == null; n++ ) {[m
[32m+[m					[32mstr += "<del>" + out.o[n] + oSpace[n] + "</del>";[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mfor ( i = 0; i < out.n.length; i++ ) {[m
[32m+[m				[32mif (out.n[i].text == null) {[m
[32m+[m					[32mstr += "<ins>" + out.n[i] + nSpace[i] + "</ins>";[m
[32m+[m				[32m}[m
[32m+[m				[32melse {[m
[32m+[m					[32m// `pre` initialized at top of scope[m
[32m+[m					[32mpre = "";[m
[32m+[m
[32m+[m					[32mfor ( n = out.n[i].row + 1; n < out.o.length && out.o[n].text == null; n++ ) {[m
[32m+[m						[32mpre += "<del>" + out.o[n] + oSpace[n] + "</del>";[m
[32m+[m					[32m}[m
[32m+[m					[32mstr += " " + out.n[i].text + nSpace[i] + pre;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn str;[m
[32m+[m	[32m};[m
[32m+[m[32m}());[m
[32m+[m
[32m+[m[32m// for CommonJS environments, export everything[m
[32m+[m[32mif ( typeof exports !== "undefined" ) {[m
[32m+[m	[32mextend( exports, QUnit.constructor.prototype );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// get at whatever the global object is, like window in browsers[m
[32m+[m[32m}( (function() {return this;}.call()) ));[m
\ No newline at end of file[m
