[32m+[m[32m/*********************************************[m
[32m+[m[32m * Mixins for readability of transitions[m
[32m+[m[32m *********************************************/[m
[32m+[m
[32m+[m[32m@mixin transition-global($style) {[m
[32m+[m	[32m.reveal .slides section[data-transition=#{$style}],[m
[32m+[m	[32m.reveal.#{$style} .slides section:not([data-transition]) {[m
[32m+[m		[32m@content;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m[32m@mixin transition-horizontal-past($style) {[m
[32m+[m	[32m.reveal .slides>section[data-transition=#{$style}].past,[m
[32m+[m	[32m.reveal .slides>section[data-transition~=#{$style}-out].past,[m
[32m+[m	[32m.reveal.#{$style} .slides>section:not([data-transition]).past {[m
[32m+[m		[32m@content;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m[32m@mixin transition-horizontal-future($style) {[m
[32m+[m	[32m.reveal .slides>section[data-transition=#{$style}].future,[m
[32m+[m	[32m.reveal .slides>section[data-transition~=#{$style}-in].future,[m
[32m+[m	[32m.reveal.#{$style} .slides>section:not([data-transition]).future {[m
[32m+[m		[32m@content;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m@mixin transition-vertical-past($style) {[m
[32m+[m	[32m.reveal .slides>section>section[data-transition=#{$style}].past,[m
[32m+[m	[32m.reveal .slides>section>section[data-transition~=#{$style}-out].past,[m
[32m+[m	[32m.reveal.#{$style} .slides>section>section:not([data-transition]).past {[m
[32m+[m		[32m@content;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m[32m@mixin transition-vertical-future($style) {[m
[32m+[m	[32m.reveal .slides>section>section[data-transition=#{$style}].future,[m
[32m+[m	[32m.reveal .slides>section>section[data-transition~=#{$style}-in].future,[m
[32m+[m	[32m.reveal.#{$style} .slides>section>section:not([data-transition]).future {[m
[32m+[m		[32m@content;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*********************************************[m
[32m+[m[32m * SLIDE TRANSITION[m
[32m+[m[32m * Aliased 'linear' for backwards compatibility[m
[32m+[m[32m *********************************************/[m
[32m+[m
[32m+[m[32m@each $stylename in slide, linear {[m
[32m+[m	[32m.reveal.#{$stylename} section {[m
[32m+[m		[32mbackface-visibility: hidden;[m
[32m+[m	[32m}[m
[32m+[m	[32m@include transition-horizontal-past(#{$stylename}) {[m
[32m+[m		[32mtransform: translate(-150%, 0);[m
[32m+[m	[32m}[m
[32m+[m	[32m@include transition-horizontal-future(#{$stylename}) {[m
[32m+[m		[32mtransform: translate(150%, 0);[m
[32m+[m	[32m}[m
[32m+[m	[32m@include transition-vertical-past(#{$stylename}) {[m
[32m+[m		[32mtransform: translate(0, -150%);[m
[32m+[m	[32m}[m
[32m+[m	[32m@include transition-vertical-future(#{$stylename}) {[m
[32m+[m		[32mtransform: translate(0, 150%);[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*********************************************[m
[32m+[m[32m * CONVEX TRANSITION[m
[32m+[m[32m * Aliased 'default' for backwards compatibility[m
[32m+[m[32m *********************************************/[m
[32m+[m
[32m+[m[32m@each $stylename in default, convex {[m
[32m+[m	[32m@include transition-horizontal-past(#{$stylename}) {[m
[32m+[m		[32mtransform: translate3d(-100%, 0, 0) rotateY(-90deg) translate3d(-100%, 0, 0);[m
[32m+[m	[32m}[m
[32m+[m	[32m@include transition-horizontal-future(#{$stylename}) {[m
[32m+[m		[32mtransform: translate3d(100%, 0, 0) rotateY(90deg) translate3d(100%, 0, 0);[m
[32m+[m	[32m}[m
[32m+[m	[32m@include transition-vertical-past(#{$stylename}) {[m
[32m+[m		[32mtransform: translate3d(0, -300px, 0) rotateX(70deg) translate3d(0, -300px, 0);[m
[32m+[m	[32m}[m
[32m+[m	[32m@include transition-vertical-future(#{$stylename}) {[m
[32m+[m		[32mtransform: translate3d(0, 300px, 0) rotateX(-70deg) translate3d(0, 300px, 0);[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*********************************************[m
[32m+[m[32m * CONCAVE TRANSITION[m
[32m+[m[32m *********************************************/[m
[32m+[m
[32m+[m[32m@include transition-horizontal-past(concave) {[m
[32m+[m	[32mtransform: translate3d(-100%, 0, 0) rotateY(90deg) translate3d(-100%, 0, 0);[m
[32m+[m[32m}[m
[32m+[m[32m@include transition-horizontal-future(concave) {[m
