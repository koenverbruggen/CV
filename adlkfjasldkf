[32m+[m[32m/* Default Print Stylesheet Template[m
[32m+[m[32m   by Rob Glazebrook of CSSnewbie.com[m
[32m+[m[32m   Last Updated: June 4, 2008[m
[32m+[m
[32m+[m[32m   Feel free (nay, compelled) to edit, append, and[m
[32m+[m[32m   manipulate this file as you see fit. */[m
[32m+[m
[32m+[m
[32m+[m[32m@media print {[m
[32m+[m
[32m+[m	[32m/* SECTION 1: Set default width, margin, float, and[m
[32m+[m	[32m   background. This prevents elements from extending[m
[32m+[m	[32m   beyond the edge of the printed page, and prevents[m
[32m+[m	[32m   unnecessary background images from printing */[m
[32m+[m	[32mhtml {[m
[32m+[m		[32mbackground: #fff;[m
[32m+[m		[32mwidth: auto;[m
[32m+[m		[32mheight: auto;[m
[32m+[m		[32moverflow: visible;[m
[32m+[m	[32m}[m
[32m+[m	[32mbody {[m
[32m+[m		[32mbackground: #fff;[m
[32m+[m		[32mfont-size: 20pt;[m
[32m+[m		[32mwidth: auto;[m
[32m+[m		[32mheight: auto;[m
[32m+[m		[32mborder: 0;[m
[32m+[m		[32mmargin: 0 5%;[m
[32m+[m		[32mpadding: 0;[m
[32m+[m		[32moverflow: visible;[m
[32m+[m		[32mfloat: none !important;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/* SECTION 2: Remove any elements not needed in print.[m
[32m+[m	[32m   This would include navigation, ads, sidebars, etc. */[m
[32m+[m	[32m.nestedarrow,[m
[32m+[m	[32m.controls,[m
[32m+[m	[32m.fork-reveal,[m
[32m+[m	[32m.share-reveal,[m
[32m+[m	[32m.state-background,[m
[32m+[m	[32m.reveal .progress,[m
[32m+[m	[32m.reveal .backgrounds {[m
[32m+[m		[32mdisplay: none !important;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/* SECTION 3: Set body font face, size, and color.[m
[32m+[m	[32m   Consider using a serif font for readability. */[m
[32m+[m	[32mbody, p, td, li, div {[m
[32m+[m		[32mfont-size: 20pt!important;[m
[32m+[m		[32mfont-family: Georgia, "Times New Roman", Times, serif !important;[m
[32m+[m		[32mcolor: #000;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/* SECTION 4: Set heading font face, sizes, and color.[m
[32m+[m	[32m   Differentiate your headings from your body text.[m
[32m+[m	[32m   Perhaps use a large sans-serif for distinction. */[m
[32m+[m	[32mh1,h2,h3,h4,h5,h6 {[m
[32m+[m		[32mcolor: #000!important;[m
[32m+[m		[32mheight: auto;[m
[32m+[m		[32mline-height: normal;[m
[32m+[m		[32mfont-family: Georgia, "Times New Roman", Times, serif !important;[m
[32m+[m		[32mtext-shadow: 0 0 0 #000 !important;[m
[32m+[m		[32mtext-align: left;[m
[32m+[m		[32mletter-spacing: normal;[m
[32m+[m	[32m}[m
[32m+[m	[32m/* Need to reduce the size of the fonts for printing */[m
[32m+[m	[32mh1 { font-size: 28pt !important;  }[m
[32m+[m	[32mh2 { font-size: 24pt !important; }[m
[32m+[m	[32mh3 { font-size: 22pt !important; }[m
[32m+[m	[32mh4 { font-size: 22pt !important; font-variant: small-caps; }[m
[32m+[m	[32mh5 { font-size: 21pt !important; }[m
[32m+[m	[32mh6 { font-size: 20pt !important; font-style: italic; }[m
[32m+[m
[32m+[m	[32m/* SECTION 5: Make hyperlinks more usable.[m
[32m+[m	[32m   Ensure links are underlined, and consider appending[m
[32m+[m	[32m   the URL to the end of the link for usability. */[m
[32m+[m	[32ma:link,[m
[32m+[m	[32ma:visited {[m
[32m+[m		[32mcolor: #000 !important;[m
[32m+[m		[32mfont-weight: bold;[m
[32m+[m		[32mtext-decoration: underline;[m
[32m+[m	[32m}[m
[32m+[m	[32m/*[m
[32m+[m	[32m.reveal a:link:after,[m
[32m+[m	[32m.reveal a:visited:after {[m
[32m+[m		[32mcontent: " (" attr(href) ") ";[m
[32m+[m		[32mcolor: #222 !important;[m
[32m+[m		[32mfont-size: 90%;[m
[32m+[m	[32m}[m
[32m+[m	[32m*/[m
[32m+[m
[32m+[m
[32m+[m	[32m/* SECTION 6: more reveal.js specific additions by @skypanther */[m
[32m+[m	[32mul, ol, div, p {[m
[32m+[m		[32mvisibility: visible;[m
[32m+[m		[32mposition: static;[m
[32m+[m		[32mwidth: auto;[m
[32m+[m		[32mheight: auto;[m
[32m+[m		[32mdisplay: block;[m
[32m+[m		[32moverflow: visible;[m
[32m+[m		[32mmargin: 0;[m
[32m+[m		[32mtext-align: left !important;[m
[32m+[m	[32m}[m
[32m+[m	[32m.reveal pre,[m
[32m+[m	[32m.reveal table {[m
[32m+[m		[32mmargin-left: 0;[m
[32m+[m		[32mmargin-right: 0;[m
[32m+[m	[32m}[m
[32m+[m	[32m.reveal pre code {[m
[32m+[m		[32mpadding: 20px;[m
[32m+[m		[32mborder: 1px solid #ddd;[m
[32m+[m	[32m}[m
[32m+[m	[32m.reveal blockquote {[m
[32m+[m		[32mmargin: 20px 0;[m
[32m+[m	[32m}[m
[32m+[m	[32m.reveal .slides {[m
[32m+[m		[32mposition: static !important;[m
[32m+[m		[32mwidth: auto !important;[m
[32m+[m		[32mheight: auto !important;[m
[32m+[m
[32m+[m		[32mleft: 0 !important;[m
[32m+[m		[32mtop: 0 !important;[m
[32m+[m		[32mmargin-left: 0 !important;[m
[32m+[m		[32mmargin-top: 0 !important;[m
[32m+[m		[32mpadding: 0 !important;[m
[32m+[m		[32mzoom: 1 !important;[m
[32m+[m
[32m+[m		[32moverflow: visible !important;[m
[32m+[m		[32mdisplay: block !important;[m
[32m+[m
[32m+[m		[32mtext-align: left !important;[m
[32m+[m		[32m-webkit-perspective: none;[m
[32m+[m		[32m   -moz-perspective: none;[m
[32m+[m		[32m    -ms-perspective: none;[m
[32m+[m		[32m        perspective: none;[m
[32m+[m
[32m+[m		[32m-webkit-perspective-origin: 50% 50%;[m
[32m+[m		[32m   -moz-perspective-origin: 50% 50%;[m
[32m+[m		[32m    -ms-perspective-origin: 50% 50%;[m
[32m+[m		[32m        perspective-origin: 50% 50%;[m
[32m+[m	[32m}[m
[32m+[m	[32m.reveal .slides section {[m
[32m+[m		[32mvisibility: visible !important;[m
[32m+[m		[32mposition: static !important;[m
[32m+[m		[32mwidth: auto !important;[m
[32m+[m		[32mheight: auto !important;[m
[32m+[m		[32mdisplay: block !important;[m
[32m+[m		[32moverflow: visible !important;[m
[32m+[m
[32m+[m		[32mleft: 0 !important;[m
[32m+[m		[32mtop: 0 !important;[m
[32m+[m		[32mmargin-left: 0 !important;[m
[32m+[m		[32mmargin-top: 0 !important;[m
[32m+[m		[32mpadding: 60px 20px !important;[m
[32m+[m		[32mz-index: auto !important;[m
[32m+[m
[32m+[m		[32mopacity: 1 !important;[m
[32m+[m
[32m+[m		[32mpage-break-after: always !important;[m
[32m+[m
[32m+[m		[32m-webkit-transform-style: flat !important;[m
[32m+[m		[32m   -moz-transform-style: flat !important;[m
[32m+[m		[32m    -ms-transform-style: flat !important;[m
[32m+[m		[32m        transform-style: flat !important;[m
