[32m+[m
[32m+[m[32m// These tests expect the DOM to contain a presentation[m
[32m+[m[32m// with the following slide structure:[m
[32m+[m[32m//[m
[32m+[m[32m// 1[m
[32m+[m[32m// 2 - Three sub-slides[m
[32m+[m[32m// 3 - Three fragment elements[m
[32m+[m[32m// 3 - Two fragments with same data-fragment-index[m
[32m+[m[32m// 4[m
[32m+[m
[32m+[m
[32m+[m[32mReveal.addEventListener( 'ready', function() {[m
[32m+[m
[32m+[m	[32m// ---------------------------------------------------------------[m
[32m+[m	[32m// DOM TESTS[m
[32m+[m
[32m+[m	[32mQUnit.module( 'DOM' );[m
[32m+[m
[32m+[m	[32mtest( 'Initial slides classes', function() {[m
[32m+[m		[32mvar horizontalSlides = document.querySelectorAll( '.reveal .slides>section' )[m
[32m+[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides section.past' ).length, 0, 'no .past slides' );[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides section.present' ).length, 1, 'one .present slide' );[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides>section.future' ).length, horizontalSlides.length - 1, 'remaining horizontal slides are .future' );[m
[32m+[m
[32m+[m		[32mstrictEqual( document.querySelectorAll( '.reveal .slides section.stack' ).length, 2, 'two .stacks' );[m
[32m+[m
[32m+[m		[32mok( document.querySelectorAll( '.reveal .slides section.stack' )[0].querySelectorAll( '.future' ).length > 0, 'vertical slides are given .future' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32m// ---------------------------------------------------------------[m
[32m+[m	[32m// API TESTS[m
[32m+[m
[32m+[m	[32mQUnit.module( 'API' );[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.isReady', function() {[m
[32m+[m		[32mstrictEqual( Reveal.isReady(), true, 'returns true' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.isOverview', function() {[m
[32m+[m		[32mstrictEqual( Reveal.isOverview(), false, 'false by default' );[m
[32m+[m
[32m+[m		[32mReveal.toggleOverview();[m
[32m+[m		[32mstrictEqual( Reveal.isOverview(), true, 'true after toggling on' );[m
[32m+[m
[32m+[m		[32mReveal.toggleOverview();[m
[32m+[m		[32mstrictEqual( Reveal.isOverview(), false, 'false after toggling off' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.isPaused', function() {[m
[32m+[m		[32mstrictEqual( Reveal.isPaused(), false, 'false by default' );[m
[32m+[m
[32m+[m		[32mReveal.togglePause();[m
[32m+[m		[32mstrictEqual( Reveal.isPaused(), true, 'true after pausing' );[m
[32m+[m
[32m+[m		[32mReveal.togglePause();[m
[32m+[m		[32mstrictEqual( Reveal.isPaused(), false, 'false after resuming' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.isFirstSlide', function() {[m
[32m+[m		[32mReveal.slide( 0, 0 );[m
[32m+[m		[32mstrictEqual( Reveal.isFirstSlide(), true, 'true after Reveal.slide( 0, 0 )' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 1, 0 );[m
[32m+[m		[32mstrictEqual( Reveal.isFirstSlide(), false, 'false after Reveal.slide( 1, 0 )' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 0, 0 );[m
[32m+[m		[32mstrictEqual( Reveal.isFirstSlide(), true, 'true after Reveal.slide( 0, 0 )' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.isFirstSlide after vertical slide', function() {[m
[32m+[m		[32mReveal.slide( 1, 1 );[m
[32m+[m		[32mReveal.slide( 0, 0 );[m
[32m+[m		[32mstrictEqual( Reveal.isFirstSlide(), true, 'true after Reveal.slide( 1, 1 ) and then Reveal.slide( 0, 0 )' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.isLastSlide', function() {[m
[32m+[m		[32mReveal.slide( 0, 0 );[m
[32m+[m		[32mstrictEqual( Reveal.isLastSlide(), false, 'false after Reveal.slide( 0, 0 )' );[m
[32m+[m
[32m+[m		[32mvar lastSlideIndex = document.querySelectorAll( '.reveal .slides>section' ).length - 1;[m
[32m+[m
[32m+[m		[32mReveal.slide( lastSlideIndex, 0 );[m
[32m+[m		[32mstrictEqual( Reveal.isLastSlide(), true, 'true after Reveal.slide( '+ lastSlideIndex +', 0 )' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 0, 0 );[m
[32m+[m		[32mstrictEqual( Reveal.isLastSlide(), false, 'false after Reveal.slide( 0, 0 )' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.isLastSlide after vertical slide', function() {[m
[32m+[m		[32mvar lastSlideIndex = document.querySelectorAll( '.reveal .slides>section' ).length - 1;[m
[32m+[m
[32m+[m		[32mReveal.slide( 1, 1 );[m
[32m+[m		[32mReveal.slide( lastSlideIndex );[m
[32m+[m		[32mstrictEqual( Reveal.isLastSlide(), true, 'true after Reveal.slide( 1, 1 ) and then Reveal.slide( '+ lastSlideIndex +', 0 )' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.getTotalSlides', function() {[m
[32m+[m		[32mstrictEqual( Reveal.getTotalSlides(), 8, 'eight slides in total' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.getIndices', function() {[m
[32m+[m		[32mvar indices = Reveal.getIndices();[m
[32m+[m
[32m+[m		[32mok( indices.hasOwnProperty( 'h' ), 'h exists' );[m
[32m+[m		[32mok( indices.hasOwnProperty( 'v' ), 'v exists' );[m
[32m+[m		[32mok( indices.hasOwnProperty( 'f' ), 'f exists' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 1, 0 );[m
[32m+[m		[32mstrictEqual( Reveal.getIndices().h, 1, 'h 1' );[m
[32m+[m		[32mstrictEqual( Reveal.getIndices().v, 0, 'v 0' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 1, 2 );[m
[32m+[m		[32mstrictEqual( Reveal.getIndices().h, 1, 'h 1' );[m
[32m+[m		[32mstrictEqual( Reveal.getIndices().v, 2, 'v 2' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 0, 0 );[m
[32m+[m		[32mstrictEqual( Reveal.getIndices().h, 0, 'h 0' );[m
[32m+[m		[32mstrictEqual( Reveal.getIndices().v, 0, 'v 0' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.getSlide', function() {[m
[32m+[m		[32mequal( Reveal.getSlide( 0 ), document.querySelector( '.reveal .slides>section:first-child' ), 'gets correct first slide' );[m
[32m+[m		[32mequal( Reveal.getSlide( 1 ), document.querySelector( '.reveal .slides>section:nth-child(2)' ), 'no v index returns stack' );[m
[32m+[m		[32mequal( Reveal.getSlide( 1, 0 ), document.querySelector( '.reveal .slides>section:nth-child(2)>section:nth-child(1)' ), 'v index 0 returns first vertical child' );[m
[32m+[m		[32mequal( Reveal.getSlide( 1, 1 ), document.querySelector( '.reveal .slides>section:nth-child(2)>section:nth-child(2)' ), 'v index 1 returns second vertical child' );[m
[32m+[m
[32m+[m		[32mstrictEqual( Reveal.getSlide( 100 ), undefined, 'undefined when out of horizontal bounds' );[m
[32m+[m		[32mstrictEqual( Reveal.getSlide( 1, 100 ), undefined, 'undefined when out of vertical bounds' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.getSlideBackground', function() {[m
[32m+[m		[32mequal( Reveal.getSlideBackground( 0 ), document.querySelector( '.reveal .backgrounds>.slide-background:first-child' ), 'gets correct first background' );[m
[32m+[m		[32mequal( Reveal.getSlideBackground( 1 ), document.querySelector( '.reveal .backgrounds>.slide-background:nth-child(2)' ), 'no v index returns stack' );[m
[32m+[m		[32mequal( Reveal.getSlideBackground( 1, 0 ), document.querySelector( '.reveal .backgrounds>.slide-background:nth-child(2) .slide-background:nth-child(1)' ), 'v index 0 returns first vertical child' );[m
[32m+[m		[32mequal( Reveal.getSlideBackground( 1, 1 ), document.querySelector( '.reveal .backgrounds>.slide-background:nth-child(2) .slide-background:nth-child(2)' ), 'v index 1 returns second vertical child' );[m
[32m+[m
[32m+[m		[32mstrictEqual( Reveal.getSlideBackground( 100 ), undefined, 'undefined when out of horizontal bounds' );[m
[32m+[m		[32mstrictEqual( Reveal.getSlideBackground( 1, 100 ), undefined, 'undefined when out of vertical bounds' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.getSlideNotes', function() {[m
[32m+[m		[32mReveal.slide( 0, 0 );[m
[32m+[m		[32mok( Reveal.getSlideNotes() === 'speaker notes 1', 'works with <aside class="notes">' );[m
[32m+[m
[32m+[m		[32mReveal.slide( 1, 0 );[m
[32m+[m		[32mok( Reveal.getSlideNotes() === 'speaker notes 2', 'works with <section data-notes="">' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.getPreviousSlide/getCurrentSlide', function() {[m
[32m+[m		[32mReveal.slide( 0, 0 );[m
[32m+[m		[32mReveal.slide( 1, 0 );[m
[32m+[m
[32m+[m		[32mvar firstSlide = document.querySelector( '.reveal .slides>section:first-child' );[m
[32m+[m		[32mvar secondSlide = document.querySelector( '.reveal .slides>section:nth-child(2)>section' );[m
[32m+[m
[32m+[m		[32mequal( Reveal.getPreviousSlide(), firstSlide, 'previous is slide #0' );[m
[32m+[m		[32mequal( Reveal.getCurrentSlide(), secondSlide, 'current is slide #1' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.getProgress', function() {[m
[32m+[m		[32mReveal.slide( 0, 0 );[m
[32m+[m		[32mstrictEqual( Reveal.getProgress(), 0, 'progress is 0 on first slide' );[m
[32m+[m
[32m+[m		[32mvar lastSlideIndex = document.querySelectorAll( '.reveal .slides>section' ).length - 1;[m
[32m+[m
[32m+[m		[32mReveal.slide( lastSlideIndex, 0 );[m
[32m+[m		[32mstrictEqual( Reveal.getProgress(), 1, 'progress is 1 on last slide' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.getScale', function() {[m
[32m+[m		[32mok( typeof Reveal.getScale() === 'number', 'has scale' );[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32mtest( 'Reveal.getConfig', function() {[m
