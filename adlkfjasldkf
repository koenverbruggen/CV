[32m+[m[32m(function() {[m
[32m+[m	[32mvar multiplex = Reveal.getConfig().multiplex;[m
[32m+[m	[32mvar socketId = multiplex.id;[m
[32m+[m	[32mvar socket = io.connect(multiplex.url);[m
[32m+[m
[32m+[m	[32msocket.on(multiplex.id, function(data) {[m
[32m+[m		[32m// ignore data from sockets that aren't ours[m
[32m+[m		[32mif (data.socketId !== socketId) { return; }[m
[32m+[m		[32mif( window.location.host === 'localhost:1947' ) return;[m
[32m+[m
[32m+[m		[32mReveal.setState(data.state);[m
