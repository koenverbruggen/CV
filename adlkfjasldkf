[32m+[m[32m// START CUSTOM REVEAL.JS INTEGRATION[m
[32m+[m[32m(function() {[m
[32m+[m	[32mif( typeof window.addEventListener === 'function' ) {[m
[32m+[m		[32mvar hljs_nodes = document.querySelectorAll( 'pre code' );[m
[32m+[m
[32m+[m		[32mfor( var i = 0, len = hljs_nodes.length; i < len; i++ ) {[m
[32m+[m			[32mvar element = hljs_nodes[i];[m
[32m+[m
[32m+[m			[32m// trim whitespace if data-trim attribute is present[m
[32m+[m			[32mif( element.hasAttribute( 'data-trim' ) && typeof element.innerHTML.trim === 'function' ) {[m
[32m+[m				[32melement.innerHTML = element.innerHTML.trim();[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Now escape html unless prevented by author[m
[32m+[m			[32mif( ! element.hasAttribute( 'data-noescape' )) {[m
[32m+[m				[32melement.innerHTML = element.innerHTML.replace(/</g,"&lt;").replace(/>/g,"&gt;");[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// re-highlight when focus is lost (for edited code)[m
[32m+[m			[32melement.addEventListener( 'focusout', function( event ) {[m
[32m+[m				[32mhljs.highlightBlock( event.currentTarget );[m
[32m+[m			[32m}, false );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m})();[m
[32m+[m[32m// END CUSTOM REVEAL.JS INTEGRATION[m
[32m+[m
[32m+[m[32m// highlight.js v8.9.1 with support for all available languages[m
